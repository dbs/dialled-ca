//alert("page loaded");

jQuery('#detect-donation').click(function() {
	if (jQuery('input:radio[name=detect-donation-choose]:checked').val() == "No"){
		jQuery("#hide1").hide();
		jQuery("#hide2").hide();
		//jQuery("#hide3").hide();
    }
	else if (jQuery('input:radio[name=detect-donation-choose]:checked').val() == "Yes, as a gift in memory of" || jQuery('input:radio[name=detect-donation-choose]:checked').val() == "Oui, don à la mémoire de"){
		jQuery("#hide1").show();
		jQuery("#hide2").hide();
		//jQuery("#hide3").hide();
    }
	else if (jQuery('input:radio[name=detect-donation-choose]:checked').val() == "Yes, as a gift in honour/on behalf of" || jQuery('input:radio[name=detect-donation-choose]:checked').val() == "Oui, don en l'honneur ou au non"){
		jQuery("#hide2").show();
        jQuery("#hide1").hide();
		//jQuery("#hide3").hide();
	}
	else{
		jQuery("#hide2").show();
        jQuery("#hide1").show();
	}
});
		//Display if select postal-email option
jQuery('#postal-mail').click(function() {
	if (jQuery('input:radio[name=notify]:checked').val() == "I will notify this person on my own" || jQuery('input:radio[name=notify]:checked').val() == "J'informerai cette personne moi-même"){
    	jQuery("#hide3").hide();
		//jQuery("#hide1").hide();
		//jQuery("#hide2").hide();
	}
	else if (jQuery('input:radio[name=notify]:checked').val() == "Request that CAD send a card by postal mail" || jQuery('input:radio[name=notify]:checked').val() == "Je désire que l'ASC l'informe par la poste"){
    	jQuery("#hide3").show();
		//jQuery("#hide1").hide();
		//jQuery("#hide2").hide();
	}
	else{
		
	}
});

