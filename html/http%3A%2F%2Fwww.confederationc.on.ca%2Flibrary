  var rotator_row_count;
  var rotator_index;
  
  $(document).ready(function() {
    rotator_row_count = $('.view-rotating-micro-banner').find('.views-row-even, .views-row-odd').length;
    rotator_index = rotator_row_count - 1;
    
    if (rotator_row_count > 1)
      window.setInterval(change_picture, 8000);
  });
  
  function change_picture() {
    
    if (rotator_index == 0) {
      $('.view-rotating-micro-banner .view-content').children().eq(rotator_index).css('z-index', '2');
      $('.view-rotating-micro-banner .view-content').children().show();
      $('.view-rotating-micro-banner .view-content').children().eq(rotator_index).fadeOut('slow', function () { $(this).css('z-index', '').show(); });
    } else {
      $('.view-rotating-micro-banner .view-content').children().eq(rotator_index).fadeOut('slow');
    }
    
    rotator_index--;
    if (rotator_index < 0)
      rotator_index = rotator_row_count;
  }

//--><!]]>
</script>
