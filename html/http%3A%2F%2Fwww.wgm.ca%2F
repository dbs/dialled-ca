<script src="http://code.jquery.com/jquery-1.8.3.min.js"></script>
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script src="http://google-maps-utility-library-v3.googlecode.com/svn/trunk/markerclusterer/src/markerclusterer.js"></script>
<script type="text/javascript">
	(function() {
		var clusterer;
		var json;
		var markers = [];  
		
		window.onload = function() {		
			var map = new google.maps.Map(document.getElementById("map"), {
				center: new google.maps.LatLng(45.30583,-31.76417),
				zoom : 2,
				minZoom : 2,
				mapTypeId: google.maps.MapTypeId.SATELLITE,
			});	
			
			var image = new google.maps.MarkerImage(
				'http://i.imgur.com/3YJ8z.png',
				new google.maps.Size(19, 25),
				new google.maps.Point(0, 0)
			);

			var infoWindow = new google.maps.InfoWindow();

			var mcOptions = {
				gridSize : 50,
				maxZoom:3,
				minimumClusterSize : 20,
			};
			clusterer = new MarkerClusterer(map, [], mcOptions);

			$.getJSON('/map/map_info.php', function(data) {
					json = data;	
					for (var i = 0, length = json.length; i < length; i++) {
						var data = json[i],
								latLng = new google.maps.LatLng(data.lat, data.lng),
								category = data.category;

						var marker = new google.maps.Marker({
							position : latLng,
							title : data.title,
							category : data.category
						});
			
						markers.push(marker);			
					(function(marker, data) {
						google.maps.event.addListener(marker, "click", function (e) {
							var info =
								"<div class='panel'>" +
								"<h3>" + data.title + "</h3>" +
								"<table width='350px'" +
									"<tr><td width='22.5%'>Commodity :</td><td>" + data.category[0] + "</td></tr>" + 
									"<tr><td>Description :</td><td>" + data.description + "</td></tr>" +
									"<tr><td>Location :</td><td>" + data.location + "</td></tr>" +						
								"</table></div>";
									infoWindow.setContent(info);
									infoWindow.open(map, marker);
								});
							})(marker, data);
			} // END for loop
					$("#filters select").change();
		});

	$("#filters button").on("click", function() {
		map.setZoom(2);
	});
	$("#filters select").change(function(){
				var activityVal = $("#activity").val();
				var commodityVal = $("#commodity").val();
	
				clusterer.clearMarkers();

				if (activityVal == "all" && commodityVal == "all") {
		clusterer.addMarkers(markers);
	}
	else if (activityVal != "all" && commodityVal == "all") {
		for(var i = 0; i < markers.length; i++) 
			if ($.inArray(activityVal, markers[i].category) != -1)
				clusterer.addMarker(markers[i]);
	}
				else if (commodityVal != "all" && activityVal == "all") {
		for(var i = 0; i < markers.length; i++) 
			if ($.inArray(commodityVal, markers[i].category) != -1)
				clusterer.addMarker(markers[i]);
	} 
	else {
		for(var i = 0; i < markers.length; i++) 
			if ($.inArray(activityVal, markers[i].category) != -1 && $.inArray(commodityVal, markers[i].category) != -1)
				clusterer.addMarker(markers[i]);
	}
			});
	}// END window.onload
	})();
</script>
