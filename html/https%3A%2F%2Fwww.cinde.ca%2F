<style type="text/css">
#slidecontainer {
	display: none;
	position: absolute;
	width:100%;
	height:100%;
	overflow: hidden;
	text-align:center;
	margin:0px;
	padding:0px;
}
figslide{
  position: absolute;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
  opacity: 0;
  transition: 1s opacity;
}
figcaption{
  position: absolute;
  font-family: sans-serif;
  font-size: 1.5em;
  top: .45em;
  right: .35em;
  padding: .25em;
  margin-left: .35em;
  color: #fff;
  background: rgba(0,0,0, .45);
  border-radius: 5px;
}
figslide.showslide{
  position: static;
  opacity: 1;
  transition: 1s opacity;
}
.nextslide, .prevslide{
  color: #fff;
  position: absolute;
  background: rgba(0,0,0, .6);
  top: 50%;
  z-index: 100;
  font-size: 2em;
  margin-top: -.75em;
  opacity: .3;
  user-select: none;
}
.linkslide{
  position:absolute;
  top:0;
  bottom:0;
  left:2em;
  right:2em;
  z-index:101;
}
.nextslide:hover, .prevslide:hover, .linkslide:hover{
  cursor: pointer;
  opacity: 1;
}
.nextslide{
  right: 0;
  padding: 10px 5px 15px 10px;
  border-top-left-radius: 3px;
  border-bottom-left-radius: 3px;
}
.prevslide{
  left: 0;
  padding: 10px 10px 15px 5px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}
</style>
<script type="text/javascript">
if ([].forEach) {
var slide_counter = 0;
var slide_time = 5; //Seconds before slide automatically changes
var slideitems = document.getElementById('slidecontainer').querySelectorAll('figslide');
var slide_numItems = slideitems.length;
document.getElementById("slidecontainer").style.display='inline-block';
showCurrentSlide(slide_counter);
var slide_interval=setInterval(showNextSlide,slide_time*1000);
document.querySelector('.nextslide').addEventListener('click', function(){showNextSlide(); clearInterval(slide_interval); slide_interval=setInterval(showNextSlide,slide_time*1000);}, false);
document.querySelector('.prevslide').addEventListener('click', function(){showPrevSlide(); clearInterval(slide_interval); slide_interval=setInterval(showNextSlide,slide_time*1000);}, false);
}
function showCurrentSlide(counter) {
	var itemToShow = Math.abs(counter%slide_numItems);
	[].forEach.call( slideitems, function(el){
		el.className='';
	});
	slideitems[itemToShow].className='showslide';
};
function showNextSlide() {
	slide_counter++;
	showCurrentSlide(slide_counter);
}
function showPrevSlide() {
	slide_counter--;
	showCurrentSlide(slide_counter);
}
function GoURL() {
	var GoLoc = Math.abs(slide_counter%slide_numItems);
	window.location.assign(GoURLs[GoLoc]);
}
</script>
