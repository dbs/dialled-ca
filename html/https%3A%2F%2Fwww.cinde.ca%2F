 <script type="text/javascript"><!--
 function fixMenus() {
  var firstMenu=document.getElementById('traiMenu');
  var lastMenu=document.getElementById('abouMenu');
  var firstRect=firstMenu.parentElement.getBoundingClientRect();
  var lastRect=lastMenu.parentElement.getBoundingClientRect();
  if (lastRect.top > firstRect.top) {
   lastMenu.className = lastMenu.className.replace( /(?:^|\s)rightMenu(?!\S)/ , ' leftMenu' );
  } else {
   lastMenu.className = lastMenu.className.replace( /(?:^|\s)leftMenu(?!\S)/ , ' rightMenu' );
  }
 }
 //--></script>
