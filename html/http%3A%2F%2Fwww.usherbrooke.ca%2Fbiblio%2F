                <h1>Outil de découverte</h1>
                <div class="tab-content">
	<!--  CONTENT ELEMENT, uid:54853/html [begin] -->
		<div id="c54853" class="csc-default">
		<!--  Raw HTML content: [begin] -->
			<div>
             
             <p>Pour chercher des livres, articles de périodiques, disques, mémoires, thèses, partitions, DVD, etc.</p>
             <script language="JavaScript">
               function LanceRecherche(){
                 var search = encodeURIComponent( document.form1.query.value );
                 search     = search.replace("%0A", " ");
                 ligne      = "http://usherbrooke.summon.serialssolutions.com/search?utf8=%E2%9C%93&s.q=" + search + "&keep_r=true";
                 window.location = ligne;
               }
             </script>
             <form enctype="application/x-www-form-urlencoded" onsubmit="LanceRecherche()" method="post" action="javascript:LanceRecherche()" name="form1" class="formRechercheCatalogue">
               <label>Termes de recherche :</label>
               <div class="input_catalogue">
                 <!-- <textarea rows="2" name="query" id="query"></textarea> -->
                 <input type="text" name="query" id="query" />
               </div>
               <div id="submit_input">
                 <input type="submit" value="Chercher"/><input type="text" name="e-mail" value="" style="display:none!important;" /><input type="text" name="first-name" value="" class="tx_spamshield_honey1" /><input type="text" name="last-name" value="" style="display:none!important;" /><input type="text" name="nick-name" value="" class="tx_spamshield_honey1" /><input type="hidden" name="spamshield[mark]" value="true" />
               </div>

             </form>
          </div>
		<!--  Raw HTML content: [end] -->
			</div>
	<!--  CONTENT ELEMENT, uid:54853/html [end] -->
		
