        function parseHours(hours)
        {
          var i;
          i = hours.indexOf(';');
          if (i==-1)
          {
            return hours;
          }
          else
          {
            if (i == (hours.length -1))
            {
              s = hours.substring(0, i);
              return s;  
            }

            else
            {
              s = hours.substring(0, i);
              
              return s + '<BR />' + parseHours(hours.substring(i+1, hours.length));
        }
        }
        }



