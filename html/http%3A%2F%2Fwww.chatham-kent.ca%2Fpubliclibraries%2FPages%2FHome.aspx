      function footprintToggleChildren(listElementID, showFlag)
      {                     
        var subMenuDiv = document.getElementById("div_" + listElementID);            
        if (subMenuDiv != null)
        {
            if(showFlag){  
              if (lastSubMenuElem != listElementID)
              {
                var lastSubMenuElem = document.getElementById("div_" + lastSubMenuElementID);
                if (lastSubMenuElem != null)
                  lastSubMenuElem.style.display = "none";        
              }
              subMenuDiv.style.display='block';
            }
            else {    
              if (lastSubMenuElem != listElementID)      
                clearTimeout(itvSubMenu);

              itvSubMenu =  setTimeout('document.getElementById(\'div_' + listElementID + '\').style.display=\'none\';', 500);              
            }
        }
      }

      function footprintMenuOnMouseOver(flag)
      {
        
        footprintToggleChildren(lastSubMenuElementID, false); 
        if (flag)
        {        
          showFootprint(true);
        }
