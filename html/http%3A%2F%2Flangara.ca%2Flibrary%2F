    <script src="http://www.google.com/recaptcha/api/js/recaptcha_ajax.js" type="text/javascript"></script>
    <script src="../_files/responsive/javascript/langara-captcha-responsive.js" type="text/javascript"></script>
    
    <script language="javascript" type="text/javascript">// 
    
    <!--Javascript for highlighting current section/page in side-nav -->
    
    $(document).ready(function(){
        //first breadcrumb link
        //var firstFolder = $(".header-block .breadcrumbs a:nth-child(1)").text().trim();
        // ^ this is throwing an error in IE that prvents subsequent javascript from executing
        
        var firstFolderStr = $(".header-block .breadcrumbs a:nth-child(1)").text();
        var firstFolder = $.trim(firstFolderStr);

        
        //second breadcrumb link
        //var secondFolder = $(".header-block .breadcrumbs a:nth-child(3)").text().trim();
        var secondFolderStr = $(".header-block .breadcrumbs a:nth-child(3)").text();
        var secondFolder = $.trim(secondFolderStr);
       
        
        //var pageTitle = $(".header-block h1").text().trim();
        var pageTitleStr = $(".header-block h1").text();
        var pageTitle = $.trim(pageTitleStr);
        
        var title = "";
        // if there is a first folder, this will be the main section that needs to open
        if (firstFolder && firstFolder != "") {
            title = firstFolder;
        } else {
            // This is an index page for a main section that needs to open
            title = pageTitle;
        }
        // open the main section
        if (title && title != "" && firstFolder != "Departments") {
            var possibleTitles = $(".primary-nav-inner a:contains("+ title +")");
            // the above may return more than one element if the text is contained in other elements too
            // IE: News will be in both "News and Events" and "News"
            possibleTitles.each(function() {
                var possibleTitleStr = $(this).text();
                var possibleTitle = $.trim(possibleTitleStr);
                if (possibleTitle == title) {
                    // highlight / activate the text if it matches
                    $(this).parent('li').addClass("active open");
                }
            });
        }
        // determine the second level nav
        var levelTwoText = "";
        // if second breadcrumb, this will be the second level nav
        if (secondFolder && secondFolder != "") {
            levelTwoText = secondFolder;
        } else {
            // if there was only a first breadcrumb, the pagetitle will be the second level nav
            if (firstFolder && firstFolder != "") {
                levelTwoText = pageTitle;
            }
        }
        // activate the second level nav
        if (firstFolder != "Departments" && levelTwoText && levelTwoText != "") {
            var possibleTitles = $(".primary-nav-inner a:contains("+ levelTwoText +")");
            // the above may return more than one element if the text is contained in other elements too
            // IE: FAQ will be in both "Book Return FAQ" and "FAQ"
            possibleTitles.each(function() {
                var possibleParentTitleStr = $(this).parent("li").parent("ul").parent("li").children("a").text();
                var possibleParentTitle = $.trim(possibleParentTitleStr);
                var possibleTitleStr = $(this).text();
                var possibleTitle = $.trim(possibleTitleStr);
                // Only highlight it if it is a perfect match, and if it is under the correct parent title
                if ( possibleParentTitle == title && possibleTitle == levelTwoText) {
                    // highlight / activate the text if it matches
                    $(this).parent('li').parent('ul').children('li').removeClass("active");
                    $(this).parent('li').addClass("active");
                }
            });
        }
        //Remove Flash Suppressor
        $('div#flash-suppressor').hide();
    });
    
    </script>
    

        
