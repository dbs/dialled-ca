			<!-- Google Analytics Starts --><script type="text/javascript">
    function trackPEISearchTerms(category, action, label) {
        try {
            ga('send', 'event', category , action, label);
        } catch(err){}
    }

function addwords(t)
{

try {
            ga('send', 'event', 'PEISEARCH' , 'Search', t.value);
        } catch(err){}
t.value = "prince+edward+island+"+t.value;

}
</script>

