<input type="hidden" id="currnt_slid" value="1" />
<script>


function showDiv () {
	
	var id = document.getElementById("currnt_slid");	
	
    var total_slide = 5;	
	
	if(total_slide == 1){
		var slide1 = document.getElementById("tab1");			
		slide1.style.display = "none";	
		
		if(id.value == 1){
			id.value = 1;
			slide1.style.display = "block";
		}
		
		
	}else if(total_slide == 2){
		var slide1 = document.getElementById("tab1");
		var slide2 = document.getElementById("tab2");	
		
		slide1.style.display = "none";
		slide2.style.display = "none";
		
		if(id.value == 1){
			id.value = 2;
			slide1.style.display = "block";
		}else if(id.value == 2){
			id.value = 1;
			slide2.style.display = "block";
		}	
		
	}else if(total_slide == 3){
		var slide1 = document.getElementById("tab1");
		var slide2 = document.getElementById("tab2");
		var slide3 = document.getElementById("tab3");	
		
		slide1.style.display = "none";
		slide2.style.display = "none";
		slide3.style.display = "none";	
		
		if(id.value == 1){
			id.value = 2;
			slide1.style.display = "block";
		}else if(id.value == 2){
			id.value = 3;
			slide2.style.display = "block";
		}else if(id.value == 3){
			id.value = 1;
			slide3.style.display = "block";
		}	
		
	}else if(total_slide == 4){
		var slide1 = document.getElementById("tab1");
		var slide2 = document.getElementById("tab2");
		var slide3 = document.getElementById("tab3");
		var slide4 = document.getElementById("tab4");		
		
		slide1.style.display = "none";
		slide2.style.display = "none";
		slide3.style.display = "none";
		slide4.style.display = "none";	
		
		if(id.value == 1){
			id.value = 2;
			slide1.style.display = "block";
		}else if(id.value == 2){
			id.value = 3;
			slide2.style.display = "block";
		}else if(id.value == 3){
			id.value = 4;
			slide3.style.display = "block";
		}else if(id.value == 4){
			id.value = 1;
			slide4.style.display = "block";
		}		
		
	}else if(total_slide == 5){
		var slide1 = document.getElementById("tab1");
		var slide2 = document.getElementById("tab2");
		var slide3 = document.getElementById("tab3");
		var slide4 = document.getElementById("tab4");
		var slide5 = document.getElementById("tab5");		
		
		slide1.style.display = "none";
		slide2.style.display = "none";
		slide3.style.display = "none";
		slide4.style.display = "none";
		slide5.style.display = "none";	
		
		if(id.value == 1){
			id.value = 2;
			slide1.style.display = "block";
		}else if(id.value == 2){
			id.value = 3;
			slide2.style.display = "block";
		}else if(id.value == 3){
			id.value = 4;
			slide3.style.display = "block";
		}else if(id.value == 4){
			id.value = 5;
			slide4.style.display = "block";
		}else if(id.value == 5){
			id.value = 1;
			slide5.style.display = "block";
		}		
	}
	


	
   	
}; // function to loop through divs and show correct div



showDiv(); // show first div    

setInterval(function () {
	showDiv(); // show next div
}, 10 * 1000); // do this every 8 seconds    




</script>
