<script type="text/javascript"><!--
(function($) {
$.fn.randomize = function(childElem) {
  return this.each(function() {
      var $this = $(this);
      var elems = $this.children(childElem);
      elems.sort(function() { return (Math.round(Math.random())-0.5); });  
      $this.remove(childElem);  
      for(var i=0; i < elems.length; i++)
        $this.append(elems[i]);      
  });    
}
})(jQuery);
$(document).ready(function(){
  if( $('#imgList>li').length == 1)
  {
    var content = $('#imgList>li').html();
    $('#imgList').hide();
    $('#imgListFiller').remove();
    $('#imgList').after(content);
    $('#imgList').remove();
  }
  else
  {
    $('#imgList>li').parent().randomize('li');
    $('#imgList>li').parent().css('position', 'static');
    $('#imgList>li').parent().css('right', 'auto');
    $('#imgList>li').parent().css('top', 'auto');
    $('#imgList>li').parent().jqGalScroll({
        ease: null,
        speed: 0,
        height: 385,
        width: 462,
        titleOpacity: .60,
        direction: 'vertical'
    });
  }
});
