<div>&nbsp;Ressources similaires: &nbsp;&nbsp; <a title="Vitrine de diffusion des mémoires et thèses de l'École" target="_blank" style="text-decoration: underline;" href="http://espace.etsmtl.ca/view/">Espace ÉTS</a> &nbsp;&nbsp; <a title="M&#233;moires et th&#232;ses du Canada, des &#201;tats-Unis et du reste du monde" target="_blank" style="text-decoration: underline;" href="http://search.proquest.com/pqdtglobal/advanced?accountid=27231">ProQuest Dissertations &amp; Theses</a></div></div><script type="text/javascript">
$(function() {
     $('body').append('<form id="formRecherche" action="http://decouverte.uquebec.ca/primo_library/libweb/action/dlSearch.do?" target="_blank" name="primo" method="get" onsubmit="validateForm()"><input type="hidden" name="tab" value="default_tab" />  <input type="hidden" name="institution" value="ETS" />  <input type="hidden" name="onCampus" value="false" />  <input type="hidden" name="indx" value="1" />       <input type="hidden" name="dum" value="true" />      <input type="hidden" name="vid" value="ETS" />      <input type="hidden" name="group" value="GUEST" />   <input type="hidden" name="query" id="primobibrec" value="any,contains," /><input type="hidden" name="search_scope" id="search_scope"/><input type="hidden" value="fre" name="lang" /><input type="hidden" name="inputQueryPrimo" id="inputQueryPrimo"/></form>');
});
function lancerRecherche(id){
  if(id == 1){
    $('#search_scope').val('ETSPLUS');
    $('#inputQueryPrimo').val($('#inputQueryPrimo1').val());
  }else if(id == 2){
    $('#search_scope').val('Primo_central');
    $('#inputQueryPrimo').val($('#inputQueryPrimo2').val());
  }else if(id == 3){
    $('#search_scope').val('eprint_ets');
    $('#inputQueryPrimo').val($('#inputQueryPrimo3').val());
  }
  $('form#formRecherche').submit();
}
function validateForm() 
{ 
        var x=replaceAccents(document.getElementById("inputQueryPrimo").value); 
        if (x==null || x=="") 
        { 
                alert("Entrez un terme de recherche"); 
                return false; 
        } else {
  $('#primobibrec').val("any,contains,"+x);} 
} 
function replaceAccents(s){ 
var sR = s; 
sR = sR.replace(/à/g,"a"); 
sR = sR.replace(/á/g,"a"); 
sR = sR.replace(/â/g,"a"); 
sR = sR.replace(/ã/g,"a"); 
sR = sR.replace(/ä/g,"a"); 
sR = sR.replace(/ç/g,"c"); 
sR = sR.replace(/è/g,"e"); 
sR = sR.replace(/é/g,"e"); 
sR = sR.replace(/ê/g,"e"); 
sR = sR.replace(/ë/g,"e"); 
sR = sR.replace(/ì/g,"i"); 
sR = sR.replace(/í/g,"i"); 
sR = sR.replace(/î/g,"i"); 
sR = sR.replace(/ï/g,"i"); 
sR = sR.replace(/ñ/g,"n"); 
sR = sR.replace(/ò/g,"o"); 
sR = sR.replace(/ó/g,"o"); 
sR = sR.replace(/ô/g,"o"); 
sR = sR.replace(/õ/g,"o"); 
sR = sR.replace(/ö/g,"o"); 
sR = sR.replace(/ù/g,"u"); 
sR = sR.replace(/ú/g,"u"); 
sR = sR.replace(/û/g,"u"); 
sR = sR.replace(/ü/g,"u"); 
sR = sR.replace(/ý/g,"y"); 
sR = sR.replace(/ÿ/g,"y"); 
return sR; 
} 
