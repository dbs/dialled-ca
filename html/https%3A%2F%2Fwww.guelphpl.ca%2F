<script type="text/javascript">
function clearDefault(el) {
	var textBoxValue = $( "#search-box" ).val();
	var selSearchValue = $( "#selSearch" ).val();
	var webText = "Programs, events & information!";
	var catText = "Books, movies & more";
	var resText = "eResources, the catalog & more!";

	if(textBoxValue == webText || textBoxValue == catText || textBoxValue == resText){
		$('#search-box').val('');
		console.log('test2');
	}

}

$( "#selSearch" ).change(function(){
		var textBoxValue = $( "#search-box" ).val();
		var selSearchValue = $( "#selSearch" ).val();
		var webText = "Programs, events & information!";
		var catText = "Books, movies & more";
		var resText = "eResources, the catalog & more!";


		if(selSearchValue == "Website"){
				if(textBoxValue == webText || textBoxValue == catText || textBoxValue == resText ){
					$( "#search-box" ).val(webText);
				}
			}

		if(selSearchValue == "Catalogue"){
				if(textBoxValue == webText || textBoxValue == catText || textBoxValue == resText ){
					$( "#search-box" ).val(catText);
				}
			}

		if(selSearchValue == "Resources"){
				if(textBoxValue == webText || textBoxValue == catText || textBoxValue == resText ){
					$( "#search-box" ).val(resText);
				}
			}
});


</script>
