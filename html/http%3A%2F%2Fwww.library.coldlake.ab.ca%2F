<script>
jQuery(document).ready(function($) {

		//Single Image Lightbox: for latest news thumb popup
		$('.new_post .image-popup-vertical-fit').magnificPopup({
			type: 'image',
			closeOnContentClick: true,
			mainClass: 'mfp-img-mobile',
			image: {
				verticalFit: true	
			},
			zoom: {
				enabled: true,
				duration: 300 // don't foget to change the duration also in CSS
			}
		});	
		
});
</script>









	
