					var htmlDivCss = unescape(".zeus-2%20.tp-tab%20%7B%20%0A%20%20opacity%3A1%3B%20%20%20%20%20%20%0A%20%20box-sizing%3Aborder-box%3B%0A%7D%0A%0A.zeus-2%20.tp-tab-title%20%7B%20%0Adisplay%3A%20block%3B%0Atext-align%3A%20center%3B%0Abackground%3A%20rgba%280%2C0%2C0%2C0.25%29%3B%0Afont-family%3A%20%27MuseoSlab500Regular%27%2C%20arial%2C%20helvetica%2C%20sans-serif%3B%20%0Afont-weight%3A%20700%3B%20%0Afont-size%3A%2018px%3B%20%0Aline-height%3A%2020px%3B%0Acolor%3A%20%23fff%3B%20%0Apadding%3A%209px%2010px%3B%20%7D%0A%0A.zeus-2%20.tp-tab%3Ahover%20.tp-tab-title%2C%0A.zeus-2%20.tp-tab.selected%20.tp-tab-title%20%7B%0A%20color%3A%20%23000%3B%0A%20%20background%3Argba%28255%2C255%2C255%2C1%29%3B%20%0A%7D%0A");
