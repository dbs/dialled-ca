$(function(){
	var menuhtml = '<ul class="parentsmenu">';
	$.each(menujson, function(k,v) {    
      var class1 = '';
      if (v.active) {
        class1 = 'active';
      }
        menuhtml+='<li>';// class="menuselect"
        menuhtml += '<a href="' + v.path + '" class="' + class1 + '">' + v.name.toUpperCase() + '</a>';
        if(v.children.length>0){
			menuhtml+='<ul class="firstchildmenu">';
	    		$.each(v.children, function(k1,v1) {
	    			menuhtml+='<li><a href="'+v1.path+'">'+v1.name.toUpperCase()+'</a>';
        			if(v.children.length>0){
					menuhtml+='<ul class="secontchildmenu">';
		    			$.each(v1.children, function(k2,v2) {
	    					menuhtml+='<li><a href="'+v2.path+'">'+v2.name.toUpperCase()+'</a></li>';
		    			});
					menuhtml+='</ul>';
				}
        			menuhtml+='</li>';
	    		});
	    		menuhtml+='</ul>';
        }
        menuhtml+='</li>';
	});
	menuhtml+='</ul>';
	$(".abctopmenu").html(menuhtml);
	
	$(".parentsmenu>li").on('mouseover',function(){
		$(".parentsmenu>li").removeClass("menuhover");
		$(this).addClass("menuhover");
		//$(".parentsmenu>li>.firstchildmenu").stop(true);
		$(this).children(".firstchildmenu").slideDown();
	});
	$(".parentsmenu>li").on('mouseleave',function(){
		$(".parentsmenu>li").removeClass("menuhover");
		$(this).children(".firstchildmenu").slideUp();
	});
	$(".firstchildmenu>li").on('mouseover',function(){
		$(".firstchildmenu>li").removeClass("menuhover");
		$(this).addClass("menuhover");
		$(this).children(".secontchildmenu").slideDown();
	});
	$(".firstchildmenu>li").on('mouseleave',function(){
		$(".firstchildmenu>li").removeClass("menuhover");
		$(this).children(".secontchildmenu").slideUp();
	});
	$(".secontchildmenu>li").on('mouseover',function(){
		$(".secontchildmenu>li").removeClass("menuhover");
		$(this).addClass("menuhover");
	});
	$(".secontchildmenu>li").on('mouseleave',function(){
		$(".secontchildmenu>li").removeClass("menuhover");
	});
});

</script>  
   
