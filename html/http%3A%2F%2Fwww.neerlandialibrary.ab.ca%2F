<div id="loginDialogFront" title="Patron Login" style="display:none;font-size: 0.8em;">
	<form id="dialog-login-form" onsubmit="loginDialogPost(); return false;">
	<div class="form-item" id="edit-name-wrapper">
		<label for="edit-name">Barcode: <span class="form-required" title="This field is required.">*</span></label>
		<input type="text" maxlength="32" id="dlg2-edit-name" name="name" size="15" value="" class="form-text required" />
	</div>
	<div class="form-item" id="edit-pass-wrapper">
		<label for="edit-pass">Pin: <span class="form-required" title="This field is required.">*</span></label>
		<input type="password" id="dlg2-edit-pass"  maxlength="32"  name="pass" size="15"  class="form-text required" />
	</div>
	<p>
		<input type="submit" value="Log in" />
	</p>
	</form>
</div>
<script type="text/javascript">
$(function() {
	$("#loginDialogFront").dialog({
		bgiframe: true,
			autoOpen: false,
			height: 228,
			width: 300,
			modal: true,
	});
});

function loginDialogPost() {
	if (!$('#edit-pass').length) { alert('Unable to perform patron login on this page.'); }
	$('#edit-pass').val($('#dlg2-edit-pass').val());
	$('#edit-name').val($('#dlg2-edit-name').val());
	$('#user-login-form').submit();
}
</script>

