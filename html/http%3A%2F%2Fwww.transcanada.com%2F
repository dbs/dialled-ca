<script language="javascript" type="text/javascript">
// Set the size of the flyoutWrapper to the largest div  /tc
// Select all the divs in the flyout
$(document).ready(function() {
    var a = "";
    var biggestDIV = 0;
    //var biggestDIVWidth = 0;
    $('#flyout > div').each(function(index){
        dh = eval("$('#" + this.id + "').height()");
        //dw = eval("$('#" + this.id + "').width()");
        if (dh > biggestDIV){biggestDIV = dh;}
        //if (dw > biggestDIVWidth){biggestDIVWidth = dw;}
        //alert("width= " + dw + " height= " + dh);
    });
    biggestDIV += 80; // to account for bottom gradient
    // Set the flyoutWrapper to the largest size to prevent the footer from
    // jumping up and down.
    $('#flyoutWrapper').height(biggestDIV);
    if ($.browser.msie && $.browser.version < 8){
            $("#flyout").css("margin-bottom","-20px");
    }
});
</script>

</div>

<script type="text/javascript">      
$(function(){
    $('div.site_clicker').click(function(event){
        event.stopPropagation();
        event.preventDefault();
        var was_visible = $(this).siblings('ul').is(':visible');
        $('li.site_selector ul').hide();
        if (!was_visible) {
            $(this).siblings('ul').show();
        }
    });
    $('body').click(function(event){
        $('li.site_selector ul').hide();
        $('div.site_pager').hide();
    });
});
</script>
