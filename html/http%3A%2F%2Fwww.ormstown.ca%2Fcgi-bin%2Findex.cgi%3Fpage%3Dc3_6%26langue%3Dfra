  <!--============================================================-->
  <!--======= VOICI LE SCRIPT POUR LE CHANGEMENT DE LANGUE =======-->
  <!--============================================================-->
    
  	<script type="text/javascript">
	
		var langue = "";
		var newLangue = "";
		var urlEntete = window.location;
		var lien = String(urlEntete);
		var newLien = "";
		
		if(lien.indexOf("langue") == -1){
			langue = "fra";
		}else{
			langue = lien.substring(lien.indexOf("langue=")+7,lien.indexOf("langue=")+10);
		}
		
		if(langue == "fra"){
			newLangue = "eng";
		}else if(langue == "eng"){
			newLangue = "fra";
		}
		
		if(lien.indexOf("langue") == -1){
			if(lien.indexOf("?") == -1){
				newLien = lien + "?";
			}else{
				newLien = lien + "&";
			}
		}else{
			var tabLien = [];
			if(lien.indexOf("?langue") != -1){
				tabLien	= lien.split("?langue="+langue);
			}else{
				tabLien	= lien.split("&langue="+langue);
			}
			
			var lienTmp = tabLien[0] + tabLien[1];
			
			if(lienTmp.indexOf("?") == -1){
				newLien = lienTmp + "?";
			}else{
				newLien = lienTmp + "&";
			}
		}
		
		if(newLangue == "fra"){
			document.write("<a href='" + newLien + "langue=" + newLangue + "' title='Français'>Français</a>");
		}else if(newLangue == "eng"){
			document.write("<a href='" + newLien + "langue=" + newLangue + "' title='English'>English</a>");
		}
		
	</script>
  
      
  <!--===================================-->
  <!--========== Fin du script ==========-->
