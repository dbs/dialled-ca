    <script language="javascript" type="text/javascript">
        //set body class based on language
        var bodyClass = "en";
        var currUrl = window.location.href.toLocaleUpperCase();
        if ((currUrl.indexOf("/FR/") > -1) || (currUrl.indexOf(".ACEF.") > -1) || (currUrl.indexOf(".FCF-CTF.") > -1)) {
            bodyClass = "fr";
        }
        document.body.className = bodyClass;

        //set menu item class based on menu item text
        var mainMenu = document.getElementById("ctl00_TemplateMainNavigation_Primary_NavMenu");
        var spanArray = mainMenu.getElementsByTagName("span");
        var affectedPagesEN = new Array("Online Learning", "CTF Publications", "Research Centre", "TaxFind Online", "Young Practitioners", "Careers in Tax");
        var affectedPagesFR = new Array("À propos de la FCF", "Apprentissage en ligne", "Publications de la FCF", "Centre de recherche", "TaxFind en ligne", "Jeunes fiscalistes", "Carrières en fiscalité");
        if (bodyClass == "fr") {
            for (var f = 0; f < spanArray.length; f++) {
                for (var q = 0; q < affectedPagesFR.length; q++) {
                    if (spanArray[f].innerText == affectedPagesFR[q] || spanArray[f].textContent == affectedPagesFR[q]) {
                        spanArray[f].setAttribute('class', 'double');
                    }
                }
            }
        }
        else {
            for (var e = 0; e < spanArray.length; e++) {
                for (var p = 0; p < affectedPagesEN.length; p++) {
                    if (spanArray[e].innerText == affectedPagesEN[p] || spanArray[e].textContent == affectedPagesEN[p]) {
                        if (spanArray[e].innerText == "Careers in Tax" || spanArray[e].textContent == "Careers in Tax") {
                            spanArray[e].setAttribute('class', 'double careers');
                        }
                        else {
                            spanArray[e].setAttribute('class', 'double');
                        }
                    }
                }
            }
        }

        //set selected menu item class based on URL
        var li = null;
        var liArray = mainMenu.getElementsByTagName("li");
        for (var j = 0; j < liArray.length; j++) {
            var aArray = liArray[j].getElementsByTagName("a");
            for (var a = 0; a < aArray.length; a++) {
                if (aArray[a].className.indexOf("rmSelected") > -1) {
                    li = liArray[j];
                    break;
                }
            }
            if (li != null) {
                li.className += " selected";
                break;
            }
        }
    </script>
