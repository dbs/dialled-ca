"10am&ndash;10pm", 
"10am&ndash;10pm", 
"10am&ndash;10pm", 
"10am&ndash;10pm", 
"10am&ndash;10pm", 
"10am&ndash;10pm"
];

var todays_hours = hours[n];


</script>
<img id="info__hours--img" src="/images/sprites/tiffcolour.png" alt="TIFF Bell Lightbox">
