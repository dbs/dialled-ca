satpo_sNavSide.stealthNav = $s('navOuter-sNavSide').outerHTML;
accordionTreeObjs.satpo_sNavSide = satpo_sNavSide;

satpo_sNavSide.rootPage = "-5";
satpo_sNavSide.subNav = "1";
