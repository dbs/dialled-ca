function pageJS() {	
	//startValue - should be the default CSS value for the element
	//speed - how fast to scroll
	//scrollDir - false negative growth, true positive growth
	function scrolledStart(startValue,speed,scrollDir) {
		
		var scrollTop = $(window).scrollTop();
		
		if(scrollDir){
			var scrollValue = startValue + Math.round(speed * scrollTop);
	
			if((startValue + scrollTop) > startValue) {
				return scrollValue;
			}
			else{
				return startValue;
			}
			
		}
		else if(!scrollDir){
			var scrollValue = startValue -  Math.round(speed * scrollTop);
	
			if((startValue - scrollTop) < startValue){
				return scrollValue;
			}
			else{
				return startValue;
			}
		}
		else {
			return startValue;
		}
	}

	setInterval(function(){
	    if (window.didScroll){
	        window.didScroll = false;
	        scroller();
	   	}
	}, 10);
	
	function scroller(){	
		
		if ($(window).width() > 1040 && isMobile == false) {
			$(".icc-homeTopBImg").css("top",scrolledStart(0,0.5,true) + "px");
		} else {
			$(".icc-homeTopBImg").css("top", "0px");
		}
	}
    
    $(document).ready(function () {
        $(window).scroll(function(e){
            window.didScroll = true;
            scroller();
        });	
    });
	
	if(pageID == 1) {
		$('#topLinks').show();
	}
	else {
		$('#topLinks').hide();
	}
	
	if ($( ".slideMenu" ).length) {
	 $(".slideMenu .benefitInfo").hide();
	 $(".slideMenu h3").removeClass("active");
	 	 
     $(".slideMenu h3").click(function () {
		 if ($(this).siblings(".benefitInfo").is(':hidden')) {		 
            $(".benefitInfo").hide(0);
			$(".slideMenu h3").removeClass("active");
			$(this).addClass("active");
            $(this).siblings(".benefitInfo").toggle(600);
		 }
		 else  {
			$(this).siblings(".benefitInfo").hide(600);
			$(this).removeClass("active");
		 }
        });
	 } 
}

$(window).scroll(
		function(){		
			var scrollTop = $(window).scrollTop();
			if ($(window).width() > 1100) {
				if(scrollTop < 1){
					$('#navOuter-tNav').removeClass('onScroll');
					//$('#topBarContainer').removeClass('onScroll');
				}
			else {
					$('#navOuter-tNav').addClass('onScroll');
					//$('#topBarContainer').addClass('onScroll');
				}
			}
		}
);		$(window).load(function(){
					});
		// -->
		</script>
	</body>
</html>