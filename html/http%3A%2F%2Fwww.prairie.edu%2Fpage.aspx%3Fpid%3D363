//initialize any CKEditors for this instance of this module
if(tempObjArray.length > 0){
	window["pTopBImgThumb3131"].loadCKEditor(tempObjArray);
}
//initialize any time pickers for this instance of this module
if(window["pTopBImgThumb3131"].timeFieldArray.length > 0){
	var tempArray = window["pTopBImgThumb3131"].timeFieldArray;

	//loop through all our time fields by class
	for(var x = 0; x < tempArray.length; x++) {
		var tempClassArray = $$s(tempArray[x]);

		//loop through all the elements with that class
		for(var y = 0; y < tempClassArray.length; y++) {
			
			tempClassArray[y].onclick = function(){
				showTimePicker(this);
			};
		}
	}
}



/*not the login module. login module must be isTemplate 1, because we update 
the innerHTML of it using the paginate function. we don't want it sortable*/
if(window["pTopBImgThumb3131"].moduleID != 37){
	if($$s("moduleContainer") && $$s("moduleContainer").length > 0){
		//only let level one modules be sortable
		$(".pcpy > #mfmc-pTopBImgThumb #mfmcc-pTopBImgThumb").sortable({
			items:".mi-pTopBImgThumb",
			update:function(nativeEvent,ui){
				try{					
					//send out JSON object to the server to update the order of the records
					var sortAjax = ajaxObj();
					var modData = {};
					modData["function"] = "updateRecordOrder";
					
					//create a delimited list of our id's
					var idArray = JSON.stringify(
						$("#mfmcc-pTopBImgThumb").sortable("serialize")
					).split("&");
					var tempLen = idArray.length;
					
					for (var i = 0; i < tempLen; i++){
						idArray[i] = parseInt(
							idArray[i].substring(idArray[i].indexOf("=")+1, idArray[i].length)
						);
					}
					
					var idList = idArray.join(",");
	
					modData.recordList = idList;
					var modJson = encodeURIComponent(JSON.stringify(modData));
					
					ajaxPost(
						sortAjax,
						window["pTopBImgThumb3131"].apiPath,
						"modData=" + modJson,
						false,
						null,
						null,
						false
					);
				}
				catch(e){
					alert(e);
				}
			}}
		);
	}
}
//call the 'constructor' for this module
if(typeof window["pTopBImgThumb3131"].construct !== "undefined"){
	window["pTopBImgThumb3131"].construct();
}

setGalleryFancyBox();
				
				window[mIP.instanceProp.className].parentClassName = "pTopBImg";
				window[mIP.instanceProp.className].parentDisplayType = "0";
				
				moduleInstanceItems = $(".mi-" + mIP.instanceProp.className);
				var tmpMiCnt = moduleInstanceItems.length;

				for(var m = 0; m < tmpMiCnt; m++){
					moduleInstanceItems[m].onclick = function(){pTopBImgThumb.parentFade(this);}
				}
				if( /Android|webOS|iPhone|iPod|iPad|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
	var isMobile = true;
} else {
 	var isMobile = false;
}
	
function bannerChange(){
	// Apply now
	if(pageID==2471){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3116"));
		pBtmBImgThumb.parentFade($s("gi_pBtmBImgThumb_3117"));
	}
	// Admissions
	else if(pageID==2472){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3118"));
		pBtmBImgThumb.parentFade($s("gi_pBtmBImgThumb_3119"));
	}
	// Fees & Financial Aid
	else if(pageID==2473){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3120"));
		pBtmBImgThumb.parentFade($s("gi_pBtmBImgThumb_3121"));
	}
	// Campus visits
	else if(pageID==2474){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3122"));
		pBtmBImgThumb.parentFade($s("gi_pBtmBImgThumb_3123"));
		pMdBImgThumb.parentFade($s("gi_pMdBImgThumb_3124"));
	}
	// Impact Groups
	else if(pageID==2475){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3125"));
		pBtmBImgThumb.parentFade($s("gi_pBtmBImgThumb_3126"));
		pMdBImgThumb.parentFade($s("gi_pMdBImgThumb_3127"));
	}
	// Housing
	else if(pageID==2476){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3128"));
	}
	// Spiritual Development
	else if(pageID==2477){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3131"));
	}
	// FAQ
	else if(pageID==2478){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3132"));
		pBtmBImgThumb.parentFade($s("gi_pBtmBImgThumb_3133"));
	}
	// Programs
	else if(pageID==2467){
		pTopBImgThumb.parentFade($s("gi_pTopBImgThumb_3135"));
	}
