	<script src="javascript/plugin.js"></script>
	<script>
		/* --- Hover couvertures dans carrousel ----------------------------------------- */
		function manageTooltipCarrousel(o, ouvre) {
			leTooltip = $('.aboveWrapAllSliders .tooltip_carrousel');
			if(1 == ouvre) {
				splitted = o.attr('title').split('::');
				leTooltip.html( '<span>'+splitted[0]+'</span><br>'+splitted[1]+'<br>'+splitted[2]+'<br>'+splitted[3] );
				larg_img = o.width();
				larg_toolt = leTooltip.width();
				if(larg_toolt>larg_img) {
					laPosit = o.position().left - Math.round((larg_toolt-larg_img)/2);
				} else {
					laPosit = o.position().left + Math.round((larg_img-larg_toolt)/2);
				}
				leTooltip.css('left', (laPosit+20)+'px' ).show(); // 4 = padding
			} else {
				//leTooltip.hide();
			}
		}
		
		$('.wrapAllSliders').mouseout(function() {
			leTooltip.hide(); 
		});
		
		$('.wrapAllSliders ul.slider img').hover(function() {
			if( 'images/cadre-hover.png' == $(this).attr('src') ) { 
				// user has quit browser window (such as ALT+Tabbed) while hovering...  so hovering never went back to normal state
			} else { // normal hover effect, expected...
				$(this).css('background-image', 'url(' + $(this).attr('src') + ')').css('opacity','1.0').css('background-size','125px 174px').attr('jqTempSrc', $(this).attr('src') ).attr('src','images/cadre-hover.png');							  
			}
			manageTooltipCarrousel( $(this) , 1 );
		}, function(e) {
			$(this).attr('src', $(this).attr('jqTempSrc') ).attr('jqTempSrc','').css('opacity','0.9');
			manageTooltipCarrousel( $(this) , 0 );
		});
		
		/* --- Navig. gauche-droite dans les 3 différents carrousels ----------------------- */
		function bonsControles(o) {
			i = o.index();
			$('.aboveWrapAllSliders a.controleCacher').removeClass('controleCacher'); // remove to some before adding all ;-)
			$('.aboveWrapAllSliders a').addClass('controleCacher'); // add to all
			$('.aboveWrapAllSliders a.prev'+(i+1)).removeClass('controleCacher'); // remove (so: show) good ones
			$('.aboveWrapAllSliders a.next'+(i+1)).removeClass('controleCacher'); // remove (so: show) good ones
		}
		$('.anchor-enfants').click(function() {
			bonsControles( $(this) );
			$(this).addClass('anchor-enfants-actif');
			$('.anchor-adultes-actif').removeClass('anchor-adultes-actif');
			$('.anchor-autres-actif').removeClass('anchor-autres-actif');
		});
		$('.anchor-autres').click(function() {
			bonsControles( $(this) );
			$(this).addClass('anchor-autres-actif');
			$('.anchor-adultes-actif').removeClass('anchor-adultes-actif');
			$('.anchor-enfants-actif').removeClass('anchor-enfants-actif');
		});
		$('.anchor-adultes').click(function() {
			bonsControles( $(this) );
			$(this).addClass('anchor-adultes-actif');
			$('.anchor-enfants-actif').removeClass('anchor-enfants-actif');
			$('.anchor-autres-actif').removeClass('anchor-autres-actif');
		});
		$('.toggleSlider a').live('click', function() {
			i=$(this).index();
			newPos = i*200;
			$('.outerslidewrap').animate({ 'top' : '-'+newPos+'px' }, 500);
			return false;
		});		
		
		/* --- INIT. des Sliders ------------------------------ */
		$(document).ready(function() {
			$('.slidewrap1').carousel({
				
				slider: '.slider',
				slide: '.slide',
				//slideHed: '.slidehed',
				nextSlide : '.next1',
				prevSlide : '.prev1',
				//addPagination: true,
				addNav : false
				
			});
			$('.slidewrap2').carousel({
				slider: '.slider',
				slide: '.slide',
				//slideHed: '.slidehed',
				nextSlide : '.next2',
				prevSlide : '.prev2',
				//addPagination: true,
				addNav : false
			});
			$('.slidewrap3').carousel({
				slider: '.slider',
				slide: '.slide',
				//slideHed: '.slidehed',
				nextSlide : '.next3',
				prevSlide : '.prev3',
				//addPagination: true,
				addNav : false
			});

		});			
	</script>
 <br clear="all" />&nbsp;
