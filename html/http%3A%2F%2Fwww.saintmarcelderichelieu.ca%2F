        <script type="text/javascript">
$(document).ready(function () {

    $('#googlemap').addClass('scrolloff');                // set the mouse events to none when doc is ready

    $('#googlemapoverlay').on("mouseup",function(){          // lock it when mouse up
        $('#googlemap').addClass('scrolloff'); 
        //somehow the mouseup event doesn't get call...
    });
    $('#googlemapoverlay').on("mousedown",function(){        // when mouse down, set the mouse events free
        $('#googlemap').removeClass('scrolloff');
    });

    $("#googlemap").mouseleave(function () {              // becuase the mouse up doesn't work... 
        $('#googlemap').addClass('scrolloff');            // set the pointer events to none when mouse leaves the map area
        // or you can do it on some other event
    });

});
        </script>

