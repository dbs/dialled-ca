		<h3 class="module-title">Library Card PIN # Lookup</h3>	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/themes/redmond/jquery-ui.css" />
<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
<script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.12.0/jquery.validate.min.js"></script>
<script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.12.0/additional-methods.min.js"></script>


<script>
    $.noConflict();

    jQuery(document).ready(function($) {

           $('form').submit(function(event){

    $.ajax({

        type    :'POST',
        url     :'/cus_scripts/pinlookup/pinlookup.php',
        data    : $('#PINLookUP').serialize(),

    })

        .done(function(data){
            $('#dialog').html(data);
            $('#dialog').dialog({
                    width:400,
                    show:{
                        effect: "blind",
                        duration: 1000
                    },
                    modal:true,
                    buttons: {
                            ok:function(){
                                $(this).dialog("close");
                            }
                    }
            });
            console.log(data);
        });

    event.preventDefault();

    });
    $(function() {
        $("#birthdate").datepicker({
            dateFormat: "yy-mm-dd",
            showOn: "button",
            buttonImage: '/cus_scripts/pinlookup/calendar.png',
            buttonImageOnly: true,
            changeMonth: true,
            changeYear: true,
            yearRange: "1930:2016"
        });
    });

});

</script>

<form action="" method="post" id="PINLookUP">
<div id="dialog" title="PIN Lookup Results"></div>
<table>
    <tr>

        <td style="color:#000!important;">Library Card Number:</td>
        <td><input type="text" id="barcode" name="barcode" size="15" maxlength="15" value=""> </td>
    </tr>
    <tr>
        <td style="color:#000!important;">Your Birth Date:</td>
        <td><input type="text" id="birthdate" name="birthdate" size="10"  READONLY value=""></td>
    </tr>
    <tr>
        <td colspan="2"><input type="submit" name="lookup" id="lookup" value="Lookup my PIN"> </td>
    </tr>
</table>
