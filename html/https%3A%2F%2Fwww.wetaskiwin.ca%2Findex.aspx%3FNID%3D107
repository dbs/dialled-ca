		{
			SC129 = 0;
			var div = document.getElementById("div_share129");
			div.style.display = 'none';
		}
	}
 
	function LaunchShareSite129(site)
	{
		var div = document.getElementById("div_share129");
		div.style.display = 'none';
		SC129 = 0;
 
		if (site.indexOf('intShareStumbleUpon')>-1)
			window.open('http://www.stumbleupon.com/submit?url='+document.location.href+'&title='+document.title, "ShareWindow");
		else if (site.indexOf('intShareMySpace')>-1)
			window.open('http://www.myspace.com/Modules/PostTo/Pages/?t='+document.title+'&c=&u='+document.location.href, "ShareWindow");
		else if (site.indexOf('intShareFacebook')>-1)
			window.open('http://www.facebook.com/share.php?u='+document.location.href+'&t='+document.title, "ShareWindow");
		else if (site.indexOf('intShareGoogle')>-1)
			window.open('http://www.google.com/bookmarks/mark?op=add&title='+document.title+'&bkmk='+document.location.href, "ShareWindow");
		else if (site.indexOf('intShareYahoo')>-1)
			window.open('http://my.yahoo.com/index.php?.src=bookmarks&.folder=1&.name='+document.title+'&url='+document.location.href, "ShareWindow");
		else if (site.indexOf('intShareDigg')>-1)
			window.open('http://digg.com/submit/?url='+document.location.href+'&title='+document.title, "ShareWindow");
		else if (site.indexOf('intShareDelicious')>-1)
			window.open('http://del.icio.us/post?url='+document.location.href+'&title='+document.title, "ShareWindow");
		else if (site.indexOf('intShareTwitter')>-1)
			window.open('http://twitter.com/home?status='+document.location.href, "ShareWindow");
		else if (site.indexOf('intShareBlogger')>-1)
			window.open('http://www.blogger.com/blog_this.pyra?'+document.location.href, "ShareWindow");
	}
 
//--></script>
