        var $moduleSpotlight = jQuery182('#moduleSpotlight');

        if ($moduleSpotlight != null && $moduleSpotlight.children().length > 1) {
            var controlStyles = {
                play: 'display: none;',
                pause: 'display: none;',
                next: "",
                previous: ""
            };

            var wrapInlineStyles = {
                
            };

            $moduleSpotlight.jshowoff({ speed: 10000, links: false, effect: 'none', controls: true, autoPlay: false,
                                        controlStyles: controlStyles, wrapInlineStyles: wrapInlineStyles });
        }

        var $facilities = jQuery182('#moduleSpotlightFacilities');

        if ($facilities != null) {
            $facilities.unbind().bind({
                click: function (e) {
                    window.location = '/Facilities';
                }
            });
        }
    });
