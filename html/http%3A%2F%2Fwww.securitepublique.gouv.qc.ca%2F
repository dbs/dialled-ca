<script type="text/javascript">
  window.onload=readCookie; 


 
  function readCookie(){
    
    
    var ca = document.cookie.split(';');
    var nameEQ = "bandeau_urgence=";
    for(var i=0;i < ca.length;i++) {
    var c = ca[i];
      while (c.charAt(0)==' ') c = c.substring(1,c.length);
      if (c.indexOf(nameEQ) == 0) {
        var valeur = c.split('=');
        document.getElementById('bandeau_urgence').setAttribute("class",valeur[1]);
        document.getElementById('bandeau_urgence').className =valeur[1];
        if(valeur[1] == 'bandeau_ferme'){
          document.getElementById('texte_urgence').style.display = 'none';
          document.getElementById('ajuster_taille_lien').innerHTML = 'Afficher les détails';
        }
        else{
          document.getElementById('texte_urgence').style.display = 'block';
        }    
      }
    }

  }
  function setCookie(){

    className = document.getElementById('bandeau_urgence').getAttribute("class");
    cookieName = "bandeau_urgence";
    if(className == "bandeau_ouvert"){
      document.getElementById('bandeau_urgence').setAttribute("class","bandeau_ferme");
      document.getElementById('bandeau_urgence').className ="bandeau_ferme";
      document.getElementById('texte_urgence').style.display = 'none';
      document.getElementById('ajuster_taille_lien').innerHTML = 'Afficher les détails';

    }
    else{
      if(className == "bandeau_ferme"){
         document.getElementById('bandeau_urgence').setAttribute("class","bandeau_ouvert");
         document.getElementById('bandeau_urgence').className ="bandeau_ouvert";
         document.getElementById('texte_urgence').style.display = 'block';
         document.getElementById('ajuster_taille_lien').innerHTML = 'Réduire ce bandeau';
      document.getElementById('ajuster_taille_lien').innerHTML = '';
      }
      else{
           document.getElementById('ajuster_taille_lien').innerHTML = 'Afficher les détails';
          }
    }
    
    var date = new Date();
    

    date.setTime(date.getTime()+(365*24*60*60*1000));
    var expires = "; expires="+date.toGMTString();
    
    className = document.getElementById('bandeau_urgence').getAttribute("class");
    
    document.cookie = cookieName+"="+className+expires; 
  }
  </script>
