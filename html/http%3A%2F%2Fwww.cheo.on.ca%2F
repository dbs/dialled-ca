<script type="text/javascript">
    //http://0.s3.envato.com/files/43917021/index.html
    //$(document).ready(function(){$('.carousel').carousel({carouselWidth:930,carouselHeight:330,directionNav:true,shadow:true,buttonNav:'bullets'});});  //Sample 1

    $(document).ready(function () {

        // $('.carousel').carousel({ hAlign: 'center', vAlign: 'top', hMargin: 0.5, vMargin: 0.2 });
        var car = $('.carousel').carousel({ hAlign: 'center', vAlign: 'center', hMargin: 0.37, autoplay: true });
        // $('.carousel').carousel({ hAlign: 'center', vAlign: 'center', hMargin: 0.37, directionNav: true, buttonNav: 'bullets' });       


        $('.startgal').click(function () {
            car.resume();
        });

        $('.stopgal').click(function () {
            car.pause();
        });
    });



</script>
