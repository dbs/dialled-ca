<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script>
$(document).ready(function () {
    function showNotificationBar(message, bgColor, txtColor, height) {
        /*set default values*/
        bgColor = typeof bgColor !== 'undefined' ? bgColor : "#E00000";
        txtColor = typeof txtColor !== 'undefined' ? txtColor : "#FFFFFF";
        height = typeof height !== 'undefined' ? height : 40;
var wi = $(window).width();  
        /*create the notification bar div if it doesn't exist*/
        if ($('#notification-bar').size() === 0) {
            var HTMLmessage = "<div class='notification-message' style='text-align:center; margin-top:8px; padding-top:5px;'> " + message + "</div> ";
            $('body').prepend("<div id='notification-bar' style='display:block; width:100%; background-color: " + bgColor + "; z-index: 100; color: " + txtColor + ";border-bottom: 1px solid #990000; margin-top:-10px;'>" + HTMLmessage + "</div>");
        }
        /*animate the bar*/
        if(wi>500){
        $("body").css('background-position-y', '11em');
        }else{
        $("body").css('background-position-y', '66px');
        }
        $('#notification-bar').css('top', '0');                
        }
    showNotificationBar('
    ');
});
</script>


