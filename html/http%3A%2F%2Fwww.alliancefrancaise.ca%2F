<script>
jQuery(document).ready(function() {
	jQuery('#slide_note').insertAfter(jQuery('#identity'));
});

jQuery(window).load(function() { 
	jQuery('#slide_note').animate({ opacity: 0.75 }, { duration: 1500, queue: false }); 
	jQuery('#slide_note').animate({ top: "60px" }, 1500, null, function() { setTimeout(function() { jQuery('#slide_note').animate({ top: "-100px", opacity: 0 }, 1000) }, 8000); });
});


if(typeof(g_arrFormValidations['nav_login']) == 'undefined') { g_arrFormValidations['nav_login'] = []; }
g_arrFormValidations['nav_login'].push(function()
{
	var nItem = document.querySelectorAll('form#nav_login #contact_email');
	if(!nItem.length) { return ''; }
	nItem = nItem[0];
	var strError = '';
	if(nItem.nodeName.toLowerCase() == 'select')
	{
		var strVal = nItem.options[nItem.selectedIndex].value;
		if(!strVal || strVal == 'null')
		{
			strError += 'Please select a value.\r\n';
		}
	}
	else if(nItem.getAttribute('data-validation-mandatory'))
	{
		strError += nItem.getAttribute('data-validation-mandatory');
	}
	else if(typeof(nItem.value) != 'undefined' && !nItem.value.length)
	{
		strError += 'Please enter a value.\r\n';
	}

	if(strError)
	{
		while(nItem.offsetWidth == 0 && nItem.offsetHeight == 0) { nItem = nItem.parentNode; }
		jQuery(nItem).qtip({
			content: { text: strError },
			show: { event: false, ready: true },
			hide: { event: 'keypress change', leave: false },
			position: { my: 'middle left', at: 'middle right', container: jQuery(nItem.parentNode) },
			style: {classes: 'qtip-s8-error qtip-rounded' }
		});
		try { if(nItem.focus) { nItem.focus(); } } catch (e) {}
   }
   return strError;
});

