(function(){
	var nLink = document.getElementById("forms_submitlink_1");
	nLink.addEventListener('click', function(evt) {
		evt.preventDefault();
		document.getElementById("forms_submitlink_1_input").value = '1';
		var nForm = getClosest(nLink, 'form');
		var bDoDefualt = triggerEventCrossBrowser(nForm, 'submit');
		if(bDoDefualt)
		{
			nForm.submit();
		}
	});
})();

