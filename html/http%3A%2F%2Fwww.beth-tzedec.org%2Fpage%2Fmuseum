<script type="text/javascript">
$(function(){
  if ($('.breadcrumb').length > 0) {
    $.each(Breadcrumbs,function(i,el){
      if (i < (Breadcrumbs.length-1)) {
        $("#BreadcrumbTmpl").tmpl(el).appendTo(".breadcrumb");
      } else {
        $("#ActiveBreadcrumbTmpl").tmpl(el).appendTo(".breadcrumb");
      }
    });
  }
});
</script>

