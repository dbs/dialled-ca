    <script>
  function blogroll_loadmore(render_id, content_id, item_number, page, columns)
  {
    var topic_params = "";
    if(topic_dimension && topic_dimension.length > 0 && topic_category && topic_category.length > 0) {
        topic_params = "&topic_dimension="+topic_dimension+"&topic_category="+topic_category;
    }
    
    YUI().use("io-base", function(Y) {
      
      // AJAX URL for the next page of content
      // item in query-string is added as a way to differentiate content if limit is changed
      var uri = "/cmlink/"+content_id+"?"+content_id+"page="+page+topic_params+"&gridCols="+columns+"&item="+item_number+"#"+content_id;

      // Define a function to handle the response data.
      function complete(id, o, args) {
          var id = id; // Transaction ID.
          var data = o.responseText; // Response data.

          var button_id = "blogroll" + render_id + "_page" + page
		  var element_c = document.getElementById(button_id);
          var blogroll_render_parent = element_c.parentNode;

          // Remove pressed button div
          blogroll_render_parent.removeChild(element_c);

          // Find parent of button div, append data to the bottom of the element
          blogroll_render_parent.innerHTML = blogroll_render_parent.innerHTML + data;
          
          window.onload();
          
      };
      
      // AJAX call
      Y.on('io:complete', complete, Y);
      var request = Y.io(uri);
    });
  }
  </script>
  	

      
  
