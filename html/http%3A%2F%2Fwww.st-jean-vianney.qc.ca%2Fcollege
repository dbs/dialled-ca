  <![endif]-->


   

<script type="text/javascript">

    var $ = jQuery;

    $(document).ready(function () {

        $('.tous').addClass('active');

        $('.lienDiapo').each(function (index) {
            if ($(this).attr('href') == '') {
                $(this).attr('href', 'javascript:void(0);')
                $(this).css('cursor', 'default');
            }
        });

        Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(BeginRequestHandler);
        function BeginRequestHandler(sender, args) {

            $('.lienEvenement').click(function (event) {
                $('.lienEvenement').removeClass('active');
                $(this).addClass('active');
                $('#imgWaitEvenement').css('display', 'block');
            });
        }

        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler);
        //doit redéfinir le scrolable après le post de l'updatePanel
        function EndRequestHandler(sender, args) {

            $('#imgWaitEvenement').css('display', 'none');
        }

        $('.lienEvenement').click(function (event) {
            $('.lienEvenement').removeClass('active');
            $(this).addClass('active');
            $('#imgWaitEvenement').css('display', 'block');
        });

    });

    function LoadEvenement(type) {
        __doPostBack('cphContenu_upEvenement', type);
    }

</script>




