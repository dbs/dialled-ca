function buildTable(img,Title,Description,Value,Offset,Bottom,Modified)
{
var htmlContent = '<div class="dashindicator">'
				 	+'<table class="cpcs" cellpadding="0" cellmargin="0" border="0">'
						+'<tr>'
							+'<td class="arrowCont">'
								+ img
							+'</td>'
							+'<td class="dataCont">'
								+'<div class="dataTop">'
									+'<div>'
										+'<span class="indicatortitle">' + Title + '</span>'
										+'<br/>'
										+'<span class="indicatorDesc">'
										+'<span class="dashgroup">' + Description + '</span> <br/>'
										+'<span class="dashdescription"></span></span>'
									+'</div>'
									+'</div>'
									+'<div class="dataBottom">'
										+'<div>'
											+'<span class="indicatorval">' + Value  + ' </span>'
											+ Offset
											+'<span class="indicatorunit"></span>'
											+'</span>'
										+'</div>'
									+'</div>'
									+'<div class="dataBottomItalic">'
										+'<span class="indicatorscope">' + Bottom + '</span>'
										+'<br />'
										+'Last updated: <span class="lastupdate">' + Modified + '</span>'
									+'</div>'
								+'</td>'
							+'</tr>'
						+'</table>'
					+'</div>'
	
	return  htmlContent;
}
</script>

