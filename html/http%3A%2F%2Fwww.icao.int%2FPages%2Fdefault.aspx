					items.push('<div class="noEvents">There is not items at the moment. Kindly check out later.<div>');
				}
				
				$(data.d.results).each(function(){
				var Modified = this.Modified.substring(0,10);
				
				var ACC = this.ACountCurrent;
				var ACL = this.ACountLast;
				var ACImg = "";
				var ACOffset = "";
				if(ACC > ACL)
				{
					ACImg = '<img src="/SiteAssets/SafetySnaphots/img/upred.png" />'
					var i = parseInt(ACC) - parseInt(ACL);
					var ACOffset = '<span class="YOY" style="color:red"><span class="indicatorgrowth">(+' + i + ')</span>';
				}
				else
				{
					ACImg = '<img src="/SiteAssets/SafetySnaphots/img/downgreen.png" />';
					var i = parseInt(ACL) - parseInt(ACC);
					var ACOffset = '<span class="YOY" style="color:green"><span class="indicatorgrowth">(-' + i + ')</span>';
				}
				var ACContent = buildTable(ACImg,"Accidents","Global<br/>",ACC,ACOffset,"Scheduled commercial flights on airplanes above 5.7t only",Modified);
				$("#c11").html(ACContent);
				
				
				var ARC = this.ARateCurrent;
				var fARC = parseFloat(ARC).toFixed(2);
				var ARL = this.ARateLast;
				var ARImg = "";
				var AROffset = "";
				if(ARC > ARL)
				{
					ARImg = '<img src="/SiteAssets/SafetySnaphots/img/upred.png" />'
					var i = (parseFloat(ARC) - parseFloat(ARL))/parseFloat(ARC);
					var AROffset = '<span class="YOY" style="color:red"><span class="indicatorgrowth">(+' + i.toFixed(2) + '%)</span>';
