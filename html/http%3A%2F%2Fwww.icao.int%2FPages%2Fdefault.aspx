	$.ajax({
			url: "/_api/web/lists/getbytitle('SafetySnapshots')/items?$select=Title,ACountCurrent,ACountLast,ARateCurrent,ARateLast,FACountCurrent,FACountLast,FCurrent,FLast,Modified&$orderby=Title asc'",
