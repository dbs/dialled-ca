<form action="http://search.library.utoronto.ca/search" class="enableUnloadProtection enableAutoFocus" enctype="multipart/form-data" id="fg-base-edit" method="get" name="edit_form" onsubmit="return checkNtk(this);">
<div class="field ArchetypesStringWidget " id="archetypes-fieldname-searchfield">
    <div>
        <input class="blurrable firstToFocus onesearch_input_cat kellysearch onesearch_input_default" id="cat_Ntt" name="Ntt" title="Find books, eBooks, journals, more" type="text" value="">
    </div>
    <div>
        <select id="Ntk" name="Ntk" title="Limit to" class="kellysearch">
            <option value="Anywhere">anywhere</option>
            <option value="Title">title</option>
            <option value="Author">author</option>
            <option value="6979">journal title</option>
            <option value="Series Title">series title</option>
            <option value="Subject">subject</option>
            <option value="p_call_num_949">call number</option>
        </select>
        <input type="radio" name="N" id="lib-kelly" value="4294967210"><label for="lib-kelly">Kelly Library</label>
        <input type="radio" name="N" id="lib-all" value="0" checked="checked"><label for="lib-all">All UofT Libraries</label>


        <script>
        function checkNtk(frm) {
            var ntk_name = frm.Ntk[frm.Ntk.selectedIndex].innerHTML;
            var ntk_value = frm.Ntk.value;
            if (ntk_name == "journal title") {
                frm.N.value = frm.N.value + " " + ntk_value;
                frm.Ntk.value = "Title";
            }
            return true;
        }

        // default text effect
        jQuery('#cat_Ntt').focus(function() {
            if (jQuery(this).val() == jQuery(this)[0].title) {
                jQuery(this).removeClass('onesearch_input_default');
                jQuery(this).val("");
            }
        });
        jQuery('#cat_Ntt').blur(function() {
            if (jQuery(this).val() == "") {
                jQuery(this).val(jQuery(this)[0].title);
                jQuery(this).addClass('onesearch_input_default');
            }
        });
        jQuery('#cat_Ntt').blur();

</script><div><div class="searchButton_cont_cat kellysearch">
  
  <input class="searchButton" name="form_submit" type="submit" value="Search">
  
  <a advanced="" href="http://search.library.utoronto.ca/advanced" style="text-align:right;">Advanced Search</a>

</div><input name="Ntx" type="hidden" value="mode+matchallpartial"> <input name="fieldset" type="hidden" value="default"> <input name="form.submitted" type="hidden" value="1"> <input name="add_reference.field:record" type="hidden" value=""> <input name="add_reference.type:record" type="hidden" value=""> <input name="add_reference.destination:record" type="hidden" value=""> <input name="last_referer" type="hidden" value="http://content.library.utoronto.ca/"></div></div></div></form>

</div>
			
		

