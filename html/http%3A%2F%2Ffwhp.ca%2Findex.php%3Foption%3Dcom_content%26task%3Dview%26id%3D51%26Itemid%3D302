	  if (active != 'cmenu_pic350') 
    {	
		  event = new Event(event).stop();
		  $('cmenu_pic350').setStyles({	 opacity:0, display:'block'	});	
		  fx350.effect('opacity',{duration:550}).start(0,1);
  	
		  $(active).setStyles({ opacity:0,	 display:'none'	});
		  active =	'cmenu_pic350';
	  }
    if (active == 'cmenu_pic350') 
    {
		  $('cmenu_pic350').setStyles({	 opacity:1, display:'block'	});	
    }
		
