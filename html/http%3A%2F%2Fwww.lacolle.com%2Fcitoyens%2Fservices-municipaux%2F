	$(document).ready(function(){
		
		var $pArr = $('#list_services_municipaux div.serv_muni_block');
		var pArrLen = $pArr.length;
		var pPerDiv = 2;
		for (var i = 0;i < pArrLen;i+=pPerDiv){
			$pArr.filter(':eq('+i+'),:lt('+(i+pPerDiv)+'):gt('+i+')').wrapAll('<div class="serv_muni_wrap" />');
			//$('.serv_muni_block:nth-child(odd)').addClass('odd');
		}
		
    });
    </script>
	
