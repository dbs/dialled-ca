<script type="text/javascript">
    function GetQueryString(parameter) {
        var loc = location.search.substring(1, location.search.length);
        var param_value = false;
        var params = loc.split("&");
        for (i = 0; i < params.length; i++) {
            param_name = params[i].substring(0, params[i].indexOf('='));
            if (param_name == parameter)
                param_value = params[i].substring(params[i].indexOf('=') + 1)
        }
        if (param_value)
            return param_value;
        else
            return false; //Here determine return if no parameter is found
    }
    function GetCookieClient(cookieName) {
        var cookieVal = 1036;
        if (document.cookie)	   //only if exists
        {
            var arr = document.cookie.split((escape(cookieName) + '='));
            if (arr.length >= 2) {
                var arr2 = arr[1].split(';');
                cookieVal = unescape(arr2[0]); //unescape() : Decodes the String
            }
        }
        return cookieVal;
    }
    function SetCookieTitlePage(c_name, value, exdays) {
        var exdate = new Date();
        exdate.setDate(exdate.getDate() + exdays);
        var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
        document.cookie = c_name + "=" + c_value;
    }
    function GetLanguageCookie(name) {
        var url = window.location.href;
        if (url.indexOf(name) > 0) {
            var curLanguage = GetQueryString(name);
            //SetCookieTitlePage(name,curLanguage,30);
            return curLanguage;
        }
        else
            return GetCookieClient(name); //return its value  
    }
    if (document.getElementsByTagName) {
        var titleElement = document.getElementsByTagName('title')[0];
        if (titleElement) {
            //var curLang= '1036';
            var curLang = GetLanguageCookie('PFLG');
            if (this.document.title == "Title field value.")
                this.document.title = (curLang == 1036 ? "L'IRCM" : "The IRCM");
        }
    }
    var oHdfLang = document.getElementById('hdfLangID');
    if (oHdfLang) {
        //var curLang= '1036';
        var curLang = GetLanguageCookie('PFLG');
        oHdfLang.value = curLang;
    }
    function UnloadHandler() {
        document.body.onUnload = "";
        window.onbeforeunload = "";
    } 											
</script>
