                                    function SetlayoutForsearch(number)
                                    {
                                        var obj = document.getElementById("ctl00_searchHomePage_imgFind");            
                                        if(navigator.appName.indexOf("Netscape") >=0 || navigator.appName.indexOf("Opera")>= 0)
                                        {          
                                            obj.style.paddingTop = number;                
                                        }                
                                    }        
                                    function trim(myString) {
								        return myString.replace(/^\s+/g, '').replace(/\s+$/g, '')
								    }
								    function EmptyTextSearch(value) {
								        if (trim(value) == "") {
								            alert("S'il vous plaît entrer un ou plusieurs termes de recherche.");
								            return false;
								        }
								        return true;
								    }
								    
								    function replaceAll(value, keyreplace, key) {
								        while (value.indexOf(keyreplace) > -1) {
								            value = value.replace(keyreplace, key);
								        }								
								        return value;
								    }
								    
								    function KeypressSearch(evt, txtID) {
								                           
                                        try         
                                        {    
								            var keyCode = (window.event) ? event.keyCode : evt.which;								
								            if (keyCode == 13 || keyCode == 0 || keyCode == 1) {
								                GotoPageSearch(txtID);
								                return false
								            }
								            return true;
                                        }
                                        catch(ex){};
								    }
								
								    function GotoPageSearch(txtID) {
                                        try{
								            var value = document.getElementById(txtID).value;
								            if (EmptyTextSearch(value)) 
                                            {
								                var siteSearch = "";
								                siteSearch = window.location.protocol + "//" + window.location.host ;
                                                
								                var url = siteSearch + "/SearchCenterIRCM/results_IRCM.aspx?k=" + trim(value) + "&cs=This%20Site&u=" + replaceAll(replaceAll(siteSearch, "/", "%2F"), ":", "%3A");
                                                
								                location.replace(url);
								            }
                                        }
                                        catch(ex){};
								    }											                                     
                                  </script>
                                    
