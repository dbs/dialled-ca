											<script type="text/javascript">
											    function ClickLanguage(newlanguage, oldlanguage) {
											        var url = window.location.href;
											        if (url.indexOf("?") > -1) {
											            if (url.indexOf("lan") < 0)
											                url = url + "&lan=" + newlanguage;
											            else
											                url = url.replace('lan=' + oldlanguage, 'lan=' + newlanguage);

											            if (url.indexOf("PFLG") < 0)
											                url = url + "&PFLG=" + newlanguage;
											            else
											                url = url.replace('PFLG=' + oldlanguage, 'PFLG=' + newlanguage);
											        }
											        else {
											            url = url + '?PFLG=' + newlanguage + '&lan=' + newlanguage;

											        }
											        //document.cookie = "PFLG="+newlanguage;
											        location.replace(url);
											        return false;
											    }
											</script>
