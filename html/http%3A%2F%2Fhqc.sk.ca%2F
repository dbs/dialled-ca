    <div id="dnn_ctr531_ContentPane"><!-- Start_Module_531 --><div id="dnn_ctr531_ModuleContent" class="DNNModuleContent ModMushroomLTEC">
	
<script type="text/javascript">
    jQuery(document).ready(function () {
        var m_rss_url = 'http://feeds.feedburner.com/sk/QReview';
        var m_loaded='true';
        if (m_rss_url != ''&&m_loaded!='true') {
            jQuery('#dnn_ctr531_ViewMushroomLTE_divRSS').load('/DesktopModules/MushroomLTE/ConfigSvc.ashx?ModuleId=531&PortalId=0&method=LoadRss',on_rss_load531);
        }else if(m_loaded=='true'){
            on_rss_load531();
        }
    });
    function on_rss_load531() {
        var show_tint = 'false';
        if(show_tint=='true'){
            setTimeout("do_tint531()",100);
            setTimeout("do_tint531()",1000);
            jQuery(window).resize(function() {
               setTimeout("do_tint531()",50);
            });
        }

        var open_mode='new';
        if(open_mode=='popup'){

            jQuery('#dnn_ctr531_ViewMushroomLTE_divRSS').find('a').colorbox({width:"80%", height:"80%", top: false, left: false,iframe:true});
        }else if(open_mode=='inline'){
            var links = jQuery('#dnn_ctr531_ViewMushroomLTE_divRSS').find('a');
            jQuery.each(links, function (i, link) {
                var rel=jQuery(link).attr("rel");
                if(rel!='undefined'&&rel!=null&&rel!=''){
                    jQuery(link).colorbox({width:"80%", height:"80%",  top: false, left: false,inline:true, href:jQuery(link).attr("rel"),href_org:jQuery(link).attr("href")});
                }
            });
            
        }
    }

    function do_tint531(){
        
        var tint_alpha = 1;
        var tint_alphaIE = 'alpha(opacity=100)';
        var tint_color = '#FFFFFF';

        var images = jQuery('#dnn_ctr531_ViewMushroomLTE_divRSS').find('img');
        jQuery.each(images, function (i, image) {
            var overlay = jQuery('#img_overlay_531_'+i);
            if(overlay.length<=0){
                var html='<div id="img_overlay_531_'+i+'" class="iwebs-rss-overlay"></div>';
                var image_object=jQuery(image).parent();
                jQuery(html).appendTo(image_object);
            }
            
            overlay.width(jQuery(image).css("width"));
            overlay.height(jQuery(image).css("height"));

            overlay.css("top", jQuery(image).offset().top + "px");
            overlay.css("left", jQuery(image).offset().left + "px");         
            overlay.css("background-color",tint_color);
            overlay.css("opacity",tint_alpha);
            overlay.css("filter",tint_alphaIE);




        });

    }
</script>

