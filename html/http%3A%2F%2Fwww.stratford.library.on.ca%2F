    <div class="region-inner clearfix">
      <section class="panel-pane pane-views-panes pane-featured-panel-pane-1 featured block">
  <div class="block-inner clearfix">
    
              <h2 class="pane-title block-title">Highlight of the Week</h2>
        
    
    <div class="block-content">
      <div class="view view-featured view-id-featured view-display-id-panel_pane_1 view-dom-id-c88f8417173c2f8f85e61215bb85e4b4">
        
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
      
