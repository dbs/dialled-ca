	<style>
	  #accessibility-skip-to {
	    height: 1.5em;
	    line-height: 1.5em;
	    font-size: 1em;
	    width: 100%;
	    color: #fff;
	    background-color: #3a7cd2;
	    padding-left: 0.7em;
	  }
	  #accessibility-skip-to.accessibility-invisible {
        position: absolute;
        height: 1px;
        width: 1px;
        left: -1px;
        top: -1px;
        margin: 0;
        padding: 0;
	    border: 0;
        overflow: hidden;
	  }
	</style>
	<div id="accessibility-skip-to" class="accessibility-invisible">
	  <div class="center-content">
	    <a href="#accessibility-main-section" id="accessibility-skip-to-main" class="accessibility-skip-to-link">skip to main</a>
	  </div>
	</div>
	<script type="text/javascript">
	  $(document).ready(function() {
	    var accessibilitySkipTo = $('#accessibility-skip-to');
	    var accessibilitySkipToLink = $('#accessibility-skip-to-main');
	    accessibilitySkipToLink.focus(function(e) {
			if (accessibilitySkipTo.hasClass('accessibility-invisible')) {
				accessibilitySkipTo.removeClass('accessibility-invisible');
			}
	    }).blur(function(e) {
			accessibilitySkipTo.addClass('accessibility-invisible');
			var nextSibling = accessibilitySkipTo.parent().next();
			if (!nextSibling.is(':tabbable')) {
			  nextSibling.find(':tabbable').first().focus();
			} else {
			  nextSibling.focus();
			}	      
	    });
	    accessibilitySkipToLink.on('click', function(e) {
	    	$('#accessibility-main-section').focus();
	    	return false;
	    });
	    
	  });
	</script>

  
