	<script type="text/javascript">
		var timeOut;
		function scrollToTop() {
			if (document.body.scrollTop != 0
					|| document.documentElement.scrollTop != 0) {
				window.scrollBy(0, -50);
				timeOut = setTimeout('scrollToTop()', 10);
			} 
			else
				clearTimeout(timeOut);
		}
	</script>
	
	<style>
		
	</style>
	<script>
		require(["dojo/on", "dojo/query", "dojo/window", "dojo/domReady!"], function(on, query, win) {
			var updateCollapsiblesCustom = function() {
				var vs = win.getBox();
				var expanded;
				var linkWidgetCollapsibles = query("#footerRow1 .footer-espot .left_espot .linkWidget.collapsible");
				if (vs.w < 943) {
					expanded = false;
					linkWidgetCollapsibles.attr("aria-expanded", expanded.toString());
				} else {
					expanded = true;
					linkWidgetCollapsibles.attr("aria-expanded", expanded.toString());
				}
			};
			updateCollapsiblesCustom();
			on(window, "resize", function(event) {
				updateCollapsiblesCustom();
			});
		});
	</script>

