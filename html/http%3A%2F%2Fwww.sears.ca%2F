<div id ="MiniShopCartContents" dojoType="wc.widget.RefreshArea" widgetId="MiniShopCartContents" controllerId="MiniShopCartContentsController" aria-labelledby="MiniShoppingCart_Label">
</div>

<script type="text/javascript">
  dojo.addOnLoad(function() {
		setMiniShopCartControllerURL(getAbsoluteURL()+'MiniShopCartDisplayView?storeId=10151&catalogId=10051&langId=-1');
		wc.render.getRefreshControllerById("MiniShopCartContentsController").url = getAbsoluteURL()+'MiniShopCartDisplayView?storeId=10151&catalogId=10051&langId=-1&page_view=dropdown';
		if(dojo.byId('MiniShoppingCart') != null && !multiSessionEnabled){
			if($('#ShopCartDisplay').length == 0) { //don't load the mini cart count if on the shopping cart page
				var orderId = shoppingActionsJS.getOrderId();
				var cartCount = shoppingActionsJS.getCartCount();
				if (orderId != null && cartCount != null){
					$('#minishopcart_total').text(' (' + cartCount + ') ');
				}else{
					$('#minishopcart_total').text(' (' + 0 + ') ');
				}
			}
			else { //take the count from the cart page rather than fetching the mini cart
				var cartCountObj = $('.item-count:first'), cartCount = 0;
				if(cartCountObj.length == 0) { //if cart count doesn't exist in pagination, take from cart header instead
					cartCountObj = $('#cart-title');
				}
				
				if(cartCountObj.length > 0) {
					cartCount = cartCountObj.text().replace(/\D/g, '');
				}
				
				$('#minishopcart_total').text(' (' + cartCount + ') ');
				
				var cartTotalCookie = shoppingActionsJS.getCartTotalCookie();
				if(cartTotalCookie != null & cartTotalCookie != 'undefined') {
					var cartTotalArray = cartTotalCookie.split(';');
					cartTotalArray[0] = cartCount;
					dojo.cookie("WC_CartTotal_" + shoppingActionsJS.getOrderId(), cartTotalArray.join(';'), {path:'/'});
				}
			}
		}		
	});
</script>


			</li>
		
		</ul>
