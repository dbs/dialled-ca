<script id="igodigital-script" type="text/javascript">

	$(document).ready(function(){
		showIGoRecsJSON("mini","iGoRecommendationCallback","item_count=1");
	});
	
	function iGoRecommendationCallback(response) {
		var recResponse = response[0]
		recDiv = buildIGoRecsHTML(recResponse, "igo-rec-zone", "vertical", "right", "-1");
		
		var recContainer = $("#igodigital-mini-zone").find("#igodigital-recommendation-container");
		recContainer.append(recDiv);
		
	}
	
