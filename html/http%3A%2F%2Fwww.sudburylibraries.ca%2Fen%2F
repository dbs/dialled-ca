                         <div class="newSocialFeeds">
<div id="socialUpdates">
    <noscript><br /><p>JavaScript Disabled. Please enable JavaScript to view this content.</p></noscript>
</div>
<div id="fb-root"></div>
</div>
<script type="text/javascript">

    var feed = new Array();

    jQuery(function () {
		jQuery("#socialUpdates").html("Loading...");
	
        jQuery.ajax({
            url: '/en/Facebook/FacebookFeed.ashx',
            async: false, // false means other script won't load until this is done.
            //timeout: 5000,
            error: function (xhr, textStatus, thrownError) {
                feed[0] = "Unable to retrieve Facebook feed";
            },
            dataType: 'json',
            success: function (data) {
                feed[0] = "<a href='https://www.facebook.com/GSPLibrary/posts/" + data.data[0].id.split('_')[1] + "' target='_blank'>" + (data.data[0].message != null ? data.data[0].message.length > 220 ? data.data[0].message.substring(0, 220) + " ..." : data.data[0].message : data.data[0].description != null ? data.data[0].description.length > 220 ? data.data[0].description.substring(0, 220) + " ..." : data.data[0].description : data.data[0].story.length > 220 ? data.data[0].story.substring(0, 220) + " ..." : data.data[0].story) + "</a>";
            }
        });

        jQuery.ajax({
            url: '/en/News/topFive.aspx?limit=1&IsSocialFeedItem=true',
            cache: false,
            async: false,
            failure: function (err) {
                //alert("fail" + err);
            },
            error: function (xhr, textStatus, thrownError) {
                feed[1] = "Unable to retrieve News feed";
            },
            success: function (html) {
                feed[1] = jQuery(html).find('#topFiveNewslist').html();
            }
        });

        jQuery.ajax({
            url: '/en/Twitter/TwitterTimeline.ashx?count=1&exclude_replies=true',
            async: false,
            dataType: 'json',
            error: function (xhr, textStatus, thrownError) {
                feed[2] = "Unable to retrieve Twitter feed";
            },
            success: function (data) {
                for (var i in data) {
                    feed[2] = "<a href='https://twitter.com/GSPLibrary/status/" + data[i].id_str + "' target='_blank'>" + (data[i].text.length > 300 ? data[i].text.substring(0, 300) + ' ...' : data[i].text) + "</a>";
                }
            }
        });
/*
        jQuery.ajax({
            url: '/en/iBlog/iBlogFeed.ashx',
            async: false,
            dataType: 'json',
            error: function (xhr, textStatus, thrownError) {
                feed[3] = "Unable to retrieve Blog feed";
            },
            success: function (data) {
                feed[3] = "<a href='" + data.Link + "'>" + data.Title + "</a><br/>";
            }
        });
*/
        jQuery("#socialUpdates").empty();
        var count = 1;

        for (var i in feed) {
            jQuery("#socialUpdates").append("<div id='socialUpdate" + count + "'><h2>Current News: </h2><div class='socialItem'>" + feed[i] + "</div></div>");
            count++;
        }

        jQuery("#socialUpdates").linkify();

        jQuery('#socialUpdates').cycle({
            fx: 'fade',
            delay: 5000,
            speed: 1000,
            cleartypeNoBg: true
        });

    });
</script>
