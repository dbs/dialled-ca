<script type="text/javascript" language="javascript">
    jQuery(function () {

        var randomnumber = Math.floor(Math.random() * 5)
        //alert(randomnumber);
        if (randomnumber == '0') {
            jQuery("#spotlight01").addClass("spotlight currentLink");
        } else {
            if (randomnumber == '1') {
                jQuery("#spotlight01").removeClass("spotlight currentLink");
                jQuery("#spotlight02").addClass("spotlight currentLink");
            } else {
                if (randomnumber == '2') {
                    jQuery("#spotlight01").removeClass("spotlight currentLink");
                    jQuery("#spotlight03").addClass("spotlight currentLink");
                } else {
                    if (randomnumber == '3') {
                        jQuery("#spotlight01").removeClass("spotlight currentLink");
                        jQuery("#spotlight04").addClass("spotlight currentLink");
                    } else {
                        if (randomnumber == '4') {
                            jQuery("#spotlight01").removeClass("spotlight currentLink");
                            jQuery("#spotlight05").addClass("spotlight currentLink");
                        }
                    }
                }
            }
        }
    });
</script>

