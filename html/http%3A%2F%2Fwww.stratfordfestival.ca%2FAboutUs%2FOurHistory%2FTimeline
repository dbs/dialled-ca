<script type="text/javascript">$(document).ready(function () { $('#ctl00_ctl00_ucNavMain_FullPlayBill_sfPlaybillSlider').carousel({ interval: false });}); </script>

<script>
    $("document").ready(function () {

        // PlaybillSlider : Slider Control Event
        $(".slider-control").on('click', function (e) {
            var o = $(".slider-outer").width();
            var w = $(".slider-link:first").width() * $(".slider-link").length;
            var p = $(".slider-outer").scrollLeft();
            if ($(this).hasClass("slider-left")) {
                p = p - o;
                if (p < 0) p = 0;
                //$(".slider-outer").scrollLeft(p);
                $(".slider-outer").animate({ scrollLeft: p }, 800);
                console.log('Slider Left Clicked')
            }
            if ($(this).hasClass("slider-right")) {
                p = p + o;
                if (p > (w-o)) p = (w-o);
                //$(".slider-outer").scrollLeft(p);
                $(".slider-outer").animate({ scrollLeft: p }, 800);
                console.log('Slider Right Clicked')
            }
            e.preventDefault();
        });

    });
</script>

