<SCRIPT>
var globalIsSubmitClicked = false;
function startPB(itemID, progressDivID, progressMessageText) {
//alert ("Start PB");
	if (globalIsSubmitClicked == true)
	{
		var sItem = document.getElementById(itemID);
		if (sItem) sItem.disabled = true;
		return (false);
	}
	globalIsSubmitClicked = true;
	var tItem = document.getElementById('submitProgressTextID');
	if (tItem) tItem.innerHTML=progressMessageText;
	var pItem = document.getElementById(progressDivID);
	if (pItem) pItem.style.display = 'block';
	//pb=hX.getComponentById("view" + "ns_7_48RACFN0QNFG20IUOO48DOQG60_" + ":" + "callerInfoForm:submitProgressBar");
	//pb.visible();
	//pb.start('Please wait...');
//	var f = document.getElementById("view" + "<portlet:namespace/>" + ":" + thisObjectFormID);
//	alert ("type of item for form: " + f.getAttribute('type') + "|" + f.tagName + "|" + f.getAttribute('name'));
//	if (f) f.submit();
	return (true);
}
</SCRIPT>
