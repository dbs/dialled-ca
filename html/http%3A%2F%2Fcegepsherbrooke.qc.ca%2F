		    });
		});*/
		
		//back to top
		jQuery(".btn_up").click(function(){
			jQuery( 'html, body' ).animate( { scrollTop: 0 }, 'slow' );
		});
		
		heightComplements();
		
		/* Homepage */
		if(jQuery( '.home_news' ).width() > "991"){
			var $bloc1 = jQuery('.news').outerHeight();
			var $bloc2 = jQuery('.events').outerHeight();
			var $blocHigh = Math.max($bloc1,$bloc2);
			
			jQuery('.news').css('height',$blocHigh);
			jQuery('.events').css('height',$blocHigh);
		}
		
		/* Page Section */
		jQuery('.section_sidebar').css('height','inherit');
		jQuery('.section_content').css('height','inherit');
		
		if(jQuery( '.section_bloc_text' ).width() > "767"){
			var $bloc_section1 = jQuery('.section_sidebar').outerHeight();
			var $bloc_section2 = jQuery('.section_content').outerHeight();
			var $blocHigh_section = Math.max($bloc_section1,$bloc_section2);
			
			jQuery('.section_sidebar').css('height',$blocHigh_section + 80);
			jQuery('.section_content').css('height',$blocHigh_section + 80);
		}
		
		/* Banniere */
		var $width_ban = 1920;
