			var $blocHigh5 = Math.max($blocComp13,$blocComp14,$blocComp15);
			jQuery('.view-emplois-offres .views-row-13 .lien_savoir_plus').css('height',$blocHigh5);
			jQuery('.view-emplois-offres .views-row-14 .lien_savoir_plus').css('height',$blocHigh5);
			jQuery('.view-emplois-offres .views-row-15 .lien_savoir_plus').css('height',$blocHigh5);
			
			var $blocComp1 = jQuery('.view-emplois-videos .views-row-1').outerHeight();
			var $blocComp2 = jQuery('.view-emplois-videos .views-row-2').outerHeight();
			var $blocComp3 = jQuery('.view-emplois-videos .views-row-3').outerHeight();
			var $blocHigh1 = Math.max($blocComp1,$blocComp2,$blocComp3);
			jQuery('.view-emplois-videos .views-row-1 .lien_savoir_plus').css('height',$blocHigh1);
			jQuery('.view-emplois-videos .views-row-2 .lien_savoir_plus').css('height',$blocHigh1);
			jQuery('.view-emplois-videos .views-row-3 .lien_savoir_plus').css('height',$blocHigh1);
		}
	}
</script>	</body>

</html>
