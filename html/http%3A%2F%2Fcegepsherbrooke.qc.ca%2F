			jQuery('html, body').animate({  
		        scrollTop: (jQuery("#bloc_complements").offset().top)   
		    }, 'slow'); 
		    return false;  
		})
		
		
		/* Menu */
		jQuery(".btn_mobile1").click(function(){
			if(jQuery(".navbar-mobile-2 > div").hasClass("in")){
				jQuery(".navbar-mobile-2 > div").removeClass('in');
				jQuery(".navbar-mobile-2 > div").addClass('collapse');
				jQuery(".navbar-mobile-2 > div").css('height','0px');
			}
			if(jQuery(".navbar-search > div").hasClass("in")){
				jQuery(".navbar-search > div").removeClass('in');
