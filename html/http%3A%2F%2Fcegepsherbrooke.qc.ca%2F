                if (!(jQuery(this).is(e.target) || jQuery(this).has(e.target).length > 0) 
                     && jQuery(this).siblings('.tooltip').length !== 0 
                     && jQuery(this).siblings('.tooltip').has(e.target).length === 0)                  
                {
                     //Remove the popover element from the DOM          
