				jQuery(this).removeClass('active');
			}else{
				jQuery(this).addClass("active");
			}
		  jQuery( ".header_microsite" ).animate({
		    height: "toggle"
		  }, 200, function() {
		    // Animation complete.
		  });
		})
		
