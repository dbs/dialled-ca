                    <script type="text/javascript">
                        var adBlockStarted;
                        var lastShownButton;

                        function GetRotator() {
                            return $find("ctl00_ctl00_MasterContentContainer_MainContentContainer_rrAdBlock");
                        }

                        function showItemByIndex(index) {
                            // gets reference to the rotator object
                            var oRotator = GetRotator();

                            // Sets currently shown item by its index
                            oRotator.set_currentItemIndex(index);

                            if (lastShownButton)
                                lastShownButton.removeClass("active");

                            var currentButton = getButtonByIndex(index);
                            currentButton.addClass("active");
                            lastShownButton = currentButton;
                        }

                        function OnClientItemShown(oRotator, args) {
                            var currentIndex = args.get_item().get_index();

                            if (lastShownButton)
                                lastShownButton.removeClass("active");

                            var currentButton = getButtonByIndex(currentIndex);
                            currentButton.addClass("active");
                            lastShownButton = currentButton;
                        }

                        function getButtonByIndex(index) {
                            var buttonIdSelector = String.format("#Button{0}:first", index);
                            var currentButton = $telerik.$(buttonIdSelector)

                            return currentButton;
                        }

                        function OnClientLoad(oRotator, eventArgs) {
                            adBlockStarted = true;
                            oRotator.autoIntervalID = window.setInterval(function () {
                                oRotator.showNext(Telerik.Web.UI.RotatorScrollDirection.Left);
                            }, oRotator.get_frameDuration());
                        }

                        function startStopRotator() {
                            var rotator = GetRotator();
                            var startStopButton = $telerik.$("#btnStartStop");
                            var direction = Telerik.Web.UI.RotatorScrollDirection.Left;

                            if (!adBlockStarted) {
                                rotator.autoIntervalID = window.setInterval(function () {
                                    rotator.showNext(direction);
                                }, rotator.get_frameDuration());
                                adBlockStarted = true;
                                startStopButton.removeClass("startButton");
                                startStopButton.addClass("stopButton");
                                document.getElementById("btnStartStop").title = "Stop";
                            }
                            else {
                                window.clearInterval(rotator.autoIntervalID);
                                rotator.autoIntervalID = null;
                                adBlockStarted = false;
                                startStopButton.removeClass("stopButton");
                                startStopButton.addClass("startButton");
                                document.getElementById("btnStartStop").title = "Start";
                            }
                        }

                        function showNextItem(clickedButton, direction) {
                            var rotator = GetRotator();
                            rotator.showNext(direction);
                        }

                    </script>
                
