<script>
/*
 * Header Editors Pick SLider
*/
jQuery(document).ready(function(){

	function featureSlider(){
		var featureUnit = jQuery('.pmeditorspick ul');
		var liContent = jQuery('.pmeditorspick ul li').length;
		var featureUnitW = featureUnit.width();
		var currentUnit = 1;
		var arrow = jQuery('.arrow');
		var counter = jQuery('.counter');
		
		featureUnit.width(liContent * featureUnitW );
		
		jQuery('.total').text(liContent);
		jQuery('.current').text(currentUnit);
		
		jQuery('.arrow, .counter').click(function(){
			if(liContent > currentUnit){
				featureUnit.animate({right:featureUnitW*currentUnit},500);
				currentUnit++;
				jQuery('.current').text(currentUnit);
			} else {
				currentUnit = 1;
				jQuery('.current').text(currentUnit);
				featureUnit.animate({right:0},500);
			}
		});
	};
	
	function featureSliderLoad(){
		var feature = setInterval(function(){ 
			var adWidth = jQuery('#gpt-leaderboard div iframe').width();
			var adUnit = jQuery('.adunit');

			if(adWidth != 0 && adWidth != null){
				if(adWidth > 728){
					jQuery(adUnit).parent('.l-top-content').find('.pmeditorspick').css({'visibility':'hidden'});
					jQuery(adUnit).css({'float':'none', 'margin-left':'0px'});
				} else {
					jQuery(adUnit).siblings('.pmeditorspick').css({'display':'block','opacity':'0'}).animate({opacity:1},500);
				}
			}
			if(jQuery('#gpt-leaderboard div iframe').width() > 100){
				clearInterval(feature);
			}
		}, 2000);
	};
	
	featureSliderLoad();
	
	setTimeout(function() { 
		if ( !jQuery( 'body' ).hasClass( 'single-storyline' ) ){
			featureSlider();
		}
	}, 500);
});
	
	
</script>
