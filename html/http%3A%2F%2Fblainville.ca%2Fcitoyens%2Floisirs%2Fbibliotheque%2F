<script>
	var windowSizeClass = function () {
		var _ww = $(window).width();
		if (_ww < 767) {
			size = 'xs';
		}
		if (_ww > 767) {
			size = 'sm';
		}
		if (_ww > 992) {
			size = 'md';
		}
		if (_ww > 1199) {
			size = 'lg';
		}
		if (_ww > 1399) {
			size = 'xl';
		}

		jQuery('body').removeClass('xs sm md lg xl').addClass(size);
	};

	jQuery(window).resize(function () {
		windowSizeClass();
	});

	jQuery(window).load(function () {
		windowSizeClass();
	});
</script>
