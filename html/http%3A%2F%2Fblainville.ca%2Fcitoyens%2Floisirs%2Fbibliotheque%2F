<script>
	jQuery(document).ready(function ($) {
		var handler = $('#legende h2');

		handler.css('cursor', 'pointer');

		handler.on('click', function () {
		 var event = '#cal .events-wrap .event';
		 var type = $(this).data('type');
		 if(type == 'tous'){
		  $(event).show();
		  return false;
		 }
		 $(event).hide();
		 $(event + '.' + type).closest('.event').show();
  		});
	});
</script></div>        
