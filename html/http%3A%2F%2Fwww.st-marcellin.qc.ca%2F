hs.graphicsDir = 'highslide/graphics/';
hs.align = 'center';
hs.transitions = ['expand', 'crossfade'];
hs.fadeInOut = true;
hs.outlineType = 'rounded-white';
hs.showCredits = false;
hs.wrapperClassName = 'dark';
//hs.dimmingOpacity = 0.75;
// Add the controlbar
if (hs.addSlideshow) hs.addSlideshow(
               {
                slideshowGroup: ['album', 'cartes', 'sponsors', 'bottomsponsors'],
	interval: 5000,
	repeat: true,
	useControls: true,
	fixedControls: 'fit',
	overlayOptions: {
		opacity: .6,
		position: 'bottom center',
		hideOnMouseOut: true
	}
});
if (hs.addSlideshow) hs.addSlideshow(
               {
                slideshowGroup: ['infos', 'annonces', 'docs'],
	interval: 5000,
	repeat: true,
	useControls: true,
	fixedControls: 'fit',
	overlayOptions: {
		opacity: .6,
		position: 'bottom center',
		hideOnMouseOut: true,
		offsetY: -6
	}
});
if (hs.addSlideshow) hs.addSlideshow(
               {
                slideshowGroup: ['adtex', 'seao'],
	interval: 5000,
	repeat: true,
	useControls: false,
	fixedControls: 'fit',
	overlayOptions: {
		opacity: .6,
		position: 'bottom center',
		hideOnMouseOut: true,
		offsetY: -6
	}
});
