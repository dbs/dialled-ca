<script type="text/javascript">


//Set the language cookie
	function setLanguage(lang, currentLang){
			
		jQuery.cookie('lang', "1", { expires: 365, path: '/'});
		
		if (lang == currentLang)
		{
			//en or fr 
			jQuery.unblockUI();
			
		}
		else
		{
			window.location = "/" + lang + "/";
		}
	}
	
</script>
