Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(function() {
   $(document).ready(function() {

function PM(this_I) {
      var str = $(this_I).html();
      var t = str.match(/[\d:]+(?= PM)/g);
      if (t !== null) {
        for (var i = 0; i < t.length; i++) {
          var match = t[i].split(':');
          if (match[0] == 12) {
            var th = +match[0] + ' h';
          } else {
            var th = +match[0] + 12 + ' h';
          }
          var tm = ' ' + match[1];
          var ft = th + tm;
          var str = str.replace(t[i], ft).replace(/PM/g, '');
        }
      } else {}
      var t_AM = str.match(/[\d:]+(?= AM)/g);
      if (t_AM !== null) {
        for (var j = 0; j < t_AM.length; j++) {
          var match_AM = t_AM[j].split(':');
          var th_AM = +match_AM[0] + ' h';
          var tm_AM = ' ' + match_AM[1];
          var ft_AM = th_AM + tm_AM;
          var str = str.replace(t_AM[j], ft_AM).replace(/AM/g, '');
        }
      } else {}
      var str = str.replace(/00/g, '');
      var str = str.replace(/All Day Event/g, 'Toute la journée');
      $(this_I).html(str);
    }
    //************************Change AM/PM**********
    //*************Page  Calendar View / voir le calendrier - CalendarViewCalendarContainer  ***************
  $('.CalendarViewCalendarContainer').each(function() {
    PM(this);
  });
  //************* Page Détails de l’évènement  ***************
  $("[id*='CalendarEvent1_LabelDates']").html(function() {
    PM(this);
  });
  //************* Page Afficher la liste ***************
  $("[id*='EventCalendar1_Repeater']").html(function() {
    PM(this);
  });
  //************* idEventCalendar1_Repeater ***************
  $('.HighlightEventTime').each(function() {
    PM(this);
  });
  //************* RSS ***************
  $('.NewsChannelItemDesc').each(function() {
    PM(this);
  });
  $('.HighlightEventTime').each(function() {
    PM(this);
  });

//Changer mois
$(".HighlightEventDate").each(function () { 
     $(this).html($(this).html().replace(/Jan/g,'janvier')); 
    $(this).html($(this).html().replace(/Feb/g,'février')); 
    $(this).html($(this).html().replace(/Mar/g,'mars')); 
    $(this).html($(this).html().replace(/Apr/g,'avril')); 
    $(this).html($(this).html().replace(/May/g,'mai')); 
    $(this).html($(this).html().replace(/June/g,'juin'));   
    $(this).html($(this).html().replace(/July/g,'juillet')); 
    $(this).html($(this).html().replace(/Aug/g,'aout'));   
    $(this).html($(this).html().replace(/Sep/g,'septembre')); 
    $(this).html($(this).html().replace(/Oct/g,'octobre'));   
    $(this).html($(this).html().replace(/Nov/g,'novembre')); 
    $(this).html($(this).html().replace(/Dec/g,'décembre'));   
});
//Changer jours de la semaine
$(".HighlightEventDate").each(function () { 
    $(this).html($(this).html().replace(/Mon,/g,'Lundi')); 
    $(this).html($(this).html().replace(/Tue,/g,'Mardi')); 
    $(this).html($(this).html().replace(/Wed,/g,'Mercredi')); 
    $(this).html($(this).html().replace(/Thu,/g,'Jeudi')); 
    $(this).html($(this).html().replace(/Fri,/g,'Vendredi')); 
    $(this).html($(this).html().replace(/Sat,/g,'Samedi'));   
    $(this).html($(this).html().replace(/Sun,/g,'Dimanche')); 
});

}); 
});

