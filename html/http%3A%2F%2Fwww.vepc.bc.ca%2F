<script type="text/javascript">

//define the days shows will occur on
var eventDays = [[03,13,2013],[03,20,2013],[03,06,2013],[02,20,2013],[02,27,2013],[03,18,2013],[03,25,2013],[02,13,2013],[03,04,2013],[02,25,2013],[03,11,2013],[10,23,2012],[02,06,2013],[02,18,2013],[09,08,2012],[09,09,2012],[09,16,2012]];



function setScheduledDays(date) {

    var isScheduled = true;

    var isEvent = true;

    var scheduleStatus = "";

	
    
    // Check for show
    if (eventDays != null)
		
		

    {


        for (i = 0; i < eventDays.length; i++) {



          if (date.getMonth() == eventDays[i][0] -1 && date.getDate() == eventDays[i][1] && date.getFullYear() == eventDays[i][2])

          {

            isEvent = false;
						
          }

        }

    }   

    if (isEvent)

        return [false];

    else

      return [true];

}  

			$(function(){
				
				// Datepicker
				$('#datepicker').datepicker({					
					inline: true,
					dateFormat:'yy-mm-dd',
					beforeShowDay: setScheduledDays,
					onSelect: function(date) {
							//get_films(date, 'date',0);update_filters(this); return false;
							window.location="/events.html?date="+date;

					}					
					
				});
				
					
				//hover states on the static widgets
				$('#dialog_link, ul#icons li').hover(
					function() { $(this).addClass('ui-state-hover'); }, 
					function() { $(this).removeClass('ui-state-hover'); }
				);
				
			});
					
		</script>


