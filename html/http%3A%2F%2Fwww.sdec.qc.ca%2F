	<script>
	
	$(document).ready(function() {
		$(".ul-mobile form input").focus(function(){
				var id = $(this).attr("data");
				var offset = $(id).offset().top;
				$('.ul-mobile').animate({scrollTop: offset}, 1000);
		});

		$('div.menu-uti .icon:last-child').click(function()
		{
			$('div.search').addClass('active');
		});
		$('div.search span.close-search').click(function()
		{
			$('div.search').removeClass('active');
		});

		$('div.menu-mobile').click(function(e)
		{
			$("div.menu-prin section:last-child > ul.ul-mobile").toggleClass("active");
			$("div.menu-prin section:last-child > ul.ul-mobile li ul").removeClass("active");
			e.stopPropagation();
		});

		$('div.menu-prin section:last-child > ul li').click(function(e)
		{
			var ul = $(this).find('ul');

			$("div.menu-prin section:last-child > ul li ul").not(ul).slideUp('500');

			if ($(ul).hasClass("active")) {
				$(ul).slideUp('500');
				console.log('a');
			}
			else
			{
				$(ul).slideDown('500');
				console.log('b');
			}

			$("div.menu-prin section:last-child > ul li ul").not(ul).removeClass("active");
			$("div.menu-prin section:last-child > ul li").not(this).removeClass("li-active");


			$(ul).toggleClass("active");
			$(this).toggleClass("li-active");

			e.stopPropagation();

		});

		$('body').click(function() {
			$("div.menu-prin section:last-child > ul li ul").removeClass("active");
			$("div.menu-prin section:last-child > ul").removeClass("active");
			$("div.menu-prin section:last-child > ul li").removeClass("li-active");
		});

		if ($(window).width() < 768) {
			$("div.menu-prin section:last-child > ul.ul-mobile").removeClass("active");
		}

		$(window).resize(function()
		{
			if ($(window).width() > 768) {
				$("div.menu-prin section:last-child > ul.ul-mobile").removeClass("active");
				//mu_none();
			}
		});

	/*if ($(window).width() > 768) {
			mu_none();
		}*/

		/*function mu_none()
		{
			jQuery(function($) {
			    var $nav = $('.menu-prin'),
			        $win = $(window),
			        winH = $win.height();    // Get the window height.
			    $win.on("scroll", function () {
	 				$nav.toggleClass("menu-none", $(this).scrollTop() > winH);
	 				$('div.search').removeClass('active');
			    }).on("resize", function(){ // If the user resizes the window
			       winH = $(this).height(); // you'll need the new height value
			    });
			});
		}*/

		if ($(window).width() < 768) {

			jQuery(function () {

			   		var previousScroll = 0;

				    $(window).scroll(function(){

				    	var currentScroll = $(this).scrollTop();

						if (currentScroll > previousScroll){
						  $('.menu-prin').addClass("menu-none",'slow');
						} else {
						  $('.menu-prin').removeClass("menu-none",'slow');
						}

		       			previousScroll = currentScroll;

				   	 });
			});

		}
	});


</script>

