var slide2836;
jQuery(document).ready(function(){ slide2836 = new umgSlide("umg_images2836", 2836);slide2836.umg_transition_caption_do = function(index){this.updateCaption(index);}slide2836.umg_transition_image_align = function(objImage, objImgTag){var position = this.getSuggestedPosition(objImgTag);objImgTag.css("margin-left", position.suggestedX);objImgTag.css("margin-top", position.suggestedY);objImgTag.css("opacity", 0);objImgTag.attr("src", objImage.src);}slide2836.umg_transition_image_activate = function(objCurrent, objPrevious){if (objPrevious.length > 0)objPrevious.fadeTo("slow", 0);objCurrent.fadeTo("slow", 1);}slide2836.buildThumbList = function(){for (var i=0;i<this.umg_images.length ;i++ ){var item = "";if (this.umg_mouseoverbehavior == "moveto"){item = "<a href='javascript:void(0);' onmouseover='slide2836.navigateTo("+i+")'  class='umg_page' id='page2836_"+i+"'><img border='0' src='"+jQuery(this.umg_images[i]).attr("alt")+"' alt='' /></" + "a>";}else if (this.umg_mouseoverbehavior == "scroll"){item = "<a href='javascript:slide2836.navigateTo("+i+")' onmouseover='slide2836.hoverTo("+i+")' class='umg_page' id='page2836_"+i+"'><img border='0' src='"+jQuery(this.umg_images[i]).attr("alt")+"' alt='' /></" + "a>";}else //default, do nothing{item = "<a href='javascript:slide2836.navigateTo("+i+")' class='umg_page' id='page2836_"+i+"'><img border='0' src='"+jQuery(this.umg_images[i]).attr("alt")+"' alt='' /></" + "a>";}this.umg_thumbs.append(item);}}slide2836.previousHover = -1;slide2836.hoverTo = function(index){this.ensureVisibility(index, this.previousHover, 2000);slide2836.previousHover = index;}slide2836.onMoveTo = function(index){if (this.umg_index >= 0){jQuery(this.umg_thumbs.find("#page2836_" + this.umg_index)).removeClass("active");}var currentThumb = jQuery(this.umg_thumbs.find("#page2836_" + index));currentThumb.addClass("active");if (this.umg_more.css("display")!="none"){this.umg_more.attr("innerHTML", "");}this.ensureVisibility(index, this.umg_index, "fast");}slide2836.ensureVisibility = function(index, compareToIndex, speed){if(this.umg_thumbs.width() <= this.umg_large.width()) return;var currentThumb = jQuery(this.umg_thumbs.find("#page2836_" + index));if (index > compareToIndex){//scroll rightvar xOffset = currentThumb.position().left+currentThumb.width()+this.umg_thumbs.position().left;if (xOffset > this.umg_container.width() * .67){var scrollTo = (currentThumb.width() / 2 + currentThumb.position().left)*-1 + this.umg_container.width() * .5;if (scrollTo + this.umg_thumbs.width() < this.umg_container.width()){scrollTo = this.umg_container.width() - this.umg_thumbs.width();}this.umg_thumbs.animate({"left":scrollTo},"fast");}}else{//to leftvar xOffset = currentThumb.position().left+this.umg_thumbs.position().left;if (xOffset < this.umg_container.width() * .33){var scrollTo = (currentThumb.width() / 2 + currentThumb.position().left)*-1 + this.umg_container.width() * .5;if (scrollTo > 0){scrollTo = 0;}this.umg_thumbs.animate({"left":scrollTo},"fast");}}}slide2836.play({mode:"Image", autoPlay:"True", slideTimeout:"3", loopPlay:"Loop", disabledControls:"CaptionTitle", keyboardControl:"Use", mouseOver:"Scroll", easing:"swing"});
});//]]></script></td></tr></table></div><div id="dnn_ctr2836_UltraPhotoGallery_UP_Prog" style="display:none;"><img src="/images/progressbar.gif" alt="ProgressBar" style="border-width:0px;" /></div></div><!-- End_Module_2836 --></TD></TR><TR><TD class="containerrow"><TABLE width="100%" border="0" cellpadding="0" cellspacing="0"><TR><TD align="left" valign="middle" nowrap></TD><TD align="right" valign="middle" nowrap></TD></TR></TABLE></TD></TR></TABLE></div></td><td id="dnn_RightPane" class="RightPane"><div class="DnnModule DnnModule-Banners DnnModule-2559"><a name="2559"></a><div id="dnn_ctr2559_ContentPane"><!-- Start_Module_2559 --><div id="dnn_ctr2559_ModuleContent" class="DNNModuleContent ModBannersC"><table id="dnn_ctr2559_DisplayBanners_lstBanners" cellspacing="0" cellpadding="4" summary="Banner Design Table" border="0" style="border-collapse:collapse;"><tr><td style="border-width:0px;"><span id="dnn_ctr2559_DisplayBanners_lstBanners_ctl00_lblItem"><object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=4,0,2,0" width="277" height="425"><param name=movie value="/Portals/26/Banners/arc_flash.swf"><param name=quality value=high><embed src="/Portals/26/Banners/arc_flash.swf" quality=high pluginspage="http://www.macromedia.com/shockwave/download/index.cgi?P1_Prod_Version=ShockwaveFlash" type="application/x-shockwave-flash" width="277" height="425"></embed></object></span></td></tr></table></div><!-- End_Module_2559 --></div></div></td></tr></table><div class="footer"><span id="dnn_dnnCOPYRIGHT_lblCopyright" class="footer">Copyright 2009 by UCAMA</span></span><span class="loglinks"> <a id="dnn_dnnPRIVACY_hypPrivacy" class="footer" rel="nofollow" href="http://www.ucama.com/Home/tabid/867/ctl/Privacy/Default.aspx">Privacy Statement</a>&nbsp;&nbsp;|&nbsp;&nbsp;<a id="dnn_dnnTERMS_hypTerms" class="footer" rel="nofollow" href="http://www.ucama.com/Home/tabid/867/ctl/Ter