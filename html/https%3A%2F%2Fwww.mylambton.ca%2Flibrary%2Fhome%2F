    <!-- hover delay on menus -->
    <!-- see http://www.thatsquality.com/articles/creating-delayed-drop-down-menus-in-jquery-without-losing-accessibility -->
    <script type="text/javascript">
        $(document).ready(function () {
            $("#menu li.drop").hoverIntent({
                interval: 50, // ms delay before onMouseOver
                over: drops_show,
                timeout: 250, // ms delay before onMouseOut
                out: drops_hide
            });
            $("#menu li.drop").addClass('with-js'); //make menu open delay work
        });

        function drops_show() { $(this).addClass('show'); $(this).removeClass('with-js'); }
        function drops_hide() { $(this).removeClass('show'); $(this).addClass('with-js'); }
    </script>
