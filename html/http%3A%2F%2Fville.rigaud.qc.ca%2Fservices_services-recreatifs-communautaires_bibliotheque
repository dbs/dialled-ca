	<div class="tribe-events-loop">

					
			<!-- Event  -->
			<div class="type-tribe_events post-4866 tribe-clearfix tribe-events-category-activites-et-evenements tribe-events-venue-1705 tribe-events-organizer-3167 tribe-events-first">
				
<div class="tribe-mini-calendar-event event-0  first ">

	<div class="list-date">
					<span
				class="list-dayname">lun</span>
		
		<span
			class="list-daynumber">12</span>
	</div>

	<div class="list-info">
		
