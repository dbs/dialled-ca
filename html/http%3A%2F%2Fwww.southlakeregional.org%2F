 <script type="text/javascript" language="javascript">
     function searchTextFocus(e) {
         if (!$(e).hasClass('searchTextFocused')) {
             $(e).val('');
             $(e).addClass('searchTextFocused');
         }
     }
     function searchTextBlur(e) {
         if (typeof String.prototype.trim != 'function'){
            String.prototype.trim = function() {
                return this.replace(/^\s+|\s+$/g, '');
            }
         }
         if ($(e).val().trim() == '') {
             $(e).removeClass('searchTextFocused');
             $(e).val('Search');
         }
     }
     function checkSearchText() {
         if (!$('#txtSearch').hasClass('searchTextFocused')) {
             $('#txtSearch').val('');
         }
     }
     
     function redirectPage(){
        var searchtext = document.getElementById("txtSearch").value;
        window.location =  "GoogleSearch.aspx?searchtext=" + searchtext + " ";
     }
     
    function RightClick(e) { 
        var event1;
        
        if(window.event){
            event1 = window.event;
            if (event1.which == 3 || event1.keycode == 3 || event1.button == 2 ) {
                alert('Southlake’s logo is a registered trademark and may only be used with the permission of Southlake’s Corporate Communications department. ');
                return false;
            }
        } else {
            if(e.buttons || e.which || e.keyCode){
               if ((e.which == 3) || (e.keyCode == 3) || (e.buttons ==2) ) {
                    alert('Southlake’s logo is a registered trademark and may only be used with the permission of Southlake’s Corporate Communications department. ');
                    return false;
               } else {
                    return true;
              }
            }

       }
    }

    function keyDownClick(e) { 
        return true;
    }
        
</script>

