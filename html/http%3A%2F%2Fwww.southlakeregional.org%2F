    var timerId;
    var bannerScrollTimeout = 6000; //milliseconds

    function rotateBannerRight(halt) {
        if (halt)
            stopAutoRotate();
        var currentItem = getBannerCurrentItem();
        $(currentItem).find('.rotatingBannerPage').fadeOut('slow');
        if ($(currentItem).next().length > 0) {
            $(currentItem).fadeOut('slow', function () {
                $(currentItem).next().fadeIn('slow');
                $(currentItem).next().find('.rotatingBannerPage').fadeIn('slow');
            });
        }
        else {
            $(currentItem).fadeOut('slow', function () {
                $('.rotatingBannerPageContainer:first').fadeIn('slow');
                $('.rotatingBannerPageContainer:first').find('.rotatingBannerPage').fadeIn('slow');
            });
        }
    }
    function rotateBannerLeft() {
        stopAutoRotate();
        var currentItem = getBannerCurrentItem();
        $(currentItem).find('.rotatingBannerPage').fadeOut('slow');
        if ($(currentItem).prev().length > 0) {
            $(currentItem).fadeOut('slow', function () {
                $(currentItem).prev().fadeIn('slow');
                $(currentItem).prev().find('.rotatingBannerPage').fadeIn('slow');
            });
        }
        else {
            $(currentItem).fadeOut('slow', function () {
                $('.rotatingBannerPageContainer:last').fadeIn('slow');
                $('.rotatingBannerPageContainer:last').find('.rotatingBannerPage').fadeIn('slow');
            });
        }
    }
    function startAutoRotate() {
        $('#rotatingBannerControlButton').attr('href', 'javascript:stopAutoRotate();');
        $('#rotatingBannerControlButton').attr('title', 'Pause Rotating Banner');
        $('#rotatingBannerControlButton img').attr('src', '../imgs/pause_btn.gif');
        $('#rotatingBannerControlButton img').attr('alt', 'Pause Button');
        timerId = window.setInterval(function () { rotateBannerRight(false); }, bannerScrollTimeout);
    }
    function stopAutoRotate() {
        window.clearInterval(timerId);
        $('#rotatingBannerControlButton').attr('href', 'javascript:rotateBannerRight(false);startAutoRotate();');
        $('#rotatingBannerControlButton').attr('title', 'Play Rotating Banner');
        $('#rotatingBannerControlButton img').attr('src', '../imgs/play_btn.gif');
        $('#rotatingBannerControlButton img').attr('alt', 'Play Button');
    }
    function goToBannerItem(itemIndex) {
        stopAutoRotate();
        var currentItem = getBannerCurrentItem();
        $(currentItem).fadeOut('slow', function () {
            $('.rotatingBannerPageContainer:eq(' + itemIndex + ')').fadeIn('fast');
            $('.rotatingBannerPageContainer:eq(' + itemIndex + ')').find('.rotatingBannerPage').fadeIn('fast');
        });
    }
    function getBannerCurrentItem() {
        return $('.rotatingBannerPageContainer:visible');
    }
    $(function () {
        startAutoRotate();
    });
-->
</script>

