        var Language = function () {

            var _Language = jQuery('.languageSwitch').children().html();

            if ((typeof (_Language) != 'undefined') && (_Language != ''))
            {
                switch (_Language)
                {
                    case "Français": // Page is in English
                        lang = "en";
                        break;
                    case "English": // Page is in French
                        lang = "fr";
                        break;
                    default:
                        lang = "en";
                        break;
                }
            }
            else
                lang = "en";

            return lang;
// Was based on cookie but was not reliable.  Code is based on the link to switch language
        }
        var HelpTitle = function (lang) {
            if (Language() == "en")
                return "Frequently Asked Questions/Help";
            else
                return "Foire aux questions/Aide";
        }
        var CMA_Global = {};
        
        jQuery(document).ready(function () {

            if (Language() == 'en')
                CMA_Global = { "student": "medical student", "resident": "medical resident", "physician": "practising physician", "retired": "retired physician", "public": "member of the public" }
            else
                CMA_Global = { "student": "étudiant en médecine", "resident": "médecin résident", "physician": "médecin praticien", "retired": "médecin retraité", "public": "membre du public" };

            var lastValue = TabPanelValue();

            if ((typeof (lastValue) != 'undefined') && (lastValue != '')) {

                SetOptionChoice();

                if (lastValue == 'student') { jQuery('.student').show(); jQuery('.resident, .physician, .retired, .public').hide(); }
                if (lastValue == 'resident') { jQuery('.resident').show(); jQuery('.student, .physician, .retired, .public').hide(); }
                if (lastValue == 'physician') { jQuery('.physician').show(); jQuery('.student, .resident, .retired, .public').hide(); }
                if (lastValue == 'retired') { jQuery('.retired').show(); jQuery('.student, .physician, .resident, .public').hide(); }
                if (lastValue == 'public') { jQuery('.public').show(); jQuery('.retired, .student, .physician, .resident').hide(); }

                jQuery("#DropdownControl").find('option').each(function () {
                    var val = jQuery(this).attr("value");

                    if (val == lastValue)
                        jQuery(this).attr("selected", "selected");
                    else
                        jQuery(this).removeAttr("selected");
                });

            }
            else {
                SetOptionChoice();
            }
        });

        function SetOptionChoice() {
            var Option = jQuery("#DropdownControl").find('option');

            if ((typeof (Option) != 'undefined') && (Option != '')) {

                jQuery(Option).each(function (index, item) {
                    var opt = jQuery(this);
                    switch (opt.val()) {
                        case "resident":
                            jQuery(opt).html(CMA_Global.resident);
                            break;
                        case "retired":
                            jQuery(opt).html(CMA_Global.retired);
                            break;
                        case "student":
                            jQuery(opt).html(CMA_Global.student);
                            break;
                        case "physician":
                            jQuery(opt).html(CMA_Global.physician);
                            break;
                        case "public":
                            jQuery(opt).html(CMA_Global.public);
                            break;
                    }
                });

            }
        }
        function getval(sel) {
            if (typeof (sel) != 'undefined') {
                var status = sel.value;

                Cookies.set('TabPanelValue', status);

                if (status == 'student') { jQuery('.student').show(); jQuery('.resident, .physician, .retired, .public').hide(); }
                if (status == 'resident') { jQuery('.resident').show(); jQuery('.student, .physician, .retired, .public').hide(); }
                if (status == 'physician') { jQuery('.physician').show(); jQuery('.student, .resident, .retired, .public').hide(); }
                if (status == 'retired') { jQuery('.retired').show(); jQuery('.student, .physician, .resident, .public').hide(); }
                if (status == 'public') { jQuery('.public').show(); jQuery('.retired, .student, .physician, .resident').hide(); }
            }
        }
