<script>
  
  $(function(){
    $('#main-menu .dropdown').hoverIntent({
	       over: makeAppear, 
	       timeout: 0, 
	       out: makeDisappear
    });
    $('#minisite-menu .dropdown').hoverIntent({
	       over: makeAppear, 
	       timeout: 0, 
	       out: makeDisappear
    });
   
    function makeAppear(){
	$(this).addClass('open');
     }
    function makeDisappear(){
       $(this).removeClass('open');
    }
  
    $('#incfont').click(function(){    
        curSize= parseInt($('.main-content').css('font-size')) + 2;
	lineSize= parseInt($('.main-content').css('line-height')) + 4;
	if(curSize<=20){
	      $('.main-content').css('font-size', curSize);
	      $('.main-content').css('line-height', lineSize+'px');
	}
    });  
    $('#decfont').click(function(){    
	curSize= parseInt($('.main-content').css('font-size')) - 2;
	lineSize= parseInt($('.main-content').css('line-height')) - 4;
	if(curSize>=12){
	      $('.main-content').css('font-size', curSize);
	      $('.main-content').css('line-height', lineSize+'px');
	}
    });
    
  /*
    $('#colonne-menu').affix({
	offset: {
	top: 560
      , bottom: function () {
	  return (this.bottom = $('footer').outerHeight(true))
	}
      }
    })
*/
 
  });

</script>



