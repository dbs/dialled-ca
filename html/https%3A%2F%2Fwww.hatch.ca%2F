<script>
    function SwitchLanguage(control) {
        var lang = control.value;
        $.ajax({
            url: "",
            methode: "POST",
            dataType: "application/json",
            data: {
                language: lang
            },
            complete: function () {
                var href = location.href.replace(/\/(en|fr|es)\//i, "/" + lang + "/");
                if (href.indexOf(lang) === -1) {
                    location.pathname = "/" + lang + location.pathname;
                } else {
                    location.href = href;
                }
            }
        })
    }

    function GoogleSearch(e) {
        if (e.keyCode == 13) {
            var keyword = $("#q").val();
            window.location = '/Search-Results' + '?q=' + keyword;
        }
    }
</script>
