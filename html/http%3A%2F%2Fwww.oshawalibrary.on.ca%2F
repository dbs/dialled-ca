    <script type="text/javascript">
<!--//--><![CDATA[// ><!--

function validinput (input)
	{
	if (input!='Search Our...' && input!='')
		{
		return true;
		}
	else
		{
		return false;
		}
	}

//--><!]]>
</script><script src="/sites/all/scripts/ebscohostsearch.js" type="text/javascript">
<!--//--><![CDATA[// ><!--


//--><!]]>
</script><script>
<!--//--><![CDATA[// ><!--

jQuery(document).ready( function() 
	{
	jQuery("#standintext").keyup(function () 
		{
		jQuery("input[name='ebscohostsearchtext']").val(jQuery("input[name='standintext']").val());
		jQuery("input[name='qu']").val(jQuery("input[name='standintext']").val());
		jQuery("input[name='q']").val(jQuery("input[name='standintext']").val());
		jQuery("input[name='FullTextCriteria']").val(jQuery("input[name='standintext']").val());
		});
	/* Catch Mouse Click on the standin button */
	jQuery("#submitform").click(function()
		{
		if (validinput(jQuery('#standintext').val()))
			{
			if (jQuery('#selectlist').val() == 'ebsco')
				{
				jQuery("#ebscohostCustomSearchBox").submit();
				}
			if (jQuery('#selectlist').val() == 'catalogue')
				{
				jQuery("#catform").submit();
				}
			if (jQuery('#selectlist').val() == 'images')
				{
				jQuery.get("http://www.oshlib.ca/link.php?db=NEWOB");
				jQuery("#imagesform").submit();
				}
			if (jQuery('#selectlist').val() == 'ebooks')				
				{
				$searchval=jQuery("input[name='standintext']").val();
				window.open('https://oshawa.overdrive.com/search?q='+$searchval,'OverDrive');				
				}
			if (jQuery('#selectlist').val() == 'website')
				{
				$searchval=jQuery("input[name='standintext']").val();
				window.open('/search/node/'+$searchval,'OPL'); /* IE9 fails if space in Window Title using window.open method - stupid but true */
				}
			}
		});
	/* Catch Enter press on the standin field */
	jQuery("#standintext").keypress(function(event) 
		{		
		if (validinput(jQuery('#standintext').val()))
			{
			if (event.which == 13) 
				{
				event.preventDefault();
				if (jQuery('#selectlist').val() == 'ebsco')
					{
					jQuery("#ebscohostCustomSearchBox").submit();
					}
				if (jQuery('#selectlist').val() == 'catalogue')
					{
					jQuery("#catform").submit();
					}
				if (jQuery('#selectlist').val() == 'images')
					{
					jQuery.get("http://www.oshlib.ca/link.php?db=NEWOB");
					jQuery("#imagesform").submit();
					}
				if (jQuery('#selectlist').val() == 'ebooks')
					{
					$searchval=jQuery("input[name='standintext']").val();
					window.open('https://oshawa.overdrive.com/search?q='+$searchval,'OverDrive');				
					}
				if (jQuery('#selectlist').val() == 'website')
					{
					$searchval=jQuery("input[name='standintext']").val();
					window.open('/search/node/'+$searchval,'OPL');
					}
				}
			}
		});
	});

//--><!]]>
</script><form onsubmit="return false;" id="search-block-form" accept-charset="UTF-8">
