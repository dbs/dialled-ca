				<script>
					$(function(){
						var carouselHeight = 0;
						var nouvellesCarousel = $('.blocNouvelles .owl-carousel');
							nouvellesCarousel.owlCarousel({
							loop:true,
							margin:26,
							nav:true,
							items:2,
							stagePadding: 90,
							navContainer:'.blocNouvelles',
							lazyLoad : true,
							responsive:{
								0:{
									items:1,
									stagePadding: 0,
								},
								500:{
									items:2,
									stagePadding: 0,
								},
								830:{
									items:3,
									stagePadding: 0,
								},								
								1175:{
									items:3
								},
								1300:{
									items:2
								}
							},
							onRefresh: function () {
								//nouvellesCarousel.find('div.owl-item').height('');
							},
							onRefreshed: function () {
								//console.log(nouvellesCarousel.height());
								//nouvellesCarousel.find('div.owl-item').height(nouvellesCarousel.height());
							}
						});	
						
						$('.blocNouvelles .owl-carousel .owl-item img').each(function(){
							$(this).load(function(){								
								if($(this).parents('.owl-item article').height() > carouselHeight){
									carouselHeight = $(this).parents('.owl-item article').height();
									$('.blocNouvelles .owl-carousel .owl-item').height(carouselHeight);									
								}
							});
						});
					});									
				</script>
			</section>
