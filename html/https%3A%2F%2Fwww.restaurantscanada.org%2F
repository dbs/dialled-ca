<script type="text/javascript">
    function HTMLDecode(s) {
        return jQuery('<div></div>').html(s).text();
    }
    if (window.location.href.toString().indexOf("/fr") != -1) {
        google.load('search', '1', { "language": "fr" });
    }
    else {
        google.load('search', '1', { "language": "en" });
    }

    google.setOnLoadCallback(function () {
        var customSearchOptions = {}; var customSearchControl = new google.search.CustomSearchControl(
    '016536478225312344497:kfird1hmlne', customSearchOptions);

        var options = new google.search.DrawOptions();
        if (window.location.href.toString().indexOf("/en") != -1) {
            options.enableSearchboxOnly(
                      "https://" + document.domain + "/en/Results", "q", false, "?")
        }
        else if (window.location.href.toString().indexOf("/fr") != -1) {
            options.enableSearchboxOnly(
                      "https://" + document.domain + "/fr/Resulter", "q", false, "?")
        }
		else if (window.location.href.toString().indexOf("") != -1) {
            options.enableSearchboxOnly(
                      "https://" + document.domain + "/en/Results", "q", false, "?")
        }
        customSearchControl.draw('cse', options);
        var tdbutton = $('td.gsc-search-button');
        tdbutton.insertBefore($('td.gsc-input'));
        if (window.location.href.toString().indexOf("/fr") != -1) {
            $('input.gsc-input').attr('placeholder', HTMLDecode('Entrer un mot-cl&eacute;'));
        }
        else {
            $('input.gsc-input').attr('placeholder', 'Enter search term');
        }
        //$('input.gsc-input').attr('placeholder','Enter search term');
        tdbutton.show();
    }, true);

</script>


