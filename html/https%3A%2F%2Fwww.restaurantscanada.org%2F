<script type="text/javascript">



    $(function () {
        if ($.browser.version === "8.0") {
            $('.flexslider').parent().addClass('row');
            $('.flexslider1').css('margin-left', '0%').css('width', '25.5%');
        }
        $('.dcsmt.static').css('width', '100%');
        $('.tab-inner.loaded').css('width', '100%');
        $('ul.dcsmt-slider li.tab-content').css('width', '100%');
        $('.social_feed_title').insertBefore($('ul.social-tabs li:first'));
        var slideshowSpeed = 9000;
        var hdHomePageCarouselSlidingSpeed = $('[id$=hdHomePageCarouselSlidingSpeed]');
        if (hdHomePageCarouselSlidingSpeed.length && hdHomePageCarouselSlidingSpeed.val().length > 0) {
            slideshowSpeed = hdHomePageCarouselSlidingSpeed.val();
        }

        var animationSpeed = 800;
        $('.flexslider').flexslider({
            animation: "slide", slideshowSpeed: slideshowSpeed, animationSpeed: animationSpeed,
            start: function (slider) {
                $('body').removeClass('loading');
            }, useCSS: false
        });
        $('.flexslider1').flexslider({
            animation: "slide", namespace: "flex1-", slideshowSpeed: 1200000, animationSpeed: animationSpeed,
            start: function (slider) {
                $('body').removeClass('loading');
            }
        });
        reAdjustCaraousel();
       // alert($(window).width());

    });
    $(window).resize(function () {
        reAdjustCaraousel();
    });
    function reAdjustCaraousel() {
        var windowWidth = $(window).width();
        if (!$('.mobile_nav').is(':visible') || windowWidth > 384) {
            var bannerPaneWidth = $('[id$=BannerPane]').width();
            var left_slider_content_width = (bannerPaneWidth * 9 * 5 / 144);
            var left_slider_navigation = $('.flex-control-nav');
            var left_slider_navigation_width = left_slider_navigation.outerWidth();
            left_slider_navigation.css('right', (left_slider_content_width - left_slider_navigation_width) / 2);
            var left_slider_image_width = (bannerPaneWidth * 9 * 7 / 144);
            var bannerLeftSliderImageHeight = parseInt((359 * left_slider_image_width) / 475);
            var bannerRightSlider = $('#bannerRightSlider');
            bannerRightSlider.height(bannerLeftSliderImageHeight);
            var bannerRightSliderImageWidth = (bannerPaneWidth * 3 / 12);
            var bannerRightSliderImageHeight = parseInt((230 * bannerRightSliderImageWidth) / 283);
            var rightPager = $('.flex1-control-nav');
            var rightPagerWidth = $('#bannerRightSlider .flex1-control-nav > li').length * 15;
            rightPager.css({ 'bottom': (bannerLeftSliderImageHeight - bannerRightSliderImageHeight) - 45, 'left': (bannerRightSliderImageWidth / 2) - rightPagerWidth + 10 });
        }
    }
</script>
