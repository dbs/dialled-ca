<script type="text/javascript">
    $(function () {
        var nativename = '';
        var languageISOCode = $('#hdLanguageISOCode').val();
        $('.language-object .Language.selected').remove();
        $('.Language a:not(.selected)').each(function () {
            nativename = $(this).html();
            $(this).html(nativename.substring(0, nativename.indexOf('(') - 1)).show();
            $(this).attr('title', nativename.substring(0, nativename.indexOf('(') - 1));
        });
        //alert(languageISOCode);
    });
