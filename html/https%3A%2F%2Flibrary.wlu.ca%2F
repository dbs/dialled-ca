              <h2 class="block__title block-title">Primo search</h2>
        
    <script>
  window.onload = function() {
    document.forms.searchForm.query_tug.value = null;
    document.forms.searchForm.query.value = null;
  }; 

  window.onunload = function(){};

  function validateForm() {
    var x=document.forms.searchForm.query_tug.value;
    if (x==null || x=="") {
      alert("Please enter a search term");
      return false;
    } else {
      x = x.replace(/[,]/g,' ');
      document.forms.searchForm.query.value="any,contains,"+x;
    }
  }
</script>

<form action="http://primo.tug-libraries.on.ca/primo_library/libweb/action/dlSearch.do?" class="EXLSearchForm" enctype="application/x-www-form-urlencoded; charset=utf-8" id="searchForm" method="get" name="searchForm" onsubmit="return validateForm()">
