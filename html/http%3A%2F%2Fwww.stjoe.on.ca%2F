			$(document).ready(function(){
				//To switch directions up/down and left/right just place a "-" in front of the top/left attribute
			
				//Caption Sliding (Partially Hidden to Visible)
				$('.boxgrid.caption').hover(function(){
					$(".cover", this).stop().animate({top:'180px'},{queue:false,duration:160});
				}, function() {
					$(".cover", this).stop().animate({top:'0px'},{queue:false,duration:160});
				});
			});
		</script>
        
<script language="javascript">

var error = false;

function validation() {

	clearboxes();
	
	
	//email


	var email_expr=new RegExp("^[A-Z0-9a-z._%+-]+@(?:[A-Za-z0-9-]+\.)+[A-Za-z]{2,4}$");
	if (document.signupform.emailaddress.value == "" || email_expr.test(document.signupform.emailaddress.value) == false) {
		document.signupform.emailaddress.style.backgroundColor = "#FFFFCC";
		error = true;
	}
	
	
	if (error) {
		document.getElementById('error_message').className="visible";	
		return false;
	} else {
		document.getElementById('sendform').value="yes";	
		document.signupform.submit();
		return true;
	}
}

function clearboxes() {


	document.signupform.emailaddress.style.backgroundColor = "#FFFFFF";
	error = false;
}

</script>
        
