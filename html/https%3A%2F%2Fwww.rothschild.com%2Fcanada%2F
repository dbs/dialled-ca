<script type="text/javascript">

    $(document).ready()
    {
        $('.mapoverlay').hide();
        var mouse_is_inside = false;

        $('#overlays').hover(function () {
            mouse_is_inside = true;
        }, function () {
            mouse_is_inside = false;
        });

        $("body").mouseup(function () {
            if (!mouse_is_inside) {
                if ($("div#ctl00_ContentPlaceHolder_UxOfficeLocation_UxRegionContent").hasClass("greyBlockClass")) {
                    $("div#ctl00_ContentPlaceHolder_UxOfficeLocation_UxRegionContent").removeClass("greyBlockClass");
                }
                $(".mapoverlay").animate({ opacity: 'hide' }, "fast");
            }
        });



        $(".btn_close").click(function () {
            if ($("div#ctl00_ContentPlaceHolder_UxOfficeLocation_UxRegionContent").hasClass("greyBlockClass")) {
                $("div#ctl00_ContentPlaceHolder_UxOfficeLocation_UxRegionContent").removeClass("greyBlockClass");
            }
            $(".mapoverlay").animate({ opacity: 'hide' }, "fast");
        });
    };

    function showOverlay(link) {
        if (!$("div#ctl00_ContentPlaceHolder_UxOfficeLocation_UxRegionContent").hasClass("greyBlockClass")) {
            $("div#ctl00_ContentPlaceHolder_UxOfficeLocation_UxRegionContent").addClass("greyBlockClass");
        }

        // Reload iframe for IE.
        //$('.googlemap iframe').attr("src", $('.googlemap iframe').attr("src"));

        $('.mapoverlay').animate({ opacity: 'hide' }, "fast");
        var overlay = "#overlay_" + $(link).attr("id");
        var overlayInner = overlay + " #map_nether";

        var googleMap = overlay + " .googlemap";
        var googleMapIframe = overlay + " .googlemap iframe";

        var newIFramw = document.createElement('iframe');
        newIFramw.src= $(googleMapIframe).attr("src");
        newIFramw.setAttribute("height","337");
        newIFramw.setAttribute("width", "464");
        newIFramw.setAttribute("scrolling", "no");
        newIFramw.setAttribute("frameborder", "0");
        
        $(googleMapIframe).remove();
        $(googleMap).append(newIFramw);

        $(overlayInner).show();
        $(overlay).animate({ opacity: 'show' }, "slow");
    }

    function openPrint(id) {
        window.open("/Print.aspx?id=" + id);
    }

</script>

