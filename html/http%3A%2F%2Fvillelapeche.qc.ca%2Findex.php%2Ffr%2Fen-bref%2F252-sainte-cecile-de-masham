			<h6>Calendar</h6>
			
			<script type="text/javascript">
			    var m_events = [{"title":"Regular meeting 2016-09-06","date_start":1473134400000,"date_end":0,"options":{"summary":"Regular meeting of the council 2016-09-06"},"link":"en\/diffusions\/conseil\/?date=1473134400&article=regular-meeting-2016-09-06","date":"September 6, 2016"},{"title":"Regular meeting 2016-09-19","date_start":1474257600000,"date_end":0,"options":{"summary":"Regular meeting of the council 2016-09-19"},"link":"en\/diffusions\/conseil\/?date=1474257600&article=regular-meeting-2016-09-19","date":"September 19, 2016"}];
			
			    $(document).ready(function() {
			        ivw_calendar(1473611413000, ivw_calendarMonthCallBack);
			        ivw_calendarSelectDateJSON(document.getElementById("calendar-container"), m_events);
			    });
			    
			    var ivw_calendarMonthCallBack = function(p_month) {
			        var data = "a=month&feed=nouvelles%26evenements%26conseil&lang=2&date=" + p_month;
			        i_ivwanaAjax("http://villelapeche.qc.ca//system/managers/FeedManager.php?lang=en", data, function(response) {
			            m_events = jQuery.parseJSON(response);
			            ivw_calendarSelectMonth(p_month);
			            ivw_calendarSelectDateJSON(document.getElementById("calendar-container"), m_events);
			        });
			    }
			
			    function ivw_calendarDateCallBack(p_response, p_object) {
			        var html = "";
			        var lastDate = null;
			
			        if (!p_response || (p_response && p_response.length === 0)) {
			            $("#ivw_calendarInformation").hide();
			            return false;
			        }
			
			        for (var i in p_response) {
			            if (typeof p_response[i] === "object") {
			                if (lastDate !== p_response[i].date) {
			                    html += '<h4>' + p_response[i].date + '</h4>';
			                    lastDate = p_response[i].date;
			                }
			                html += '<a href="http://villelapeche.qc.ca/' + p_response[i].link + '">' + p_response[i].title + '</a>';
			            }
			        }
			        
			        $("#ivw_calendarInformation").html(html);
			        $("#ivw_calendarInformation").show();
			
			        $("#ivw_calendarInformation").css("left", p_object.position().left + (142 - $("#ivw_calendarInformation").outerWidth()));
			        $("#ivw_calendarInformation").css("top", p_object.position().top - $("#ivw_calendarInformation").outerHeight() + 75);
			    }
			    
			    function ivw_calendarConvertDateToString(p_date) {
			        var string = "";
			        
			        ivw_calendarConvertDateToString
			
			        return string;
			    }
			</script>
