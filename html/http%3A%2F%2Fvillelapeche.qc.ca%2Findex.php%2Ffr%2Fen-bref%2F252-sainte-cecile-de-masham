			        if (!p_callback) {
			            p_callback = function(response) {
			                            console.log(response);
			                         }
			        }
			        
			        ivw_calendarCallback = p_callback;
			        
			        p_dateInMiliseconds += (new Date().getTimezoneOffset() * 60000);
			        ivw_calendarSelectMonth(p_dateInMiliseconds, p_monthsToDisplay);
			        document.getElementById("calendar-container").style.display = "block";
			    }
			
			    function ivw_calendarSelectMonth(p_dateInMiliseconds, p_monthsToDisplay) {
			        var html = ivw_calendarRender(new Date(p_dateInMiliseconds), p_monthsToDisplay);
			
			        document.getElementById("calendar-container").innerHTML = html;
			
			        if (typeof ivw_calendarDateCallBack === "function") {
			            $(".days > td").mouseover(function() {
			                var date  = parseInt($(this).attr("class").split(" ")[0]);
			                var group = new Array();
			                for(var i in m_events) {
			                    if ((date >= m_events[i]["date_start"] && date <=  m_events[i]["date_end"]) || (date === m_events[i]["date_start"] && m_events[i]["date_end"] === 0)) {
			                        group.push(m_events[i]);
			                    }
			                }
			                ivw_calendarDateCallBack(group, $(this));
			            });
			            $(".days > td").mouseout(function() {
			                ivw_calendarDateCallBack(false, $(this));
			            });
			        }
			
			        if (typeof ivw_calendarDateClickCallBack === "function") {
			            $(".days > td").click(function() {
			                var date  = parseInt($(this).attr("class").split(" ")[0]);
			                var group = new Array();
			                for(var i in m_events) {
			                    if ((date >= m_events[i]["date_start"] && date <=  m_events[i]["date_end"]) || (date === m_events[i]["date_start"] && m_events[i]["date_end"] === 0)) {
			                        group.push(m_events[i]);
			                    }
			                }
			                $("#calendar-container").find(".current").removeClass("current");
			                $(this).addClass("current");
			                ivw_calendarDateClickCallBack(group, $(this));
			            });
			        }
			        if (typeof ivw_calendarDateMonthCallBack === "function") {
			            $("#calendar-controls > tbody > tr > td:nth-child(2)").mouseover(function() {
			                ivw_calendarDateMonthCallBack(m_events, $(this));
			            });
			            $("#calendar-controls > tbody > tr > td:nth-child(2)").mouseout(function() {
			                ivw_calendarDateMonthCallBack(false, $(this));
			            });
			        }
			        if (typeof ivw_calendarDateMonthClickCallBack === "function") {
			            $("#calendar-controls > tbody > tr > td:nth-child(2)").click(function() {
			                ivw_calendarDateMonthClickCallBack(m_events, $(this));
			            });
			        }
			    }
			
			    function ivw_calendarRender(p_date, p_monthsToDisplay) {
			        var today  = new Date();
			        var months = [["January", "Janvier"],
			                        ["February", "Février"],
			                        ["March", "Mars"],
			                        ["April", "Avril"],
			                        ["May", "Mai"],
			                        ["June", "Juin"],
			                        ["July", "Juillet"],
			                        ["August", "Août"],
			                        ["September", "Septembre"],
			                        ["October", "Octobre"],
			                        ["November", "Novembre"],
			                        ["December", "Décembre"]];
			                    
			        var days   = [["S", "D"],
			                        ["M", "L"],
			                        ["T", "M"],
			                        ["W", "M"],
			                        ["T", "J"],
			                        ["F", "V"],
			                        ["S", "S"]];
			
			        var monthPre = new Date(p_date.getFullYear(), p_date.getMonth() - 1, 1);
			        var monthEnd = new Date(p_date.getFullYear(), p_date.getMonth() + 1, 1);
			        var monthRef = new Date(p_date.getFullYear(), p_date.getMonth(), 1);
			        var html = "";
			        
			        while(monthRef.getDay() > 0) {
			            monthRef.setTime(monthRef.getTime() - 86400000);
			        }
			
			        html += '\n\
			<table id="calendar-controls">\n\
			    <tbody>\n\
			        <tr>\n\
			            <td><a class="previous" onclick="ivw_calendarCallback(' + monthPre.getTime() + ')">&lt;&lt;</a></td>\n\
			            <td class="' + p_date.getTime().toString() + '">' + months[p_date.getMonth()][1] + ' ' + p_date.getFullYear() + '</td>\n\
			            <td><a class="next" onclick="ivw_calendarCallback(' + monthEnd.getTime() + ')">&gt;&gt;</a></td>\n\
			        </tr>\n\
			    </tbody>\n\
			</table>\n\
			<table>\n\
			    <thead>\n\
			        <tr class="list-days">\n\
			            <td>' + days[0][1] + '</td><td>' + days[1][1] + '</td><td>' + days[2][1] + '</td><td>' + days[3][1] + '</td><td>' + days[4][1] + '</td><td>' + days[5][1] + '</td><td>' + days[6][1] + '</td>\n\
			        </tr>\n\
			    </thead>\n\
			    <tbody>\n\
			        <tr class="days">\n\
			    ';
			
			            while (monthRef < monthEnd) {
			                var info = "";
			
			                if (monthRef.getMonth() !== p_date.getMonth()) {
			                    info += ' disabled';
			                }
			                if (monthRef.getDate() === today.getDate() && monthRef.getMonth() === today.getMonth()) {
			                    info += ' today';
			                }
			                if (monthRef.getDay() === 0) {
			                    html += '\
			        </tr><tr class="days">';
			                }
			
			                html += '\n\
			            <td class="' + monthRef.getTime() + info + '">' + monthRef.getDate() + '</td>';
			
			                monthRef.setDate(monthRef.getDate() + 1);
			            }
			
			            html += '\
			        </tr>\n\
			    </tbody>\n\
			</table>';
			        
			        return html;
			    }
			
			    function ivw_calendarSelectDateJSON(p_container, p_dateInMiliseconds) {
			        for (var i in p_dateInMiliseconds) {
			            ivw_calendarSelectDate(p_container, p_dateInMiliseconds[i]["date_start"], p_dateInMiliseconds[i]["date_end"]);
			        }
			    }
			
			    function ivw_calendarSelectDate(p_container, p_dateInMilisecondsStart, p_dateInMilisecondsEnd) {
			        var startDate = new Date(p_dateInMilisecondsStart);
			        var endDate   = new Date(p_dateInMilisecondsEnd);
			        var activeCalendar = new Date(parseInt($(p_container).find("#calendar-controls > span:first").attr('class')));
			        var started = false;
			
			        $(p_container).find(".days > td").each(function(index, item) {
			            var currentDate = parseInt($(item).attr("class"));
			            if (started && currentDate <= endDate.getTime()) {
			                $(item).addClass("selected");
			                if (currentDate < endDate.getTime()) {
			                    $(item).css({"border-left": "none", "border-right": "none", "border-radius": "0px"});
			                } else {
			                    $(item).css({"border-left": "none", "border-bottom-left-radius": "0px", "border-top-left-radius": "0px"});
			                }
			            }
			            if (currentDate === startDate.getTime()) {
			                $(item).addClass("selected");
			                if (endDate.getTime() > startDate.getTime()) {
			                    $(item).css({"border-right": "none", "border-bottom-right-radius": "0px", "border-top-right-radius": "0px"});
			                }
			                started = true;
			            }
			        });
			    }
			    
			    function ivw_calendarHover()  {
			        document.getElementById("calendar-container").style.display = "none";
			    }
			    
			    function ivw_calendarClose()  {
			        document.getElementById("calendar-container").style.display = "none";
			    }
			//-->
			</script>
