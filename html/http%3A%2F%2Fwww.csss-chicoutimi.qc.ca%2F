  function XMod_SetToggleVisibility(objLink, defaultState) {
    var objectID = objLink.id;
    var nStart = objectID.indexOf("xmod_lnkToggle_")
    if (nStart >= 0) {
      var NewObjectID = objectID.substring(0, nStart);
      NewObjectID += "xmod_pnlToggle_";
      NewObjectID += objectID.substring(nStart + 15, objectID.length);
      var object = document.getElementById(NewObjectID);
      try {
        if (object.style.display == 'none') {
          object.style.display = 'block';}
        else {
          object.style.display = 'none';}
      }
      catch (errMsg) {
        object.style.display = defaultState;
      }
    }
  }
