	<!-- Menu generated by Navigation Suite for DotNetNuke , www.dnn360.net,  --><script type="text/javascript" >//><!--
sfHover = function() {
	var sfEls = document.getElementById("navSideMenu").getElementsByTagName("LI");
	for (var i=0; i<sfEls.length; i++) {
		sfEls[i].onmouseover=function() {
			this.className+=" sfhover";
		}
		sfEls[i].onmouseout=function() {
			this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
		}
	}
}
if (window.attachEvent) window.attachEvent("onload", sfHover);

var browserType;

if (document.layers) {browserType = "nn4"}
if (document.all) {browserType = "ie"}
if (window.navigator.userAgent.toLowerCase().match("gecko")) {
   browserType= "gecko"
}

function switchVisibility(itemToHide, visible) {
  var obj;
  if (browserType == "gecko" )
     obj = eval('document.getElementById("' + itemToHide + '")');
  else if (browserType == "ie")
     obj = eval('document.getElementById("' + itemToHide + '")');
  else
     obj = eval('document.layers["' + itemToHide + '"]');

  if (visible)
  	obj.style.display = "block";
  else
  	obj.style.display = "none";
}
