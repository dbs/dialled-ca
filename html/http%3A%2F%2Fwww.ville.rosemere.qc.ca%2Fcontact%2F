    <div id="leftSidebar">
   	  <script type="text/javascript" language="javascript">
$(document).ready(function(){
	var mois = '03';
	var annee = '2016';
	var filter = '';

	$("#link_precedent,#link_precedent_2").click(function(e){
		e.preventDefault();
		if(mois == 1){
			mois = 12;
			annee--;
		}else{
			mois--;
		}
		$.post("http://www.ville.rosemere.qc.ca/templates/include/change_month.php",{m:mois,a:annee,lang:"en",fl:filter},function(data){
			$("#calendarCalendar").html(data);
		});
	});
	$("#link_suivant,#link_suivant_2").click(function(e){
		e.preventDefault();
		if(mois == 12){
			mois = 1;
			annee++;
		}else{
			mois++;
		}
		$.post("http://www.ville.rosemere.qc.ca/templates/include/change_month.php",{m:mois,a:annee,lang:"en",fl:filter},function(data){
			$("#calendarCalendar").html(data);
		});
	});
	
	$("input[type=checkbox]").click(function(){
		filter = "";
		$("input[type=checkbox]:checked").each(function(){
			filter+=$(this).attr("value")+",";
		});
		$.post("http://www.ville.rosemere.qc.ca/templates/include/change_month.php",{m:mois,a:annee,lang:"en",fl:filter},function(data){
			$("#calendarCalendar").html(data);
		});
	});
	
});
</script>
<div id="change_month"><a id="link_precedent" href="#">&lt;</a> <a id="link_suivant" href="#">&gt;</a></div>
