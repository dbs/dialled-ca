function setPageTabHeight() {
	var highest = null;
	$("#compid_b1e9598f5958b310VgnVCM1000006cd60f89RCRD .nav-tabs a").height("auto");	
	$("#compid_b1e9598f5958b310VgnVCM1000006cd60f89RCRD .nav-tabs a").each(function(){ var h = $(this).height(); if(h > highest){highest = $(this).height();}});
