		$(function(){
			
			// process calendars 
			// and events
			$('.app_calendar').each(function() {
				
				// set calendar day square
				var day_w = $(this).find('.square').outerWidth();
				$(this).find('.square').height( day_w );
				
				// events
				var $events = $(this).find('.event');
				// on click
				$events.click(function() {
					var datestr = $(this).attr('data-date');
					window.location.href = 'http://www.mediatheque.qc.ca/app/calendar/?app_action=display_day&date='+datestr;
				});
				// images & icons
				$events.each(function() {
					var $e = $(this);
					var show_image = '';
					if( $e.hasClass('has_icon') ) {
						// user icon file
						show_image = $e.attr('data-icon');
					} else {
						if( $e.hasClass('has_image') ) {
							// show image
							show_image = $e.attr('data-image');
						}
					}
					
					if( show_image ) {
						var $icon_canvas = $('<div/>')
							.addClass("icon_canvas")
							.width( $e.outerWidth() )
							.height( $e.outerHeight() );
							
						var $i = $('<img/>');
						$i
							.load(function() {
								// icon must not be larget thant the whole
								// day square
								var w = $(this).width();
								var h = $(this).height();
								// base longuer size
								if( w < h ) {
									
									// base on width
									// make sure icon is not larger
									// thant container
									if( w > $e.width() ) {
										
										// to resize to max x% of its container
										$(this)
											.width( $e.width() )
											.css('height','auto');
											
									}
									
								} else {
										
									// base on height
									if( w > $e.height() ) {
										
										// to resize to max x% of its container
										$(this)
											.height( $e.height() )
											.css('width','auto');
											
									}
								}
							})
							.attr('src',show_image)
							.addClass('icon')
							.prependTo( $icon_canvas );
							$icon_canvas.prependTo( $e );
					}
					
				});
				
			});
			
		});
		</script>
		</div>
			</div>
			
