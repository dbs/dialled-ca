	<script type="text/javascript">
	var $item_menu_pop;
	var item_menu_tm;
	$(function(){
		// setup menu html
		var $m = $('.layout_item_popmenu .item_menu');
		$item_menu_pop = $m;
		$m.find('#item_menu_group_0').children('.categ').each(function() {
			var $group = $('<div class="main_group"></div>');
			$group.append($(this).clone());
			$group.append($(this).next().clone());
			$group.appendTo($m);
		});
		$m.find('#item_menu_group_0').remove();
		//$m.find('.group_level_2,.categ_level_2').remove();
		// setup menu hover
		$('.layout_menu .page_menu .page_menu_item.type_categ_default').hover(item_menu_show,item_menu_close);
		$item_menu_pop.hover(item_menu_show,item_menu_close);
		
	});
	function item_menu_show() {
		//console.log( 'show' );
		clearTimeout( item_menu_tm );
		$item_menu_pop.show();
	}
	function item_menu_close() {
		//console.log( 'close delay' );
		clearTimeout( item_menu_tm );
		item_menu_tm = setTimeout( function() {
			console.log( 'close' );
			$item_menu_pop.hide();
		},700 );
	}
	</script>
	<script>
	 $(function(){
		// set promo tag
		$('.item_list .item_price_base').each(function() {
		var $el = $(this).closest('.item');
		var url = $el.find('.item_link:first').attr('href');
		$el.prepend( '<div class="promo"><a href="'+url+'"><img src="/user/1496/template/files/promo.png" /></a></div>' );
		});
	 	});
	</script>
	
	<script type="text/javascript">
		 var font_size = 1;
		 function font_toggle() {
		  // reset
		  $('.font_zoom *').css('font-size','');
		  $('.font_zoom p, .font_zoom div, .font_zoom td').css('font-size','inherit');
		  $('.font_zoom').removeClass('font_size'+font_size);
		  
		  font_size++;
		  if( font_size > 3 ) {
		   font_size = 1;
		  }
		  
		  $('.font_zoom').addClass('font_size'+font_size);
		 }
	 
	 </script>
