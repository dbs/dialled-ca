<script type="text/javascript">
	var ws_lang = "fr";
//	var lang = "fr"; // this is overriding the lang() function
	var ws_live = true;
	var store_url = "http://www.mediatheque.qc.ca/";
	var store_email = "communications//mediatheque.qc.ca";
	var logon_domain = "";
	var screen_size = "";
	var mobile = false;
	
	
	var item_order = "";
	var item_order_desc = "";
	
	$(function() {
		try{
			
			////////////////////
			// screen size
			////////////////////
			function check_screen_size() {
				if( $("#check_screen_size").css("z-index") == 3 ) {
					
						window.location.href = store_url+"?mobile=1";
						
				} else if( $("#check_screen_size").css("z-index") == 2 ) {
					$("body").removeClass("small_screen");
					$("body").addClass("medium_screen");
					screen_size = "medium";
					if( mobile ) {
						$.get(store_url,{ ax:true, action: "unset_mobile" });
					}
					mobile = false;
				} else {
					$("body").removeClass("small_screen");
					$("body").removeClass("medium_screen");
					screen_size = "big";
					if( mobile ) {
						$.get(store_url,{ ax:true, action: "unset_mobile" });
					}
					mobile = false;
				}
			}
			$("body").append('<div id="check_screen_size"></div>');
			$(window).load(check_screen_size).resize(check_screen_size);
			check_screen_size();
	
			////////////////////
			// auto fill form fields
			////////////////////
			if( window.posted ) {
				if( ! $.isEmptyObject(posted) ) {
					//console.log( posted );
					for(var e in posted) {
						// only for input text and textarea
						var $iel = $(":input[name="+e+"]");
						if( $iel.length && $iel.attr("type") != "hidden" ) {
							$iel.val( posted[e] );
						}
					}
				}
			}
	
			////////////////////
			// contact form
			////////////////////
			// find form
			var $contact_form;
			$("form").each(function() {
				var url = $(this).attr("action");
				if( url && url.indexOf("form2mail.php") > -1 ) {
					$contact_form = $(this);
				}
			});
			//console.log( $contact_form );
			// process
			if( $contact_form.length ) {
				// add user creds
				var url = $contact_form.attr("action");
				$contact_form.attr("action",url+"?user=1496");
				// manage requirements
				$contact_form.submit(function() {
					var missing = false;
					var $req = $(this).find("[name=required]");
					if( ! $req.val() ) { return true }
					var r=$req.val().split(",");
					if( ! r || ! r.length ) { return true }
					for( var e in r ) {
						var f = r[e];
						var $f = $(this).find("[name="+f+"]");
						if($f.length && ! $f.val()) {;
							missing = true;
							$f.css({border: "2px solid #FFA100"});
						}
					}
					if(missing) {
						alert("Veuillez remplir les champs manquants");
						return false;
					} else { return true }
				})
			}
		} catch(e){ /*alert(e);*/ }
	});
</script>


