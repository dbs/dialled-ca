		function change_act() {
			var act_actif = 1
						if (document.getElementById('act_1').style.display == 'block') { act_actif = 1 }
									document.getElementById('act_1').style.display = 'none'
			
			if (act_actif < 1) { num_act = act_actif + 1 }
			else { num_act = 1 }

			document.getElementById('act_'+num_act).style.display = 'block'
		}

		function change_job() {
			var job_actif = 1
						if (document.getElementById('job_1').style.display == 'block') { job_actif = 1 }
