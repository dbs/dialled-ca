						if (document.getElementById('job_11').style.display == 'block') { job_actif = 11 }
						if (document.getElementById('job_12').style.display == 'block') { job_actif = 12 }
						if (document.getElementById('job_13').style.display == 'block') { job_actif = 13 }
						if (document.getElementById('job_14').style.display == 'block') { job_actif = 14 }
						if (document.getElementById('job_15').style.display == 'block') { job_actif = 15 }
						if (document.getElementById('job_16').style.display == 'block') { job_actif = 16 }
