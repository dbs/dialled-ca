$(document).ready(function() {
	
	
	$('#program-interest').change(function(){
		$.ajax({
		  type: 'POST',
		  url: 'http://www.durhamcollege.ca/wp-admin/admin-ajax.php',
		  data: 'action=get_programs_for_interest&interest_id=' + $("#program-interest").val(),
		  success: function(data) {
			   $('#program-selected').html(data);
			}
		});
		
		return false;
	});
	
	
	
});
</script><div
