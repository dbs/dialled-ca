		function spellcheck()
		{
			var searchterm = document.Entry.query.value;
			searchterm = escape(searchterm);

			var link = "/web2/tramp2.exe/goto/A2td44dh.000?screen=Home.html&query=" + searchterm;

			link += "&spell_check=check_spelling";
			location.href=link;
		}

			function replace()
        {
            var myForm = document.Entry;
            var query = "";
		var words = new Array();
		var wordIndex = 0;

            for(var index = 0; index < myForm.length; index++)
            {
                var type = myForm[index].type;
		    var name = myForm[index].name;
		    var pattern = /word.*/i;

	  	    var result = name.match(pattern);

                if(type == "select-one" && result)
                {
                    var value = selectValue( myForm[index] );
                    words[wordIndex++] = value;
                }

				if(type == "hidden" && result)
				{
						var value = myForm[index].value;
						words[wordIndex++] = value;
					}
				}

			for(var ind = 0; ind < wordIndex; ind++)
			{
				query += words[ind];
				if(ind + 1 != wordIndex)
					{
						query += " ";
					}
			}


            myForm.query.value = query;
        }

        function selectValue(select)
        {
            var result = "";

            for(var i = 0; i < select.options.length; i++)
            {
                if( select.options[i].selected )
                {
                    result = select.options[i].text;
                }
            }

            return result;
        }
function mySendIR()
	{
		var sendlink = "/web2/tramp2.exe/download_database/A2td44dh.000?" + "screen=" + document.handheld.screen.value;
		sendlink += "&operation=" + document.handheld.operation.value;
		sendlink += "&handheld=" + document.handheld.handheld.value;
		sendlink += "&item=" + document.handheld.item.value;
		sendlink += "&server=" + document.handheld.server.value;
		sendlink += "&start=" + document.handheld.start.value;
		sendlink += "&item_source=" + document.handheld.item_source.value;
		sendlink += "&file=" + document.handheld.file.value;

		open(sendlink, "_blank", "directories = no, fullscreen = no, height = 200, location = no, menubar = no, resizable = no, scrollbars = no, status = no, titlebar = no, toolbar = no, width = 600");
		}
