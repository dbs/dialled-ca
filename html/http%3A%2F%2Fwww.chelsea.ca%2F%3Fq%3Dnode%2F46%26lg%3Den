<script type="text/javascript">
  jQuery(document).ready(function() {
    jQuery('#edit-search-block-form--2').val('Search');
    
    jQuery('a[href$=".pdf"]').each(function() {
       jQuery(this).click(function(event) {
             event.preventDefault();
             event.stopPropagation();
             window.open(this.href, '_blank');
        }).addClass('pdfLink');
    });
    
    jQuery('.request_tel').keydown(function (event) {
        if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9) {
            
        }
        else {
            if ((event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 ) && (event.keyCode < 37 || event.keyCode > 40 )) {
                event.preventDefault(); 
            }   
        }
    });
    
    function html_entity_decode(str) {
        var ta = document.createElement("textarea");
        ta.innerHTML = str.replace(/</g,"&lt;").replace(/>/g,"&gt;");
        return ta.value;
    }
    jQuery('#request_submit').click(function () {
        var required_fields = '';
        if (jQuery('#request_subject').val() == '0:choice') {
            required_fields += ', subject';
        }
        if (jQuery('#request_name').val() == '') {
            required_fields += ', name';
        }
        if ((jQuery('#request_pref_tel:checked').val() != null) && ((jQuery('#request_tel1').val() == '') || (jQuery('#request_tel2').val() == '') || (jQuery('#request_tel3').val() == ''))) {
            required_fields += ', phone';
        }
        if ((jQuery('#request_pref_email:checked').val() != null) && (jQuery('#request_email').val() == '')) {
            required_fields += ', email';
        }
        if (required_fields.length > 0) {
            alert(html_entity_decode("You have to fill the required fields : " + required_fields.substring(2)));
        } else {
            var ok = 1;
            if (jQuery('#request_pref_email:checked').val() != null) {
                var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
                if(reg.test(jQuery('#request_email').val()) == false) {
                    alert(html_entity_decode('Invalid Email Address'));
                    ok = 0;
                }
            } else {
                if ((jQuery('#request_tel1').val().length != 3) || (jQuery('#request_tel2').val().length != 3) || (jQuery('#request_tel3').val().length != 4) || (isNaN(jQuery('#request_tel1').val()))|| (isNaN(jQuery('#request_tel2').val()))|| (isNaN(jQuery('#request_tel3').val()))) {
                    alert(html_entity_decode('Invalid phone number'));
                    ok = 0;
                }
            }
            if (ok == 1) {
                jQuery('#request_result').html('sending...');
                var url = '/themes/bartik/templates/request_mail.php';
                var s1 = jQuery('#request_subject').val();
                var s2 = jQuery('#request_name').val();
                var s3 = '(' + jQuery('#request_tel1').val() + ')' + jQuery('#request_tel2').val() + '-' + jQuery('#request_tel3').val();
                var s4 = jQuery('#request_email').val();
                if (jQuery('#request_pref_email').attr('checked') == 'checked') {
                    var s5 = jQuery('#request_pref_email').val();
                } else {
                    var s5 = jQuery('#request_pref_tel').val();
                }
                var s6 = jQuery('#request_detail').val();
                var s7 = 'en';
                
                jQuery.post(url, {subject: s1, name: s2, tel: s3, email: s4, pref: s5, detail: s6, lg: s7},
                    function(data) {
                        var res = data.result;
                        var txt = data.text;
                        
                        jQuery('#request_result').html(txt);
                        if(res == 1) {
                            jQuery('#request_result').css('color', '#005941');
                        }
                        jQuery('#request_result').css('display', 'block');
                    },
                    'json'
                );
            }
        }
    });
  });
</script>
