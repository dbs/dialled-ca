<script type="text/javascript">
    function SearchByKeyword() {
        RedirectToSearchByKeyword(encodeURIComponent($("#keywordSearch").val()));
    }

    function RedirectToSearchByKeyword(keyword) {
        window.location.href = '/fr/recherche/?terms=' + keyword;
    }

    $(function () {
        // Handler for .ready() called.
        $("#btnkeywordSearch").click(function () {
            SearchByKeyword();
            return false;
        });

        $('#keywordSearch').bind('keypress', function (e) {
            var code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13) { //Enter keycode
            	$('#btnkeywordSearch').click();
                //SearchByKeyword();
                return false;
            }
        });
    });

</script>

