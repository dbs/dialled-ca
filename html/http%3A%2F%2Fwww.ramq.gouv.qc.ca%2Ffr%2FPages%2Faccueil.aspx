    function ImposerMaxLong(obj, max, e) {
        if (window.event) {
            if (window.event.ctrlKey)
                isCtrl = true;
            else
                isCtrl = false;
        }
        else {
            if (e.ctrlKey)
                isCtrl = true;
            else
                isCtrl = false;
        }

