        var mod = function () {
            if (Modernizr.mq('(max-width: 1019px)')) {
                $('.flip1').each(function () {
                    $(this).before($(this).next('.flip2'));
                });
            } else {
                $('.flip2').each(function () {
                    $(this).before($(this).next('.flip1'));
                });
            }
        }

        // Shortcut for $(document).ready()
        $(function () {
            // Call on every window resize
            $(window).resize(mod);
            // Call once on initial load
            mod();
        });
    </script>


