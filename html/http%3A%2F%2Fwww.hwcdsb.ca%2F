        <script type="text/javascript">
            function showConfirm(button, customTxt, BuzzID) {
                $find('ctl00_contentarea_HiddenDeleteButton').set_text(BuzzID);  
                function callbackFn(arg) {
                    if (arg) {
                        __doPostBack("ctl00$contentarea$HiddenDeleteButton", "");
                    }
                }
                radconfirm(customTxt, callbackFn);
            }
