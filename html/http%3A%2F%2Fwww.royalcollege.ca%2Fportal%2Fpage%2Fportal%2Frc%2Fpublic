//Tracking event for PDF/doc/docx/image/ppt/html file download and audio file listening

$(document).ready(function(){
  $("[href$='.pdf']").click(function(){
	  ga('send', 'event', 'PDF Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
   });
  $("[href$='.doc']").click(function(){
	    ga('send', 'event', 'Word Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.docx']").click(function(){
	ga('send', 'event', 'Word Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.mp3']").click(function(){
	ga('send', 'event', 'Audio Files', 'Listen',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.mp4']").click(function(){
	ga('send', 'event', 'Video Files', 'Watch',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.html']").click(function(){
	ga('send', 'event', 'HTML Files', 'View',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.htm']").click(function(){
	ga('send', 'event', 'HTML Files', 'View',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.ppt']").click(function(){
	ga('send', 'event', 'PowerPoint Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.pptx']").click(function(){
	ga('send', 'event', 'PowerPoint Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.xlsx']").click(function(){
	ga('send', 'event', 'Excel Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.xls']").click(function(){
	ga('send', 'event', 'Excel Files', 'Download',  $(this).attr("href").substring($(this).attr("href").lastIndexOf('/')+1));
	   });
	  $("[href$='.jpg']").click(function(){
