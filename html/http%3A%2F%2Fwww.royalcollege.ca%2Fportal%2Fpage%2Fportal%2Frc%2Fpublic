$(document).ready(function() {
  var searchBase = 
        (RegExp("searchBase" + '=' + '(.+?)(&|$)').exec(location.search)
      || [,null])[1];

  if (searchBase == null) {
    searchBase = window.location.pathname;
    searchBase = searchBase.substring(1);
    searchBase = (searchBase.indexOf("/") > -1) ? 
         searchBase.substring(0, searchBase.indexOf("/")) : searchBase;
  }
  $("#searchBaseInput").attr("value", searchBase);
  $("img.searchbox_submit").click(function(){
    $(this).parents().submit();
  });
});
