var prevUrl = ""
var keepInfo = false;
var outOfFrame = true;
var mouseEvent = null;
var isTouch = false;
//handle orientationChange on a mobile device:
function ctl03_epg_lb_onOrientationChange(e){
ctl03_showLightBox(e,e.data.tn,e.data.url,e.data.inw,e.data.inh,e.data.prevId,e.data.nextId,true);
};
function ctl03_showLightBox(e,tn,url,inw,inh,prevId,nextId,force) {
if(!force&&(url==prevUrl))return;
prevUrl = url;
scroll(0,0);
jQuery("html").css("overflow", "hidden");
var ww = jQuery(window).width();
var wh = jQuery(window).height();
var ih = inh;
var iw = inw;
var prop = inh/inw;
var overlay;
var frame;
var img;
var bclose;
var bprev;
var bnext;
var prevThum;
var nextThum;
var overlayExists;
var showNav = jQuery("#ctl03_hidShowNav").val() == 1;
if(!ipjEPGIsMobile){
isTouch=ipjIsTouchDevice();

if (iw > ww-160 || ih > wh-160){
//wide image
ih = wh - 160;
iw = Math.round(ih / prop) - 1;
//tall image
var iw1 = ww - 160;
var ih1 = Math.round(iw1 * prop) - 1;

if (ih1 < ih){ih=ih1;iw=iw1}
}
//small image
var iborders = "";
var ibh = ih;
var ibw = iw;
if(ih < 120){
var bh = Math.round((120 - ih)/2);
iborders += "border-top:solid " + bh + "px #fff;border-bottom:solid " + bh + "px #fff;";
ibh = 120;
}

if(iw < 180){
var bw = Math.round((180 - iw)/2);
iborders += "border-left:solid " + bw + "px #fff;border-right:solid " + bw + "px #fff;";
ibw = 180;
}
overlayExists = (jQuery(".ip-epg-lb-overlay").length == 1);

//overlay
if(!overlayExists){
overlay = jQuery("<div class='ip-epg-lb-overlay epg-lightbox'></div>");
overlay.appendTo("body");
adjustLBOverlay();
jQuery(window).bind("resize",function(){adjustLBOverlay();});
}else jQuery(".ip-epg-lb-overlay .epg-lightbox").hide();
//frame
var top = Math.round((wh - ibh - 20)/2) - 1;
var left = Math.round((ww - ibw - 20)/2) - 1;
var style = "width:" + ibw + "px;height:" + ibh + "px;top:" + top + "px;left:" + left + "px;";
if(overlayExists){
frame = jQuery(".ip-epg-lb-frame");
frame.find("img").remove();
frame.find("div.ip-epg-lb-info").remove();
frame.attr("style",style);
}else{
frame = jQuery("<div class='ip-epg-lb-frame epg-lightbox' style='" + style + "'></div>");
frame.appendTo("body");
}
//image
style = iborders + "width:" + iw + "px;height:" + ih + "px";
//url += "&Mode=Thumbnail&Width=" + iw + "&Height=" + ih; //acm6033 - blurry images with mode="thumbnail"   //NOTE, THE OVERHEAD ASSOCIATED WITH NOT USING PROPER THUMBS IS SIGNIFICANT; THERE SHOULD BE AN EDIT MODE SETTING TO CONTROL THIS
var hack="src";//prevents the export from mucking up the src
img = jQuery("<img "+hack+"='"+url+"' style='"+style+"'/>");
img.appendTo(frame);

//info
var info = jQuery(tn).find("em")
if(info.length==1){
style = "width:" + (frame.width()-20) + "px";
jQuery("<div class='ip-epg-lb-info' style='" + style + "'>" + info.html() + "</div>").appendTo(frame);
}
//button close
style = "top:" + (frame.offset().top - 23) + "px;left:" + (frame.offset().left + frame.width() - 5) + "px";
if(overlayExists){
bclose = jQuery(".ip-epg-lb-btn-close");
bclose.attr("style",style);
}else{
bclose = jQuery("<div class='ip-epg-lb-btn-close epg-lightbox' title='Close' style='" + style + "' onclick='ctl03_closeLightBox()'></div>");
bclose.appendTo("body");
}
if(showNav){
//button previous
style = "top:" + (frame.offset().top + frame.height()/2 - 25) + "px;left:" + (frame.offset().left + 18) + "px";
if(overlayExists){
bprev = jQuery(".ip-epg-lb-btn-prev");
bprev.attr("style",style);
}else{
var bprev = jQuery("<div class='ip-epg-lb-btn-nav ip-epg-lb-btn-prev epg-lightbox' title='Previous' style='" + style + "'></div>");
bprev.appendTo("body");
}
//prevThum = jQuery(".ipb-epg-thmb-bkg img[src*='did=" + prevId + "&']").parent();
prevThum = jQuery("#tdThumb"+prevId);
bprev.unbind("click");
bprev.click(function(e){mouseEvent = e;prevThum.click();});
if(prevId==0)bprev.addClass("ip-epg-lb-hide");
else bprev.removeClass("ip-epg-lb-hide");
//button next
style = "top:" + (frame.offset().top + frame.height()/2 - 25) + "px;left:" + (frame.offset().left +  + frame.width() - 26) + "px";
if(overlayExists){
bnext = jQuery(".ip-epg-lb-btn-next");
bnext.attr("style",style);
}else{
var bnext = jQuery("<div class='ip-epg-lb-btn-nav ip-epg-lb-btn-next epg-lightbox' title='Next' style='" + style + "'></div>");
bnext.appendTo("body");
}
//nextThum = jQuery(".ipb-epg-thmb-bkg img[src*='did=" + nextId + "&']").parent();
nextThum = jQuery("#tdThumb"+nextId);
bnext.unbind("click");
bnext.click(function(e){mouseEvent = e;nextThum.click();});
if(nextId==0)bnext.addClass("ip-epg-lb-hide");
else bnext.removeClass("ip-epg-lb-hide");
}

//bind events
frame.unbind("mousemove mouseleave mouseenter touchstart");
if(!isTouch){
if(showNav)frame.bind("mousemove",(function(e){lightboxMousemove(e,frame,bprev,bnext,prevThum,nextThum);}));
frame.bind("mouseleave",(function(e){lightboxMouseleave(e,frame);}));
frame.bind("mouseenter",(function(){lightboxMouseenter();}));
jQuery(document).bind('keydown', function(e){if (e.which == 27)ctl03_closeLightBox();});//close on esc
}else{
frame.bind("touchstart",(function(){lightboxTouchStart(e,frame,bprev,bnext);}));
}
//display
frame.show();
var e = e || window.event;
if(e.pageX != null)mouseEvent = e;
defineCursorPos(mouseEvent,frame);
if(isTouch){
bprev.addClass("mobile");bnext.addClass("mobile");
jQuery(".ip-epg-lb-btn-nav").stop(true,true).show().delay(2500).hide("fade", {}, 1000);
jQuery(".ip-epg-lb-info").stop(true,true).show().delay(2500).hide("fade", {}, 1000);
}else if(outOfFrame){
jQuery(".ip-epg-lb-btn-nav").hide();
jQuery(".ip-epg-lb-hover").removeClass("ip-epg-lb-hover");
keepInfo = true;
setTimeout(function(){keepInfo=false;},4000);
jQuery(".ip-epg-lb-info").show().delay(2500).hide("fade", {}, 1000);
}else{
lightboxMousemove(mouseEvent,frame,bprev,bnext,prevThum,nextThum);
}

}
else{////mobile mode////
var showInfo = jQuery(tn).find("em").length > 0;
var imgMargins = showInfo ? 90 : 45;
if (iw > ww || ih > wh-imgMargins){
//wide image
ih = wh - imgMargins;
iw = Math.round(ih / prop) - 1;
//tall image
var iw1 = ww;
var ih1 = Math.round(iw1 * prop) - 1;

if (ih1 < ih){ih=ih1;iw=iw1}
}
overlayExists = (jQuery(".ip-epg-mob-overlay").length == 1);

//overlay
if(!overlayExists){
overlay = jQuery("<div class='ip-epg-mob-overlay epg-lightbox'></div>");
overlay.appendTo("body");
}
//image
jQuery("img.ip-epg-mob-img").remove();
var top = Math.round((wh - ih + (showInfo ? 0 : 45))/2);
var left = Math.round((ww - iw)/2);
var style = "width:" + iw + "px;height:" + ih + "px;top:" + top + "px;left:" + left + "px;";
//url += "&Mode=Thumbnail&Width=" + iw + "&Height=" + ih;   //NOTE, THE OVERHEAD ASSOCIATED WITH NOT USING PROPER THUMBS IS SIGNIFICANT; THERE SHOULD BE AN EDIT MODE SETTING TO CONTROL THIS
var hack="src"; //prevents the export from mucking up the src
jQuery("<img class='ip-epg-mob-img epg-lightbox' "+hack+"='" + url + "' style='" + style + "'/>").appendTo("body");

//info
if(showInfo){
var infobar = jQuery(".ipf-epg-mob-title");
var title = jQuery(tn).find("img").attr("title");
if(infobar.length == 1){
infobar.find("td").html(title);
}else{
infobar = jQuery("<table class='ipf-epg-mob-title epg-lightbox'><tr><td>" + title + "</td></tr></table>");
infobar.appendTo("body");
}
}
//navigation
var navbar = jQuery(".ipf-epg-mob-nav");
if(navbar.length == 0){
var html = "<table class='ipf-epg-mob-nav epg-lightbox'><tr>";
html += "<td class='ip-epg-mob-close' onclick='ctl03_closeLightBox()'><td>";
if(showNav){
html += "<td class='ip-epg-mob-prev'><td>";
html += "<td class='ip-epg-mob-next'><td>";
}
html += "<td></td></tr></table>";
navbar = jQuery(html);
navbar.appendTo("body");
}
if(showNav){
//button previous
bprev = jQuery(".ip-epg-mob-prev");
bprev.unbind("click");
bprev.click(function(){jQuery(".ipb-epg-thmb-bkg img[src*='did=" + prevId + "&']").parent().click();});
if(prevId==0)bprev.addClass("ip-epg-mob-prev-grey");
else bprev.removeClass("ip-epg-mob-prev-grey");
//button next
bnext = jQuery(".ip-epg-mob-next");
bnext.unbind("click");
bnext.click(function(){jQuery(".ipb-epg-thmb-bkg img[src*='did=" + nextId + "&']").parent().click();});
if(nextId==0)bnext.addClass("ip-epg-mob-next-grey");
else bnext.removeClass("ip-epg-mob-next-grey");
}
jQuery(window).unbind('resize',ctl03_epg_lb_onOrientationChange).bind('resize', {tn:tn,url:url,inw:inw,inh:inh,prevId:prevId,nextId:nextId}, ctl03_epg_lb_onOrientationChange);
}
lightboxPreloadImage(prevId);
lightboxPreloadImage(nextId);
}
function defineCursorPos(e,frame){
outOfFrame = e.pageX < frame.offset().left ||
e.pageX > frame.offset().left + frame.width() ||
e.pageY < frame.offset().top ||
e.pageY > frame.offset().top + frame.height();
}
function lightboxShowInfo(doHide){
if(doHide && outOfFrame)jQuery(".ip-epg-lb-info").show().delay(2500).hide("fade", {}, 1000);
else jQuery(".ip-epg-lb-info").show()
}
function lightboxTouchStart(e,frame,bprev,bnext){
isTouch=true;
//frame.unbind("mousemove mouseleave mouseenter");
bprev.addClass("mobile");bnext.addClass("mobile");
jQuery(".ip-epg-lb-btn-nav").not(".ip-epg-lb-hide").stop(true,true).show().delay(2500).hide("fade", {}, 1000);
jQuery(".ip-epg-lb-info").stop(true,true).show();
}
function lightboxMouseenter(){
if(outOfFrame){
jQuery(".ip-epg-lb-btn-nav").not(".ip-epg-lb-hide").show();
lightboxShowInfo();
}
}
function lightboxMouseleave(e,frame){
defineCursorPos(e,frame);
if(outOfFrame){
jQuery(".ip-epg-lb-btn-nav").hide();
if(!keepInfo){jQuery(".ip-epg-lb-info").hide();keepInfo=false;}
jQuery(".ip-epg-lb-hover").removeClass("ip-epg-lb-hover");
}
}
function lightboxMousemove(e,frame,bprev,bnext,prevThum,nextThum){
defineCursorPos(e,frame);
var goPrev = e.pageX - frame.offset().left < frame.width()/2;
jQuery(".ip-epg-lb-btn-nav").not(".ip-epg-lb-hide").show();
jQuery(".ip-epg-lb-info").show();
jQuery((".ip-epg-lb-hover")).removeClass("ip-epg-lb-hover");
frame.unbind("click");
if(goPrev){
bprev.addClass("ip-epg-lb-hover");
frame.click(function(e){mouseEvent = e;prevThum.click();});
}
else {
bnext.addClass("ip-epg-lb-hover");
frame.click(function(e){mouseEvent = e;nextThum.click();});
}
}
function ctl03_closeLightBox() {
jQuery(document).unbind('keydown');
jQuery(window).unbind('resize',ctl03_epg_lb_onOrientationChange);
jQuery(".epg-lightbox").remove();
jQuery("html").css("overflow", "visible");
prevUrl = "";
outOfFrame = true;
}
function preloadImage(src){
if(m_arrctl03PreloadedImages.length > 0){
if (m_arrctl03PreloadedImages.indexOf(src) < 0)
{
m_arrctl03PreloadedImages.push(src);
var imgId = 'ctl03preloadImg';
var pi = jQuery("#" + imgId);
if (pi.length == 0) {
var hack="src";//prevents the export from mucking up the src
pi = jQuery("<img id='"+imgId+"' "+hack+"='"+src+"' style='visibility:hidden;width:1px;height:1px'/>");
pi.appendTo("body");
}
else
pi.attr("src", src);
}
}
}
function adjustLBOverlay(){
var maxH = jQuery(window).height() > document.documentElement.scrollHeight ? jQuery(window).height() : document.documentElement.scrollHeight;
var maxW = jQuery(window).width() > document.documentElement.scrollWidth ? jQuery(window).width() : document.documentElement.scrollWidth;
jQuery(".ip-epg-lb-overlay").css("width",maxW).css("height",maxH);
}
