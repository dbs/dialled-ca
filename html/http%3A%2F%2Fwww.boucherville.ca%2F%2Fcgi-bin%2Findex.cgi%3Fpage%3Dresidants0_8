			<script>
			jQuery(document).ready(function(){
				/*jQuery("#btn_filtrer_calendrier").click(function(event){
					calendrierChoisi(jQuery("#select_type_calendrier").val());
				})*/
			})
			function calendrierChoisi(cal)
			{	
				var urlActuel = new String(document.location);
				var nouveauUrl = "";
				if(urlActuel.indexOf("typevent")>-1){
					var reg=new RegExp("(typevent=)[0-9]", "g");
					nouveauUrl = urlActuel.replace(reg,"typevent="+cal);
				}else{				
					nouveauUrl = urlActuel+"&amp;typevent="+cal;
				}
				if(document.location.search == ""){
					nouveauUrl = urlActuel+"?typevent="+cal;
				}
				document.location = nouveauUrl;
			}
			</script>
