function cswapdata0()
{


    /*---------------------------------------------
    Content Dimensions
    ---------------------------------------------*/

	this.container_width = 230
	this.container_height = 230



    /*---------------------------------------------
    Message Timing
    ---------------------------------------------*/

	this.initial_swap_delay = 0			//measured in seconds
	this.swap_delay = 7				//measured in seconds



    /*---------------------------------------------
    Container Styles and Padding
    ---------------------------------------------*/

	this.container_padding = "0,0,0,0"
	this.container_styles = "";



    /*---------------------------------------------
    Container Styles and Padding
    ---------------------------------------------*/

	this.item_styles = "";
	this.item_link_styles = "";
	this.item_link_hover_styles = "";



    /*---------------------------------------------
    Animated Transitions (IE 5.5 & Up only)
    ---------------------------------------------*/

	this.item_transitions = "filter:progid:DXImageTransform.Microsoft.Fade(duration=1);";


}</script>

