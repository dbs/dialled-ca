  $(function() {
    $('#inc').on('click', function() {
      // to increase
      $('p,h1,h2,li').css('font-size', function(i, size) { return parseFloat(size)*1.2; });
    });    

    $('#dec').on('click', function() {
      // to decrease
      $('p,h1,h2,li').css('font-size', function(i, size) { return parseFloat(size)/1.2; });
    });

  });
})(jQuery);
} catch (error) { throw error; }

