                                        <h4>> Recherche avancée</h4>
                                        

   
  
<script type="text/javascript">

$(document).ready(function(){        
    $('#searchform').submit(function(e) { // run the submit function, pin an event to it
        var s = $( this ).find("#s"); // find the #s, which is the search input id
        if (!s.val()) { // if s has no value, proceed
            e.preventDefault(); // prevent the default submission
            alert("Veuillez entrer un mot-clé avant de lancer votre recherche."); // alert that the search is empty
            $('#s').focus(); // focus on the search input
        }
    });
});



</script>

<form method="get" id="searchform" action="http://bdeb.qc.ca" role="search">
	<div><input maxlength="33" type="text" class="field" name="s" id="s" placeholder='Mots-clés ou "expression exacte"' value=""/></div>
	<div id="submit"><input type="submit" class="submit" name="submit" id="searchsubmit" value="Lancer la recherche" /></div>
    <hr class="clear" />
</form>
