<script>
$(window).load(function() {


/* enleve le min à heure dans evenements details*/
	var myString = $("abbr.dtend").text();
	var newString = myString.substr(0, myString.length-3); 
	$("abbr.dtend").text(newString);
	
 /* remplacement des strings */
	$(".tribe-events-meta-group-details dl dt").eq(0).addClass("cat");
	$(".tribe-events-meta-group-details dl dt.cat").replaceWith( "<dt>Catégorie de l'événement :</dt>" );	
	$(".tribe-events-meta-group-details dl dt").eq(2).addClass("temps");
	$(".tribe-events-meta-group-details dl dt.temps").replaceWith( "<dt>Heure :</dt>" );
	$(".tribe-events-event-meta").fadeIn("normal");
});

</script>



