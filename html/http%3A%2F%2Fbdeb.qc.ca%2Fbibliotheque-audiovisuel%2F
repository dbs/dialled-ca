$(document).ready(function () {
 
    $("#edButtonHTML").remove();
    $('.dropdown-menu').find('.divideleft').css('height', $('.dropdown-menu').innerHeight());
    $("#advanced-searchform").submit( function(e) {
        var s = "";
        s += $("#advanced-ss").val() != "" ? $("#advanced-ss").val()+' ' : '';
        s += $("#advanced-sx").val() != "" ? '"'+$("#advanced-sx").val().replace(/"/g,'')+'"' : '';
        $("#advanced-s").val(s);
        if (s != "") {
            // reset the query string according to the combination of both fields
            $("#advanced-s").val(s);
        } else {
            e.preventDefault();
            alert("Veuillez entrer au moins un mot clé ou une expression.");
            $("#advanced-ss").focus();
        }
    });
    $("#basic-searchform").submit( function(e) {
        if ($("#basic-s").val() == "") {
            e.preventDefault();
            alert("Veuillez entrer au moins un mot clé.");
            $("#basic-s").focus();
        }
    });
	
	$(function() {
        $('a[href*=#]:not([href=#])').click(function() {
            if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
                var target = $(this.hash);
                target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
                if (target.length) {
                    $('html,body').animate({
                        scrollTop: target.offset().top - 40
                    }, 800);
                    return false;
                }
            }
        });
    });


$( "ul.sy-pager" ).wrap( "<div class='pagerwrap container'></div>" );
	
$("body").iealert();



 
});



