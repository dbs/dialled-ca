			var zr_popup_email = function(i) {
				
				$('#zr_popup_email [name=zr_to]').val(zr_crypt_array[i]);
				
				var wW = parseInt($(window).width());
				var wH = parseInt($(window).height());
				var sT = parseInt($(window).scrollTop());
											
				if(wW > 550) {
					
					var left 	= (wW-550)/2;
					var top 	= sT+((wH-400)/2);
					
					$('#zr_popup_email').css('width', 550);
					$('#zr_popup_email').css('height', 400);
					$('#zr_popup_email').css('left', left);
					$('#zr_popup_email').css('top', top);	
				}
				else {

					var left 	= 10;
					var top 	= sT+((wH-400)/2);

					$('#zr_popup_email').css('width', wW-75);
					$('#zr_popup_email').css('height', 'auto');
					$('#zr_popup_email').css('left', left);
					$('#zr_popup_email').css('top', top);						
				}
				
				$('#zr_popup_mask').show();
				$('#zr_popup_email').show();
				
				$('#zr_popup_email .close').off().click(function(e) {
					e.preventDefault();					
					$('#zr_popup_mask').hide();
					$('#zr_popup_email').hide();					
				});
			};
			
			var zr_verify_form = function() {
				
				var hasError	= false;
				var fields		= ['zr_fullname', 'zr_email', 'zr_subject', 'zr_message'];
				
				for(var i=0; i<fields.length; i++) {
					
					var val = $('#zr_popup_email [name='+fields[i]+']').val();
					
					if(val == '') {
						hasError = true;
						$('#zr_popup_email [name='+fields[i]+']').addClass('zr_error');
					} else {
						$('#zr_popup_email [name='+fields[i]+']').removeClass('zr_error');
					}
				}
				
				if(hasError) {
					setTimeout(function() {
						alert('Vérifier le formulaire, les champs rouges sont requis!');
					}, 1);	
				}

				return !hasError;
			};
			
		</script>
		
