<script>
$(document).ready(function(){  
$('.bxslider').bxSlider({
    mode: 'fade',
    captions: true,
    pager : false,
    auto: true,
    speed : 1500,
    pause: 5000
});   

var title = '';
// hide title tooltip in sliders
$('.bxslider img').hover(
	function () {			
		title = $(this).attr('title');
		$(this).attr({'title':''});
	}, 
	function () {
		$(this).attr({'title':title});
	}
);


});  
  
</script>

