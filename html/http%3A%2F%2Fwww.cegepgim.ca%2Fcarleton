	jQuery(window).ready(function() {
		var rotation = true;
		var premier = true;
		var locked = false;
		var lien;

		jQuery(".front").load(function(){jQuery(this).show()});

		var swapFunction = function() {
			jQuery(".front").fadeOut(200, function() { jQuery(".front").attr("src", ""); jQuery(".front").attr("src", jQuery(".back").attr("src")); });
			jQuery(".front").parent().attr("href", lien);
			locked = false;
		};

		jQuery(".back").load(swapFunction);

                function ChangerImage(object) {
                        var image = object.attr("alt");
                        lien = object.children("a").attr("href");
                        if (object.hasClass("active")) return false;
			if (locked) return false;
			locked = true;
                        jQuery("ul.menuimages_seulement .active").removeClass("active");
                        object.addClass("active");
                        jQuery(".back").attr("src", "");
			jQuery(".back").attr("src", image);
                }

                jQuery("ul.menuimages_seulement li").click(function() {
                        rotation = false;
			//jQuery("#cercle").fadeOut();
			ChangerImage(jQuery(this));
                        return false;
                });

		var terminerRot = function() {
			rotationFunc();
			/*jQuery("#cercle").fadeTo('fast', 0.1, rotationFunc);*/
		}
                var rotationFunc = function () {
			if (!rotation) return;
			if (!premier) {
	                        var objects = jQuery("ul.menuimages_seulement");
	                        var prochain = objects.children(".active").next();
	                        if (prochain.size() == 0) {
	                                prochain = jQuery("ul.menuimages_seulement li").first();
	                        }
	                        if (prochain.size() != 0)
	                                ChangerImage(prochain);

			} premier = false;
			window.setTimeout(terminerRot, 4000);
			  /*jQuery("#cercle").fadeTo('slow', 1.0)*/
			  /*jQuery("#cercle").rotate({
                             angle:0, 
                             animateTo:360,
