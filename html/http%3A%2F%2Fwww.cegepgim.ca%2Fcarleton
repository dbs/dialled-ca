		jQuery("#mod-finder-searchword").focus(function() {
			jQuery(this).css("background-color", "white");
			jQuery(this).css("color", "#333");
		});
		jQuery("#mod-finder-searchword").blur(function() {
			jQuery(this).css("background-color", "rgba(255, 255, 255, 0.3)");
			jQuery(this).css("color", "#eee");
		});
		var openedMenu = jQuery("#sidebar-left .menu > .active.parent, #sidebar-left .menu-red > .active.parent");
		if (openedMenu.length == 0)
			openedMenu = undefined;
		if (openedMenu != undefined)
			openedMenu = openedMenu.children("ul");	
		jQuery("#sidebar-left .menu > .parent .separator, #sidebar-left .menu-red > .parent .separator").click(function() {
			if (openedMenu != undefined && jQuery(this).parent().attr("class") == openedMenu.parent().attr("class")) 
				return;
			
			if (openedMenu != undefined) 
				openedMenu.slideToggle('slow', function() { jQuery(this).parent().toggleClass('active'); });
			
			openedMenu = jQuery(this).parent().children("ul");
			openedMenu.parent().toggleClass('active');
			openedMenu.slideToggle('slow', function() {
			});
		});
				jQuery("#footer-tab-content").ready(function() {
			jQuery("#footer-tab-content .customfooter-tab").hide();
			jQuery("#footer-tab-content .customfooter-tab:eq(0)").show();
			jQuery("#footer-tab-slider .menu li:eq(0)").addClass("current");
		});
                jQuery("#footer-tab-slider li .separator").click(function() {
                        if (jQuery(this).hasClass("current")) return;
                        jQuery("#footer-tab-slider .menu .current").removeClass("current");
                        jQuery(this).parent().addClass("current");
			jQuery("#footer-tab-content .customfooter-tab").hide();
			jQuery("#footer-tab-content .customfooter-tab:eq("+jQuery("#footer-tab-slider .menu li").index(jQuery(this).parent())+")").show();
                });
				jQuery(window).ready(function() {	
			jQuery(".onglet").click(function() {
				var idActif = jQuery(".onglet-contenu").not(".cache").attr("id").substr(-1, 1);
				var idInactif = 3 - idActif;
				if (jQuery(this).attr("id").substr(-1, 1) == idActif)
					return;
	                        jQuery(".onglet-contenu").toggleClass("cache");
				
				jQuery("#onglet"+idActif).css("top", "2px");
				jQuery("#onglet"+idActif).css("height", "30px");
				jQuery("#onglet"+idInactif).css("top", "1px");
				jQuery("#onglet"+idInactif).css("height", "32px");
			});
		});
        </script>

