<script type="text/javascript">
$(document).ready( function(event){
	var month_array = new Array( 'January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December' );

	$.getJSON('http://app.toronto.ca/tpha/json/heatAlert.html?jsonp=?', null, function( result, testStatus ){
	
	var dateData = new Date( parseInt( result['HEAT_ALERT_DATE'].substr( 0, 4 ), 10 ), parseInt( result['HEAT_ALERT_DATE'].substr( 5, 2 ), 10 ) - 1, parseInt( result['HEAT_ALERT_DATE'].substr( 8, 2 ), 10 ) );

	var dateString = month_array[ dateData.getMonth() ] + ' ' + dateData.getDate() + ', ' + dateData.getFullYear() + ', ';

	var $heatalertimage = $('<img>').attr( { 'alt':'', 'src':'/City Of Toronto/Toronto Public Health/Communications/Home Page/Files/images/A/alert_heatalert.png' } ).css( { 'float':'left', 'margin-right':'5px' } );
	var $heatalertheading = $('<strong>').css( 'text-transform', 'uppercase' ).text( 'No Heat Warning' );
	var $heatalertdate = $('<span>').text( dateString );
	var $heatalertmessage = $('<span>').text( result['HEAT_ALERT_ADVISORY'] );

	var $heatalertblock = $('<p>').css( { 'background-color':'#eee', 'border':'1px solid #ccc', 'display':'none', 'overflow':'hidden', 'padding':'7px 7px 7px 0', '-webkit-border-radius':'5px', '-moz-border-radius':'5px', 'border-radius':'5px' } ).append( $heatalertimage, $heatalertheading, $('<br>'), $heatalertdate, $heatalertmessage );

	$('#alertplaceholder').append( $heatalertblock );

	if( result['HEAT_ALERT_FLAG'] == 'HA' || result['HEAT_ALERT_FLAG'] == 'HAE' || result['HEAT_ALERT_FLAG'] == 'EHAD' )
	{
								$heatalertimage.attr( 'src', '/City Of Toronto/Toronto Public Health/Communications/Home Page/Files/images/A/alert_heatalert.png' )
								$heatalertheading.text('Heat Warning').wrapInner( $('<a>').attr( 'href', 'http://www1.toronto.ca/wps/portal/contentonly?vgnextoid=faf1ebfc2bb31410VgnVCM10000071d60f89RCRD' ) )
								$heatalertblock.css( 'background-color', '#fffbe3' ).slideDown();
							}
							else if( result['HEAT_ALERT_FLAG'] == 'EHA' || result['HEAT_ALERT_FLAG'] == 'HAU' || result['HEAT_ALERT_FLAG'] == 'EHAE')
							{
								$heatalertimage.attr( 'src', '/City Of Toronto/Toronto Public Health/Health Communications/Home Page/Files/images/A/alert_extremeheatalert.png' )
								$heatalertheading.text('Extended heat warning').wrapInner( $('<a>').attr( 'href', 'http://www1.toronto.ca/wps/portal/contentonly?vgnextoid=faf1ebfc2bb31410VgnVCM10000071d60f89RCRD' ) )
								$heatalertblock.css( 'background-color', '#f8e5e5' ).slideDown();
							};
						});

					//$.getJSON('http://pandora-1.inet.toronto.ca/alerts/alert.json?callback=?&group=DNSF', null, function( result, testStatus ) {
					
$.getJSON('http://app.toronto.ca/alerts/alert.json?callback=?&group=DNSF', null, function( result, testStatus ) {

	if(result.length > 0) {
		
		if (result.length > 1) {
			result.sort(function(a, b) {
				
				var date_a = new Date(a.date);
				var date_b = new Date(b.date);
				
				if (date_a.getTime() > date_b.getTime())
					return -1;
				
				if (date_b.getTime() > date_a.getTime())
					return 1;
				
				return 0;
			});
		}
		
		for(var n = 0; n < result.length; n++) {
			
			
			
			var $AlertImage = $('<img />').attr({
				'alt': '',
				'height': '47',
				'src': result[n].iconUrl,
				'width': '53'
			}).css({
				'float': 'left',
				'display': 'inline',
				'margin-left': '-70px'
			});
			
			var $AlertLink = $('<a />').attr({ 'href': result[n].detailsUrl }).append($('<strong>').text(result[n].title).css({'text-transform': 'uppercase' }));
			
			var $AlertDate = $('<strong />').text(result[n].date);
			
			var $AlertText = result[n].body;
			
			var $AlertMore = $('<a />').attr({ 'href': result[n].detailsUrl }).text( 'More' );
			
			$('#alertplaceholder').append($('<p />').append($AlertImage, $AlertLink, $('<br />'), $AlertDate, ', ', $AlertText, '&nbsp;&nbsp;', $AlertMore).css({
				'-webkit-border-bottom-right-radius': '5px',
				'-webkit-border-bottom-left-radius': '5px',
				'-moz-border-radius-bottomright': '5px',
				'-moz-border-radius-bottomleft': '5px',
				'border-bottom-right-radius': '5px',
				'border-bottom-left-radius': '5px',
				'-webkit-border-top-right-radius': '5px',
				'-webkit-border-top-left-radius': '5px',
				'-moz-border-radius-topright': '5px',
				'-moz-border-radius-topleft': '5px',
				'border-top-right-radius': '5px',
				'border-top-left-radius': '5px',
				'background-color': '#FFFBE3',
				'border': '1px solid #CCC',
				'margin-bottom': '3px',
				'padding': '5px 5px 5px 80px',
				'overflow': 'hidden',
				'zoom': '1'
			}));
		}
	};
});


				//if (result.length==0) {$('#alertplaceholder').parent().parent().parent().parent().parent().parent().remove()};

				});
