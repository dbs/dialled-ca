<script type="text/javascript">
$(document).ready(function ($) {
    var iec = new IECompatibility();
	if(iec.Version == "IE8" || iec.Version == "IE7" ){
        ieNoteOutput = '<!--[if lte IE 8]><div id="IE8note"><p><strong>Uh oh - it looks like you\'re using Internet Explorer 8 or older. This site doesn\'t support such an old browser.</strong></p> <p> It will work much better with IE 9 or above, <a href="http://www.browsehappy.com/" target="_new">you can find links to all the latest browsers  here.</a></p></div><![endif]-->';
		$("body").prepend(ieNoteOutput);
	}
	function IECompatibility() {
	  var agentStr = navigator.userAgent;
	  this.IsIE = false;
	  this.IsOn = undefined;  //defined only if IE
	  this.Version = undefined;
  
	  if (agentStr.indexOf("MSIE") > -1) {
		this.IsIE = true;
		this.IsOn = true;
		if (agentStr.indexOf("Trident/7") > -1) {
		  this.Version = 'IE11';
		} else if (agentStr.indexOf("Trident/6") > -1) {
		  this.Version = 'IE10';
		} else if (agentStr.indexOf("Trident/5") > -1) {
		  this.Version = 'IE9';
		} else if (agentStr.indexOf("Trident/4") > -1) {
		  this.Version = 'IE8';
		} else {
		  this.IsOn = false; // compatability mimics 7, thus not on
		  this.Version = 'IE7';
		}
	  } //IE 7
	}
  
  
  
  });
  </script>
  
