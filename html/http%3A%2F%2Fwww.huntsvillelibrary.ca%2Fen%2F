<div id="calendar" class="jsinclude"></div>
<script type="text/javascript" language="javascript" src="/en/Calendar/scripts/jquery.mini-calendar.js"></script>
<script type="text/javascript" language="javascript">
    var divElement = jQuery("#feedContent");
    divElement.html("Loading...");

    jQuery.ajax({
        url: '/en/Calendar/services/GetCalendarEvent.ashx',
        cache: false,
        async: true,
        error: function (xhr, textStatus, thrownError) {
            //debugger;
            divElement.append("<p>An error occured.</p>");
        },
        success: function (data) {
            divElement.empty();
            if (data.toString() != "") {
                var charLimit = 35;
                jQuery.each(data, function (index, item) {
                    var title = item.Title;
                    if (title.length > charLimit) {
                        if (title.indexOf(' ', charLimit) >= 0) {
                            title = title.substring(0, title.indexOf(' ', charLimit)) + "...";
                        }
                    }

                    divElement.append(jQuery("<p>").append(item.StartDate).append("<br />").append(jQuery("<a>").attr({ "href": "/en/Calendar/Details.aspx?id=" + item.Id, "title": "Event: " + title }).append(title)));
                });
            }
            else {
                divElement.append("<p>No Events found</p>");
            }
        }
    });

    jQuery("#calendar").html("Loading...");

    // Get events from all calendars
    jQuery("#calendar").empty().mini_calendar({
        source: "/en/Calendar/services/getCalendarsDateWithEvent.ashx",
        url: "/en/Calendar/default.aspx?PID=calendar"
    });
</script>
