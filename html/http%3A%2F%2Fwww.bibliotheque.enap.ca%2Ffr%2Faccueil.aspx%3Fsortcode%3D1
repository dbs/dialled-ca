<script type="text/javascript" language="javascript">

    $("document").ready(function () {
        $("#vl").blur(function () {
            var srtURL = "http://decouverte.uquebec.ca:1701/primo_library/libweb/action/dlSearch.do?tab=default_tab&institution=ENAP&onCampus=false&indx=1&dum=true&vid=ENAP&group=GUEST&search_scope=ENAP&lang=fre&query="
            $("#rechercheDecouverteA").attr("href", (srtURL + "any,contains," + replaceAccents($("#vl").val())));
        });

 $("#vl").keypress(function (e) {
    
            if (e.which == 13 || e.keyCode == 13) {

                var srtURL = "http://decouverte.uquebec.ca:1701/primo_library/libweb/action/dlSearch.do?tab=default_tab&institution=ENAP&onCampus=false&indx=1&dum=true&vid=ENAP&group=GUEST&search_scope=ENAP&lang=fre&query="
                $("#rechercheDecouverteA").attr("href", (srtURL + "any,contains," + replaceAccents($("#vl").val())));
        		window.open( $("#rechercheDecouverteA").attr("href"));
		
		        return false;

            }
        });
})

function validateForm() {
    var x = replaceAccents(document.forms.primo.query.value);
    if (x == null || x == "") {
        alert("Entrez un terme de recherche");
        return false;
    } else {

        document.forms.primo.query.value = "any,contains," + x;
    }
}

function replaceAccents(s){
            
            var sR = s;
            
            sR = sR.replace(/à/g, "a");
            sR = sR.replace(/á/g, "a");
            sR = sR.replace(/â/g, "a");
            sR = sR.replace(/ã/g, "a");
            sR = sR.replace(/ä/g, "a");
            sR = sR.replace(/ç/g, "c");
            sR = sR.replace(/è/g, "e");
            sR = sR.replace(/é/g, "e");
            sR = sR.replace(/ê/g, "e");
            sR = sR.replace(/ë/g, "e");
            sR = sR.replace(/ì/g, "i");
            sR = sR.replace(/í/g, "i");
            sR = sR.replace(/î/g, "i");
            sR = sR.replace(/ï/g, "i");
            sR = sR.replace(/ñ/g, "n");
            sR = sR.replace(/ò/g, "o");
            sR = sR.replace(/ó/g, "o");
            sR = sR.replace(/ô/g, "o");
            sR = sR.replace(/õ/g, "o");
            sR = sR.replace(/ö/g, "o");
            sR = sR.replace(/ù/g, "u");
            sR = sR.replace(/ú/g, "u");
            sR = sR.replace(/û/g, "u");
            sR = sR.replace(/ü/g, "u");
            sR = sR.replace(/ý/g, "y");
            sR = sR.replace(/ÿ/g, "y");

            sR = sR.replace(/À/g, "a");
            sR = sR.replace(/Á/g, "a");
            sR = sR.replace(/Â/g, "a");
            sR = sR.replace(/ã/g, "a");
            sR = sR.replace(/Ä/g, "a");
            sR = sR.replace(/Ç/g, "c");
            sR = sR.replace(/È/g, "e");
            sR = sR.replace(/É/g, "e");
            sR = sR.replace(/Ê/g, "e");
            sR = sR.replace(/Ë/g, "e");
            sR = sR.replace(/Ì/g, "i");
            sR = sR.replace(/Í/g, "i");
            sR = sR.replace(/Î/g, "i");
            sR = sR.replace(/Ï/g, "i");
            sR = sR.replace(/ñ/g, "n");
            sR = sR.replace(/Ò/g, "o");
            sR = sR.replace(/Ó/g, "o");
            sR = sR.replace(/Ô/g, "o");
            sR = sR.replace(/õ/g, "o");
            sR = sR.replace(/Ö/g, "o");
            sR = sR.replace(/Ù/g, "u");
            sR = sR.replace(/Ú/g, "u");
            sR = sR.replace(/Û/g, "u");
            sR = sR.replace(/Ü/g, "u");
            sR = sR.replace(/Ý/g, "y");

            return sR;
            
            }


       

       
</script>
