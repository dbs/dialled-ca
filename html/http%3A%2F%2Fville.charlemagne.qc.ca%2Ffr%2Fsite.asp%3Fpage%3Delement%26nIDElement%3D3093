														//configure the below five variables to change the style of the scroller
														var scrollerdelay='6000' //delay between msg scrolls. 3000=3 seconds.
														var scrollerwidth='160px'
														var scrollerheight='50px'
														var scrollerbgcolor='#d85a27'
														//set below to '' if you don't wish to use a background image
														var scrollerbackground=''
														
														//configure the below variable to change the contents of the scroller
														var messages=new Array()
														
															messages[0]="<a class='text_central' href='site.asp?page=evenement&nActualite=1&nAnnee=2016&nIDEvenement=16925' class='text_boites'>24 octobre 2016<br><img src='ville.charlemagne/images/spacer.gif' width='50' height='5' border='0' />Collecte et déchiquetage des branches d\'arbres</a>"
															
															messages[1]="<a class='text_central' href='site.asp?page=evenement&nActualite=1&nAnnee=2016&nIDEvenement=17991' class='text_boites'>17 octobre 2016<br><img src='ville.charlemagne/images/spacer.gif' width='50' height='5' border='0' />Collecte surplus résidus verts</a>"
															
															messages[2]="<a class='text_central' href='site.asp?page=evenement&nActualite=1&nAnnee=2016&nIDEvenement=16563' class='text_boites'>10 octobre 2016<br><img src='ville.charlemagne/images/spacer.gif' width='50' height='5' border='0' />Fermeture des services municipaux</a>"
															
															messages[3]="<a class='text_central' href='site.asp?page=evenement&nActualite=1&nAnnee=2016&nIDEvenement=10656' class='text_boites'>4 octobre 2016<br><img src='ville.charlemagne/images/spacer.gif' width='50' height='5' border='0' />Prochaine séance du conseil municipal</a>"
															
