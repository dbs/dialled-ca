    $(document).ready(function () {
        if ($('.sfPageEditor').length == 0) {
            var itemsCount = $('.carouselWrapper #slider li').length;
            //var startNum = getRandom(1, itemsCount);
            
            //if (window.location.pathname.toLowerCase().indexOf('/convention') > -1) {
            //    startNum = 1; //start slideshow at first slide for convention pages
            //}

            startNum = 1; //start slideshow at first slide everywhere
            
            // for slider options, view http://css-tricks.com/examples/AnythingSlider/
            $('.carouselWrapper #slider').anythingSlider({
                buildArrows: false,
                buildStartStop: false,
                hashTags: false,
                autoPlay: true,
                autoPlayLocked: true,
                delay: 7000,
                resumeDelay: 8000,
                startPanel: startNum,
                navigationFormatter: function (i, panel) {
                    return {
                        'html': '<a class="panel' + i + ' ' + panel.find('.sectionName').text() + '" href="#"><span>' + i + '</span></a>'
                    };
                }
            });
        }
    });

    //returns random number between a specific range
    function getRandom(min, max) {
        //http://stackoverflow.com/questions/1527803/generating-random-numbers-in-javascript-in-a-specific-range
        return Math.floor(Math.random() * (1 + max - min)) + min;
    }
