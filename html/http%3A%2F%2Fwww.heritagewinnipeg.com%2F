function mi_news_comments_window(addcomment, item_id) {
	var anchor = "";
	if (addcomment) {
		anchor = "#addcomment";
	}
	cmtwin = window.open('/index.html?d=comments&item='+item_id+anchor, 'comments','x=0,y=0,width=530,height=480,scrollbars=1,status=1,menubar=1'); 
	if(cmtwin) { 
		cmtwin.focus();
	} else {
		alert('The comments window could not be displayed. Please make sure your browser enables popups for this site.');
	}
}
