	var menuStatus;
	var wrapperLeft;
	var ulLeft;
	var current_menu;
	var parent_class;
	var sibling_class;
	var smallSize = 100;
	var largeSize = 165;
	
	//stroll.bind($( '#mobile_nav ul' ));
	
	//function for setting the height of the view_menu buttons
	function resize_menu_btn() {
