<script type="text/javascript">
(function($) {
    $.fn.listenForChange = function(options) {
        settings = $.extend({
            interval: 200 // in microseconds
        }, options);

        var jquery_object = this;
        var current_focus = null;

        jquery_object.filter(":input").add(":input", jquery_object).focus( function() {
            current_focus = this;
        }).blur( function() {
            current_focus = null;
        });

        setInterval(function() {
            // allow
            jquery_object.filter(":input").add(":input", jquery_object).each(function() {
                // set data cache on element to input value if not yet set
                if ($(this).data('change_listener') == undefined) {
                    $(this).data('change_listener', $(this).val());
                    return;
                }
                // return if the value matches the cache
                if ($(this).data('change_listener') == $(this).val()) {
                    return;
                }
                // ignore if element is in focus (since change event will fire on blur)
                if (this == current_focus) {
                    return;
                }
                // if we make it here, manually fire the change event and set the new value
                $(this).trigger('change');
                $(this).data('change_listener', $(this).val());
            });
        }, settings.interval);
        return this;
    };
})(jQuery);

(function($) {
	$.fn.fileName = function() {
		var $this = $(this),
		$val = $this.val(),
		valArray = $val.split('\\'),
		newVal = valArray[valArray.length-1],
		$button = $this.siblings('.button');
		if(newVal !== '') {
			$button.text(newVal);
	  	}
	};
})(jQuery);

// wrap children of secondary-navigation navigation with a container; WG doesn't add this in the dynamically generated markup
function containSecondary() {
	$(".main-navigation .children").each(function() {
		//Append combined button for programs and faculties
		if($(this).hasClass('li-faculties') || $(this).hasClass('li-programs')) {
			$(this).children('.secondary-navigation').append('<div class="combined-btn-holder"><a class="combined-btn" href="/faculties-departments-programs"><span></span>View all Faculties, Departments &amp; Programs</a></div>');
		}
	});

	$(".secondary-navigation").each(function() {
		$(this).children().wrapAll("<div class='container' />");
	});
}

function navigationToggle() {

	var $menu = $('.main-navigation'),
		$menulink = $('.menu-toggle');

	$menulink.click(function() {
		$menu.toggleClass('toggled');
		return false;
	});

	$(".main-navigation .children").each(function() {
		$(this).append('<a href="#" class="dropdown-toggle icon plus"><span class="dropdown-arrow">&darr;</span></a>');
	});

	$(".dropdown-toggle").click(function(e) {
		$(this).siblings("ul").stop(true, true).slideToggle(120);
		$(this).toggleClass("clicked");
		e.preventDefault();
	});
}

function backTop() {
	$('.back-to-top').click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 800);
		return false;
	});
}

$(document).ready(function(){
	navigationToggle();
	containSecondary();
	backTop();

	$('#application_process li h2').click(function() {
		if($(this).hasClass('open')){
			$(this).removeClass('open');
			$(this).next('.step-content').stop(true, true).slideUp();
		}else{
			$(this).addClass('open');
			$(this).next('.step-content').stop(true, true).slideDown();
			window.location.hash = 'how_to_apply/'+$(this).parent('.step').attr('id');
		}
	});

	$('.gallery_toggle').toggle(function() {
		$(this).toggleClass('open');
		$(this).parent().next('.gallery_slide').stop(true, true).slideDown();
	}, function() {
		$(this).toggleClass('open');
		$(this).parent().next('.gallery_slide').stop(true, true).slideUp();
	});

	$('.first_photo_preview').click(function(){
		$(this).next().next('.gallery_toggle').trigger('click');
	});

	$('a.leder-button').click(function(){
		ga('send', 'event', 'Leder Link Clicked', 'Header Logo');
	});

	$('a.ent_5416').click(function(){
		ga('send', 'event', 'Leder Link Clicked', 'Enticer');
	});

	$('a.student-aid').click(function(){
		ga('send', 'event', 'Student Aid Calculator', 'Clicked');
	});

	$('a.step-button.a5893').click(function(){
		ga('send', 'event', 'Apply Now', 'Clicked');
	});

	$('.slides .learn-more-button').click(function(){
		var position = $(this).parents('li').data('num');
		ga('send', 'event', 'Carousel Button', 'Clicked', position);
	});

	// featured event toggle and tabs

	$(".featured-event-toggle").click(function(e) {
		$(".featured-event-hidden").slideToggle(600);
		$(this).toggleClass("shown");
		e.preventDefault();
	});

	$(".featured-event-toggle-top").click(function(e) {
		$(".featured-event-hidden").slideToggle(600);
		$(this).toggleClass("shown");
		$("#main_nav").toggleClass("event-shown");
		e.preventDefault();
	});

	$(".datepicker").datepicker();
	$(".monthpicker").datepicker({
        dateFormat: 'MM yy',
        changeMonth: true,
        changeYear: true,
        showButtonPanel: true,

        onClose: function(dateText, inst) {
            var month = $("#ui-datepicker-div .ui-datepicker-month :selected").val();
            var year = $("#ui-datepicker-div .ui-datepicker-year :selected").val();
            $(this).val($.datepicker.formatDate('MM yy', new Date(year, month, 1)));
        }
    });
    
    $(".monthpicker").focus(function () {
        $(".ui-datepicker-calendar").hide();
    });

	//File Input Style
	$('input[type=file]').bind('change focus click', function() {$(this).fileName()});

	//tipsy
	$('.office_forms [title]').tipsy({trigger: 'focus', gravity: 'w'});
	
		
