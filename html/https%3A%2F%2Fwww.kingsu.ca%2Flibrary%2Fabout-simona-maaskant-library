	function hoverIn(selector){
		$('#main_nav').addClass('hover');
		$('#dropdown').slideDown(1000);
	}
	
	function hoverOut(selector){
		$('#dropdown').slideUp(1000, function(){
			$('#main_nav').removeClass('hover');
		});
	}
