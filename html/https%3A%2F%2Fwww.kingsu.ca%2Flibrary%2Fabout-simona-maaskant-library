	//Directory Search form
	var delay_submit = (function(){
		var timer = 0;
		return function(callback, ms){
			clearTimeout (timer);
			timer = setTimeout(callback, ms);
		};
	})();
	
	function set_loader(keywords, location) {
		if(keywords && keywords != ''){
			var search_type = $('.search_type').val();
			if(location == 'home') {
				var the_id = '#staff_search';
				$(the_id).fadeIn();
				$('.filter_input').addClass('results');
			} else if(location == 'footer') {
				var the_id = '#footer_staff_search';
				$(the_id).fadeIn();
				$('.filter_input').addClass('results');
			} else {
				var the_id = '#staff_search_inside';
			}
			var loader_data = '';
			if(search_type == 'contacts') {
				loader_data = '<div class="row loading">';
				loader_data += '<div class="loader">Searching</div>';
				loader_data += '</div>';
				$(the_id + ' .inner').html(loader_data);
			}
		}
	}

	$('.argument').bind('keyup', function(){
		var keywords = $(this).val();
		var location = $(this).parent().nextAll('.location').val();
		//console.log('keyup');
		//console.log('location: ' + location);
		if(location == 'directory') {
			var search_val = $(this).val();
			var selector = $('.type_filter li.option.selected').attr('data-filter');
			//console.log('keywords: ' + selector);
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry',
		        search_val : search_val,
		        filter : selector
		    });
		} else {
			set_loader(keywords, location);
			var url = $('.search_form').attr('action');
			contact_type = $('.type_id').val();
			if(keywords && keywords != '') {
				delay_submit(function(){
					old_submit_form(keywords, url, location);
				}, 300);
			} else {
				old_submit_form(keywords, url);
			}
		}
	});
	
	var contact_type = $('.type_id').val();
	
	function strstr_simple(haystack, needle, bool) {
	    var pos = 0;
	 
	    haystack += '';
	    pos = haystack.indexOf(needle);
	    if (pos == -1) {
	        return false;
	    } else {
	        return true;
	    }
	}


	function old_submit_form(keywords, url, location){
		var search_type = $('.search_type').val();
		$.ajax({
			type: 'POST',
			dataType: 'json',
			url: url,
			data: {keywords: keywords, location: location, search_type: search_type, contact_type: contact_type, method: 'ajax'},
			success: function(data){
				if(location == 'home') {
					var the_id = '#staff_search';
				} else if(location == 'footer') {
					var the_id = '#footer_staff_search';
				} else {
					var the_id = '#staff_search_inside';
				}
				$(the_id + ' .inner').empty();
				if(!(data.length)){
					if(search_type == 'contacts') {
						var html = '<div class="row no_results">';
						html += 'No matching results found.';
						html += '</div>';
					}
				} else if(data.length > 0) {
					var html = '';
					
					html = data;
				}
				if(search_type == 'contacts')
				{
					$(the_id + ' .inner').html(html);
					
					$('.row').hover(function() {
						$(this).toggleClass('hover');
					});
				}
				/*
if(location == 'directory') {
					$($iso_container).isotope({
				        itemSelector: '.row',
				        layoutMode : 'masonry'
				    });
				}
*/
			}
		});
	}
	
	$('.row').hover(function() {
		$(this).toggleClass('hover');
	});
	
	$('#inside_content.departments #main_content ul li ul').isotope({
        itemSelector: '#inside_content.departments #main_content ul li ul li',
        layoutMode : 'masonry'
    });
	
	$('.filter_input.home_search, .filter_input.footer_search').focusout(function(e){
		setTimeout("$('#staff_search, #footer_staff_search').fadeOut()", 500);
		$('.filter_row .clear.button').hide();
		$('.filter_row .search.button').show();
		$('.filter_input.home_search, .filter_input.footer_search').val('');
		//setTimeout("$('.filter_input').removeClass('results')", 500);
	});
	
	$('.filter_input.directory_search').focusout(function(){
		if( $(this).val() == '' ) {
			$(this).siblings('.clear.button').hide();
			$(this).siblings('.search.button').show();
			$(this).prev('label').show();
		}
		//setTimeout("$('.filter_input').removeClass('results')", 500);
	});

	//hide 'x' initially
	$(".filter_row .clear.button").hide();
    
    //hide/show title/ 'x' when clicking/leaving input
    $('.filter_row input').blur(function() { 
		if( $('.filter_row input').val() == '' ) {
	    	$(".filter_row .clear.button").hide();
	    	$(".filter_row .search.button").show();
		}
	}).focus(function() {
    	$(".filter_row .clear.button").show();
    	$(".filter_row .search.button").hide();
	});
    
    //hide 'x', show title when clicking 'x', reset filter
    $(".filter_row .clear.button").click(function(){
    	$(this).prevAll('.argument').val('');
    	$(this).prevAll('.argument').trigger('keyup');
    	$(".filter_row .clear.button").hide();
    	$(".filter_row .search.button").show();
    	$(this).siblings('label').show();
	});
	//End Directory Search form

	//BETTER SEARCH AND FILTER--------------------
	var search_type = $('.ajax-search').attr('data-search-type');

	$('.ajax-filter.dropdown ul.select:not(.custom) li.option:not(:has(a))').click(function(e) {
		e.stopPropagation();
		$('ul.select li.option').removeClass('selected');
		$(this).addClass('selected');
		var title = $(this).html();
		$(this).parent('ul.select').prev('.ajax-filter.dropdown .select_toggle').find('a .title').html(title);
		$(this).parent().prev('.ajax-filter.dropdown .select_toggle').click();
		if($(this).parents('ul.select').parents('.ajax-filter.dropdown').hasClass('filter')) {
			var keywords = $('.ajax-search').val() != 'Start typing...' && $('.ajax-search').val() != 'Search' ? $('.ajax-search').val() : null;
			var category = $(this).attr('name');
			var url = $('.search-form').attr('action');
			
			//If we're on the calendar page, we also need to do the special ajax request for the grid calendar
			if(search_type == 'events') {
				submit_calendar_form(keywords, category, url);
			}

			submit_form(keywords, category, url);
		}
		if($(this).parents('ul.select').parents('.ajax-filter.dropdown').hasClass('contact')) {
			var to = $(this).attr('name');
			$(this).parent('ul.select').next('input').val(to);
		}
	});
	
	function get_filters() {
		var filters = {};
		//Get all the filters values
		$('.ajax-filter.dropdown ul.select.custom').each(function(){
			var filter_type = $(this).data('filter');
			filters[filter_type] = $(this).find('li.option.selected').attr('name');
		});

		return filters;
	}

	$('.ajax-filter.dropdown ul.select.custom li.option:not(:has(a))').click(function(e) {
		e.stopPropagation();
		$(this).siblings('li.option').removeClass('selected');
		$(this).addClass('selected');
		var title = $(this).html();
		$(this).parent('ul.select').prev('.ajax-filter.dropdown .select_toggle').find('a .title').html(title);
		$(this).parent().prev('.ajax-filter.dropdown .select_toggle').click();
		if($(this).parents('ul.select').parents('.ajax-filter.dropdown').hasClass('filter')) {
			var keywords = $('.ajax-search').val() != 'Start typing...' && $('.ajax-search').val() != 'Search' ? $('.ajax-search').val() : null;
			
			var filters = get_filters();
			
			/*//Get all the filters values
			$('.ajax-filter.dropdown ul.select.custom').each(function(){
				var filter_type = $(this).data('filter');
				filters[filter_type] = $(this).find('li.option.selected').attr('name');
			});*/

			var url = $('.search-form').attr('action');

			submit_form(keywords, filters, url);
		}
		if($(this).parents('ul.select').parents('.ajax-filter.dropdown').hasClass('contact')) {
			var to = $(this).attr('name');
			$(this).parent('ul.select').next('input').val(to);
		}
	});

	var delay_submit = (function(){
		var timer = 0;
		return function(callback, ms){
			clearTimeout (timer);
			timer = setTimeout(callback, ms);
		};
	})();
	
	$('.ajax-search').bind('keyup', function(){
		var keywords = $('.ajax-search').val() != 'Start typing...' && $('.ajax-search').val() != 'Search' ? $('.ajax-search').val() : null;
		var category = $('ul.select li.option.selected').attr('name');
		var url = $('.search-form').attr('action');
		delay_submit(function(){
			//If we're on the calendar page, we also need to do the special ajax request for the grid calendar
			if(search_type == 'events') {
				submit_calendar_form(keywords, category, url);
			}

			submit_form(keywords, category, url);
		}, 300);
	});
	
	if(window.location.hash) {
		var hash = window.location.hash.replace('#', '');

		if(hash.length > 1) {
			//Break the current hash into an array, split by /
			var hash_array = hash.split('/');

			//Check if the array contains 'category'
			if(in_array('category', hash_array)) {
				//It does, so let's get the index
				var category_index = $.inArray('category', hash_array);
				//Get the value of the category
				var category = hash_array[category_index + 1];
				//Find the select menu item for this category
				var currCat = $('.ajax-filter ul.select li[name="' + category + '"]');
				//Add the selected class to it
				currCat.addClass('selected');
				//Change the display text as well
				currCat.parent('ul.select').prev('.ajax-filter.filter .select_toggle').find('a .title').html(currCat.html());
				//Submit the form
				delay_submit(function(){
					submit_form('', category);
				}, 300);
			}

			if(in_array('how_to_apply', hash_array)) {
				var step_index = $.inArray('how_to_apply', hash_array);
				var step = hash_array[step_index + 1];

				$('body,html').animate({
					scrollTop: $('.steps #'+step).offset().top
				}, 500);
				$('.steps #'+step+' h2').addClass('open');
				$('.steps #'+step+' .step-content').stop(true, true).slideDown();
			}
		}
	}

	function in_array(needle, haystack) {
		var length = haystack.length;
		for(var i = 0; i < length; i++) {
			if(haystack[i] == needle) return true;
		}
		return false;
	}

	function submit_form(keywords, category, url) {
		if(typeof category !== 'object') {
			var hash = window.location.hash.replace('#', '');
			var new_hash = '';

			if(hash.length <= 1) {
				//If hash is either nothing or '_', we just need to set it to the category
				if(category && category != '' && category != 'undefined') {
					new_hash += 'category/' + category;
				}
			} else {
				//Break the current hash into an array, split by /
				var hash_array = hash.split('/');
				
				//Check if the array contains 'category'
				if(in_array('category', hash_array)) {
					//It does, so let's get the index
					var category_index = $.inArray('category', hash_array);

					if(category && category != '' && category != 'undefined') {
						//If a new category has been passed, let's change the value in the current array
						hash_array[category_index + 1] = category;
					} else {
						//Now we'll remove that item as well as the next one (the category value)
						hash_array.splice(category_index,2);
					}
				} else {
					//Category was not in the array yet, so let's add it
					if(category && category != '' && category != 'undefined') {
						hash_array.push('category',category);
					}
				}

				//Put the array back into hash format
				var hash_string = hash_array.join('/');

				//Set the new_hash var to the hash string
				new_hash = hash_string;
			}

			//If the new hash is nothing, let's set it to _ so it doesn't jump us up the page
			if(new_hash.length < 1) {
				new_hash = '_';
			}

			//Set the new hash
			window.location.hash = new_hash;
		}
		
		$.ajax({
			type: 'POST',
			dataType: 'json',
			url: url,
			data: {keywords: keywords, category: category, search_type: search_type, method: 'ajax'},
			beforeSend: function () {
				$('.ajax-reload').addClass('loading');
				$('.ajax-loader').addClass('loading');
				$('.ajax-reload').animate({
					opacity: 0.5
				});
			},
			success: function(data){
				if(!(data.length)){
					var html = '<div class="info">';
					html += '<h3>No matching results found.</h3>';
					html += '</div>';
				} else if(data.length > 0) {
					var total = data.length;
					var html = data;
				}

				$('.ajax-reload').animate({
					opacity: 0
				}, function(){
					$('.ajax-reload').html(html);
					$('.ajax-reload').removeClass('loading');
					$('.ajax-loader').removeClass('loading');
					$('.ajax-reload').animate({
						opacity: 1
					});
				});
				$('.pagination').hide();
			},
			error: function(jqXHR, textStatus, errorThrown){
				console.log(jqXHR);
				console.log(textStatus);
				console.log(errorThrown);
			}
		});
	}

	function submit_calendar_form(keywords, category, url) {
		var year = '';
		var month = '';
		
		if($('#grid_calendar').length > 0) {
			var previous = $('.calendar_nav .previous').attr('href').replace('#','').split('/');
			var year = parseInt(previous[0]);
			var month = parseInt(previous[1]) + 1;
			if(month == 13){
				var month = 1;
				var year = parseInt(previous[0] + 1);
			}
		}

		var calendar_id = $('.ajax-search').attr('data-calendar-id');

		$.ajax({
			type: 'POST',
			dataType: 'json',
			url: url,
			data: {year: year, month: month, tag: category, keywords: keywords, calendar_id: calendar_id, search_type: 'calendar_month', method: 'ajax'},
			beforeSend: function () {
				$(".calendar").addClass('loading');
				$('.ajax-loader').addClass('loading');
			},
			success: function(data){
				$(".calendar").removeClass('loading');
				$('.ajax-loader').removeClass('loading');
				$('#grid_calendar').empty();
				if(data.length > 0) {
					var total = data.length;
					var html = '';
					
					function get_data() {
						html += data;
					}
					
					get_data();
				}
				
				$('#grid_calendar').html(html);
			}
		});
	}

	//END BETTER SEARCH AND FILTER--------------------

	//Lazy loading
	$(window).scroll(function(){
		var body_top = $(window).scrollTop();
		var load_trigger = false;
		if($('#load-more').length){
			load_trigger = $('#load-more').offset().top - window.innerHeight;
		}
		
		if(load_trigger !== false && !$('#load-more').hasClass('load-error')) {
			if(body_top > load_trigger) {
				$('.ajax-loader').addClass('loading');

				var keywords = $('.ajax-search').val() != 'Start typing...' && $('.ajax-search').val() != 'Search' ? $('.ajax-search').val() : null;
				var url = $('.search-form').attr('action');
				var category = get_filters();
				var ajax_page = $('#load-more').attr('data-ajax-next-page');

				$('#load-more').remove();

				$.ajax({
					type: 'POST',
					dataType: 'json',
					url: url,
					data: {keywords: keywords, category: category, search_type: search_type, ajax_page: ajax_page, method: 'ajax'},
					success: function(data) {
						var html;
						if(data !== '' && data.length > 0) {
							html = data;
							var new_page_num = parseInt(ajax_page) + 1;

							$('.ajax-reload').append(html);
							$('#load-more').attr('data-ajax-next-page', new_page_num);
						}

						$('.ajax-loader').removeClass('loading');
					}
				});
			}
		}
	});
	
		
	window.addEventListener('focus', function() {
	    $(window).resize();
	});
	
	$(window).resize(function() {
		body_width = $('body').width();
		$('#application_process .item .info').each(function(){
			item_height = Math.round($(this).height()/10)*10;
			$(this).height(item_height);
		});
		if(body_width_static > 1000 && body_width < 1000)
		{
			body_width_static = body_width;
			
			$($iso_container).isotope('destroy');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		} else if(body_width_static < 1000 && body_width > 1000) {
			body_width_static = body_width;
			
			if($("#mobile_menu_btn").hasClass('open')) {
				$("#mobile_menu_btn").click();
			}
			
			$($iso_container).isotope('destroy');
		    $($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		    
		   /* //Vertical align quote inside bubble HOME PAGE
			$('.home .quote p').each(function(){
				$(this).css('paddingTop' , (190 - $(this).height()) / 2 );
				$(this).css('paddingBottom' , (190 - $(this).height()) / 2 );
			});
		
			//Vertical align quote inside bubble INSIDE PAGE
			$('.inside .quote p').each(function(){
				$(this).css('paddingTop' , (156 - $(this).height()) / 2 );
				$(this).css('paddingBottom' , (156 - $(this).height()) / 2 );
			});*/
		} else if(body_width_static > 740 && body_width < 740) {
			body_width_static = body_width;
			
			$($iso_container).isotope('destroy');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		} else if(body_width_static < 740 && body_width > 740) {
			body_width_static = body_width;
			
			if($("#mobile_menu_btn").hasClass('open')) {
				$("#mobile_menu_btn").click();
			}
			
			$($iso_container).isotope('destroy');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		}
		//console.log('body_width_static: ' + body_width_static);
		//console.log('body_width: ' + body_width);
	});

});

	function fillNavSpace() {
		var container = $('.navigation-container .container');
		var menu = $('.main-navigation');
		var isSafari = /constructor/i.test(window.HTMLElement);

		var difference = container.width() - menu.width();

		if(isSafari){
			difference = difference - 100;
		}

		if (difference > 0) {
		    var count = menu.children().length;

		    var pad = (difference / count) / 2;

		    var total = 0;
		    menu.children().each(function(){
		        var el = $(this);
		        var lpad = parseInt(el.css('padding-left'), 10);
		        var rpad = parseInt(el.css('padding-right'), 10);

		        el.css('padding-left', lpad+pad);
		        total += lpad+pad;

		        if ((total+rpad+pad) < difference) {
		            el.css('padding-right', rpad+pad);
		        } else {
		            el.css('padding-right', Math.floor(rpad+pad));
		        }
		        total += rpad+pad;
		    });
		}
	}


