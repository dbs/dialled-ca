	resize_menu_btn()
	
	    $("#mobile_menu_btn").click(function(){
    	$(this).toggleClass('open');
        if(menuStatus != true){
        	$('ul.mobile_menu').show();
        	$('ul.mobile_menu').addClass('theparentmenu');
        	resize_menu_btn();
        	//$(".wrapper").css('position', 'fixed');
			$(".wrapper").animate({
				marginLeft: largeSize + "px"
			}, 300, function(){menuStatus = true}).addClass('open');
			/*
$("#mobile_header").animate({
				left: largeSize + "px",
			}, 300);
*/
			wrapperLeft = largeSize;
			ulLeft = 0;
		} else {
			//$(".wrapper").css('position', 'absolute');
			$(".wrapper").animate({
				marginLeft: "0px"
			}, 300, function(){menuStatus = false}).removeClass('open');
			/*
$("#mobile_header").animate({
				left: "0px",
			}, 300);
*/
			$('ul.mobile_menu').animate({
				left: "0px"
			}, 300);
			$("#mobile_nav li").each(function(){
				$(this).removeClass('open_children');
			});
			$('ul.mobile_menu').removeClass('small');
			$('ul.mobile_menu').find('ul').each(function(){
				$(this).css('left', 0);
				$(this).removeClass('small');
		    	$(this).hide();
	    	});
	    	$('#current_menu').html();
	    	$('#current_menu').hide();
			wrapperLeft = 0;
			ulLeft = 0;
		}
    });
    
