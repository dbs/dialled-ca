<script type="text/javascript">
//<![CDATA[

function replace2(fullString,text,by) {
// Replaces text with by in string
    var strLength = fullString.length, txtLength = text.length;
    if ((strLength == 0) || (txtLength == 0)) return fullString;

    var i = fullString.indexOf(text);
    if ((!i) && (text != fullString.substring(0,txtLength))) return fullString;
    if (i == -1) return fullString;

    var newstr = fullString.substring(0,i) + by;

    if (i+txtLength < strLength)
        newstr += replace2(fullString.substring(i+txtLength,strLength),text,by);

    return newstr;
}
//]]>

//script pour l'edition rapide (nouvelle version)

var go_afficheadmin = true;

$(document).keydown(function(e){
	//CTRL + Q keydown combo
	if(e.ctrlKey && e.keyCode == 81)
	{
		page = "4";
		if (page == "")
		{
			alert("Vous ne pouvez pas utiliser l'édition rapide dans cette page");
		}
		else
		{
			//alert('test de saisie');
	
			site_url = "http://www.ville.lorraine.qc.ca/";
			
			window.open(site_url + "admin/views/plan_site/edition_rapide.php?type_action=afficher&action=modifier&langue=FR&id=4","ed_r","width=620, height=350, scrollbars=no,status=no,menubar=no,toolbar=no,resizable=1,location=no,directories=no, top=30px,left=100px");
		}
	}
});

</script>



