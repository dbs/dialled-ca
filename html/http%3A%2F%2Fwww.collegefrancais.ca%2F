<script type="text/javascript">
(function($) {


	// Add URL parameter
	var addParameter = function (url, parameterName, parameterValue, atStart){ /*atStart : Add param before others*/
		replaceDuplicates = true;
		if(url.indexOf('#') > 0){
			var cl = url.indexOf('#');
			urlhash = url.substring(url.indexOf('#'),url.length);
		} else {
			urlhash = '';
			cl = url.length;
		}
		sourceUrl = url.substring(0,cl);

		var urlParts = sourceUrl.split("?");
		var newQueryString = "";

		if (urlParts.length > 1)
		{
			var parameters = urlParts[1].split("&");
			for (var i=0; (i < parameters.length); i++)
			{
					var parameterParts = parameters[i].split("=");
					if (!(replaceDuplicates && parameterParts[0] == parameterName))
					{
							if (newQueryString == "")
								newQueryString = "?";
							else
								newQueryString += "&";
							newQueryString += parameterParts[0] + "=" + (parameterParts[1]?parameterParts[1]:'');
					}
			}
		}
		if (newQueryString == "")
			newQueryString = "?";

		if(atStart){
			newQueryString = '?'+ parameterName + "=" + parameterValue + (newQueryString.length>1?'&'+newQueryString.substring(1):'');
		} else {
			if (newQueryString !== "" && newQueryString != '?')
					newQueryString += "&";
			newQueryString += parameterName + "=" + (parameterValue?parameterValue:'');
		}
		return urlParts[0] + newQueryString + urlhash;
	};

	$(document).ready(function() {

		$('.site_show_network_message').each(function(x){

			var el = $(this);
			var url = $(this).parents('a').attr('href');
			if( url && typeof(url) != 'undefined' && url != '' ){

				var arr = url.split("/");
				var result_url = arr[0] + "//" + arr[2] + '/wp-admin/admin-ajax.php?action=cbs_theme_aop_return&aop_show=show_network_message';

				//console.log( result_url );

				if( result_url && typeof(result_url) != 'undefined' && result_url != '' ){
					var data = {};
					$.get( result_url, data, function( data ) {
						//$( ".result" ).html( data );
						//console.log( data );

						//console.log( data.show_network_message );

						$(el).hide();
						
						if( data.show_network_message && typeof(data.show_network_message) != 'undefined' && data.show_network_message == '1' ){
							$(el).html('<em><i class="fa fa-exclamation-triangle"></i> Voir le site pour un message.</em>');
							$(el).fadeIn( 500 );
						}
					});
				}

			}

		});

	});

})(jQuery);
</script>

<style type="text/css">
	.site_show_network_message em{
		color: #333;
		font-size: 13px;
		font-style: normal;
		text-decoration: none;
	}
</style>


		
