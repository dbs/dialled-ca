								//resolve the conflict between js libraries
								//$.fn.uitabs = $.fn.tabs;
								//delete $.fn.tabs;
								
								function onYouTubePlayerReady(ytplayer) {
									ytplayer = document.getElementById("YT_Banner");
									ytplayer.addEventListener("onStateChange", "stopBanner");
									
									$("ul.lof-navigator li:not(.lof-nav-yt)").click(function() {
										ytplayer.stopVideo();																	
									});
								}
													
								function stopBanner(state) {
									if(state == 1) {
										$("ul.lof-navigator li.lof-nav-yt").trigger("click");														
									}
								}
								
							//]]>
