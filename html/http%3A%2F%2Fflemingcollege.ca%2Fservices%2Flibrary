						<!-- <script type="text/javascript" src="//ajax.microsoft.com/ajax/jquery.validate/1.7/jquery.validate.min.js"></script> -->
						<script type="text/javascript" src="//flemingcollege.ca/js/jquery.validate.min.js"></script>
						<script type="text/javascript" src="//flemingcollege.ca/theme2/lib/jquery.fancybox-1.3.4/fancybox/jquery.fancybox-1.3.4.pack.js"></script>
						<script type="text/javascript" src="//flemingcollege.ca/theme2/scripts.js"></script>
						
						<script type="text/javascript">
							$(document).ready(function() {
								$("#oldRm,#newRm").blur(function() {
									
									if($("#oldRm").val() != "" || $("#newRm").val() != "" ) {
										getRoom();
									}
																	
								});
								
								$("#checkRm").click(function(e) {
									
									e.preventDefault();
									
									if($("#oldRm").val() != "" || $("#newRm").val() != "" ) {
										getRoom();
									}
								});
								
								$("#reset").click(function(e) {
									
									e.preventDefault();
									
									$("#oldRm").val("");
									$("#newRm").val("");
									
									$("#levelID").text("?").attr("class","").addClass("wing-0");
									
								});
								
								function getRoom() {
									
									if($("#oldRm").val() != "" || $("#newRm").val() == "" ) {
									
										var oldRm_fld = $("#oldRm").val();
										var newRm_fld = false;
										
									}
									
									else if($("#oldRm").val() == "" || $("#newRm").val() != "" ) {
										
										var oldRm_fld = false;
										var newRm_fld = $("#newRm").val();
										
									}
									
									$.get("/feeds/roomdata.json",function(data) {
										
										var roomdata = $.parseJSON(data);
										
										var roomFound = false;
										$.each(roomdata, function(i, v) {
											
											if(oldRm_fld !== false) {
											
												if (v.oldRm == oldRm_fld) {
													$("#newRm").val(v.newRm);
													$("#rmType").val(v.desc);												
													
													roomFound = true;
													
												}
											}										
											else if(newRm_fld !== false) {
											
												if (v.newRm == newRm_fld) {
													$("#oldRm").val(v.oldRm);
													$("#rmType").val(v.desc);												
													
													roomFound = true;
													
													
												}
											}
																								
											return;
										});
										
										if(roomFound == false) {
											if(oldRm_fld !== false) {
												$("#newRm").val("No Result");
											}
											
											if(newRm_fld !== false) {
												$("#oldRm").val("No Result");
											}
										}
										else {
											var levelID = $("#newRm").val();
											
											if(levelID[0] == "A") {
												$("#levelID").text("A").attr("class","").addClass("wing-A");
												$("#levelID").show();
											}
											if(levelID[0] == "B") {
												$("#levelID").text("B").attr("class","").addClass("wing-B");
												$("#levelID").show();
											}
											if(levelID[0] == "C") {
												$("#levelID").text("C").attr("class","").addClass("wing-C");
												$("#levelID").show();
											}
											if(levelID[0] == "D") {
												$("#levelID").text("D").attr("class","").addClass("wing-D");
												$("#levelID").show();
											}	
											
										}
										
									});
										
								}
							
							});
						</script>
						</body></html>