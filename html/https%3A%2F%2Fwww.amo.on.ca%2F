<div id="ctl00_vaEmaillink_paneljquery">
	

<script type="text/javascript">
    var jQ = jQuery.noConflict();
    var ajaxCalled = false;
    jQ(document).ready(function () {
        //setup new person dialog
        var datadockey;
        jQ('#emailshowMain').dialog({
            modal: false,
            autoOpen: false,
            width: 650,
            title: ' E-MAIL TO FRIEND ',
            draggable: true,
            resizable: false,

            open: function (type, data) {
                jQ(this).parent().appendTo("form");

            }
        });




    });
	

	
	
	function showMainDialog(id, elem) {
        datadockey = elem.getAttribute("data-dockey");
        //console.log(" data doc key-" + datadockey);
        jQ('#' + id).dialog("open");
        jQ('#' + id).data("datadockey", datadockey);
        clearMSG();

    }

    function closeMainDialog(id) {
        jQ('#' + id).dialog("close");

    }

    function ajaxMainCall() {
	
		var captchaEntered = jQ("#ctl00_vaEmaillink_captchaElem_txtSecurityCode").val();
		var captchaEnteredtwo = jQ("#ctl00_plcMain_plcRight_lt_vaEmaillink_captchaElem_txtSecurityCode").val();
		
		if ((captchaNumber==captchaEntered) ||(captchaNumber==captchaEnteredtwo))
		{
			jQ('#loadMainSendingEmail').fadeIn("slow");
			setTimeout(function () { ajaxMainCallWaitOver() }, 500);
		
		}
		else
		{
			alert("Captcha does not match. Please re-enter the Captcha Number");
			//alert(captchaNumber);
			//alert(captchaEntered);
		}
       
    }
    function ajaxMainCallWaitOver() {

         //alert(" Doc key :" + jQ('#txtMainEmailTo').val());

        jQ.ajax({


            url: '/CMSWebParts/CustomVAControls/AMO/SendEmailHandler.ashx',
            type: 'GET',
            data: { pagename: datadockey, ToEmail: jQ('#txtMainEmailTo').val(), EmailSubject: jQ('#txtBoxMainSubject').val(), PersonName: jQ('#txtMainPersonName').val(), EmailMessage: jQ('#txtBoxMainEMailMessage').val() },
            async: false,

            success: function (data, status) {
                //  debugger;

                skills = new Array();
                skills = data.split("|");

                jQ('#innerMaindiv').addClass("VAsuccessMSG");
                jQ('#innerMaindiv').fadeIn("slow");
                jQ('#innerMaindiv').html(skills.toString().replace(/,/g, " ") + "  ").fadeIn("slow");
                jQ('#loadMainSendingEmail').hide();
 //alert("clicked s"+skills.toString().replace(/,/g, " ") + "  ");
                clearEmailText();
            },
            error: function (errorText, status, errorThrown) {
                debugger;
 //alert("clicked err");
                jQ('#innerMaindiv').addClass("VAerrorMSG");
                jQ('#innerMaindiv').fadeIn("slow");
                jQ('#innerMaindiv').html(errorText + status + errorThrown + "  ").fadeIn("slow");
                jQ('#loadMainSendingEmail').hide();

                clearEmailText();
            }

        });
    }

    function clearEmailText() {

        jQ('#txtMainEmailTo').val("");
        jQ('#txtBoxMainSubject').val("");
        jQ('#txtMainPersonName').val("");
        jQ('#txtBoxMainEMailMessage').val("");

    }
    function clearMSG() {
        jQ('#innerMaindiv').removeClass()
        jQ('#innerMaindiv').html('');
    }
   

       
    </script>
    
</div>
	
