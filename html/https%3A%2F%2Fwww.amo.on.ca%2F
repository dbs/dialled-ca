        jQuery(document).ready(function () {

            jQuery("#myCarouselHome").carousel({ interval: 4000 });

            jQuery(window).bind("resize", function () {//Adjusts image when browser resized
                imageresize();
            });

        });

        function closeVideo() {
            jQuery('#VideoPopUp1').empty();

            document.getElementById('VideoPopUp1').style.display = 'none';
            document.getElementById('VideoPopUp1Out').style.display = 'none';
            location.reload();

        }

        function imageresize() {
            var contentheight = jQuery(window).height();
        }

        function ShowVideo() {        
            document.getElementById("VideoPopUp1").Style.Display = "block";
        }

    </script>
</div>
</div>
