<script type="text/javascript" language="javascript">
    // alert("index" + location.href.indexOf("va"));

    var bannerCount = 1;
    var bannernumber = 1;
    var BannerOn = 1;
    function init() {



        if ('True' == 'False') {
            document.getElementById("sliderdot1").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot2").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot3").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot4").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot5").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot6").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot7").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot8").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot9").style.display = 'none';
        }
        if ('True' == 'False') {
            document.getElementById("sliderdot10").style.display = 'none';
        }


    }
    window.onload = init;





    if (location.href.indexOf("amo") != -1) {
        //alert(bannernumber);
        t = setTimeout("timedCount()", 0);
    }


    function clickBannerImageMain(site) {
        window.location.href = site.getAttribute("data-url");
    }
    function clickBannerImageMain(site) {
        window.location.href = site.getAttribute("data-urlmain");
    }

    function stopBanner() {
        if (BannerOn == 1) {
            BannerOn = 0;
            document.getElementById("bannerstopstart").setAttribute("class", "play-btn");
        }
        else {
            BannerOn = 1;
            document.getElementById("bannerstopstart").setAttribute("class", "pause-btn");
        }
    }
    function startBanner() {
        BannerOn = 1;
        document.getElementById("bannerstopstart").setAttribute("class", "pause-btn");
    }

    function timedCount() {



        showImage()

        t = setTimeout("timedCount()", 4000);


    }


    function showImage() {
        if (bannerCount > 10)
            bannerCount = 1;

        if (BannerOn == 1) {
            SetDots();

        }
        bannerCount = BannerCountReturn(bannerCount);
        if (bannerCount == 0) {
            bannerCount = BannerCountReturn(bannerCount);
        }


        document.getElementById('errorlbl').innerHTML = "  " + bannerCount;

        if ((bannerCount == 1) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-Content/Events/2016-Heads-of-Council.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);

            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/16_HOC2016WindsorOnly.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot1").setAttribute("class", "active-dot");


        }
        if ((bannerCount == 2) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-Content/Emergency-Services/Police/Policing.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/2014_Policing.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot2").setAttribute("class", "active-dot");


        }
        if ((bannerCount == 3) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-Content/Events/2016-AMO-Conference-Windsor.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/16_ConfRegAvail.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot3").setAttribute("class", "active-dot");

        }
        if ((bannerCount == 4) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-Content/Policy-Updates/2013/Interest-Arbitration.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/InterestArbitration.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot4").setAttribute("class", "active-dot");

        }
        if ((bannerCount == 5) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://learning.amo.on.ca/home";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("VAslider").style.backgroundImage = "url('/images/2016MCFL.jpg')";

            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot5").setAttribute("class", "active-dot");

        }
        if ((bannerCount == 6) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://amo.skillbuilder.ca/home";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);

            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/15_CoverYourAssets.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot6").setAttribute("class", "active-dot");


        }
        if ((bannerCount == 7) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-Content/Policy-Updates/2015/AMO-and-UQM-to-Collaborate-on-Climate-Action.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/15_AMOUMQclimatechange3.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot7").setAttribute("class", "active-dot");


        }
        if ((bannerCount == 8) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://amo.skillbuilder.ca/home";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/15_LUP.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot8").setAttribute("class", "active-dot");

        }
        if ((bannerCount == 9) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-PDFs/Gas_Tax/AMO_Reporting_Gas_Tax/AMO-AnnualReport14_Part1_Website.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/2014 GTF AR.jpg')";
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot9").setAttribute("class", "active-dot");

        }
        if ((bannerCount == 10) && (BannerOn == 1) && ('True' == 'True')) {
            SetDots();
            var link = "http://www.amo.on.ca/AMO-Content/Backgrounders/2015/Waste-Diversion-an-Ongoing-Success-Story.aspx";
            document.getElementById("VAslider").setAttribute("data-urlmain", link);
            var iText = "";
            document.getElementById("slider-text1").getElementsByTagName('p')[0].innerHTML = iText;
            document.getElementById("VAslider").style.backgroundImage = "url('/images/AMO/Imageslider/Waste Ad 2015-10-13.png')";

            document.getElementById("slider-text1").style.display = 'block';
            document.getElementById("sliderdot10").setAttribute("class", "active-dot");

        }
        bannerCount = bannerCount + 1;



    }
    function SetDots() {
        document.getElementById("sliderdot1").setAttribute("class", "story-dots");
        document.getElementById("sliderdot2").setAttribute("class", "story-dots");
        document.getElementById("sliderdot3").setAttribute("class", "story-dots");
        document.getElementById("sliderdot4").setAttribute("class", "story-dots");
        document.getElementById("sliderdot5").setAttribute("class", "story-dots");
        document.getElementById("sliderdot6").setAttribute("class", "story-dots");
        document.getElementById("sliderdot7").setAttribute("class", "story-dots");
        document.getElementById("sliderdot8").setAttribute("class", "story-dots");
        document.getElementById("sliderdot9").setAttribute("class", "story-dots");
        document.getElementById("sliderdot10").setAttribute("class", "story-dots");

    }
    function BannerCountReturn(count) {
        if (count == 0)
            count++;

        if (('True' == 'False') && count == 1) {
            count = count + 1;

        }
        if (('True' == 'False') && count == 2) {
            count = count + 1;


        }
        if (('True' == 'False') && count == 3) {
            count = count + 1;

        }
        if (('True' == 'False') && count == 4) {
            count = count + 1;


        }
        if (('True' == 'False') && count == 5) {
            count = count + 1;

        }
        if (('True' == 'False') && count == 6) {
            count = count + 1;

        }
        if (('True' == 'False') && count ==7) {
            count = count + 1;

        }
        if (('True' == 'False') && count ==8) {
            count = count + 1;

        }
        if (('True' == 'False') && count == 9) {
            count = count + 1;

        }
        if (('True' == 'False') && count == 10) {
            count = 0;


        }
        return count;
    }


    function gotoBanner(bannernumber) {
        //alert(bannernumber);
        bannerCount = bannernumber;

        showImage();

    }
    
    
 </script>
