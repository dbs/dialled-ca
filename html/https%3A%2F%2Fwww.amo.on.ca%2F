<script src="/CMSPages/GetResource.ashx?scriptfile=%7e%2fCMSScripts%2fCustom%2fAMO%2fautocomplete%2fgoogleapis-comajaxlibsjqueryui1.8jquery-ui.js"></script>

<script type="text/javascript">
	//Accoutocomplete 
 var searchJQ = jQuery.noConflict();
 

    searchJQ(document).ready(function () {


        /// /// /// /// /// /// ////// ---- to retreive data to provide an array for autocomplete textbox-----////
        var taglist;
        ///------ ajax call to Data handler to get data for autocomplete ajax call  following this call--//
        searchJQ.ajax({

            url: '/CMSWebParts/CustomVAControls/AMO/DataHandler.ashx',
            type: 'POST',
            async: false,
            success: function (data, status) {
                //  debugger;
                taglist = new Array();
                taglist = data.split("|");
                // alert(skills.toString() + "");
//alert("Data Handler is called"+taglist.toString() );
            },
            error: function (errorText, status, errorThrown) {
                debugger;
                alert('Data Handler is not called due to ' + errorText + status + errorThrown);
            }
        }); ///------ ajax call to DataHandler ends here--//

        ///---- organising data for autocomplete call--//

        var tagObject = new Object();
        //  var native = ("English", "Spanish", "French");
        //// contact.ComputerSkill = skills;
        tagObject.searchBoxEventPage = taglist;
        // contact.NativeLanguage = native;
        //---member filter---if we need two autocomplete textboxes
        var tagfilter = new Array();
        
        tagfilter[0] = "searchBoxEventPage";
        //  memberfilter[1] = "NativeLanguage";
        var jsonText = JSON.stringify(tagObject, tagfilter, "\t");

        ////------------- call to autocomplete jquery method-------------//


        searchJQ(".eventAutocomplete").autocomplete({

            open: function () {
                searchJQ('.ui-autocomplete').css('width', '300px');
            },
            source: function (request, response) {
                response(searchJQ.ui.autocomplete.filter(tagObject[this.element.attr("id")], request.term));
            }
        }); //-- autocomplete query end
    });

</script>
