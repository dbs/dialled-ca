        <script type="text/javascript">
        //<![CDATA[
        $(document).ready(function(){
            $('.block').setTabsBox();
            var loc_var=this.location.href; if(loc_var .indexOf('?addr=')!=-1) {
            var loc_arr=loc_var.split('?addr='); document.getElementById('myiframe').src=loc_arr[1]; }
            var internalSites = new Array(
                'http://csss-iugs.ca',
                'http://cdrv.csss-iugs.ca',
                'http://fondationvitae.csss-iugs.ca',
                'http://www.fondationvitae.ca',
                'http://www.cdrv.ca',
                'javascript:',
                'https://',
                'mailto:'
            );

            $('a').click(function(){
                var s = $(this);
                var request = s.attr('href');
                var internal = false;
                $.each(internalSites, function(id, i){
                    var reg=new RegExp("^" + i,"g");
                    if(reg.test(request))
                        internal = true;
                });
