//<![CDATA[
$(document).ready(function() {
        $(window).load(function(){
            if (!$('#search-box-terme').val().length)
            $('#search-box-terme').val('Rechercher');
        });
    $('#search-box-terme').keypress(function(key){
        if (key.which == 13){
            $('#searchLink').click();
        }
    });

    $('#search-box-terme').click(function(event){
        if($('#search-box-terme').val() == 'Rechercher')
        {
            $(this).val('');
        }
    });

    $('#search-box-terme').blur(function(event){
        if($(this).val() == '')
            $(this).val('Rechercher');
    });

    $('#searchLink').click(function(event){
        event.preventDefault();
        if($('#search-box-terme').val() != 'Rechercher'
            && $('#search-box-terme').val() != '')
        {
            var url = $('#search-url').val()+"/" + $('#search-box-terme').val();
            window.location = url;
        }
    });

});
