								<script type="text/javascript">
									jQuery.noConflict();

									(function($) {
                    function verticalCenterFooterLogo(){
                      $('.footer').each(function() {
                        if (Modernizr.mq('screen and (min-width: 768px)')) {
                          var maxHeight = Math.max.apply(null, $(this).find('.row').map(function() {
                            return $(this).height();
                          }).get());

                          var logo_height = $(this).find('.logo').height();

                          $(this).find('.logo').css('margin-top', (maxHeight / 2) - (logo_height / 2) - 10);
                        } else {
                          $(this).find('.logo').removeAttr('style');
                        }
                      });
                    }
										$(document).ready(function(){
                      verticalCenterFooterLogo();
											$(window).on('resize', function() {												
												verticalCenterFooterLogo();
											});
										});
									})(jQuery);
