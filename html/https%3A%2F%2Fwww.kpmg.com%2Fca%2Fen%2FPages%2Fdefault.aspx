	<script type="text/javascript">
function ChangeBGColor(ControlId)
{
    var curElement = document.getElementById(ControlId);
    var agt = navigator.userAgent.toLowerCase();
    var isNav = ((agt.indexOf('mozilla') != -1) && ((agt.indexOf('spoofer') == -1) && (agt.indexOf('compatible') == -1)));
    var isIE = (agt.indexOf("msie") != -1);    
   
   if(!IsInDesignMode())
   {
        if ((isIE && curElement.childNodes.length > 0) ) 
        {
                if (curElement.childNodes[0].childNodes[0].childNodes.length < 1)
                {  
                    curElement.style.display="none";
                }
        } 
        if(isNav && curElement.childNodes.length > 1)
        {
          if (curElement.childNodes[1].childNodes[1].childNodes.length < 1)
                {  
                    curElement.style.display="none";
                }
        }
    }
   
    
}

function trim(str) {
    return str.replace(/^\s+/, '').replace(/\s+$/, '');
}
function Custom_MakeInvisibleIfBlankText(ControlId) {
    var allElements = document.getElementById(ControlId);
    var imparentdiv=document.getElementById("divOptionalTitle");
    var agt = navigator.userAgent.toLowerCase();
    var isNav = ((agt.indexOf('mozilla') != -1) && ((agt.indexOf('spoofer') == -1) && (agt.indexOf('compatible') == -1)));
    var isIE = (agt.indexOf("msie") != -1);
 if(!IsInDesignMode())
   {
        if ((isIE && allElements.childNodes.length > 0) || (isNav && allElements.childNodes.length > 0)) {
            if (allElements.childNodes[0].nodeValue!=null && trim(allElements.childNodes[0].nodeValue).length <=1)
            {
           
                allElements.style.display = "none";
                imparentdiv.style.display = "none";
            }
        }
        }
    
}

