<script language="javascript">

 var tabs = new Array('tab1','tab2');
 var secs = new Array('pnlRotate2','pnlRotate3');

 function tabClick(tab) {

   for (i=0; i<tabs.length; i++) {
     if(tabs[i]!=tab.id) {
       document.getElementById(tabs[i]).className = 'TabNotSelected';
       document.getElementById(secs[i]).style.display = 'none';
     }
     else {
       document.getElementById(tabs[i]).className = 'TabSelected';
       document.getElementById(secs[i]).style.display = 'inline';
     }
   }

 }

</script>

