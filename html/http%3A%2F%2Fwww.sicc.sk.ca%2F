	/* Start: Dropdown Menu Script */
	$(document).ready(function() {
	
		//Top red Menu
	$('.topnav ul ul').parent('li').hover(function() {
		$(this).children('ul').fadeIn('medium');
	},
	function() {	
		$(this).children('ul').fadeOut('fast');
	});
	
	//Main Menu
	$('.main-nav ul ul').parent('li').hover(function() {
		$(this).children('ul').fadeIn('medium');
	},
	function() {	
		$(this).children('ul').fadeOut('fast');
	});
	
	
});
