<script type="text/javascript">
        function searchPrimo() {
          var terms = document.getElementById('primoQueryTemp').value;
          document.getElementById("primoQuery").value = "any,contains," + terms.replace(',',' ');
          document.forms["searchForm"].submit();
        }
        //this next function just submits the form if the user hits "Enter" while typing in terms.
        //this one function is used in all the examples, but not repeatedly defined in them.
        function searchKeyPress(e) {
          if (typeof e == 'undefined' && window.event) {
            e = window.event;
          }
          if (e.keyCode == 13) {
            document.getElementById('go').click();
          }
        }
      </script>
      <form name="searchForm" role="search" method="get" target="_self"
        action="http://novanet-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do"
        enctype="application/x-www-form-urlencoded; charset=utf-8">
        <label for="primoQueryTemp">Search for:</label>
        <input type="hidden" id="institution" name="institution" value="NSCC">
        <input type="hidden" id="vid" name="vid" value="NSCC">
        <input type="hidden" id="primoQuery" name="query" />
        <input type="text"   id="primoQueryTemp" name="queryTemp" size="25" value="" onkeypress="searchKeyPress();" />
        <input type="submit" id="go" value="Search" title="Search" alt="Search" onclick="searchPrimo();">
      </form>


