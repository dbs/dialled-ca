function escramble(){
 var a,b,c,d,e,f,g,h,i
 a='<a href=\"mai'
 b='library.nscc'
 c='\">'
 a+='lto:'
 b+='@'
 e='</a>'
 f='library.nscc@nscc.ca'
 b+='nscc.ca'
 g='<img src=\"'
 h=''
 i='\" border="0">'

 if (f) d=f
 else if (h) d=g+h+i
 else d=b

 document.write(a+b+c+d+e)
}

escramble()

