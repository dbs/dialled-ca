<script type="text/javascript">
function sizeText(size) {
	switch(size)
	{
		case 1.5:
			document.body.style.fontSize = "150%";
			break;
		case 1.0:
			document.body.style.fontSize = "100%";
			break;
		case 0.9:
			document.body.style.fontSize = "90%";
			break;
		default:
			document.body.style.fontSize = "100%";
	}
}

</script>

