<script type="text/javascript">

function resetCalendarFunction() {
	/* CHANGE BEHAVIOUR OF THE PROGRAM CALENDAR (click on first to toggle all) */

	$("#dnn_ctr6689_View_cboCategories_i0_lblCat").append(" (Toggle Branches)");
	$("#dnn_ctr6689_View_cboCategories_Input").val("Select Branch(es)");

	$("#dnn_ctr6689_View_cboCategories_i0_chkCat").click(function() {
		var index = 0; var value = document.getElementById("dnn_ctr6689_View_cboCategories_i0_chkCat").checked;
		while (true) { 
			var id = "dnn_ctr6689_View_cboCategories_i" + (++index) + "_chkCat";
			if(document.getElementById(id)) {
				document.getElementById(id).checked = value;
			} else {
				break;
			}
		}
	});
}

$('body').append('<link rel="stylesheet" href="/Portals/_default/skins/OxfordCounty/ocl.css" type="text/css" />');

jQuery(document).ready(function($) {

	pageLoad = function () {
		try {
			if(typeof InitJQuery == 'function') InitJQuery();
			resetCalendarFunction();
		}
		catch (err)
		{
		}
	}
});
</script>

