            jQuery(document).ready(function () {
                jQuery(document).on('click','.naDevEventsImageGallery a',function(){
                    var mainImageUrl = jQuery(".naDevMainImage").attr("src");
                    var mainImageSmallUrl = jQuery(".naDevMainImage").attr("alt");
                    var mainImageTitle = jQuery(".naDevMainImage").attr("title");

                    //set the main image to be the image that has been clicked.
                    jQuery(".naDevMainImage").attr("src",jQuery(this).attr("href"));
                    //set the main image alt tag
                    jQuery(".naDevMainImage").attr("alt",jQuery(this).find("img").attr("src"));
                    //set the title
                    jQuery(".naDevMainImage").attr("title",jQuery(this).find("img").attr("title"));

                    //set the clicked image 
                    jQuery(this).find("img").attr("src",mainImageSmallUrl);
                    jQuery(this).find("img").attr("title",mainImageTitle);   
                    jQuery(this).attr("href",mainImageUrl);                    
                    
                            
                    return false;
                });
            });

