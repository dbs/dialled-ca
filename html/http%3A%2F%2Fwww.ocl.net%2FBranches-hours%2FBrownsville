   <script type="text/javascript">
        /* <![CDATA[ */

             function pageLoad(sender, args)
             {
               
               if (args.get_isPartialLoad())
               {
                    jQuery('#eventWidgetCarousel').jcarousel();
                    setTimeout(function() {
                         jQuery(".eventPlannerLoading").hide();
                        jQuery("#eventWidgetCarousel").show();
                    }, 200);
                   

                    jQuery(".ddlSelector").change(function(){
                        jQuery(".eventPlannerLoading").show();
                    });   
               }
             }

             jQuery(document).ready(function () {        
                jQuery('#eventWidgetCarousel').jcarousel();

                setTimeout(function() {
                    jQuery("#eventWidgetCarousel").show();
                }, 200);


                jQuery(".ddlSelector").change(function(){
                   jQuery(".eventPlannerLoading").show();
                });    
           
            });
