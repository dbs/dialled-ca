		$(document).ready(function(){
			$("#PfNavMega li.HasChildren").hoverIntent({
				over: makeTall, 
				timeout: 500, 
				out: makeShort
			});
		});

		var old = null;
 
		function makeTall()  {
//			$('.shadow').css('height','220');
			$(this).find('div:first').css('display','block');
		}

		function makeShort() {
			$(this).find('div:first').fadeOut(500);
		}
	</script>

<script language="JavaScript"> 

var actionDescription = "";

function navigateMain(form,field)
{
	var searchBox = null;
	var hiddenSearchData = null;
	var targetDescription = "";
	var searchResult = "";
	var formMethod = "post";

	myform = document.getElementById('myform');
	if(null==myform)
	{
		$('body').append('<form id="myform"></form>');

		$('#myform').append('<input type="hidden" name="searchdata1"	value="">');
		$('#myform').append('<input type="hidden" name="cx"		value="011096999385603415265:2rkp0jtugwa" />');
		$('#myform').append('<input type="hidden" name="cof"		value="FORID:10" />');
		$('#myform').append('<input type="hidden" name="ie"		value="UTF-8" />');
		$('#myform').append('<input type="hidden" name="library"	value="OXFO_G" />');
		$('#myform').append('<input type="hidden" name="user_id"	value="OXHQWEB" />');
		$('#myform').append('<input type="hidden" name="password"	value="" />');
		$('#myform').append('<input type="hidden" name="new_gateway_db"	value="" />');
		$('#myform').append('<input type="hidden" name="sort_by"	value="ANY" />');
		$('#myform').append('<input type="hidden" name="srchfield1"	value="GENERAL^SUBJECT^GENERAL^^words or phrase" />');
		
		myform = document.getElementById('myform');
	}

	if (null != form)
	{
		searchBox = field; //form.searchdata1;
		hiddenSearchData = myform.searchdata1;
		if ((null != searchBox) && (null != hiddenSearchData))
		{

			searchResult = searchBox.value;	

			/*

			if(searchResult == '' || searchResult == undefined) 
			{ 
				//actionDescription = "http://www.olc.canlib.ca/uhtbin/cgisirsi/yMtuF7k8wH/HEADQUART/50000012/60/1180/X"; 
				//actionDescription = "http://olc.canlib.ca/uhtbin/cgisirsi/ljNIR4knO4/OXHQ/276850066/60/1180/X";
				//actionDescription = "http://www.olc.canlib.ca/uhtbin/cgisirsi/x/x/0/57/49?user_id=OXHQWEB";
				actionDescription   = "http://olc.canlib.ca/uhtbin/cgisirsi/x/SIRSI/0/57/60/1180/X?user_id=OXHQWEB";
			} 
			else 
			{ 
				actionDescription   = "http://olc.canlib.ca/uhtbin/cgisirsi/0/OXHQ/0/123";

				$.ajax({
					type: "GET",
					url: "/sandbox/search/test.asp?random=" + Math.ceil(1000000*Math.random()),
					dataType: "html",
					success: parseXml,
					async: false
				});

			}
			

			hiddenSearchData.value = searchBox.value; 

			*/

			actionDescription = "https://olco.ent.sirsidynix.net/client/en_US/oxford/search/results?qu=" + searchResult + "&te=ILS"

			targetDescription = "_blank";
			formMethod = "post";
			
			myform.action = actionDescription; 
			myform.target = targetDescription;
			myform.method = formMethod;
			myform.submit(); 
//			form.submit();
		}
	}
}

function parseXml(xml)
{
	$(xml).find("form").each(function()
	{
		action = ($(this).attr("action"));
		if(action.substring(action.length-3)=='123')
		{
			actionDescription =  'http://olc.canlib.ca'+action;
			return;
		}
	});
}

function render(c, t, s, d)
{
     c = document.getElementById(c);

     if (!c.checked)
     {
          document.getElementById(t).style.display="none"
	  document.getElementById(d).value = document.getElementById(s).value;
     }
     else
     {
          document.getElementById(t).style.display="block"
	  document.getElementById(s).value = document.getElementById(d).value;
     }
}


function submitSearch(s,d)
{
	document.getElementById(d).value = document.getElementById(s).value;
	__doPostBack('dnn$dnnSEARCH$cmdSearch','');
}

