<script type="text/javascript"> $(function () {$(".eSolutionsGroupPhotoGalleryV2PlaceholderDiv").each(function () { var method = $(this).attr("method"); var id = $(this).attr("id"); if (id != '') { $(this).load(id, function () { return method; })} else { $(this).html(''); }}) });</script>
<script type="text/javascript">
	$( window ).load(function() {
		var textNodes;
		var valFirst;
		var valSecond;

		$(".eSolutionsGroupPhotoGalleryV2PlaceholderDiv .ic-caption").each(function() {
			textNodes = $(this).contents().filter(function() {
			    return this.nodeType == Node.TEXT_NODE;
			});

			valFirst = textNodes[0].nodeValue;
			if (textNodes[1]){
				valSecond = textNodes[1].nodeValue;
				$(this).replaceWith( '<span class="ic-caption">'+valFirst+'<br><span style="font-weight:bold; color:#FDE3A8; font-size:14px; font-style:normal;">'+valSecond+'</span></span>' );
			} else {
				$(this).replaceWith( '<span class="ic-caption">'+valFirst+'</span>' );
			}
		});
	});
</script>  


