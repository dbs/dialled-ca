    <script type="text/javascript">
    $(document).ready(function () {
        var slider_width_connect = $('#connectSlidePanel').width();//get width automaticly
        var slider_width_map = $('#mapSlidePanel').width();//get width automaticly



        $('#connectLink').click(function (event) {
            event.preventDefault();
            if ($(this).css("margin-right") == slider_width_connect + "px" && !$(this).is(':animated')) {
                if (BrowserDetect.browser == 'Explorer' && BrowserDetect.version == 9) {
                    if ($("#connectSlidePanel").get(0).scrollHeight > $("#connectSlidePanel").innerHeight()) {
                        $('#connectLink').css('right', '0px');
                    }
                }
                $('#connectSlidePanel,#connectLink').animate({ "margin-right": '-=' + slider_width_connect }, 'slow', 'easeOutQuart');
                $('#connectLink').removeClass('is-visible');
                $('#connectSlidePanel').removeClass('is-visible');
            }
            else {
                if (!$(this).is(':animated'))//perevent double click to double margin
                {
                    if (BrowserDetect.browser == 'Explorer' && BrowserDetect.version == 9) {
                        if ($("#connectSlidePanel").get(0).scrollHeight > $("#connectSlidePanel").innerHeight()) {
                            $('#connectLink').css('right', '-17px');
                        }
                    }
                    $('#connectSlidePanel,#connectLink').animate({ "margin-right": '+=' + slider_width_connect }, 'slow', 'easeInQuart');
                    $('#connectLink').addClass('is-visible');
                    $('#connectSlidePanel').addClass('is-visible');
                }
            }
        });

        $('#connectSlidePanel .slidePanel-close').click(function (event) {
            event.preventDefault();
            if (BrowserDetect.browser == 'Explorer' && BrowserDetect.version == 9) {
                if ($("#connectSlidePanel").get(0).scrollHeight > $("#connectSlidePanel").innerHeight()) {
                    $('#connectLink').css('right', '0px');
                }
            }
            $('#connectSlidePanel,#connectLink').animate({ "margin-right": '-=' + slider_width_connect }, 'slow', 'easeOutQuart');
            $('#connectLink').removeClass('is-visible');
            $('#connectSlidePanel').removeClass('is-visible');
        });


        $('#mapLink').click(function (event) {
            event.preventDefault();
            if ($(this).css("margin-right") == slider_width_map + "px" && !$(this).is(':animated')) {
                if (BrowserDetect.browser == 'Explorer' && BrowserDetect.version == 9) {
                    if ($("#mapSlidePanel").get(0).scrollHeight > $("#mapSlidePanel").innerHeight()) {
                        $('#mapLink').css('right', '0px');
                    }
                }
                $('#mapSlidePanel,#mapLink').animate({ "margin-right": '-=' + slider_width_map }, 'slow', 'easeOutQuart');
                $('#mapLink').removeClass('is-visible');
                $('#mapSlidePanel').removeClass('is-visible');
            }
            else {
                if (!$(this).is(':animated'))//perevent double click to double margin
                {
                    if (BrowserDetect.browser == 'Explorer' && BrowserDetect.version == 9) {
                        if ($("#mapSlidePanel").get(0).scrollHeight > $("#mapSlidePanel").innerHeight()) {
                            $('#mapLink').css('right', '-17px');
                        }
                    }
                    $('#mapSlidePanel,#mapLink').animate({ "margin-right": '+=' + slider_width_map }, 'slow', 'easeInQuart');
                    $('#mapLink').addClass('is-visible');
                    $('#mapSlidePanel').addClass('is-visible');
                }
            }
        });

        $('#mapSlidePanel .slidePanel-close').click(function (event) {
            event.preventDefault();
            if (BrowserDetect.browser == 'Explorer' && BrowserDetect.version == 9) {
                if ($("#mapSlidePanel").get(0).scrollHeight > $("#mapSlidePanel").innerHeight()) {
                    $('#mapLink').css('right', '0px');
                }
            }
            $('#mapSlidePanel,#mapLink').animate({ "margin-right": '-=' + slider_width_map }, 'slow', 'easeOutQuart');
            $('#mapLink').removeClass('is-visible');
            $('#mapSlidePanel').removeClass('is-visible');
        });
    });

</script>

