<script language="javascript" type="text/javascript">
    jQuery(function () {

        // Split footer Main Nav dropdown into 4 Columns
        var totalItems = jQuery(this).find('ul#footerMainNav.first').children('li').length;  // total count
        var itemsPerColumn = parseInt(totalItems / 4);
        var remain = totalItems % 4; // mod
        if (remain > 0) {
            itemsPerColumn += 1;
        }

        var list = jQuery(this).find("ul#footerMainNav");
        var current_size = 0;
        list.children().each(function () {
            if (++current_size > itemsPerColumn) {
                var new_list = jQuery("<ul></ul>").insertAfter(list);
                list = new_list;
                current_size = 1;
            }
            list.append(this);
        });

        // Split Main Nav dropdown into 4 Columns
        jQuery('#nav').children('li').each(function () {
            var totalItems = jQuery(this).find('ul.SplitDropdown').children('li').length;  // total count
            var itemsPerColumn = parseInt(totalItems / 4);
            var remain = totalItems % 4; // mod
            if (remain > 0) {
                itemsPerColumn += 1;
            }

            var list = jQuery(this).find("ul.SplitDropdown");
            var current_size = 0;
            list.children().each(function () {
                if (++current_size > itemsPerColumn) {
                    var new_list = jQuery("<ul class=\"dropdown\"></ul>").insertAfter(list);
                    list = new_list;
                    current_size = 1;
                }
                list.append(this);
            });
        });

    });
</script>

