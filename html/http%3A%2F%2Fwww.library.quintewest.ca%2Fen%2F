                buttonText: "",
                feedbackFormId: $("#FeedbackFormId") === undefined ? "" : $("#FeedbackFormId").val(),
                language:'en'
            });
        
            var contentHeight = $("#contentInt").outerHeight();
            var subnavHeight = $("#subNavContainer") ? $("#subNavContainer").outerHeight() : 0;
            if (subnavHeight > contentHeight)
                $("#contentInt").css({ "min-height": $("#subNavContainer").height()});

            $("#printAreaContent table.datatable").addClass("table").wrap("<div class='ic-table-responsive'></div>");

        }
    });

    function PrintScreen() {
        if (iCreateObject.isSiteInICreateMode) {
            showNAMessage();
        }
        else {
            window.print();
        }
    }
    
