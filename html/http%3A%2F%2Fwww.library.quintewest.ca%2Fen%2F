    $(function () {
        if (!iCreateObject.isSiteInICreateMode) {

            $.ajax({
                url: iCreateObject.corpHome + "/" + iCreateObject.lang + "/Modules/News/services/getTopFiveNews.ashx?limit=" + $("#rotatingBanners .slide").length + "&feedId=0c75dc93-d93f-4b4b-97b7-a2e3d8e163c4", //TODO: NEWS: put in the proper feed id FEATURE: make dynamic
                async: false,
                dataType: 'jsonp',
                crossDomain:true,
                cache: false,
                success: function (data) {
                    $.each(data, function (index, element) {
                        $("#rotatingBanners .slide:eq(" + index + ")").append("<span class='ic-slider-content ic-slider-news'><div class='ic-slider-content-foreground'>" + element + "</div></span>");//TODO: LANGC: Make Dynamic and get translation
                    });
                }

            });
            
            return false;
        }
    });

  </script>



