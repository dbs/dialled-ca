<script type="text/javascript">

    var pauseNews = false;

    function toggleNews(event) {
        event.preventDefault();

        pauseNews = !pauseNews;

        if (pauseNews) {
            $("div#newsPause").addClass("selected");
            $("div#newsPause a").addClass("paused");
            $("div#newsPause a").html("Play");  //TODO: LANGC: get french translation and make dynamic
        }
        else {
            $("div#newsPause").removeClass("selected");
            $("div#newsPause a").removeClass("paused");
            $("div#newsPause a").html("Pause");
        }
    }

    $(function () {
        if (iCreateObject.isSiteInICreateMode) {
            $("#newsScrollContainer").empty().html("<ul><li><a>News feed disabled in iCreate.</a></li></ul>");//TODO: LANGC: Make Dynamic and get translation
        }
        else {
            $.fn.rotating_news = function (p) {
                var p = p || {};
                var _timerLength = p && (p.timerLength != null) ? p.timerLength : 5000,
                    _transitionLength = p && (p.transitionLength != null) ? p.transitionLength : 1000;

                var _container = jQuery(this),
                    _news = _container.children("ul").children("li"),
                    _index = 0;

                ChangeNews();

                function ChangeNews() {
                    if (!pauseNews) {
                        _news.hide();
                        _news.eq(_index).show();
                        _index++;
                        if (_index >= _news.length) {
                            _index = 0;
                        }
                    }
                    setTimeout(ChangeNews, _timerLength);
                }
            };
            
            $("#newsScrollContainer").empty().html("<ul><li><a>Loading...</a></li></ul>");//TODO: LANGC: Make Dynamic and get translation

            var NewsScrollFeedString = "";

            $.ajax({
                url: iCreateObject.corpHome + "/" + iCreateObject.lang + "/Modules/News/services/getTopFiveNews.ashx?limit=5&feedId=0c75dc93-d93f-4b4b-97b7-a2e3d8e163c4&style=scroll",
                async: false,
                dataType: 'jsonp',
                crossDomain: true,
                cache: false,
                error: function (xhr, textStatus, thrownError) {
                    $("#newsScrollContainer").html("<ul><li><a>An error occured</a></li><ul>");//TODO: LANGC: Make Dynamic and get translation
                },
                success: function (data) {
                    $.each(data, function (index, element) {
                        NewsScrollFeedString += element;
                    });
                    $("#newsScrollContainer")
                        .empty()
                        .append("<ul>" + NewsScrollFeedString + "</ul>")
                        .append("<div id=\"newsPause\"><a href='#' onclick=\"javascript:toggleNews(event)\" title=\"Pause\">Pause</a></div>");//TODO: LANGC: Make Dynamic and get translation

                    $("#newsScrollContainer").rotating_news();

                }

            });

            return false;
        }
    });

</script><div class="scrollerContainer" >

