	$(document).ready(function() {
        //if this page isn't being parsed, show the full atoz
	    if ("http://www.library.quintewest.ca/".indexOf(window.location.host) != -1)
	    {
	        jQuery("#ui-atoz-scroll").atoz_scroll({
	            width: 720,
	            height: 250,
	            padding: 8,
	            trigger: true,
	            url: iCreateObject.corpHome + "/en/Modules/AtoZ/index.aspx",
