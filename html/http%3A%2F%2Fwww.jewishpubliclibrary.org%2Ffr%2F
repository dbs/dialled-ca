					<script type="text/javascript">
						jQuery(document).ready(function(){
							var currentSlideIndex = 0;
							var numImages = jQuery('#slideContainer div.slide').length;
							var doingAnimation = false;

							function changeSlide(slideIndex)
							{
								if(!doingAnimation && slideIndex != currentSlideIndex)
								{
									doingAnimation = true;

									jQuery('span.slideButton.active').removeClass('active');
									jQuery('span.slideButton').eq(slideIndex).addClass('active');

									jQuery('#slideContainer div.slide').eq(currentSlideIndex).animate({
										opacity: 0
									}, 500, function(){
										jQuery(this).hide();
									});

									currentSlideIndex = slideIndex;

									jQuery('#slideContainer div.slide').eq(currentSlideIndex).show();

									jQuery('#slideContainer div.slide').eq(currentSlideIndex).animate({
										opacity: 1
									}, 1000, function(){
										doingAnimation = false;
									});
								}
							}

							var rotateSlidesInterval = setInterval(function(){
								changeSlide((currentSlideIndex + 1) % numImages);
							}, 5000);
						});		
					</script>					
				</td>
			</tr>
		</table>
	
