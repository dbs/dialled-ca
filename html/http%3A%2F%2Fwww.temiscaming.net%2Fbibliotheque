    <aside class="bandeau-jaune">
    	<h1 class="emploi">Intéressé par un emploi à Témiscaming?</h1>
    
    	<span class="consultez">Consultez les postes disponibles, nous vous attendons !<br /><a href="/services-aux-citoyens/emploi">Cliquez ici</a></span>
        
        

<form id="infolettre" action="#" method="post" class="infolettre subscribe-form">
	<h2>Inscrivez-vous</h2>
    <label for="email">Restez informé des dernières nouvelles<br>grâce à notre infolettre.</label>
    <input id="email" name="email" type="text" class="email" value="Votre courriel" data-placeholder="Votre courriel">
    <!--<input type="submit" class="submit" value="Envoyer" />-->
	<a class="submit" id="btn_submit">Envoyer</a>
</form>

<script>
    jQuery(document).ready(function() {
        jQuery('#btn_submit').on("click", function(e) {
            // Modification selon la loi C-28
            document.location.href = "/inscription-infolettre?c=" + jQuery("#email").val()+ "#contenu";
        });

        /*
         jQuery('#infolettre').focus(function() {
         if(jQuery(this).val() == 'votre courriel ici') {
         jQuery(this).val('');
         }
         });

         jQuery('#infolettre').blur(function() {
         if(jQuery(this).val() == '') {
         jQuery(this).val('votre courriel ici');
         }
         });
         */
    });
</script>


<script>
//Script pour la page d'ajout de courriel a l'infolettre
	function validMail(mail)   
	{  
	 if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mail))  
	  {  
	    return (true)  
	  }  
	    return (false)  
	} 


var submitted = false;


	$(document).ready(function()
	{
		$('.subscribe-form').submit(function(e)
		{
			e.preventDefault();
			e.stopPropagation();
			
			if(submitted)
			{
				return;
			}
			submitted = true;
			
			var email = $('#email').val();
			if(validMail(email))
			{
				$.ajax({
				  url: '/ajax/ajax.php',
				  data: {what: 'mailing', courriel: email, lang: 'fr'},
				  type: 'POST',
				  success: function(data)
				  {
				  	 //Afficher quelque chose qui dit que c'est un succès
                      $.fancybox(
                          '<h2>Merci de votre inscription!</h2><p>Votre adresse courriel a bien été enregistrée dans notre liste d\'envoi.</p>',
                          {
                              'autoDimensions'	: false,
                              'width'         		: 350,
                              'height'        		: 'auto',
                              'padding'             : 50,
                              'transitionIn'		: 'none',
                              'transitionOut'		: 'none'
                          }
                      );
                      submitted = false;
                  }
            });
			}
			else
			{
				$('#email').focus();
				$('#email').select();
			  	 //Afficher quelque chose qui dit que c'est un courriel invalide
                $.fancybox(
                    '<h2>Erreur!</h2><p>Votre adresse courriel semble invalide. Veuillez recommencer.</p>',
                    {
                        'autoDimensions'	: false,
                        'width'         		: 250,
                        'height'        		: 'auto',
                        'padding'             : 50,
                        'transitionIn'		: 'none',
                        'transitionOut'		: 'none'
                    }
                );
                submitted = false;
			}
		});
	});
</script>    </aside>
        
