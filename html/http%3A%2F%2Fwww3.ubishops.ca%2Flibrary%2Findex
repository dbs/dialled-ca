	var equalize = function(){
		var mh = 0;

		jQuery.each(columns, function(index,ele){
			mh = Math.max(mh,jQuery(ele).find(".su-column-inner").innerHeight());
		});	
		
		columns.css("height", jQuery(window).width() > 755 ? mh : "auto" );
		
		console.log("mh", mh);
	};
	
	equalize();
	
