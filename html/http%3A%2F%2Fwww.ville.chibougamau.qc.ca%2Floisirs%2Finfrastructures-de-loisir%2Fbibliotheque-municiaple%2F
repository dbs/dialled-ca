<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=true"></script>
<script type="text/javascript" src="/scripts/gmap-style-red.js"></script>
<script type="text/javascript">

    function pageLoad() {

        var markers = new Array();

        $("body").addClass("map");

        var mapContainer = document.getElementById("googlemap");

        var mapCenter = new google.maps.LatLng(49.914093157463206, -74.36594009399414);
        var myOptions = {
            zoom: 16,
            center: mapCenter,
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            scrollwheel: false
        };

        var bounds = new google.maps.LatLngBounds();
        var map = new google.maps.Map(mapContainer, myOptions);
        var mapCustomType = new google.maps.StyledMapType(mapStyles, myOptions);
        map.mapTypes.set('custom', mapCustomType);
        map.setMapTypeId('custom');

        $(".googlemap-marker").each(function () {
            
            var latlng = new google.maps.LatLng($(this).find(".googlemap-marker-lat").html(), $(this).find(".googlemap-marker-long").html());
            var infowindow = new google.maps.InfoWindow({
                content: $(this).find(".googlemap-marker-bubble").html()
            });
            var marker = new google.maps.Marker({
                position: latlng,
                title: $(this).find("strong").html(),
                icon: markerIcon
            });
            google.maps.event.addListener(marker, 'click', function () {
                infowindow.open(map, marker);
            });

            marker.UniqueId = $(this).data("markerid");
            markers.push(marker);
            marker.setMap(map);
            bounds.extend(latlng);
            
        });

        if ($(".googlemap-marker").length > 0) {
            
            if($(".googlemap-marker").length == 1) {
                map.fitBounds(bounds);
            }
            
            var listener = google.maps.event.addListener(map, "zoom_changed", function () {
                if (map.getZoom() > 16) map.setZoom(16);
                google.maps.event.removeListener(listener);
            });
        }

        $(".map-marker").live('click', function () {

            $('html, body').animate({ scrollTop: 0 }, 'slow');

            var that = $(this);
            var id = $(that).data("markerid");
            var currentMarker = null;

            for (var j = 0; j < markers.length; j++) {
                markers[j].setAnimation(null);
                if (markers[j].UniqueId == id) {
                    map.panTo(markers[j].position);
                    currentMarker = markers[j];
                }
            }

            if (currentMarker != null) {
                currentMarker.setAnimation(google.maps.Animation.BOUNCE);
                setTimeout(function () {
                    currentMarker.setAnimation(null);
                }, 2200);
            }

            return false;

        });

    }

    addEvt(window, "load", pageLoad);

</script>
    

