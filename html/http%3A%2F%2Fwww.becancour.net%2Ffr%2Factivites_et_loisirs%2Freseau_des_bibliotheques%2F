<script type="text/javascript">
	var fmin = 10;
	var fmax = 16;
	var step = 1;
	var fstart = 12;
	var fdef = (Cookies["fsize"]) ? Cookies["fsize"] : fstart;
	
	function fSizer(fid,up){
		fzone = document.getElementById(fid);
		fsize = (fzone.style.fontSize=='') ? fdef : fzone.style.fontSize.replace("px","");
		fsize = parseInt(fsize);
		
		if(up){
			if(up==2){
				fsize = fstart;
			}else{
				fsize = (fsize>=fmax) ? fmax : fsize+step;
			}
		}else{
			fsize = (fsize<=fmin) ? fsize : fsize-step;
		}
		
		fzone.style.fontSize = fsize+"px";
		Cookies.create("fsize", fsize, 180);
	}
</script>

