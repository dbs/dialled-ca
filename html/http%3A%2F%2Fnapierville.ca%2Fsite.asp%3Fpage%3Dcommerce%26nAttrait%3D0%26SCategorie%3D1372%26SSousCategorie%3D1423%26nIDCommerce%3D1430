		mm_menu_3.hideOnMouseOut=true;
		mm_menu_3.childMenuIcon="images/arrows.gif";
		mm_menu_3.bgColor='#ffffff';
		mm_menu_3.menuBorder=0;
		mm_menu_3.menuLiteBgColor='#ffffff';
		mm_menu_3.menuBorderBgColor='#cccccc';
		
		// Créer un menu
		window.mm_menu_4 = new Menu("root",300,24,"Verdana, Arial, Helvetica, sans-serif",12,"#ffffff","#ffffff","#97111c","#c21a21","left","middle",4,1,1000,-400,0,true,true,true,3,false,false);
		
			mm_menu_4.addMenuItem("","");	
		
