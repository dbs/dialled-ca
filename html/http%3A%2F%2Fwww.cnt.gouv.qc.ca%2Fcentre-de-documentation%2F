  jQuery(document).ready(function(){	  
 	jQuery('#nav-toggle').click(function(){
	
		jQuery('#mobile-nav').css("position", "absolute");
 		jQuery('#nav-toggle,#mobile-nav').toggleClass('active');		
		if (jQuery('#mobile-nav.active').length)	{
			jQuery('body').css('overflow','hidden');
			jQuery('#footerwrap').addClass('hidding-foot');

		}else{
			jQuery('body').css('overflow','scroll');
			jQuery('#footerwrap').removeClass('hidding-foot');
			jQuery('#mobile-nav').css("position", "relative");
		}
 	});
  });
</script>
