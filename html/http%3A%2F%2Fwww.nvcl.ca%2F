<script type="text/javascript">
    function CallBannerReady(speed) {
        if (document.getElementById('banners') != null) {
            jQuery('#banners').cycle({
                timeout: speed,
                speed: 500,
                fx: 'fade',
                pager: '.slideshowPager',
                pagerAnchorBuilder: function () {
                    return '<a href="javascript:;">&nbsp;</a>';
                }
            });
        }
    }
</script>
    
    
