<script type="text/javascript">
    jQuery(document).ready(function () {
        $('#btnHeaderMobileGo').click(function () {
            if (jQuery('#ctl20_txtMobileHeaderSearch').val() != '') {
                if (document.getElementById('ctl20_rbMobileCatalogue').checked) {

                    if (true) {
                        window.location.href = "http://nvcl.bibliocommons.com/search?t=smart&search_category=keyword&commit=Search&q=" + $('#ctl20_txtMobileHeaderSearch').val();
                } else {
                    $.ajax({
                        type: "POST",
                        url: "/CNV/Ajax/CnvService.asmx/GetLibraryCatalogueForm",
                        data: "{keyword:'" + $('#ctl20_txtMobileHeaderSearch').val() + "', catalogueAttribute:'TI^TITLE^TITLES^Title Processing^Title'}",
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            success: function(msg) {
                                var data = eval("(" + msg.d + ")");
                                $('#catalogFormToSubmit').append(data.Form);
                                $('#searchNow').click();
                            },
                            error: function(msg) {
                                alert('Error getting Data');
                            }
                        });
                    }
                            
                } else {

                    window.location.href = "/Search.aspx#lookin=All-Site&searchTerm=" + jQuery('#ctl20_txtMobileHeaderSearch').val() + "&websiteSearch=1";
            }
        }
        });

    $('#ctl20_txtMobileHeaderSearch').bind('keypress', function (event) {
        if (event && event.stopPropagation) {
            event.stopPropagation();
        } else {
            event = window.event;
            event.cancelBubble = true;
        }

        if (event.keyCode === 13) {
            $('#btnHeaderMobileGo').click();
            return false;
        }
    });

    jQuery("#mobileSearchInput").watermark("Type in keyword(s) here");

    jQuery("#mobileSearchButton").click(function () {
        if ('' != '') {
                window.location.href = '#lookin=Entire-Site&searchTerm=' + jQuery("#mobileSearchInput").val();
            }
        });

        //set slide toggle to mobile menu
        jQuery(".mobile-menu").hide();
        jQuery(".navbar-toggle").click(function () {
            $(".mobile-menu").slideToggle();

            var activeItem = $(".mobile-menu .active");
            if (activeItem && activeItem.length > 0) {
                var link = activeItem.parents('.firstLevelItem').find('.firstLevelItemLink');
                if (link && !link.hasClass('opened')) {
                    link.click();
                }
                var link2 = activeItem.parents('.secondLevelItem').find('.secondLevelItemLink');
                if (link2 && !link2.hasClass('opened')) {
                    link2.click();
                }
            }
        });

        jQuery(".subitems").hide();
        jQuery(".second-subitems").hide();

        jQuery(".firstLevelItemLink").click(function (event) {
            var li = $(this);
            if (li.parent().find("ul.subitems").length > 0) {
                li.parent().find("ul.subitems").slideToggle(function () {
                    var currentElement = $(this);
                    if (li.hasClass("closed")) {
                        li.removeClass("closed");
                        li.addClass("opened");
                    }
                    else 
                        if(li.hasClass("opened")){
                        li.addClass("closed");
                        li.removeClass("opened");
                    }
                });
            }
        });

        jQuery(".secondLevelItemLink").click(function (event) {
            var li = $(this);
            if (li.parent().find("ul.second-subitems").length > 0) {
                li.parent().find("ul.second-subitems").slideToggle(function () {
                    var currentElement = $(this);
                    if (li.hasClass("closed")) {
                        li.removeClass("closed");
                        li.addClass("opened");
                    }
                    else {
                        li.addClass("closed");
                        li.removeClass("opened");
                    }
                });
            }
        });
        });
</script>

