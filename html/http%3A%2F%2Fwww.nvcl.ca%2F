<script type="text/javascript">
    $(document).ready(function () {
        var maxItems = 4;
        var showNextPreview = $(".home-carousel-wrapper ul").children().length > maxItems;

        $(".home-carousel-wrapper li").each(function(i) {
            if (i % maxItems == 0 && i >= maxItems) {
                $(".home-carousel-wrapper")
                    .append("<ul class='home-carousel-wrapper-list'></ul>");
            }
            $(".home-carousel-wrapper .home-carousel-wrapper-list").last().append(this);
        });

        if (showNextPreview) {
            $(".home-carousel-wrapper").before('<a id="archivePrev" class="fixPNG archivePrev" href="javascript:;">P</a>');
            $(".home-carousel-wrapper").after('<a id="archiveNext" class="fixPNG archiveNext" href="javascript:;">N</a>');
        }

        $('.home-carousel-wrapper').cycle({
            fx: 'scrollHorz',
            speed: '600',
            timeout: 10000,
            next: '.archiveNext',
            prev: '.archivePrev'
        });
        
    });
</script>
