<script type="text/javascript">
    jQuery(window).load(function () {

        //Text Resize
        //        $('#plustext').bind('click', function () {
        //            if (document.body.style.fontSize == "") {
        //                document.body.style.fontSize = "0.75em";
        //            }
        //            document.body.style.fontSize = parseFloat(document.body.style.fontSize) + 0.25 + "em";

        //        });
        //        $('#minustext').bind('click', function () {
        //            if (document.body.style.fontSize == "") {
        //                document.body.style.fontSize = "0.75em";
        //            }
        //            document.body.style.fontSize = parseFloat(document.body.style.fontSize) - 0.25 + "em";

        //        });

        //MegaNav
        var topMenu = $('#ctl19_topMenu');
        var topMenuItems = topMenu.children('li');

        topMenu.children('li').each(function (index) {
            var $this = jQuery(this);
            $this.children('div').css({
                'left': '0'
            });
        });

        function realWidth(obj) {
            var clone = obj.clone();
            clone.css("visibility", "hidden");
            jQuery('body').append(clone);
            var width = clone.outerWidth();
            clone.remove();
            return width;
        }

        topMenu.children('li').each(function (index) {
            var $this = jQuery(this);
            if ($this.children('div').children().length > 0) {
                var containerWidth = 1000;

                var meganavWidthDiv = realWidth($this.children('div'));

                var menganavItemLeft = Math.round($this.position().left);
                var remainingSize = containerWidth - menganavItemLeft;

                if (meganavWidthDiv <= remainingSize) {
                    $this.children('div').css({
                        'left': 'auto'
                    });

                } else {
                    var left = containerWidth - meganavWidthDiv -3;
                    $this.children('div').css({
                        'left': left
                    });
                }
            }
        });

        var config = {
            over: makeTall,
            timeout: 200,
            out: makeShort
        };

        jQuery('#ctl19_topMenu li').hoverIntent(config);

        function makeTall() {
            var $this = jQuery(this);
            $this.addClass('slided selected');

            if ($this.children('div').children().length > 0) {
                topMenuItems.not('.slided').children('div').hide();
                $this.children('div').css('z-index', '9999').stop(true, true).slideDown(200, function () {
                    $this.removeClass('slided');
                });
            }
        }

        function makeShort() {
            var $this = jQuery(this);
            $this.removeClass('selected').children('div').css('z-index', '1');
            if ($this.parent().is('.top-menu')) {
                $this.removeClass('selected').children('div').hide();
            }
        }
    });
</script>
            
