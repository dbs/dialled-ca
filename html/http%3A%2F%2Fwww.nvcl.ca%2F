        <div id="catalogFormToSubmit" style="display: none;"></div>
        
        
            <script type="text/javascript">
            $(function () {

                $('#btnHeaderGo').click(function () {
                    if (jQuery('#txtHeaderSearch').val() != '') {
                        if (document.getElementById('rbCatalogue').checked) {

                            if (true) {
                                window.location.href = "http://nvcl.bibliocommons.com/search?t=smart&search_category=keyword&commit=Search&q=" + $('#txtHeaderSearch').val();
                            } else {
                                $.ajax({
                                    type: "POST",
                                    url: "/CNV/Ajax/CnvService.asmx/GetLibraryCatalogueForm",
                                    data: "{keyword:'" + $('#txtHeaderSearch').val() + "', catalogueAttribute:'TI^TITLE^TITLES^Title Processing^Title'}",
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: function(msg) {
                                        var data = eval("(" + msg.d + ")");
                                        $('#catalogFormToSubmit').append(data.Form);
                                        $('#searchNow').click();
                                    },
                                    error: function(msg) {
                                        alert('Error getting Data');
                                    }
                                });
                            }
                            
                        } else {

                        window.location.href = "/Search.aspx#lookin=All-Site&searchTerm=" + jQuery('#txtHeaderSearch').val() + "&websiteSearch=1";
                        }
                    }
                });

                $('#txtHeaderSearch').bind('keypress', function (event) {
                    if (event && event.stopPropagation) {
                        event.stopPropagation();
                    } else {
                        event = window.event;
                        event.cancelBubble = true;
                    }

                    if (event.keyCode === 13) {
                        $('#btnHeaderGo').click();
                        return false;
                    }
                });

                if (navigator.userAgent.indexOf('Mac') > 0)
                    $('.headerSearch').addClass('mac-os');
                });


                /* remove white space if alert message is not present */
                $( document ).ready(function() {

                    if ( $( ".alertMessage" ).length && !($( ".alertMessage" ).css('display') == 'none')) {
 
                        $('body.home-page').css('margin-top', '41px');
                        $('#home-content .alert').css('top', '-45px');
 
                    } else {

                        $('body.home-page').css('margin-top', 0);

                    }

                });
        </script>
        
    </div>
    </form>
</body>
</html>
