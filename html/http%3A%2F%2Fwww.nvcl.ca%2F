<script type="text/javascript">
    function CallBannerReady(speed) {
        if (document.getElementById('banners') != null) {
            jQuery('#banners').cycle({
                slideResize:false,
                timeout: 6000,
                speed:1000,
                fx: 'fade',
                pager: '.slideshowPager',
                pagerAnchorBuilder: function () {
                    return '<a href="javascript:;">&nbsp;</a>';
                },
                before: function(currSlideElement, nextSlideElement, options, forwardFlag) {
                    if (window.innerWidth < 1017) {
                        FixBgColor(true);
                        currSlideElement.cycleH = $(currSlideElement).height();
                        nextSlideElement.cycleH = $(nextSlideElement).height();
                        $('#banners').height($(nextSlideElement).height());
                    } else {
                        FixBgColor(false);
                    }
                }
            });
        }
    }

    function FixBgColor(isMobile) {
        $('#banners .contentWrapper').each(function() {
            $(this).css('background', $(this).attr(isMobile?'data-rgb':'data-rgba'));
        });
    }

    $(document).ready(function() {
        jQuery('#banners').css("visibility", "hidden");
        jQuery('.slideshow').css("visibility", "hidden");
        if (window.innerWidth < 1017) {
            jQuery("#banners").height(426);
            FixBgColor(true);
        } else {
            FixBgColor(false);
        }
    });

    $(window).resize(function() {
        jQuery('#banners').css("visibility", "hidden");
        jQuery('.slideshow').css("visibility", "hidden");
       
        if (window.innerWidth < 1017) {
            FixBgColor(true);
            jQuery("#banners").height(426);
        } else {
            FixBgColor(false);
        }

        jQuery('#banners').cycle('destroy'); // destroy the plugin and reset it later
        jQuery('#banners').attr('style', ''); // reset the complete style attribute because the destroy plugin doesnt do that
        jQuery('#banners').find('.slide').css('left', 0); // reset the position because there is a bug with the active slide that gets pulled a little to the left after resetting
        // reinitialize the plugin
        jQuery('#banners').cycle({
            slideResize:false,
            timeout: 6000,
            speed:1000,
            fx: 'fade',
            pager: '.slideshowPager',
            pagerAnchorBuilder: function () {
                return '<a href="javascript:;">&nbsp;</a>';
            },
            before: function(currSlideElement, nextSlideElement, options, forwardFlag) {
                if (window.innerWidth < 1017) {
                    FixBgColor(true);
                    currSlideElement.cycleH = $(currSlideElement).height();
                    nextSlideElement.cycleH = $(nextSlideElement).height();
                    $('#banners').height($(nextSlideElement).height());
                } else {
                    FixBgColor(false);
                }
            }
        });

        jQuery('#banners').css("visibility", "visible");
        jQuery('.slideshow').css("visibility", "visible");
    });


    $(window).load(function() {
        CallBannerReady();

        jQuery('#banners').css("visibility", "visible");
        jQuery('.slideshow').css("visibility", "visible");
    });
</script>

    
    
