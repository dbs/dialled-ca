					.css({left:x, top:y})
					.addClass('sliding')
					.slideDown(800, function() { jQuery(this).removeClass('sliding'); });
			},
			function() {
				jQuery(this)
					.find('>ul')
					.addClass('sliding')
					.slideUp(800, function() { jQuery(this).removeClass('sliding').hide(); });
			}
		);

		jQuery('.commentlist li:even').addClass('alt');
});
// ]]> -->
</script>
</body>
</html>