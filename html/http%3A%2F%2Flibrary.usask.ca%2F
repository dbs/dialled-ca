<script type="text/javascript">
$().ready(function() {
	$("#database").autocomplete("http://library.usask.ca/external/autocomplete/auto_database.php", {
		width: 300,
		selectFirst: false,
                matchContains: true
	});
	$("#database").result(function(event, data, formatted) {
		if (data)
			$(this).parent().next().find("input").val(data[1]);
	});
       $("#journal").autocomplete("http://library.usask.ca/external/autocomplete/auto_ejournal.php", {
		width: 300
	});
	$("#journal").result(function(event, data, formatted) {
		if (data)
			$(this).parent().next().find("input").val(data[1]);
	});
	$("#clear").click(function() {
		$(":input").unautocomplete();
	});
});

</script>


