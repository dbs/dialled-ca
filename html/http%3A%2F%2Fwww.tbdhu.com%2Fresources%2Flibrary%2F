<script language="javascript">
function CNL_HookupEvent(p_OldHandler, p_NewHandler)
{
    var oldHandler = '';
    var newHandler = '';
    if (typeof(p_OldHandler) == 'function') {
        oldHandler = p_OldHandler.toString();
        oldHandler = '{' + oldHandler.substring(oldHandler.indexOf('{') + 1, oldHandler.lastIndexOf('}')) + '}';
    }

    return new Function(oldHandler + ';' + p_NewHandler + ';');
}
</script>


