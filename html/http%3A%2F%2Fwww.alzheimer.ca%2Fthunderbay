<script>	


	var tempSections = $(".sideMenuContent").toArray();
	var sectionsName = [];
	for(i = 1; i <= tempSections.length; i++)
		sectionsName.push(i);
	var focused = false;
	var cookieName = "alzheimerSelectedMenu";
	var sections = [];
	for(section in sectionsName)
		sections.push(false);

	$(".sideMenuNav").focus(function() {
		switchTab(this);
	});

	$(".sideMenuNav").click(function() {
		for(var i = 0; i < sections.length; i++)
		{
			 if(sectionsName[i] == $(this).data("section"))
			 {
				$(".sideMenuFocusable[data-section='" + sectionsName[i] + "']").focus(); 
			 }
		}
	});

	function switchTab(item)
	{
		for(var i = 0; i < sections.length; i++)
		{
			if(sectionsName[i] == $(item).data("section"))
			{
				sections[i] = !sections[i];
				setMenuCookie(cookieName, sectionsName[i]);
				var currentCookieName = sectionsName[i];
			}
			 else
				sections[i] = false;
				
			if(sections[i])
			{
				$('section[data-section="' + sectionsName[i] + '"]').slideDown("slow");
				$(".sideMenuIcon[data-section='" + sectionsName[i] +"']").addClass("fa-caret-down").removeClass("fa-caret-right");
				$(".sideMenuFocusable[data-section='" + sectionsName[i] + "']").focus();
			}
			else
			{
				$('section[data-section="' + sectionsName[i] + '"]').slideUp("slow");
				$(".sideMenuIcon[data-section='" + sectionsName[i] +"']").addClass("fa-caret-right").removeClass("fa-caret-down");
				if(currentCookieName === sectionsName[i])
					deleteCookie(cookieName);
			}
		}
	}

	function setMenuCookie(cname, cvalue) 
	{
		date = new Date();
		date.setTime(date.getTime() + (24*60*60*1000));
		expires = "expires=" + date.toGMTString();
		document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/;";
	}

	function deleteCookie(cname)
	{
		var expires = "expires=Thu, 01 Jan 1970 00:00:01 GMT; path=/;";
		document.cookie = cname + "=value; " + expires;
	}

	function getCookie(cname) 
	{
		var name = cname + "=";
		var ca = document.cookie.split(';');
		for(var i=0; i<ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1);
			if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
		}
		return "";
	}

	function preOpenMenu()
	{
		var tabToOpen = getCookie(cookieName);
		var tabToOpenNumber = parseInt(tabToOpen) - 1;
		sections[tabToOpenNumber] = !sections[tabToOpenNumber];
		$('section[data-section="' + tabToOpen + '"]').slideDown("slow");
	}

	$(document).ready(function() {
		preOpenMenu();
	});
</script>
