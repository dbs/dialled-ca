<script>
	$(document).ready(function() {
		var offset = 220;
		var duration = 500;
		$(window).scroll(function() {
			if (jQuery(this).scrollTop() > offset) {
				jQuery('.btt').fadeIn(duration);
			} else {
				jQuery('.btt').fadeOut(duration);
			}
		});

		jQuery('.btt').click(function(event) {
			event.preventDefault();
			jQuery('html, body').animate({scrollTop: 0}, duration);
			return false;
		})
	});
</script>
<script>
	$(function(){

		$("button.menu-btn-open").click(function(){
			$(".desktop-menu-fly").toggleClass("hide");
			$(".desktop-menu-fly").toggleClass("slideIn");
		});

		$("button.menu-btn-dropdown").click(function(){
			$(".offCanvas").toggleClass("hide");
			$(".offCanvas").toggleClass("slideIn");
		});
	});
</script>
