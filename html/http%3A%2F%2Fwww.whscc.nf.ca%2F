<html>
    <head>
        <title></title>
		<script type="text/javascript">
			var defaultSite = "default.whscc";
			var newIndex = "indexSiteIntro.html";
			if( document.cookie.indexOf("skipIntro") >= 0 ) {
				document.location.href = defaultSite;
			}else{
				document.location.href = newIndex;
			}
		</script>
    </head>
    <body>
    </body>
</html>
