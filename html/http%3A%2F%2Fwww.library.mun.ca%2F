            <input type="hidden" name="tab" id="tab" value="default_tab" />
            <input type="hidden" id="vid" name="vid" value="01MUN"/>
            <input type="hidden" name="scp.scps" id="scps" value="scope:(01MUN),scope:(CONTENTdm-MUN),scope:(Eprints-MUN),primo_central_multiple_fe" />
            <div class="summon-search-box">
                <input type="text" name="vl(freeText0)" value="" placeholder="OneSearch" style="width:25em;" />
                <select id="formSelect" style="width:14em;">
                    <option value="1">Books, Articles &amp; More</option>
                    <option value="3">Catalogue</option>
                    <option value="2">Articles</option>
                    <option value="4">MUN Digitized Collections</option>
                    <option value="5">Reserves</option>
                </select>
                <input type="submit" value="Search" />
            </div>
        </form>
<script>
    document.getElementById("formSelect").onchange = function(){
        if( document.getElementById("formSelect").value == 1 ){
            document.getElementById("scps").value = "scope:(01MUN),scope:(CONTENTdm-MUN),scope:(Eprints-MUN),primo_central_multiple_fe";
            document.getElementById("tab").value = "default_tab";
        }
        else if( document.getElementById("formSelect").value == 2 ){
            document.getElementById("scps").value = "primo_central_multiple_fe";
            document.getElementById("tab").value = "default_tab";
        }
        else if( document.getElementById("formSelect").value == 3 ){
            document.getElementById("scps").value = "scope:(01MUN)";
            document.getElementById("tab").value = "default_tab";
        }
        else if( document.getElementById("formSelect").value == 4 ){
            document.getElementById("scps").value = "scope:(CONTENTdm-MUN),scope:(Eprints-MUN)";
            document.getElementById("tab").value = "default_tab";
        }
        else if( document.getElementById("formSelect").value == 5 ){
            document.getElementById("scps").value = "";//scope:(MUN_CR)";
            document.getElementById("tab").value = "reserves";
        }
    };
</script>
