function changeFontSize(curForm){
  
  document.forms.item(0).procType.value = "Font"
  document.forms.item(0).submit();
}

function printPage(printPage){
	document.forms.item(0).page_data.value = mainPrint.innerHTML.replace(/<!--START_REM-->(.|\r\n)*?<!--END_REM-->/gi,"");
	document.forms.item(0).target='_blank';
	document.forms.item(0).action="print.aspx";
	document.forms.item(0).submit();
	
	document.forms.item(0).target = "";
	document.forms.item(0).action = "";
	document.forms.item(0).page_data.value = "";
}
function clearSearchBox(textbox)
{
    if(textbox.value == "search" || textbox.value == "Search" ||textbox.value == "SEARCH"){
      textbox.value = "";
    }
}
</script>
