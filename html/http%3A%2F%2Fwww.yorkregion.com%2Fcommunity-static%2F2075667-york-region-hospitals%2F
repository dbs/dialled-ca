                    <script type="text/javascript">
        function IncrementLikeDislikeCount(assetId, increment) {
            $.ajax({
                type: "POST",
                url: "/DesktopModules/IWebCSharp/WebService.asmx/IncrementLikeDislikeCount",
                data: "assetid=" + assetId + "&increment=" + increment,
                dataType: "text",
                async: false,
                success: function (data) {
                    if (data.indexOf("Not Authenticated") > 0) {
                        alert("Please log in to continue.");
                    }
                    else if (data.indexOf("Voted") > 0) {
                        alert("Maximum vote that a user can provide to a single content is ONE");
                    }
                    else {
                        if (increment == 1) {
                            $('#spanLike').text(parseInt($('#spanLike').text()) + 1);
                        }
                        else if (increment == -1) {
                            $('#spanDislike').text(parseInt($('#spanDislike').text()) + 1);
                        }
                    }
                },
                fail: function () {
                    alert('IncrementLikeDislikeCount web service call failed.');
                }
            });
        }
    </script> 
