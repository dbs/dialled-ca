        $(document).ready(function () {
            var url = document.location.pathname;
            $('.Nav a[data-url]').each(function (i) {
                var re = new RegExp($(this).attr('data-url'));
                if (re.test(url)) {
                    $(this).parent().addClass('active');
                    return false;
                }
            });
