		<script src="http://www.mycallander.ca/wp-content/themes/blankslate/jquery/prototype.js" type="text/javascript"></script>
		<script src="http://www.mycallander.ca/wp-content/themes/blankslate/jquery/effects.js" type="text/javascript"></script>
		<script src="http://www.mycallander.ca/wp-content/themes/blankslate/jquery/scriptaculous.js?load=effects" type="text/javascript"></script>
		<script src="http://code.jquery.com/jquery-1.8.2.js"></script>
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.11/jquery-ui.min.js"></script>


		<script type="text/javascript">
		var $j = jQuery.noConflict();
        $j(document).ready(function () {
			var $globalContainer = $j('.span-24');
			var $menuContainer = $j('.menuContainer');

			$globalContainer.children('ul.ldd_menu').children('li').each(function(){
			var title = $j(this).text();
			
			if (title.toUpperCase().indexOf('MENU ') >= 0){
				title = (title.toUpperCase().replace('MENU ', '').toLowerCase().replace(/\b[a-z]/g, function(letter) { return letter.toUpperCase(); }));
				}
			
			var $color = $menuContainer.children('div#' + title).attr('class').replace('ldd_submenu ', '');
			$j(this).attr('class', '');
			$j(this).html('<span class="' + $color.toLowerCase() + '">' + title + '</span>');
			$j(this).attr('class', title);
			});

			$menuContainer.children('div').each(function(){
			var $currentDiv = $j(this);
			var currentid = $j(this).attr('id');
			$currentDiv.attr('class', $currentDiv.attr('class').toLowerCase());
			$currentDiv.clone().appendTo($j('li.' + currentid));
			$j(this).remove();
			});
		
		$j(function() {
				/**
				 * the menu
				 */
				var $menu = $j('#ldd_menu');
				/**
				* for each list element,
				* we show the submenu when hovering and
				* expand the span element (title) to 510px
				*/
				$menu.children('li').each(function(){
					var $this = $j(this);
					var $span = $this.children('span');
					$span.data('width',$span.width());
					
					$this.bind('mouseenter',function(){
						$menu.find('.ldd_submenu').stop(true,true).hide();
						$span.stop().animate({'width':'250px'},300,function(){
							$this.find('.ldd_submenu').slideDown(300);
						});
					}).bind('mouseleave',function(){
						$this.find('.ldd_submenu').stop(true,true).hide();
						$span.stop().animate({'width':$span.data('width')+'px'},300);
					});
				});				
			});
		
			//get menu
			var $menu = $j('#ldd_menu');
			//start new unordered list
			var str = '<ul style="width:140px;">';
			//counts the amount of headers used
			var headercount = 0;
			//get Headers within menu
			$menu.children('li').children('div').children('ul').children('li').each(function(){
			var ListItem = $j(this).children('a');
			
			if (ListItem.text().toUpperCase().indexOf('HEADER ') >= 0){
				ListItem.text(ListItem.text().toUpperCase().replace('HEADER ', '').toLowerCase().replace(/\b[a-z]/g, function(letter) { return letter.toUpperCase(); }));
				}
			//check for newcolumn tag, will create a new column if this tag is present
			if (ListItem.text().toUpperCase().indexOf('NEWCOLUMN ') >= 0){
				ListItem.text(ListItem.text().toUpperCase().replace('NEWCOLUMN ', '').toLowerCase().replace(/\b[a-z]/g, function(letter) { return letter.toUpperCase(); }));
					
				//if new column is found, adds an end tag to the unordered list and adds it to the div container, then starts a new list
				if(headercount >= 1) {
					str += '</ul>';
					$j(this).parent().parent().append(str);
					str = '<ul style="width:140px;">';
				}
			}
				
			//changes a href into label if header tag is present and adds it to the list
			ListItem.html("<li><label class='ldd_heading'>" + ListItem.text() + "</label></li>");
			
			if( str == '<ul style="width:140px;">'){
				str +=  ListItem.html();
			}
			else if($j.browser.version == '6.0' || $j.browser.version == '7.0' || $j.browser.version == '8.0'){
			str += '<li><br /><br /></li>' + ListItem.html();
			}
			else{
			str += '<li><br /></li>' + ListItem.html();
			}
			//gets Pages within each Header
			$j(this).children('ul').children('li').each(function(){
			//removes any children of the child UL, preventing the list from looking bad if additional indents have been made in wordpress
			$j(this).children('ul').remove();
			str += this.outerHTML;
			});
				
			//increments headercount
			headercount = headercount + 1;
			
			//variable to keep track of remaining pages
			var liCount = $j(this).parent().children().length;
			
			// if the last page has been reached, the orignal unordered list is removed and the final new unordered list is added to the div container
			if(liCount == 1){
			$j(this).parent().parent().append(str);
			str = '<ul style="width:140px;">';
			$j(this).parent().remove();
			}
			$j(this).remove();
			});	
			
			//var $page = $j('#page');
			//$page.data('background-image', 'url('http://www.mycallander.ca/wp-content/themes/blankslate/img/bg_.gif')');
		})
		
		
		</script>
				
