<input id="CalendarIDPlaceholder_eventsListing" type="hidden" value='608c0471-12f4-4dc5-9931-a61a00c36aa7' /><!--TODO: CALENDAR: This is the first Calendar ID in the array.  You may need to Change the 0 to match the CalendarID -->

<!-- TODO: LANGC: Everything on this page needs translations and FEATURE: needs to be made dynamic -->

    <div id="eventsListing"></div>
    <a href='http://calendar.georginalibrary.ca/' title="View All Events" class="moreLink">View More</a>
    <noscript>JavaScript is disabled. Please enable JavaScript to view this content.</noscript> 

<script type="text/javascript">
    function parseData_eventsListing(data, divElement) {

        var charLimit = 40;
        var descCharLimit = 128;
        divElement.empty();
        var html = "<ul class='calendarList'>";

        $.each(data, function (index, item) {
           
            var id = item.Id;
            var title = item.Title != null ? item.Title : "",
                startDate = item.StartDate,
                description = item.Description != null ? item.Description : ""
            calendarPath = "/Default/Detail/";
            if (title.length > charLimit) {
                if (title.indexOf(' ', charLimit) >= 0) {
                    title = title.substring(0, title.indexOf(' ', charLimit)) + "...";
                }
            }

            if (description.length > descCharLimit) {
                if (description.indexOf(' ', descCharLimit) >= 0) {
                    description = description.substring(0, description.indexOf(' ', descCharLimit)) + "...";
                }
            }

            html += "<li><div class='date'>" + $.datepicker.formatDate('M', $.datepicker.parseDate("mm/dd/yy", startDate.toString())) + " <span>" + $.datepicker.formatDate('dd', $.datepicker.parseDate("mm/dd/yy", startDate.toString())) + "</span></div> <a href=\"http://calendar.georginalibrary.ca/" + calendarPath + id + "\" title=\"View event: " + title.replace(/'/g, "&#39;") + " - " + startDate + "\" > " + title + "<\/a></li>";

        })
        html += "</ul>";
        divElement.html(html);
    }
    
    $(function () {
        var divElement = $("#eventsListing");
        if (iCreateObject.isSiteInICreateMode) {
            divElement.empty().html("Calendar feed disabled in iCreate.");
        }
        else {

            divElement.html("Loading...");
            var params = $.param(
                            {
                                Calendar: $("#CalendarIDPlaceholder_eventsListing").val(),
                                limit: 8,
                                Categories: ''
                            });


            $.ajax({
                url: iCreateObject.corpHome + '/Modules/Calendar/services/GetCalendarEvent.ashx?' + params,
                async: true,
                dataType: 'jsonp',
                crossDomain: true,
                cache: false,
                error: function (xhr, textStatus, thrownError) {
                    divElement.html("An error occured");
                },
                success: function (data) {                    
                    if (data != "")
                        parseData_eventsListing(data, divElement);
                    else
                        divElement.html("No Events");
                }

            });
            
        }
    }); 
</script>
							
