        $(function () {
            if (iCreateObject.isSiteInICreateMode) {
                $("#miniCalendar").html("Mini calendar is disabled in iCreate");
            }
            else {
                $("#miniCalendar").html("Loading...").mini_calendar({
                    source: '/Modules/Calendar/services/GetCalendarsDateWithEvent.ashx?calendar=608c0471-12f4-4dc5-9931-a61a00c36aa7',
