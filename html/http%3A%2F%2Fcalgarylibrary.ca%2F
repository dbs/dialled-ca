            <section id="searchWrapperCMS" class="slide_down_item clearfix " role="search">


        <div id="bccmsSearch" role="search" class="catalog_options_wrapper">
            <div class="row_1_wrap_search">

                <div class="search_title">
                    <h2>Search</h2>
                </div>

                <div id="topNavSearchFilters">
                    <span class="separator" tabindex="-1">the</span>
                    <ul role="menu" id="search_types" class="search_options_menu closed">
                        
                            <li id="catalog_options" role="menuitemradio" class="search_type active"  tabindex="0" aria-checked="true">
