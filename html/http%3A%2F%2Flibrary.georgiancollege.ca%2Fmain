<script type='text/javascript'>//<![CDATA[ 

  var libGuideAPI = "https://lgapi.libapps.com/1.0/guides?site_id=351";
  $.getJSON( libGuideAPI, function( data ) {      // get JSON text data that libGuideAPI outputs
      var researchGuides = [];
      
      // sort data array alphabetically based on the name property.  
      data.sort(function (a, b) {
          if (a.name > b.name)
              return 1;
          if (a.name < b.name)
              return -1;
          // a must equal b 
              return 0;
      });
      
      // Before menu is populated with the guide names, we must set the first menu item to be "Select your research guide"
      researchGuides.push("<option value>Select your research guide</option>");
      
      // precondition: data array is now alphabetized based on name property
      // extract only the names, and type_id.  
      // type_id=="3" means Subject Guides;  status=="1" means guide is published.
      for (i = 0; i < data.length; i++) { 
          $.each( data[i], function( key, val ) {
              if (key == "name" && data[i].status == "1" && data[i].type_id == "3") {
                  researchGuides.push("<option value='" + "http://library.georgiancollege.ca/c.php?g=" + data[i].id + "'>" + data[i].name + "</option>");
              }
          });    
      }
      
  $(function(){
      // create the dropdown menu stucture using <form> tag
      $(".menu_researchGuides").html("<form><select class='dropdownform' style='width: 200px;' onchange='if (this.value !=&#39;&#39;) { window.location=this.value; }'></select></form>");
      

      // dynamically insert menu items in dropdown menu                          
      $(".dropdownform").append( researchGuides );
  });

});//]]>  

</script>
