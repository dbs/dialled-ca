<div class="text-center" style="margin-top:1rem;"><a class="btn btn-default" href="http://library.georgiancollege.ca/hours" role="button"><i class="fa fa-clock-o"></i> View weekly&nbsp;hours</a></div>
<script>
$(function() {
var burl = "https://api3.libcal.com/api_hours_today.php";
var iid = 1300;
var lid = 0;
var format = "json";
$.ajax({
    dataType: 'jsonp',
    url: burl,
    type: "GET",
    data: {
        iid: iid,
        lid: lid,
        format: format
    },
    success: function (msg) {

            // https://api3.libcal.com/api_hours_today.php?iid=1300&lid=0&format=json
            // Each location has 9 objects: 0=Barrie, and ending with 8=AskON 
            $.each(msg, function (locations, location) {
               
               if (location[0].rendered !== undefined) {
                  $('#hours_Barrie').html("Barrie Library: " + location[0].rendered);
               }
              
               if (location[1].rendered !== undefined) {
                  $('#reshelp_Barrie').html("Research Help: " + location[1].rendered);
               }

               if (location[2].rendered !== undefined) {
                  $('#hours_Midland').html("Midland Library: " + location[2].rendered);
               }
                          
               if (location[3].rendered !== undefined) {
                  $('#reshelp_Midland').html("Customer service: " + location[3].rendered);
               } 
              
               if (location[4].rendered !== undefined) {
                  $('#hours_Orillia').html("Orillia Library: " + location[4].rendered);
               }
              
               if (location[5].rendered !== undefined) {
                  $('#reshelp_Orillia').html("Research help: " + location[5].rendered);
               }
              
               if (location[6].rendered !== undefined) {
                  $('#hours_OwenSound').html("Owen Sound Library: " + location[6].rendered);
               }
              
               if (location[7].rendered !== undefined) {
                  $('#reshelp_OwenSound').html("Research help: " + location[7].rendered);
               }
              
               if (location[6].rendered !== undefined) {
                  $('#hours_askon').html("AskON Research Help: " + location[8].rendered);
               }               
               
            });
    } //success
}); //ajax
});
</script>
