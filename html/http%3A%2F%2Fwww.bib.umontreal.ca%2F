<script type="text/javascript">
jQuery(function($){
	var $listeAutres = jQuery('#liste-services-autres').hide(),
		$listeAutresToggle = jQuery('#toggle-liste-services-autres');
	$listeAutresToggle
		.data('texte', {ouvrir: $listeAutresToggle.text(), fermer: '...moins'})
		.on('click', function(e){
			e.preventDefault();
			$listeAutres.slideToggle();
		})
		.toggle(
			function(){
				$(this).text($(this).data('texte').fermer);
			},
			function(){
				$(this).text($(this).data('texte').ouvrir);
			}
		);
});
</script>
