    <div class="jvTwitterProfile thumbnail well well-small" data-bind="visible: showProfile" style="display: none">
        <div class="media">
            <a class="pull-left" target="_blank" data-bind="visible: params().profile_avatar == 1, attr:{href: profile().url}">
