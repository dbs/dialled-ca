function loadScript() {
	// Prevents click event to be duplicated
	if (typeof google != 'undefined') {
		google = undefined;
		// init_gmap(); // @todo check necessity
		// return false;
	}
	var script = document.createElement('script');
	script.type = 'text/javascript';
	script.src = 'http://maps.googleapis.com/maps/api/js?sensor=false&language=fr&callback=init_gmap';
	document.body.appendChild(script);
	// alert('initmap adding script');
}

$(document).ready(function() {

	$(document).off('click touchstart','.btn-close');
	// alert('test');
	$(document).on('click touchstart','.btn-close',function(e) {
		e.preventDefault();
		e.stopPropagation();
		var $this = $(this);
		charcoal_map.clearInfoboxes();
		SJ.map.infobox_open = false;

		// Charcoal.trigger('selectObject',{ id : id });
	});

});


function init_gmap() {

	gmap_options['onClick'] = function(elem,opts,ev) {
		opts['ev'] = ev;

		if (typeof opts.onClick != 'undefined') {
			if (opts.onClick !== false) {
				Charcoal.trigger('selectObject',opts);
			}
		} else {
			Charcoal.trigger('selectObject',opts);
		}


		if (typeof opts.onClick == 'string') {
			if (typeof window[opts.onClick] == 'function') {
				window[opts.onClick](elem,opts,ev);
			}
		}
	}

	gmap_options['ready'] = function(map) {
		Charcoal.trigger('mapReady',map);
	}

	gmap_options['onMouseOver'] = function(elem,opts,ev) {
		if (opts.type == 'area') {
			opts.google_object.setOptions(opts.opts.styles.hover);
		}

		if (typeof opts.onMouseOver == 'string') {
			if (typeof window[opts.onMouseOver] == 'function') {
				window[opts.onMouseOver](elem,opts,ev);
			}
		}
	}
	gmap_options['onMouseOut'] = function(elem,opts,ev) {
		if (opts.type == 'area') {
			opts.google_object.setOptions(opts.opts.styles.defaults);
		}
		if (typeof opts.onMouseOut == 'string') {
			if (typeof window[opts.onMouseOut] == 'function') {
				window[opts.onMouseOut](elem,opts,ev);
			}
		}
	}
	$('#map_canvas_').gmap(gmap_options);
}



</script>

<script type="text/javascript">

	/*
	*	Event selectObject
	*	When an object is selected
	*
	*	Param: e // datas
	*	Requires e.id
	*/

	Charcoal.attach('selectObject', function(e)
	{
		var key = e.key;
		var place = e;
		var marker = e.google_object;
		var type = place.type;
		var ev = e.ev;
		// In case the widget list doesn't fit the map list
		if(place != undefined) {
			displayInfos = !place.display;
			place.display = displayInfos;

			if (!displayInfos) {
				// New icon on CLICK (keeps original values)

				if (type == 'marker') {
					var new_icon = new google.maps.MarkerImage(place.icon.image
						,new google.maps.Size(place.icon.width, place.icon.height)   // Width, Height.
						,new google.maps.Point(0, 0)    // Origin for this image; X, Y.
						,new google.maps.Point(place.icon.width, place.icon.height)  // Anchor for this image; X, Y.
						,new google.maps.Size(place.icon.width,place.icon.height)
					)
				}


			}
			else {
				for (var i in charcoal_map.places) {

					if (charcoal_map.places[i].type == 'marker') {
						// New icon on CLICK (keeps original values)
						var new_icon = new google.maps.MarkerImage(charcoal_map.places[i].icon.image
							,new google.maps.Size(charcoal_map.places[i].icon.width, charcoal_map.places[i].icon.height)   // Width, Height.
							,new google.maps.Point(0, 0)    // Origin for this image; X, Y.
							,new google.maps.Point(charcoal_map.places[i].icon.width, charcoal_map.places[i].icon.height)  // Anchor for this image; X, Y.
							,new google.maps.Size(charcoal_map.places[i].icon.width,charcoal_map.places[i].icon.height)
						)
						if (i == key) {

							charcoal_map.places[i]['google_object'].setIcon(new_icon);


							charcoal_map.places[i]['google_object'].setZIndex(9999);
							charcoal_map.places[i].active = true;
						}
						else {
							charcoal_map.places[i].active = false;
							charcoal_map.places[i].display = false;
							charcoal_map.places[i]['google_object'].setIcon(new_icon);
							charcoal_map.places[i]['google_object'].setZIndex(1000);
						}
					}
				}
			}
		}


		if (typeof place.bulle != 'undefined') {
			if (charcoal_map.infoBoxes[key] != undefined) {
				charcoal_map.infoBoxes[key].remove();
				charcoal_map.infoBoxes[key].setMap(null);
				charcoal_map.infoBoxes[key] = undefined;
				charcoal_map.clearInfoboxes();
			}
			else {
				charcoal_map.clearInfoboxes(key);
				var position = '';
				if (type == 'marker') {
					position = marker.getPosition();
				}
				if (type == 'area') {
					position = ev.latLng;
				}
				if (type == 'line') {

				}

				SJ.map.display_infobox_single(gmap_options['places'][ key ].id, position);

				// charcoal_map.infoBoxes[key] = new InfoBox({
				// 	latlng: position,
				// 	map: charcoal_map.map,
				// 	div: place.bulle,
				// 	index:key
				// });

			}
		}
		charcoal_map.map.setCenter(position); // @todo check if necessary.

	},
	'map');
	
</script>

