    <script>
      window.onload = function(){
          assignerTabIndex('h1');
          assignerTabIndex('h2');
          assignerTabIndex('h3');
          assignerTabIndex('h4');
          assignerTabIndex('h5');
      }

      function assignerTabIndex(strTagName)
      {
          for (var i=0;i<document.getElementsByTagName(strTagName).length;i++)
          {
              var objEl = document.getElementsByTagName(strTagName)[i];
              objEl.setAttribute("tabIndex","-1");
          }
      }

      function mettreFocusPremierElement()
      {
        if (document.getElementsByTagName('h1').length > 1)
        {
          document.getElementsByTagName('h1')[1].focus();
        }
        else if (document.getElementsByTagName('h2').length > 0)
        {
          document.getElementsByTagName('h2')[0].focus()
        }
        else if (document.getElementsByTagName('h3').length > 0)
        {
          document.getElementsByTagName('h3')[0].focus()
        }
        else if (document.getElementsByTagName('h4').length > 0)
        {
          document.getElementsByTagName('h4')[0].focus()
        }
        else if (document.getElementsByTagName('h5').length > 0)
        {
          document.getElementsByTagName('h5')[0].focus()
        }
      }
      </script>
