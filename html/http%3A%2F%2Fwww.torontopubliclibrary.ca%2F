<div id="reasons" class="align-top widget" aria-live="off" style="display:none"></div>
<script type="text/javascript">
var url="https://spreadsheets.google.com/feeds/list/189-cDD--41AWVZmJwTe6e-nc-a4tl1Y3O6YsKNisYGo/od6/public/values";
/*var spotlightScript = document.createElement('script'); 
spotlightScript.type = 'text/javascript';
spotlightScript.src = url;
jQuery("head").append(spotlightScript);
*/
jQuery(document).ready(function(){ 
	jQuery.ajax({
		url: url,
		dataType: 'jsonp',
		data: {"alt" : "json"},
		cache: true,
		timeout: 1000 //1 second timeout
	}).done(function( data ) {
		var link_cell = data.feed.entry[0].gsx$_cpzh4;
		var image_cell = data.feed.entry[1].gsx$_cpzh4;
		if (link_cell && image_cell){
			
		//console.log(data.feed.entry);
		var link = link_cell.$t; 
		var image = image_cell.$t;
		var page = "home";
		jQuery("#reasons").html("<a onclick=\"tracking('100 Reasons','"+page+" page spotlight');\" href=\""+link+"\"><img alt=\"100 Reasons to check out Toronto Public Library\" src=\""+image+"\"></a>");
		jQuery("#reasons").show();
		}
	});
});  
</script>
					
			




	
	
	
		









	









