/* see more and less function - sandeep */

function seemore(k,id){



			if(jQuery('span#hide1')==null)

			{	alert("null");

			}

			else

			{

				var c="span";

				var d=c.concat('#',k);	

				jQuery(d).toggle(1000);

				jQuery(d).css('display','block');

				var e="a";

				var f=e.concat('#',id);	

				var t=jQuery(f).text();	

				if(t!='See Less')

				{

					jQuery(f).text('See Less');

				}

				else

				{jQuery(f).text('See More');}

			}

	}			

