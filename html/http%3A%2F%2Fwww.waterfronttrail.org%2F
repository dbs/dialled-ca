<script type="text/javascript">

$j(document).ready(function() {
  $j('ul.maximenuck2 > li.level2 > a').each(function () {
    $j(this).focus(function () {
      var menuParent = $j(this).closest('div').parent().parent().parent();
      $j(menuParent).addClass('dropdown-visible');
      var menuParent3 = $j('ul.maximenuck2 ul.maximenuck2').closest('div').parent().parent().parent();
      $j(menuParent3).removeClass('dropdown-visible');
    });
  });
  $j('ul.maximenuck > li.level1 > a').focus(function () {
    $j('div').removeClass('dropdown-visible');
  });
  $j('ul.maximenuck2 > li.level3 > a').each(function () {
    $j(this).focus(function () {
      var menuParent2 = $j(this).closest('div').parent().parent().parent();
      $j(menuParent2).addClass('dropdown-visible');
    });
  });
});
</script>

<script type="text/javascript">



$j(document).ready(function(){    

  //detect the width on page load

    var current_width = $j(window).width();

     //do something with the width value here!

    if(current_width < 720){

    $j(".Navhead").addClass("ShoW");

    $j("a.PoPouT").prop("#Threediv");

    }  

});

//update the width value when the browser is resized (useful for devices which switch from portrait to landscape)

$j(window).resize(function(){

    var current_width = $j(window).width();

   //do something with the width value here!

    if(current_width < 720){

    $j(".Navhead").addClass("ShoW");

    $j('ul.maximenuck2').css('width', window.innerWidth - 24);

    $j("a.PoPouT").prop("#Threediv");

  }

  else {

    $j(".Navhead").removeClass("ShoW");  

    $j("#topmenudivContent").show();

    }

});

</script>



