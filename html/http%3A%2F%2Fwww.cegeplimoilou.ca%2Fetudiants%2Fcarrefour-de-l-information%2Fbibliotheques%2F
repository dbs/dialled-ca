        <script>
            
            (function ($) {
                var imgSliderResizer = function() {
                    $("#in-page-slider").length && $("#in-page-slider").each(function () {
                        var baseWidth = $(this).width();

                        $(".main-slide-img", $(this)).each(function () {
                            var $t = $(this);

                            if (baseWidth <= $t.data("width")) {
                                var dim = $t.data("dim");
                                dim && $t.css("min-height", ~~(baseWidth / parseFloat(dim.toString().replace(",", "."))));
                            } else {
                                $t.css("min-height", $t.data("height"));
                            }
                        });
                    });
                }

                $(window).resize(function() {
                    imgSliderResizer();
                });

                imgSliderResizer();
            })(jQuery);

            $(document).ready(function () {
                Foundation.set_namespace = function () {
                    this.global.namespace = [];
                };
                $(document).foundation();
            });

        </script>
    
        
    
