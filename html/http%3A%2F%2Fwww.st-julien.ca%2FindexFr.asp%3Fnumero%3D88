<script type="text/javascript">

function changement1()
{

	document.recherche.numeroMat.value = ""
}

function changement2()
{
	document.recherche.numeroMat.value = ""
}
function changement3()
{
	document.recherche.rue.value = ""
	document.recherche.numeroCivique.value = ""
}
