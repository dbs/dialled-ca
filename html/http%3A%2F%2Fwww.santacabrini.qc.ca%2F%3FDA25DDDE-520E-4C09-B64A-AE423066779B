	<script Language="javascript">	

		function ID3_findObj(n, d) {

			var p,i,x;

			if(!d) d=document;

			if((p=n.indexOf("?"))>0&&parent.frames.length)

			{

				d=parent.frames[n.substring(p+1)].document;

				n=n.substring(0,p);

			}

	

			if(!(x=d[n])&&d.all) x=d.all[n];

	

			for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];

	

			for(i=0;!x&&d.layers&&i<d.layers.length;i++)

				x=ID3_findObj(n,d.layers[i].document);

	

			if(!x && d.getElementById)

				x=d.getElementById(n);

	

			return x;

		}

		function lf_ChangeLanguage(sLang)  {

		

			var sLangCh;

			if (sLang=='FR') {

				sLangCh = 'IT';

			} else {

				sLangCh = 'FR'

			}

			KScript.SetLanguage(sLangCh,"FR|IT");

		}

		function ImprimerContenu(){

			if (window.print) {

			  winId = window.open('','newwin');

			  with (winId.document) {

				write('<script language=javascript>function lf_print(){var oTmr;window.focus();window.print();oTmr = window.setTimeout ("window.close();",1000);}<\/script>');

				write('<body onLoad="lf_print();" style="background-image:none;"><B>SRAM</B><br>'+ID3_findObj("PageContent","").innerHTML+'<\/body>');

				write('<link rel="stylesheet" type="text/css" href="/templates/24541E11-626C-4098-88D5-BA22567651C5/css/accueil.css">');

				close();

			  }

			}

		}

		function preloadImages(){

		//onLoad="preloadImages('chemin/img.ext')"

			var d=document;

			if(d.images){

				if(!d.MM_p)d.MM_p=new Array();

				var i,j=d.MM_p.length,a=MM_preloadImages.arguments;

				for(i=0; i<a.length; i++)

					if(a[i].indexOf("#")!=0){d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}

			}

		}

		function lf_SwapImg(img,newsrc){

			img.oldsrc = img.src;

			img.src = newsrc;

		}

		function lf_RestoreImg(img){

			img.src = img.oldsrc;

		}

		

		function menuRapide(lang,type){

			var retour;

			if(type=='img'){

				if (lang=='FR') {

					retour = '<map name="img11">';

					retour += '<AREA SHAPE="circle" COORDS="27,17,15" HREF="/liens" alt="Liens utiles">';

					retour += '<AREA SHAPE="circle" COORDS="67,17,15" HREF="/plansite" alt="Plan de site">';

					retour += '<AREA SHAPE="circle" COORDS="108,17,15" HREF="/contact" alt="Nous joindre">';

					retour += '<AREA SHAPE="circle" COORDS="148,17,15" HREF="/accueil" alt="Accueil">';

					retour += '</map>';

				} else {

					retour = '<map name="img11">';

					retour += '<AREA SHAPE="circle" COORDS="27,17,15" HREF="/liens" alt="Link utili">';

					retour += '<AREA SHAPE="circle" COORDS="67,17,15" HREF="/plansite" alt="Mappa del sito">';

					retour += '<AREA SHAPE="circle" COORDS="108,17,15" HREF="/contact" alt="Contattarci">';

					retour += '<AREA SHAPE="circle" COORDS="148,17,15" HREF="/accueil" alt="Accoglienza">';

					retour += '</map>';

				}

			}else{

				if (lang=='FR') {

					retour = '<div class="Menu_Haut"><a href="/liens">Liens utiles</a> | <a href="/plansite">Plan de site</a> | <a href="/contact">Nous joindre</a> | <a href="/fr">Nouvelles</a> | <a href="/recrutement">Recrutement <img src="/templates/24541E11-626C-4098-88D5-BA22567651C5/images/recruitement.jpg" width="21" height="25" border="0" align="absmiddle"></a></div>';

				} else {

					retour = '<div class="Menu_Haut"><a href="/liens">Link utili</a> | <a href="/plansite">Mappa del sito</a> | <a href="/contact">Contattarci</a> | <a href="/it">Notizie</a> | <a href="/recrutement">Reclutamento <img src="/templates/24541E11-626C-4098-88D5-BA22567651C5/images/recruitement.jpg" width="21" height="25" border="0" align="absmiddle"></a></div>';

				}

			}

			return retour;

		}



		function lf_Load(){

			var objet1 = new ID3FlaObj("/templates/24541E11-626C-4098-88D5-BA22567651C5/general.swf", "general", "859", "174", "7", "#ffffff");

			objet1.addParam("menu","false");

			objet1.addVar("langue","FR");

			objet1.addVar("xmlURL","/templates/24541E11-626C-4098-88D5-BA22567651C5/xml.xml");

			objet1.addVar("currentPage","<!-- K.Params.Section -->");

			objet1.code("zoneGeneral");

		}

	</script>

