<script language="javascript" type="text/javascript">
index1 = -1
index2 = -1
images=new Array(12)
images[0]="<img src='images/IndexPagePics/AC_Admin_Demo.jpg' title='Aurora College administration building demolition!' height='250px' />";
images[1]="<img src='images/IndexPagePics/ChiefSunrise1.jpg' title='Chief Sunrise Education Centre opening' height='250px' />";
images[2]="<img src='images/IndexPagePics/ChiefSunrise2.jpg' title='Chief Sunrise Education Centre' height='250px' />";
images[3]="<img src='images/IndexPagePics/FGH_Tankfarm.jpg' title='Fort Good Hope Tank farm' height='250px' />";
images[4]="<img src='images/IndexPagePics/IS_Site_Constr.jpg' title='Inuvik School construction' height='250px /'>";
images[5]="<img src='images/IndexPagePics/KakisaShelter2.jpg' title='Kakisa kitchen shelter' height='250px' />";
images[6]="<img src='images/IndexPagePics/KitchenShelter.jpg' title='Kitchen shelter' height='250px' />";
images[7]="<img src='images/IndexPagePics/LogConstruction.jpg' title='Architectural feature using logs' height='250px' />";
images[8]="<img src='images/IndexPagePics/Mechanical.jpg' title='Mechanical systems' height='250px' />";
images[9]="<img src='images/IndexPagePics/Piles.jpg' title='Inuvik School pile installation' height='250px' />";
images[10]="<img src='images/IndexPagePics/Sambaa_Deh_Kitchen.jpg' title='Sambaa Deh kitchen shelter' height='250px' />";
images[11]="<img src='images/IndexPagePics/sir_john_renos.jpg' title='Sir John Franklin High School renovation' height='250px' />";
function GetFirstPic(){
	index1 = Math.floor(Math.random() * images.length);  
	GetSecondPic(index1)  
}
function GetSecondPic(FirstIndex){
	index2 = Math.floor(Math.random() * images.length);
	if(FirstIndex == index2){
		GetSecondPic(FirstIndex) 
	}
}
