<div id="dnn_ctr1534_ContentPane" class="contentpane"> <script type="text/javascript">

if (document.getElementById) { window.onload = swap };
function swap() {
var numimages=5;
rndimg = new Array("/portals/0/upload/img_bg_1.jpg","/portals/0/upload/img_bg_2.jpg", "/portals/0/upload/img_bg_3.jpg", "/portals/0/upload/img_bg_4.jpg", "/portals/0/upload/img_bg_5.jpg","/portals/0/upload/img_bg_1.jpg"); 
x=(Math.floor(Math.random()*numimages));
randomimage=(rndimg[x]);
document.getElementById("banner").style.backgroundImage = "url("+ randomimage +")"; 
}
</script>
