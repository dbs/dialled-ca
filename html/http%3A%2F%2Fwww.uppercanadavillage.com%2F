		<script>
			$(function() {
				$('.modal-dialog .close').on({
					click: function() {
						$('.modal-dialog').animate({
							'margin-top': '-80px',
							'opacity': '0'
						}, 250, function() {
							$(this).remove();
							$('.modal-dialog-shield').remove();

							$.cookie('MODALDIALOGHIDDEN', 'true', { path: '/', domain: 'www.uppercanadavillage.com' });
						});
					}
				});
			});
