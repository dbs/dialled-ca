jQuery().ready(function() { 
  window.slideshow_custom_settings.pager = false;
  window.slideshow_custom_settings.controls = true;
  window.slideshow_custom_settings.nextSelector = null;
  window.slideshow_custom_settings.prevSelector = null;
  window.slideshow_custom_settings.autoPlay = true;
  window.slideshow_custom_settings.captions = 0;
  window.slideshow_custom_settings.layout = "no-thumb";
  window.slideshow_custom_settings.mode = "horizontal";
	jQuery(".slider").bxSlider( window.slideshow_custom_settings ); 
});
