<script type="text/javascript">
$(function() {
	$("#loginDialogFront").dialog({
		bgiframe: true,
			autoOpen: false,
			height: 228,
			width: 300,
			modal: true,
	});
});

function loginDialogPost() {
	if (!$('#edit-pass').length) { alert('Unable to perform patron login on this page.'); }
	$('#edit-pass').val($('#dlg2-edit-pass').val());
	$('#edit-name').val($('#dlg2-edit-name').val());
	$('#user-login-form').submit();
}
</script>

