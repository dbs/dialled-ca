        function searchPrimo() {
            var searchText = document.getElementById("primoQueryTemp").value;
            if (searchText.length > 0) {
                document.getElementById("searchTerms").value = searchText;
                document.forms["searchbox"].submit();
            }//if
        }

        function searchPrimoReserves() {
            document.getElementById("primoQueryReserves").value = "any,contains," + document.getElementById("primoQueryReservesTemp").value;
            document.forms["primoSearchReservesForm"].submit();
        }
        function searchPrimoCatalogue() {
            document.getElementById("primoQueryCatalogue").value = "any,contains," + document.getElementById("primoQueryCatalogueTemp").value;
            document.forms["primoSearchCatalogueForm"].submit();
        }
        
