<div id="main">
	<script type="text/javascript">
    var originalFontSize = 'normal';
    $(document).ready(function () {
        originalFontSize = $('#content .wrapper').css('font-size')
        //resetFont();
    });
    function resetFont() {
        $('#content .wrapper').css('font-size', originalFontSize);
    }
    function increaseFontSize() {
        var currentFontSize = $('#content .wrapper').css('font-size');
        var currentFontSizeNum = parseFloat(currentFontSize, 10);
        var newFontSize = currentFontSizeNum * 1.2;
        $('#content .wrapper').css('font-size', newFontSize);

        $('div.content p').css('line-height', 1.3);
        return false;
    }
    function decreaseFontSize() {
        var currentFontSize = $('#content .wrapper').css('font-size');
        var currentFontSizeNum = parseFloat(currentFontSize, 10);
        var newFontSize = currentFontSizeNum * 0.8;
        $('#content .wrapper').css('font-size', newFontSize);

        return false;
    }
</script>
