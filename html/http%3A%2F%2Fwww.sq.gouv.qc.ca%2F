// Fonction changeRecherche - Sert à changer les informations affichées des recherchés
function changeRecherche(){
	vImg = $("jRechercheImage"); // Modification de l'image
	vImg.src = lstSrcImageRecherches[vNumRechercheCourant];
	vImg.alt = lstAltImageRecherches[vNumRechercheCourant];
	vImg.title = lstTitleImageRecherches[vNumRechercheCourant];

        vLienImgr = document.getElementById("jRechercheLienImg"); // Modification du lien de l'image
	vLienImgr.href = listeUrlPageRecherches[vNumRechercheCourant];

	vLien = $("jRechercheLien"); // Modification de l'url
	vLien.innerHTML = listeNomsRecherches[vNumRechercheCourant]; //Affiche le nom du suspect
	vLien.title = listeNomsRecherches[vNumRechercheCourant];
	vLien.href = listeUrlPageRecherches[vNumRechercheCourant];

	vResume = $("jRechercheResume"); // Modification du Resume (Teaser)
        vResume.innerHTML = listeResumeRecherches[vNumRechercheCourant];
        vResume.title = listeNomsRecherches[vNumRechercheCourant];
//      vResume.href = listeUrlPageRecherches[vNumRechercheCourant];

        vPlus = $("jRecherchePlus"); // Modification du Lien +
        vPlus.title = listeNomsRecherches[vNumRechercheCourant];
        vPlus.href = listeUrlPageRecherches[vNumRechercheCourant];
        
        vNum = $("jRechercheNumero"); // Modification du numéro
        vNum.innerHTML = vNumRechercheCourant+1 + " de 10 sur " + nombreRecherches;


}

//Fonction rechercheSuivant - Sert à passer au prochain recherché
function rechercheSuivant(){
	if (vNumRechercheCourant < vNbrePersonneMax) { vNumRechercheCourant++; }
	else {vNumRechercheCourant = 0;};
	changeRecherche();
}


//Fonction recherchePrecedent - Sert à passer au recherché précédent
function recherchePrecedent(){
	if (vNumRechercheCourant == 0) { vNumRechercheCourant = vNbrePersonneMax; }
	else {vNumRechercheCourant--;};
	changeRecherche();
}
