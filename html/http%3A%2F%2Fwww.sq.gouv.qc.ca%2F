var vNbrePersonneMax = 9; //Nombre de personnes dans les Array - 1

//************* DISPARUS *************
var vNumDisparuCourant = 0;	//Index dans le tableau du disparu affiché
var nombreDisparus = 91; //Nombre total de disparus sur le site de la SQ
var lstSrcImageDisparus = new Array("/personnes-disparues/frederic-tremblay_min.jpg", "/personnes-disparues/marie-claude-hebert_min.jpg", "/personnes-disparues/andre-laflamme_min.jpg", "/personnes-disparues/jean-claude-legresley_min.jpg", "/personnes-disparues/hector-manuel-garcia-tupas_min.jpg", "/personnes-disparues/shannon-alexander_min.jpg", "/personnes-disparues/maisy-odjick_min.jpg", "/personnes-disparues/benoit-desrosiers_min.jpg", "/personnes-disparues/marc-verville_min.jpg", "/personnes-disparues/richard-desnoyers_min.jpg"); //URLs des miniatures des disparus
var lstAltImageDisparus = new Array("photo Frédéric Tremblay", "Photo de Marie-Claude Hébert", "photo André Laflamme", "Jean-Claude Legresley", "Hector Manuel Garcia Tupas", "Shannon Alexander", "Maisy Odjick", "Benoit Desrosiers", "Marc Verville", "photo Richard Desnoyers"); //ALTs des disparus
var lstTitleImageDisparus = new Array("photo nom personne", "Photo de Marie-Claude Hébert", "photo André Laflamme", "Jean-Claude Legresley", "Hector Manuel Garcia Tupas", "Shannon Alexander", "Maisy Odjick", "Benoit Desrosiers", "Marc Verville", "photo Richard Desnoyers"); //TITLEs des disparus
var listeNomsDisparus = new Array("Tremblay, Frédéric", "Hébert, Marie-Claude", "Laflamme,André", "Legresley, Jean-Claude", "Garcia Tupas, Hector Manuel", "Alexander, Shannon", "Odjick, Maisy", "Desrosiers, Benoit", "Verville, Marc", "Desnoyers, Richard"); //Noms des disparus
var listeUrlPageDisparus = new Array("/personnes-disparues/frederic-tremblay.jsp", "/personnes-disparues/marie-claude-hebert.jsp", "/personnes-disparues/andre-laflamme.jsp", "/personnes-disparues/jean-claude-legresley.jsp", "/personnes-disparues/hector-manuel-garcia-tupas.jsp", "/personnes-disparues/shannon-alexander.jsp", "/personnes-disparues/maisy-odjick.jsp", "/personnes-disparues/benoit-desrosiers.jsp", "/personnes-disparues/marc-verville.jsp", "/personnes-disparues/richard-desnoyers.jsp"); //URLs des pages des disparus
var listeResumeDisparus = new Array("Demande d'aide pour retrouver Frédéric Tremblay, 43 ans.", "Marie-Claude Hébert, de Plessisville, disparue depuis le 18 août.", "La Sûreté du Québec demande l’aide de la population afin de retrouver André Laflamme", "La SQ demande l'aide du public pour retrouver Jean-Claude Legresley", "Disparition de Hector Manuel Garcia Tupas", "La Sûreté du Québec fait de nouveau appel à la population pour Shannon Alexander, de Maniwaki.", "La Sûreté du Québec fait de nouveau appel à la population pour Maisy Odjick, de Maniwaki.", "35 ans, disparu de Notre-Dame-des-Prairies", "La SQ fait de nouveau appel à la population dans le but de retrouver Marc Verville.", "La Sûreté du Québec sollicite l’aide de la population afin de retrouver un homme porté disparu"); //Resume des Disparus 
// alert(listeResumeDisparus);
// Fonction changeDisparu - Sert à changer les informations affichées des disparus
function changeDisparu(){
	vImg = $("jDisparuImage"); // Modification de l'image
	vImg.src = lstSrcImageDisparus[vNumDisparuCourant];
	vImg.alt = lstAltImageDisparus[vNumDisparuCourant];
	vImg.title = lstTitleImageDisparus[vNumDisparuCourant];
	vLienImg = document.getElementById("jDisparuLienImg"); // Modification du lien de l'image
	vLienImg.href = listeUrlPageDisparus[vNumDisparuCourant];

	vLien = $("jDisparuLien"); // Modification de l'url
	vLien.innerHTML = listeNomsDisparus[vNumDisparuCourant];
	vLien.title = listeNomsDisparus[vNumDisparuCourant];
	vLien.href = listeUrlPageDisparus[vNumDisparuCourant];

	vResume = $("jDisparuResume"); // Modification du Resume (Teaser)
        vResume.innerHTML = listeResumeDisparus[vNumDisparuCourant];
        vResume.title = listeNomsDisparus[vNumDisparuCourant];
        vResume.href = listeUrlPageDisparus[vNumDisparuCourant];
        
        vPlus = $("jDisparuPlus"); // Modification du lien +
        vPlus.title = listeNomsDisparus[vNumDisparuCourant];
        vPlus.href = listeUrlPageDisparus[vNumDisparuCourant];
        
	vNum = $("jDisparuNumero"); // Modification du numéro
	vNum.innerHTML = vNumDisparuCourant+1 + " de 10 sur " + nombreDisparus;
}

// Fonction disparuSuivant - Sert à passer au prochain disparu
function disparuSuivant(){
	if (vNumDisparuCourant < vNbrePersonneMax) { vNumDisparuCourant++; }
	else {vNumDisparuCourant = 0;};
	changeDisparu();
}

// Fonction disparuPrecedent - Sert à passer au disparu précédent
function disparuPrecedent(){
	if (vNumDisparuCourant == 0) { vNumDisparuCourant = vNbrePersonneMax; }
	else {vNumDisparuCourant--;};
	changeDisparu();
}

//************* RECHERCHES *************
