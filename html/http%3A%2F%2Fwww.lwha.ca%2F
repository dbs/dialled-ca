<script language="javascript" type="text/javascript">

$(window).load(function() {
    
    checkFontCookie();
});


function changeButton(image, imageName, onOff, lang){
	if(onOff == 'Off'){
		image.src = "../imgs/" + imageName + lang + ".gif";
	}else{
		image.src = "../imgs/" + imageName + "_over" + lang + ".gif";
	}
}

var strSmallFont = "on";
var strMediumFont = "off";
var strLargeFont = "off";
var bChangeImage = false;    
function changeFont(title) {
  
    var i, a, main;
    for (i = 0; (a = document.getElementsByTagName("link")[i]); i++) {
        if (a.getAttribute("rel").indexOf("style") != -1
        && a.getAttribute("title")) {
            a.disabled = true;
            if (a.getAttribute("title") == title) a.disabled = false;            
        }
    }
   
    if (title == 'StylesSmall') {
        strSmallFont = "on";
        strMediumFont = "off";
        strLargeFont = "off";
        

    }
    else if (title == 'StylesMedium') {
        strSmallFont = "off";
        strMediumFont = "on";
        strLargeFont = "off";
    }
    else if (title == 'StylesLarge') {
        strSmallFont = "off";
        strMediumFont = "off";
        strLargeFont = "on";
    }

    try {
        document.getElementById('fontChangeSmall').src = '/imgs/section/text_sm_' + strSmallFont + '.gif';
        document.getElementById('fontChangeMedium').src = '/imgs/section/text_med_' + strMediumFont + '.gif';
        document.getElementById('fontChangeLarge').src = '/imgs/section/text_lg_' + strLargeFont + '.gif';
    } catch (ex) {
        bChangeImage = true;
    }
    setFontCookie(title);


}

function printPage(){
	document.forms.item(0).page_data.value = document.getElementById("defaultMain").innerHTML.replace(/<!--START_REM-->(.|\r\n|\r|\n)*?<!--END_REM-->/gi,"");
	document.forms.item(0).printBreadCrumb.value = document.getElementById("sectionLeftBreadcrumb").innerHTML.replace(/<!--START_REM-->(.|\r\n|\r|\n)*?<!--END_REM-->/gi,"");
	document.forms.item(0).target='_blank';
	document.forms.item(0).action='/print.aspx';
	document.forms.item(0).submit();
	
	document.forms.item(0).target = "";
	document.forms.item(0).action = "";
	document.forms.item(0).page_data.value = "";
	document.forms.item(0).printBreadCrumb.value = "";
}
function emailpopUp(URL) {
  window.open(URL, 'Email', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width=500,height=325');
}

function clickButton(e, buttonid) {
    var evt = e ? e : window.event;
    var bt = document.getElementById(buttonid);
    if (bt) {
        if (evt.keyCode == 13) {
            bt.click();
            return false;
        }
    }
}



function setFontCookie(value) {
    var exdate = new Date();
    exdate.setDate(exdate.getDate() + 100);
    document.cookie = "fontstyleguide=" + escape(value) + ";expires=" + exdate.toGMTString();
}
function getFontCookie(c_name) {
    if (document.cookie.length > 0) {
        c_start = document.cookie.indexOf("fontstyleguide=");
        if (c_start != -1) {
            c_start = c_start + c_name.length + 1;
            c_end = document.cookie.indexOf(";", c_start);
            if (c_end == -1) c_end = document.cookie.length;
            return unescape(document.cookie.substring(c_start, c_end));
        }
    }
    return "";
}
function checkFontCookie() {
    var theFont;
    theFont = getFontCookie('fontstyleguide');

    if (theFont != null && theFont != "") {
        changeFont(theFont);
    }
    else {
        changeFont('StylesSmall');
    }
}
    
