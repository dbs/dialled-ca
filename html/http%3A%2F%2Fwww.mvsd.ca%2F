<script type="text/javascript">
	//this is used to show events and info alert popups. it calls the popupinfo page and loads that in an iframe.
	function modalPopup(srcUrl){
		$.modal('<div style="width:640px; height:480px;"><iframe src="' + srcUrl + '" width="100%" height="100%" style="border:0; margin:0;"></div>',{
			onOpen: function (dialog) {dialog.overlay.fadeIn('fast', function () {dialog.data.hide();dialog.container.fadeIn('normal', function () {dialog.data.slideDown('fast');});});},
			onClose: function (dialog) {dialog.data.slideUp('fast', function () {dialog.container.fadeOut('normal', function () {dialog.overlay.fadeOut('fast', function () {$.modal.close();});});});},
			containerCss:{background:"#ffffff",padding:"0"}			  				  
		});//end $.modal()
	}//end modalPopup
	
	//this should only ever be called from the vodPreview file
	function modalVideo(vidToPlay){
		//log the hit using playCount - 7/18/14
		videoHit(vidToPlay);	
		var flashvars = {};
		flashvars.colorVar1 = "071e11";
		flashvars.textVar1 = vidToPlay;
		flashvars.textVar2 = "rtmp://videos.schoolworld.com/vod/sw/";
		
						
	
		$('#sw-modalVod').modal({
			onOpen: function (dialog) {dialog.overlay.fadeIn('normal', function () {dialog.data.hide();dialog.container.fadeIn('normal', function () {
				dialog.data.show();
				
				swfobject.embedSWF("/myicons/pix/vod/videoPlayer.swf", "sw-vodPopup", "640", "360", "9", false, flashvars, params={wmode:"transparent",allowscriptaccess:"always",allowfullscreen:"true"}, attributes={});
			});});},
			onClose: function (dialog) {dialog.data.hide(); dialog.container.fadeOut('normal', function () {dialog.overlay.fadeOut('normal', function () {$.modal.close();});});},
			containerCss:{width:"640px",height:"360px"},
			opacity:"80"
		});//end $('#sw-modalVod').modal()
	}//end modalVideo
	
	function modalGrowl(growlText,growlType){
		
			
	   $.modal('<p class="sw-'+growlType+'Info">'+growlText+'</p>',{
			modal:false,
			position:[25,'78%'],
			onOpen:function(dialog){  dialog.overlay.show(1, function(){  dialog.container.show(1, function(){  dialog.data.show(1); var t=setTimeout("$.modal.close()",1200);  });  });  },
	
			onClose: function (dialog){
				dialog.container.fadeOut('slow', function(){  $.modal.close();  });
			},
			
			escClose:false,
			containerCss:{background:"none",padding:"0", border:'none', width:"20%"}			  				  
		});//end $.modal()
		
		

	}//end modalMsg
	
</script>


<script>	
//adding the ability to record hits - 7/18/14 MPR - simply adds date and time of video played
 function videoHit(hpVideo)
		{$.ajax({url: "videos/playCount.cfm?hpVideo=" + hpVideo})}	
</script>



<script type="text/javascript">
	var curActive = 1;
	
	$(document).ready(function(){
		switchTabs('1');
	}); 
	
	//accepts a number, and changes the current tab to be that number
	function switchTabs(whichTab){
		if(curActive != whichTab){
			$('#sw-tabBar').css("backgroundImage", 'url(pix/tab'+whichTab+'Active.png)');
			$('#sw-tab'+curActive+'Content').slideUp( function(){ $('#sw-tab'+whichTab+'Content').slideDown( function(){ $('#sw-tab'+whichTab+'Content').css("minHeight", "1%"); try{resize();}catch(err){} }); });
			curActive = whichTab;
		}
	}

</script>

        
        <script>
			function submitSearch(){
				if(document.searchForm.search.value != "" && document.searchForm.search.value != "Search Our Site"){
					document.searchForm.submitBtn.disabled = true;	
					document.searchForm.submit();
				}				
			}
		</script>
        

        



