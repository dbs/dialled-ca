    <div class="headingWrap">
        <h2>Find a Teacher</h2>
        <label for="ctl03_TextBoxFAT"><em>Search our Public Register</em></label>
    </div>
    
    <div id="ctl03_Panel1" class="form" onkeypress="javascript:return WebForm_FireDefaultButton(event, &#39;ctl03_ButtonFAT&#39;)">
	
        <input name="ctl03$TextBoxFAT" type="text" id="ctl03_TextBoxFAT" placeholder="E.g. &quot;B Smith&quot; or 907654" title="Find a Teacher" />
     

        <input type="submit" name="ctl03$ButtonFAT" value="Find" id="ctl03_ButtonFAT" class="btnStyle" title="Find" />
    
