						<script type="text/javascript">
var laDate = new Date();
var semaineArray = new Array("Dimanche","Lundi","Mardi","Mercredi","Jeudi","Vendredi","Samedi");
var semaine = semaineArray[laDate.getDay()];
var leJour = laDate.getDate();
var leMoisArray = new Array("janvier","février","mars","avril","mai","juin","juillet","août","septembre","octobre","novembre","décembre");
var leMois = leMoisArray[laDate.getMonth()];
var annee = laDate.getFullYear();
var fullDate = semaine+"&nbsp;"+leJour+"&nbsp;"+leMois+"&nbsp;"+annee
document.write("<div class='dateDuJour'>"+fullDate+"</div>");
</script>            
