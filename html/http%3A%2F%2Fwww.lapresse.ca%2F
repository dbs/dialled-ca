                    <script type="text/javascript">//<![CDATA[
                        function getNextURL() {
                            var paramsNextURL = '';
                            if (document.getElementById("nextAction")) {
                                var re = new RegExp(/\?/);
                                if (document.location.href.match(re)) {
                                    var delimiter = "&";
                                } else {
                                    var delimiter = "?"
                                }

                                var nextAction = document.getElementById("nextAction");
                                var id = document.getElementById("id");
                                var paramsNextURL = delimiter + 'action=' + nextAction.value + '&id=' + id.value + '&nextUrl=' + document.location.href;
                            }
                            return document.location.href + paramsNextURL;
                        }
                        $(document).ready(function() {
                            $('.fb-connect').click(function() {
                                FB.login(function(response) { if (response.authResponse) {
                                    document.location.href = 'http://usager.lapresse.ca/Login/FacebookTokenAutologin?access_token=' + response.authResponse.accessToken + '&nextUrl=' + escape(getNextURL());
                                }}, {scope:'email'});
                                return false;
                            });
                        });
                        //]]></script>
		           
