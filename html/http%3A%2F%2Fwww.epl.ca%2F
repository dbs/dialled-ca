      <li class="dropdown_trigger"  tabindex="0" data-selected="fiction_carousel"><span class="current_val">New Fiction</span><span class="down_chevron"></span>
        <ul class="dropdown_menu" style="display: none;">
                      <li data-value="fiction_carousel">New Fiction</li>
                      <li data-value="nonfiction_carousel">New Non-Fiction</li>
                      <li data-value="kids_carousel">New For Kids</li>
                      <li data-value="teens_carousel">New For Teens</li>
