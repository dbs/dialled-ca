  <div id="fiction_carousel_loader"></div>
  <div id="nonfiction_carousel_loader"></div>
  <div id="kids_carousel_loader"></div>
  <div id="teens_carousel_loader"></div>
  <div id="movies_carousel_loader"></div>


<script type="text/javascript">
  $(window).load(function() {
    setTimeout(function() {
      $.each(["fiction","nonfiction","kids","teens","movies"], function( index, key ) {
        var url = 'https://epl.bibliocommons.com/widgets/multicarousel_widget/'+key+'?no_sdk=true&bghex=%23ffffff';
        if (key == 'movies') {
          url = 'https://epl.bibliocommons.com/widgets/new_titles_by_format/movie?no_sdk=true&bghex=%23ffffff';
        }
        if (key == 'book' || key == 'ebook') {
          url = 'https://epl.bibliocommons.com/widgets/new_titles_by_format/' + key + '?no_sdk=true&bghex=%23ffffff';
        }
        if (key == 'dvds') {
          url =  'https://epl.bibliocommons.com/widgets/multicarousel_widget?no_sdk=true&bghex=%23ffffff&superformat=MOVIES_TV_VIDEO';
        }
                $('<iframe />', {
          src: url,
          style: ' min-width: 100%; width: 100px; height: 250px;',
          title: 'new in ' + key + ' carousel',
          scrolling: 'no',
          id: key+'_carousel',
          class:'carousel_widget'
        }).replaceAll('#'+key+'_carousel_loader');
      });
      $('.channel_carousel_widget_block').slideDown(300, function() {
      });
    }, 300);
  });
</script>      </div>

        <div  id="bc_cms_block_3"  class="block-3 channel_extra_small_block  analytics_index_4">
      <a href="channel/epl-picks"
         class="channel-name stripe-three">EPL Picks</a>
        <div class="channel_carousel">

          <div class="ch-carousel-arrow ch-carousel-arrow-left"><i class="icon-angle-left"></i></div>
