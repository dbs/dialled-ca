<script type="text/javascript">
  $(window).load(function() {
    setTimeout(function() {
      $.each( ['fiction', 'nonfiction', 'kids', 'movies', 'teens'], function( index, key ) {
        var url = 'https://epl.bibliocommons.com/widgets/multicarousel_widget/'+key+'?no_sdk=true&bghex=%23ffffff';
        if(key == 'movies') {
          var url = 'https://epl.bibliocommons.com/widgets/new_titles_by_format/movie?no_sdk=true&bghex=%23ffffff';
        }
        $('<iframe />', {
          src: url,
          style: ' min-width: 100%; width: 100px; height: 250px;',
          scrolling: 'no',
          id: key+'_carousel',
          class:'carousel_widget'
        }).replaceAll('#'+key+'_carousel_loader');
      });
      $('.channel_carousel_widget_block').slideDown(300, function() {

      });
    }, 300);
  });
</script>      </div>

