<script type="text/javascript">
    (function ($) {
        var searchTitle = 'Search';

        var searchTxt = '#globalmasthead_0_searchTextBox';
        var searchBtn = '#globalmasthead_0_searchButton';

        $(searchTxt)
            .blur(function () {
                if (this.value == '') { this.value = searchTitle; }
            })
            .focus(function () {
                if (this.value == searchTitle) { this.value = ''; }
            })
            .keypress(function (e) {
                if (e.which == 13) {
                    $(searchBtn).click();
                    return false;
                }
            });

        $(searchBtn).click(function(e) {
            e.preventDefault();

            var searchTerms = encodeURIComponent($(searchTxt).val());
            window.location = '/en/sk/Search?q=' + searchTerms;
        });


    })(jQuery);
</script>

<script type="text/javascript">



    (function ($) {
        var $url = "";
        $("#location_change_en a").each(function (index) {
            // do not do anything with links that are opening in new window, or do postback (and are not "Sign Up" button), and are not help tooltip links
            if ($(this).attr("target").length == 0 && ($(this).attr("href"))) {
                $(this).bind(
                    "click",
                    function (event) {
                        //event.preventDefault();
                        // get the URL of the clicked link
                        $url = $(this).attr("href");
                        setCookie("splashlinkclicked", $url, 365);

                        // return false;
                    });
            }
        });
    })(jQuery);


    (function ($) {
        var $url = "";
        $("#location_change_fr a").each(function (index) {
            // do not do anything with links that are opening in new window, or do postback (and are not "Sign Up" button), and are not help tooltip links
            if ($(this).attr("target").length == 0 && ($(this).attr("href"))) {
                $(this).bind(
                    "click",
                    function (event) {
                        //event.preventDefault();
                        // get the URL of the clicked link
                        $url = $(this).attr("href");
                        setCookie("splashlinkclicked", $url, 365);

                        // return false;
                    });
            }
        });
    })(jQuery);
</script>



