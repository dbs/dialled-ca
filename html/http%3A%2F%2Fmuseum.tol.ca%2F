<script language="javascript" type="text/javascript">

    $(document).ready(function () {

        function fbCall(div, ev) {

            var fbId = '#' + div.id.replace("im_", "").toString();

            $.fancybox({
                'href': fbId,
                'titlePosition': 'over',
                'transitionIn': 'elastic',
                'transitionOut': 'none',
                'overlayColor': '#000',
                'overlayShow': true,
                'overlayOpacity': 0.52,
                'titleFormat': formatTitle2,
                'onComplete': function () {
                    // Change some default formating for the grid
                    $("#fancybox-title").css({ 'left': '-10px', 'top': '0px', 'bottom': 'auto' });
                    $("#fancybox-close").css({ 'top': '20px', 'right': '8px' });
                }
            });

            // Grid - apply 'tol_fb_title' css to fancybox title
            function formatTitle2(title, currentArray, currentIndex, currentOpts) {
                // get title
                for (i = 0; i < div.childNodes.length; i++) {
                    if (div.childNodes[i].nodeName == "A")
                        title = div.childNodes[i].title;
                }
                return '<div class="tol_fb_title">' +
                    '<img src="' + $(fbId + '_image').attr("src") + '"/>' +
                    (title && title.length ? '<b>' + title + '</b>' : '') + '</div>';
            }
        }

        // use to find coordinates
        //$('#tol_im_grid_quick').imagecoords();

        $('#tol_im_grid_quick').imagemap([
            { top_x: 153, top_y: 61, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_about').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_news').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_services').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_exhibits').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_collections').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_research').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_gift').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
        $('#tol_im_grid_places').imagemap([
            { top_x: 153, top_y: 126, bottom_x: 170, bottom_y: 143, callback: fbCall }
        ]);
    });
</script>

