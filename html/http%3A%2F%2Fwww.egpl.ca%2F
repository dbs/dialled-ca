    <script type="text/javascript">

                var ws = $('website_search');
                var cs = $('catalogue_search');
                function catalogue_search_cb(){

                    ws.style.backgroundImage='url(/templates/egpl/css/../img/search_top_green.png)';
                    cs.style.backgroundImage='url(/templates/egpl/css/../img/search_top_blue.png)';

                    $('search_form').style.display='none';
                    $('search_cat').style.display='';

                    return false;
                }
                function website_search_cb(){

                    cs.style.backgroundImage='url(/templates/egpl/css/../img/search_top_green.png)';
                    ws.style.backgroundImage='url(/templates/egpl/css/../img/search_top_blue.png)';
                    $('search_form').style.display='';
                    $('search_cat').style.display='none';

                    return false;

                }



        $('catalogue_search').onclick=catalogue_search_cb;
        $('website_search').onclick=website_search_cb;
        //$('search_form').style.display='none';
    </script>

