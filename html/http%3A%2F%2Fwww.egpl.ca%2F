<script type="text/javascript">




Array.each($$('ul.nav.sf-menu > li'),function(el,index){


    el.addEvent('mouseenter',function(){

        var par = el.getParent().getPosition();
        var me = el.getPosition();
        var bg = el.getParent().getStyle('background-image');
        var pcoor = el.getParent().getCoordinates();
        el.setStyle('background-image',bg);

        el.setStyle('background-position', (-(pcoor.width + me.x - par.x)) + 'px ' + (-(pcoor.height + me.y - par.y)) + 'px');

    });
    el.addEvent('mouseleave',function(){
        el.setStyle('background-image','none');
    });


});

</script>

