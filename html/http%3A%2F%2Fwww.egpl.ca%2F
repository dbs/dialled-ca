        <script type="text/javascript">
            var myFx;
            var curFx;
            Array.each($ES('.ucd_item','row2'),function(el,index){
                $E('.ucd_title',el).setStyle('display', 'none');



                if (index) {
                    myFx = new Fx.Slide(el);
                    myFx.hide();
                    //$(el).setStyle('display', 'none');
                } else {
                    curFx= new Fx.Slide(el);
                }

            });

            $("branchswitchLink").addEvent('click',function(){
            	alert('clicked');
                curFx.toggle().chain(function(){
                    myFx.toggle().chain(function(){
                        var tmp;
                        tmp = curFx;
                        curFx= myFx;
                        myFx = tmp;
                    });
                });
            });
		</script>



