function init_hsrc() {
  if (!document.getElementById) return
  var imgOriginSrc;
  var MouseOverHappened;
  var imgTemp = new Array();
  var imgarr = document.getElementsByTagName('img');
  for (var i = 0; i < imgarr.length; i++) {
	if (imgarr[i].getAttribute('hsrc')) {
		imgTemp[i] = new Image();
		imgTemp[i].src = imgarr[i].getAttribute('hsrc');
		imgarr[i].onmouseover = function() {
			imgOriginSrc = this.getAttribute('src');			
			this.setAttribute('src',this.getAttribute('hsrc'));
			MouseOverHappened=true;
		}
		imgarr[i].onmouseout = function() {
			if(MouseOverHappened)
				this.setAttribute('src',imgOriginSrc);
		}
	}
  }
}

function init_custom_tags(){
	init_status();
	init_status_td();
	init_status_tr();
	init_status_div();
	init_hsrc();
}
onload=init_custom_tags;

	function ChangeLanguage(lang)
	{
		var _url = String(window.location);
		var _l = _url.length;
		var _x = _url.indexOf("?");
		var _i
		
		if(_x < 0) 
			_i="page=0"
		else
			_i = _url.substring(_x+1,_l);	
								
		var languageParam = _i.indexOf("&language=");		
		
		if(languageParam>0) _i= _i.substring(0,languageParam);			

		var topAnchor = _i.indexOf("#top");
		if(topAnchor>0) _i= _i.substring(0,topAnchor);			
		window.location = "index.asp?" + _i + "&language=" + lang;
	}



	// Les 2 fonctions suivantes activent/dÃ©sactivent un textbox selon sa case Ã  cocher associÃ©e.
	// Par exemple, si on veut activer/desactiver un textbox selon une case Ã  cocher, on doit
	// dÃ©finir un checkbox avec le nom "chkNomQuelconque". Le textbox associÃ© doit avoir le 
	// meme nom que le checkbox mais avec le prÃ©fixe "txt" au lieu de "chk"
	
	function InitialiserChampsTexte()
	{
		var i;	
		var oInputCheck;
		var oInputText;
		var textboxname;
						
		for (i=0;i<=document.getElementsByTagName('input').length-1;i++)
      {
      oInputCheck = document.getElementsByTagName('input').item(i)
      if (oInputCheck.type == "checkbox")
      {
      // est-ce qu'il y a un textbox associÃ©?
      textboxname = oInputCheck.name;
      textboxname = "txt" + textboxname.substr(3,textboxname.length);
      oInputText = document.getElementsByName(textboxname);

      if(oInputText(0)!=null)
      {
      oInputCheck.onclick = RendreDispoChampTexte;
      if(!oInputCheck.checked)
      {
      oInputText(0).disabled = true;
      oInputText(0).value = "";
      }

      if(bPermettreModifBGInput)
      {
      oInputText(0).style.backgroundImage = StyleDisabled;
      }
      }
      }
      }
      }

      // **********************************************

      function RendreDispoChampTexte()
      {
      textboxname = this.name;
      textboxname = "txt" + textboxname.substr(3,textboxname.length);
      var oInputText;
      oInputText = document.getElementsByName(textboxname);

      if (this.checked == true)
      {
      oInputText(0).disabled 	= false;
      oInputText(0).focus();
      if (bPermettreModifBGInput)
      {
      oInputText(0).style.backgroundImage = StyleEnabled;
      }
      }
      else
      {
      oInputText(0).disabled 	= true;
      oInputText(0).value	= "";
      if (bPermettreModifBGInput)
      {
      oInputText(0).style.backgroundImage = StyleDisabled;
      }
      }
      }

      function OpenPopupCenter(aPage,aName,aWidth,aHeight)
      {
      var LeftPos = screen.availWidth/2-(aWidth/2);
      var TopPos  = screen.availHeight/2-(aHeight/2);
      window.open(aPage,aName,"status=no,location=no,scrollbars=yes,resize=no,directories=no,toolbar=no,menubar=no,copyhistory=no,width=" + aWidth + ",height=" + aHeight + ",left=" + LeftPos + ",top=" + TopPos);return false;
      }
      function OpenPopupCenter2(aPage,aName,aWidth,aHeight)
      {
      var LeftPos = screen.availWidth/2-(aWidth/2);
      var TopPos  = screen.availHeight/2-(aHeight/2);
      window.open(aPage,aName,"status=no,location=no,scrollbars=no,resize=no,directories=no,toolbar=no,menubar=no,copyhistory=no,width=" + aWidth + ",height=" + aHeight + ",left=" + LeftPos + ",top=" + TopPos);return false;
      }


