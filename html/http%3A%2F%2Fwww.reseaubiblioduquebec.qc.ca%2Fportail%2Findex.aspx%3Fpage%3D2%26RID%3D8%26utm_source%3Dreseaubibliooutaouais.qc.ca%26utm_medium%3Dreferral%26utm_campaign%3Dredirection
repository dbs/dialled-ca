									    	            <script language="JavaScript">

  //********************************************************************************************************************
  function lstResultatsKeyCode(target){
  	var keyCode;
  	if (window.event != null) {
    	keyCode=window.event.keyCode;
		if (keyCode == 13){
			document.getElementById(target).click();
		}
  	}
  }

  function autoJumpKeyDown(aTB){
	document.getElementById(aTB).lastValue = document.getElementById(aTB).value;
	return true;
  }

  function autoJumpKeyUp(initialTB,targetTB,MaxLength){
	var tb1;
	var tb2;		
	tb1=document.getElementById(initialTB);
	tb2=document.getElementById(targetTB) 
	if(tb1.value.length == MaxLength && tb1.value!=tb1.lastValue && tb1.lastValue!=null)
	{
		tb2.focus();
		tb1.lastValue=tb1.value;
  		tb1.value = tb1.value.toUpperCase();
	}
	return true;
  }

</script>


<script language="JavaScript">
  function chkSelection(){  	
  	var lstOption = document.getElementById('lstResultats')
  	if (lstOption.selectedIndex>=0){
  		if (lstOption.value!="-1"){
  			return true;
  		}
  	}
  	alert('Vous devez sÃ©lectionner une bibliothÃ¨que.');
  	return false;
  }

  function lstSelection(){
    if (chkSelection()){
		document.getElementById('centerRepeater_ctl00_columnHolder_ctl00_bValider').click();
	}
  }

  function lstKeyCode(){
  	var keyCode;
  	if (window.event != null) {
    	keyCode=window.event.keyCode;
		if (keyCode == 13){
			lstSelection();
		}
  	}
  }
</script>

<input name="centerRepeater$ctl00$columnHolder$ctl00$NbRows" type="hidden" id="centerRepeater_ctl00_columnHolder_ctl00_NbRows" value="6" />


									                
