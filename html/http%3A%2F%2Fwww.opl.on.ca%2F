 <script src="./assets/script/biblio/base_external.js" type="text/javascript" charset="utf-8"></script>
	<div id="main">
	
    <div id="left">
          <script type="text/javascript">
	window.addEvent('domready', function() {
		Element.implement({
			show: function() {
				this.setStyle('display','');
			},
			hide: function() {
				this.setStyle('display','none');
			}
		});
		hideAll();
	});

    function hideAll() {
		$('hrsWidgContact').removeClass('highlight');
		$('hrsWidgLoc').removeClass('highlight');
		$('hrsWidgHours').removeClass('highlight');

		$('contact').hide();
		$('location').hide();
		$('hours').hide();
	}
	function showHours() {
		$('hrsWidgContact').removeClass('highlight');
		$('hrsWidgLoc').removeClass('highlight');
		$('hrsWidgHours').addClass('highlight');

		$('contact').hide();
		$('location').hide();
		$('hours').show();
	}
	function showContact() {
		$('hrsWidgHours').removeClass('highlight');
		$('hrsWidgLoc').removeClass('highlight');
		$('hrsWidgContact').addClass('highlight');
		
		$('contact').show();
		$('hours').hide();
		$('location').hide();
	}
	function showLocation() {
		$('hrsWidgHours').removeClass('highlight');
		$('hrsWidgContact').removeClass('highlight');
		$('hrsWidgLoc').addClass('highlight');

		$('contact').hide();
		$('hours').hide();
		$('location').show();
	}

	function getBranchInfo() {
		//new Event(event).stop();
		var req = new Request({
			method: 'get',
			url: './assets/widget/opl/branches/getInfo.php',
			data: { 'branch': $('fldBranch').get('value')},
			onFailure: function() {alert('Failure reported while loading branch hours.');},
			onException: function(headerName, value) {alert('Exception reported while loading branch hours at: ' + headerName);},
			//onComplete: function(response) {alert('yeeee');},
			onSuccess: function(txtResponse, xmlResponse) {
				xml = xmlResponse.getElementsByTagName("branch_info");
				hrs = xml[0].getElementsByTagName("hours");

				//if the xml doesn't have times for everyday of the week, set every day to closed, and update the days that we ARE open
				if(hrs.length < 7)
					$('branchInfo').getElements('table#hours tbody tr td').set('html','Closed');
				
				for(x=0;x<hrs.length;x++) {
					strRowName = (hrs[x].getElementsByTagName("day")[0].childNodes[0].nodeValue.toLowerCase() + 'Hours');
					$('hours').getElements('tr#' + strRowName + ' td').set('html',hrs[x].getElementsByTagName("open")[0].childNodes[0].nodeValue + ' - ' + hrs[x].getElementsByTagName("close")[0].childNodes[0].nodeValue);
				}
				
				address = xml[0].getElementsByTagName("address");
				$('branchPhone').getElements('td').set('html',address[0].getElementsByTagName("phone_num")[0].childNodes[0].nodeValue);
				$('branchFax').getElements('td').set('html',address[0].getElementsByTagName("fax_num")[0].childNodes[0].nodeValue);
				$('location').getElements('p').set('html',
					address[0].getElementsByTagName("street_num")[0].childNodes[0].nodeValue + ' ' +
					address[0].getElementsByTagName("street_name")[0].childNodes[0].nodeValue + '<br>' +
					address[0].getElementsByTagName("town")[0].childNodes[0].nodeValue + ', ' +
					address[0].getElementsByTagName("postal")[0].childNodes[0].nodeValue
				);
				$('location').getElements('a').set('href',
					'http://maps.google.ca/maps?f=q&hl=en&geocode=&q=' +
					address[0].getElementsByTagName("street_num")[0].childNodes[0].nodeValue + '+' +
					address[0].getElementsByTagName("street_name")[0].childNodes[0].nodeValue.replace(/ /g,'+') + ',+' +
					address[0].getElementsByTagName("town")[0].childNodes[0].nodeValue.replace(' ','+') + '+' + 'ON' +
					'&ie=UTF8iwloc=addr'
				);
				$('moreBranchInfo').getElements('a').set('href', './about/branches?branch='+$('fldBranch').get('value'));
			}
		}).send();
	}
</script>
