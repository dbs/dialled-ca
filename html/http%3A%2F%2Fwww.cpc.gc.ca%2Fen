                                                                                <div class="tabs-panel">
                                                                                    <div class="tabs-content-pad" id="tab1">
                                                                                        <section>
                                                                                            <div class="slider-left">
                                                                                                <div class="text-container">
                                                                                                    <div class="headline"><a href="/en/emdpl">New Executive Master's degree in Police Leadership</a></div>
