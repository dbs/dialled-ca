jQuery(window).bind("load resize",function(){
var $rightSide = jQuery("#rt-sidebar-a").parent(".rt-grid-6");
	$leftSide = jQuery("#rt-content-top").parent(".rt-grid-6");

	if (jQuery("#rt-sidebar-a").css("clear") == "left" ){
		//console.log("move to top");
		$rightSide.insertBefore($leftSide);
		//jQuery("#rt-sidebar-a").parent(".rt-grid-6").insertBefore(".rt-grid-6");
	}
	
	if (jQuery("#rt-sidebar-a").css("clear") == "right" ){
		//console.log("normal");
		$rightSide.insertAfter($leftSide);
		//jQuery("#rt-main .rt-container > .rt-grid-6:nth-child(1)").insertAfter("#rt-main .rt-container > .rt-grid-6:nth-child(2)");
		
		//jQuery("#rt-sidebar-a").parent(".rt-grid-6").insertAfter(".rt-grid-6");
	}
});


