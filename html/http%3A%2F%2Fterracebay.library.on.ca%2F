function reloadImage()
{
if (document.images.hcIcon) {
   document.images.hcIcon.src = 'http://server.lon.liveperson.net/hc/91508229/?cmd=repstate&site=91508229&channel=web&&ver=1&imageUrl=http://terracebay.library.on.ca/wp-content/uploads/2009/09&skill=public?' + 
Math.random(); }
}
{
setInterval('reloadImage()', 20000); // 20 seconds
}

</script>
<script type="text/javascript">

function reloadPageOnNewDate()
{
var currentTime = new Date()
var hours = currentTime.getHours()
var minutes = currentTime.getMinutes()
if (hours == 0 && minutes == 1)
{
history.go();
}
}
{
setInterval('reloadPageOnNewDate()', 60000); // 60 seconds
}
</script>

