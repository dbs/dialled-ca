<script type="text/javascript">
	
$("#search-lawyer").on("focus", function(){
$("#search-lawyer").attr("placeholder", "Type Lawyer First Or Last Name").val("").focus().blur();
});

$("#search-lawyer").keyup(function(){
      var text = $(this).val();
      var inputElement = $(this); 
      if (text.length>0)
      {		    
          var div = $("#search-result");          
          div.load("staffbyname.asp?q=" + text, 
            function(){
              $("#search-result-holder").show(); 
              div.removeClass("hidden"); 
              div.parent().addClass("cs-active");
              div.find("li span").click(function(){
                  div.parent().removeClass("cs-active");
                  inputElement.val($(this).text());
              });
            });        
      } else { 
        $("#search-result-holder").hide();
		  } 
    });     
$("#search-lawyer").focusout(function(){
  window.setTimeout(function(){$("#search-result-holder").hide();}, 500);  
});
$("#search-lawyer").change(function(){
    if($(this).val().length==0) $("#search-result-holder").hide();  
});
</script>


