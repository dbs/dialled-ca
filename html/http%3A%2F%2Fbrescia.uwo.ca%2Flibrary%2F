<script type="text/javascript">// <![CDATA[
function rusearch(){
var url="http://refuniv.odyssi.com/cgi-bin/member/search/r?dbs+ruprime:@and+@syn1+";
var searchInputEl = document.getElementById("ruSearchInput");
if ( searchInputEl.value.indexOf('"') == -1 ) {
  document.location=url + '@w15 + (+' + encodeURIComponent(searchInputEl.value) + '+)';
}
else {
  document.location=url + encodeURIComponent(searchInputEl.value);
}
}
// ]]></script>
<script type="text/javascript">// <![CDATA[
function getKeyValue(e,form){
var key=e.keyCode || e.which;
if (key==13){
rusearch();
}
}
// ]]></script>

<!--Make any style changes you want, but don't change the ids or names-->
<div id="ruSearchBox"><input id="ruSearchInput" class="searchbox_reference" onkeypress="getKeyValue(event,this.form)" value="" type="text" /> <input onclick="rusearch()" value="Search" type="button" /></div>
