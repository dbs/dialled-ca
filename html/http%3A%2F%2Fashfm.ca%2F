<script type="text/javascript">
	
	var slider_range12_values = new Array(
	"	);
	
	jQuery(document).ready(function() {
					var length = -1;
		
				jQuery("#amount12").val(slider_range12_values[0] + " - " + slider_range12_values[length]);
jQuery("input#slider_range12_val").val(slider_range12_values[0] + " - " + slider_range12_values[length]);
				
		jQuery("#slider_range12")[0].slide = null;
		jQuery("#slider_range12").slider({
			values: [ 0, length ],			range: true,
			min: 0,
			max: -1,
			slide: function(event, ui) {
				if(slider_range12_values[ui.values[0]] == "")
					jQuery("#amount12").val("0 - " + slider_range12_values[ui.values[1]]);
				else 
					jQuery("#amount12").val(slider_range12_values[ui.values[0]] + " - " + slider_range12_values[ui.values[1]]);	
				
				if (slider_range12_values[ui.values[1]] == "")
					jQuery("#amount12").val("");
				
				var vals = jQuery("#amount12").val();
				jQuery("input#slider_range12_val").val(vals);
			},
			stop: function( event, ui ) {
											}
		});
	});
	</script>

	<div class="k2filter-field-slider k2filter-field-2">
