									<script type="text/javascript">
	if (typeof jQuery == 'undefined') {
		document.write('<scr'+'ipt type="text/javascript" src="http://ashfm.ca/modules/mod_k2_filter/assets/js/jquery-1.8.2.min.js"></scr'+'ipt>');
