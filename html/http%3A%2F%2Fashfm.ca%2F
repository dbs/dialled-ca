	}
	
	document.write('<scr'+'ipt type="text/javascript" src="http://ashfm.ca/modules/mod_k2_filter/assets/js/jquery.multiselect.js"></scr'+'ipt>');
	document.write('<link type="text/css" href="http://ashfm.ca/modules/mod_k2_filter/assets/js/jquery.multiselect.css" rel="stylesheet" />');
	
	jQuery(document).ready(function() {
		jQuery("#K2FilterBox119 form").submit(function() {
			jQuery(this).find("input, select").each(function() {
				if(jQuery(this).val() == '') {
					jQuery(this).attr("name", "");
				}
			});
		});
			});
	
	function submit_form_119() {
			
		jQuery("#K2FilterBox119 form").submit();
	}
	
</script>

<div id="K2FilterBox119" class="K2FilterBlock">
