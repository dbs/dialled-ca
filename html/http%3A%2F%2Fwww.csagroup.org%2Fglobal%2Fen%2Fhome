<script>
      (function($) {
          $(document).ready(function() {

		   $(".navbar").show();

		   $('.dropdown-toggle').click(function(e) {
		  e.preventDefault();
		  setTimeout($.proxy(function() {
		    if ('ontouchstart' in document.documentElement) {
		      $(this).siblings('.dropdown-backdrop').off().remove();
		    }
		  }, this), 0);
		});


		if(location.search){

				$('#languages-navbar .dropdown-menu a').each(function( index ) {
	  				//	console.log( index + ": " + $( this ).attr('href') );
							var url = $(this).attr('href');

							if(url && url.indexOf('?')===-1)
							$(this).attr('href', url + location.search);
				});
		  }


			applyFlag($);

		});

    })

    (jQuery);

// 	Hover on function
	(function($) {
        $(".dropdown").hover(
            function(){ $(this).addClass('open') },
            function(){ $(this).removeClass('open') }
        );
    })
    (jQuery);

function applyFlag($){

var p = window.location.pathname.split('/');
var region = p[1];


if(p[1].length == 2)
region = p[2];

if(region === 'deutschland')
region = 'germany';

if(region === 'united-states')
region = 'us';

if(region === 'united-kingdom')
region = 'uk';

if(region === 'netherlands' || region === 'nederland')
region = 'netherland';



var c = 'page-template-' + region + '-regional-page';

if(!$('body').hasClass(c))
	$('body').addClass(c);

console.log(region);
}
</script>

