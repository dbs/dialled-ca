<script type="text/javascript">
//renderMapCentre
SLF.Maps.directionMapData.didYouMeanText = "Did you mean:";
SLF.Maps.directionMapData.directionMapError = 'We are temporarily unable to provide a map and/or directions. Please go directly to Google maps at <a href="http://maps.google.ca" target="_blank">maps.google.ca</a> for assistance.';
SLF.Maps.directionMapData.partialMatch = "No results were found. Please make sure all words are spelled correctly when reentering a postal code, city, or address near you into the textbox.";

function _initLocationMap() {
	var mapCenter = new google.maps.LatLng(48.43246, -80.32417);
    var mapOptions = {
		zoom: 12,
		center: mapCenter,
		mapTypeId: google.maps.MapTypeId.ROADMAP
	};
    SLF.Maps.initializeDirectionMap("map", mapOptions);
    var latLongs = [{'mapCenter':mapCenter,'title':'Black River- Matheson Public Library: Ramore Branch','index':0,'channel':"LIBRARY"}];
	SLF.Maps.createMarkers(SLF.ImagesFolderContext, latLongs);    
};

$(function() {
	_initLocationMap();	
});
   
</script>
