<script type="text/javascript">
    // function to create tiles
    function createTiles(category) {
        var tiles = $.grep(allTiles, function (n) { return n.category == category; });

        if (tiles.length > 0) {
            for (var i = 0; i < tiles.length; i++) {
                jQuery("div.items").append("<div><ul><li><a href='" + tiles[i].targeturl + "' class='' onclick=\"saveSelection('" + tiles[i].tile + "');\"><img src='" + tiles[i].imageurl + "' height='55' width='65' alt='" + tiles[i].tile + "' /><p>" + tiles[i].tile + "</p><div class='qm_arrow'></div></a></li></ul></div>");
            }
        }
        createNavis();
    }

    // function to create dots
    function createNavis() {
        var count = jQuery("div.items").children().length;
        for (var i = 1; i < count; i++) {
            if (i == 1 && count > 7)
                jQuery("div.navi").append("<a href='#' class='active'></a>");
            else if (i == 1)
                jQuery("div.navi").append("<span>&nbsp;</span>");
            else if (i % 7 == 0 && count > 7)
                jQuery("div.navi").append("<a href='#'></a>");
        }
    }

    function getSelectionFromCookie() {
        // cookie: vol_selected_tile
        // value: Jobs %3B Tile%203
        var cookieValue = $.cookie('vol_selected_tile');

        if (cookieValue != null) {
            var selection = cookieValue.split(";");

            if (selection[0] != "")
                jQuery("#qmCategory").val(selection[0]);

            if (selection[1] != "") {
                var tiles = jQuery("div.items div ul li a");

                for (var i = 0; i < tiles.length; i++) {

                    //Since some browsers do not support innerText
                    var innText = tiles[i].innerText;
                    if (innText == null) {
                        innText = tiles[i].textContent;
                    }

                    //removing the leading newline chat from FF and chrome
                    innText = innText.replace(/\r\n|\r|\n/g, '');

                    if (innText == selection[1]) {
                        jQuery("div.items div ul li a")[i].className = "current";
                        break;
                    }
                }
            }
        }
    }

    // save selection to cookie
    // cookie format: "category ID; tile name"
    function saveSelection(tileName) {

        var setCookieName = "vol_selected_tile";

        var category = jQuery("#qmCategory").val();
        var value = category + ";" + tileName;

        $.cookie(setCookieName, value, { expires: 1, path: "/" });

    }
</script>
