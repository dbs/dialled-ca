<script type="text/javascript">
    function ReplaceAll(category) {
        while (category.indexOf("'") != -1)
            category = category.replace("'", "*+#");
        while (category.indexOf('"') != -1)
            category = category.replace('"', '');
        while (category.indexOf('\\') != -1)
            category = category.replace('\\', '');

        return category;
    }

    jQuery(document).ready(function () {
        jQuery('.Items').selectbox({ inputClass: "notranslate selectbox", containerClass: "notranslate selectbox-wrapper" });
        // initial
        var category = jQuery("#qmCategory option:selected").text();

        category = ReplaceAll(category);

        createTiles(category);

