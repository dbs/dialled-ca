jQuery(document).ready( function($) {
     /* Add "sel" & "parent" class to vertical menu style 2 parents when on a child page... */
     $('.WaGadgetMenuVertical.menuStyle002 ul.firstLevel > li').has('ul li.sel').addClass('sel parent');
