<script>
jQuery(document).ready(function($) {
if ( $(window).width() > 767) { 
 
$( "#SiteNavBar" ).wrap( '<div class="stickyContainer"></div>' );

$( ".stickyContainer" ).prepend( '<h2 class="stickyLogo"><a href="http://www.lamont.ca" title="Return to home"><img src="http://cloud.townlife.ca/system/298/stickylogo.png" alt="Town of Lamont Logo"/></a></h2>' );

$( ".stickyContainer" ).append( '<div class="stickySearch"><form method="get" class="SearchBox clearfix" action="/search/Canada/Alberta/Lamont/298"><input type="hidden" id="site_id" name="site_id" value="298" /><input id="q" name="q" type="text" /><input id="search_btn" type="submit" name="commit" value="Go" /></form></div>' );

// Sticky nav start
$(document).ready(function() {  
var stickyNavTop = $('#mainNavigation, #mainNav').offset().top;  
  
var stickyNav = function(){  
var scrollTop = $(window).scrollTop();  
	   
if (scrollTop > stickyNavTop) {   
	$('#mainNavigation, #mainNav').addClass('stickynav');
} else {  
	$('#mainNavigation, #mainNav').removeClass('stickynav');
}  
};  
  
stickyNav();  
  
$(window).scroll(function() {  
	stickyNav();  
});  
});

}

else {
	
jQuery(document).ready(function($) {
  $.getScript('http://cloud.townlife.ca/themes/Shared/Javascript/mobile-jq.js', function() {
  });
});

}
});
</script><!-- STICKY and MOBILE ends here -->




	


