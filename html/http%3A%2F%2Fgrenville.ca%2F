    <script type="text/javascript">
        var $j = jQuery.noConflict();

        //variables

        var widthSlideShow = 690;
        var currentImg = 1;

        $j(document).ready(function(){
            $j('.btnDefilant').click(defilerBanner);

            var interval = setInterval(function() {
                  defilerBanner()
            }, 8000);
        });

        function defilerBanner(){
            // Faire défiler juste si il n'y a pas de hover
            var isHovered = $j('#bannerDefilant').is(":hover");

            // Vérifier quel boution a ete cliqué. Si aucun, cest le setinterval qui a appelé la fonction
            if ($j(this).hasClass("btn1")){currentImg = 1;}
            else if ($j(this).hasClass("btn2")){currentImg = 2;}
            else if ($j(this).hasClass("btn3")){currentImg = 3;}
            else if ($j(this).hasClass("btn4")){currentImg = 4;}
            else {if (isHovered==false)currentImg++}
            if (currentImg>=5){currentImg=1;}
                    
            // Changer la couleur des boutons
            for (index=1; index <= 4; index++){
                name = String(".btn"+index);
                if (index==currentImg){$j(name).css({ "background-image":"url(http://grenville.ca/wp-content/themes/theme_grenville/images/fleche.png)" });}
                else{$j(name).css({ "background-image":"none" });}
            }
            // Bouger le slideshow
            $j("#conteneurSlides").stop().animate({ "margin-left":(currentImg-1)*-widthSlideShow },500); 

        }

        //datepicker

