<script type="text/javascript">
	$(document).ready(function() {
	    /* Search Panel: Select All */
	    $('.clbc-SelectAll').click(function (e) {
	        var checkBoxes = $('.clbc-SelectSources input[type=checkbox]');
	        checkBoxes.prop('checked', !checkBoxes.prop('checked'));
	    });

		/* Search Panel: Selected Sources */
		$('.clbc-SelectSources .header span').click(function (e) {
		    $(this).parent().parent().toggleClass('expanded');
		    $('.clbc-SelectSources .content').slideToggle('slow');
		});

		$('.clbc-RadioLabel span').show();
	});
</script>



