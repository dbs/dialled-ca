<script type="text/javascript">
function chr( ascii ) {
    return String.fromCharCode(ascii);
}
cleanText = function (text) {
  var newTxt = new String(text);
  var trim = new Array("\n");
  var randString = 'st7ju8UC9CaPreBa';

  var toTrim = new RegExp("\n\n", "g");
  newTxt = newTxt.replace(toTrim, randString);  
  
  toTrim = new RegExp("\n", "g");
  newTxt = newTxt.replace(toTrim, ' ');

  toTrim = new RegExp("st7ju8UC9CaPreBa", "g");
  newTxt = newTxt.replace(toTrim, "\n\n");

  return text;
}
function searchLibraryCatalogue(){
	var elem       = document.getElementById('search');
	var searchFor  = cleanText(elem.value);
	var url        = 'http://catalogue.bramlib.on.ca/polaris/search/searchresults.aspx?';
	var url_params = 'ctx=1.1033.0.0.3' + chr(38) + 'type=Keyword' + chr(38) + 'term=' + escape(searchFor) + chr(38) + 'by=KW' + chr(38) + 'sort=RELEVANCE' + chr(38) + 'limit=TOM=' + chr(42) + chr(38) + 'query=' + chr(38) + 'page=0';
	location.href  = url + url_params;
	return false;
}
</script>



