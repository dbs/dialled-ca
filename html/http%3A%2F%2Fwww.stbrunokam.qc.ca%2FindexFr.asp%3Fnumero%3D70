<script language="JavaScript">
<!-- start Javascript
function echeck(str) {

		var at="@"
		var dot="."
		var lat=str.indexOf(at)
		var lstr=str.length
		var ldot=str.indexOf(dot)
		if (str.indexOf(at)==-1){
		   return false
		}

		if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
		   return false
		}

		if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
		    return false
		}

		 if (str.indexOf(at,(lat+1))!=-1){
		    return false
		 }

		 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
		    return false
		 }

		 if (str.indexOf(dot,(lat+2))==-1){
		    return false
		 }

		 if (str.indexOf(" ")!=-1){
		    return false
		 }

 		 return true
	}


function  validation1() {

	if (document.formulaire.nom.value =="")
	{
		alert("Vous devez inscrire votre nom.")
		return false;
	}

	if (document.formulaire.email.value =="")
	{
		alert("Vous devez inscrire votre courriel.")
		return false;
	}

	var emailID=document.formulaire.email


	if (echeck(emailID.value)==false){
		alert("Votre courriel est invalide.")
		return false
	}

return true;
}

function  validation2() {

	if (document.courriel.nom.value =="")
	{
		alert("Vous devez inscrire votre nom.")
		return false;
	}

	if (document.courriel.email.value =="")
	{
		alert("Vous devez inscrire votre courriel.")
		return false;
	}

	if (document.courriel.commentaire.value =="")
	{
		alert("Vous devez inscrire votre commentaire.")
		return false;
	}

	var emailID=document.courriel.email


	if (echeck(emailID.value)==false){
		alert("Votre courriel est invalide.")
		return false
	}



return true;
}


// end JavaScript -->
