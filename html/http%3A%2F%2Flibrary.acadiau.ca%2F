  <p>Search Acadia and other Novanet libraries for books, articles, and more.</p>

<script type="text/javascript">

    function dlSearch(formName) {
      var userQuery, f = document.forms[formName];

      //remove commas and trim extra spaces
      userQuery = f.queryTemp.value.replace(/,/g, ' ').replace(/\s+/g, ' ').replace(/^\s|\s$/, '');

      if (userQuery.length < 1){
        //Primo shows an error message if there are no terms.  Don't submit.
        f.queryTemp.value = '';
        alert("Please enter some search terms.");
        return false;
      }

      //put the formatted query in place and submit the form
      f.query.value = 'any,contains,' + userQuery;
      return true;
    }

  </script>

  <form method="get" role="search" name="primoBasicSearch" onSubmit="return dlSearch('primoBasicSearch');"
