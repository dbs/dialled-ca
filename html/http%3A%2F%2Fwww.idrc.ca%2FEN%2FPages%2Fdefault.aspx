<script type="text/javascript">

    var TimeToFade = 1000.0;
    var flag = 0;
    var activeElement = 1;
    var numberOfImgsStr = '4';
    var numberOfImgs = parseInt(numberOfImgsStr);
    var refreshIntervalStr = '10000'
    var refreshInteval = parseInt(refreshIntervalStr);
    var refreshIntervalId = setInterval('choose()', refreshInteval);
    //var refreshIntervalId = setInterval('choose()', 5000);


    function choose() {

 //       alert("choose");
        var id;
        var nextId;

        if (activeElement > numberOfImgs) {
            activeElement = 1;

            id = 'Image' + activeElement.toString();
            nextId = 'Image' + (activeElement + 1).toString();
            //nextId = 'Image1';		    
        }
        else if (activeElement == numberOfImgs) {
            id = 'Image' + activeElement.toString();
            nextId = 'Image1';
        }
        else {
            id = 'Image' + activeElement.toString();
            nextId = 'Image' + (activeElement + 1).toString();
        }


        var element = document.getElementById(id);  //.style.display = 'none';

        fade(id);

        document.getElementById(nextId).style.display = 'block';

        fade(nextId);

        hide(id);

        activeElement++;
    }

    function hide(id) {
  //      alert("hide");
        document.getElementById(id).style.display = 'none';
    }


    function fade(eid) {
 //       alert("fade");
        var element = document.getElementById(eid);
        if (element == null)
            return;

        if (element.FadeState == null) {
            if ((element.style.opacity == null
			|| element.style.opacity == ''
			|| element.style.opacity == '1')
		        && (element.style.opacity != undefined)) {
                element.FadeState = 2;
            }
            else if ((eid == 'Image1') && (element.style.opacity == undefined)) {
                element.FadeState = 2;
            }
            else {
                element.FadeState = -2;
            }
        }

        if (element.FadeState == 1 || element.FadeState == -1) {
            element.FadeState = element.FadeState == 1 ? -1 : 1;
            element.FadeTimeLeft = TimeToFade - element.FadeTimeLeft;
        }
        else {

            element.FadeState = element.FadeState == 2 ? -1 : 1;
            element.FadeTimeLeft = TimeToFade;
            //setTimeout(animateFade(new Date().getTime(),'eid'), 33);"
            setTimeout(" + animateFade(" + new Date().getTime() + ",'" + eid + "')", 33);
        }
    }


    function animateFade(lastTick, eid) {
  //      alert("animateFade");
        var curTick = new Date().getTime();
        var elapsedTicks = curTick - lastTick;

        var element = document.getElementById(eid);

        if (element.FadeTimeLeft <= elapsedTicks) {
            element.style.opacity = element.FadeState == 1 ? '1' : '0';
            element.style.filter = 'alpha(opacity = '
			+ (element.FadeState == 1 ? '100' : '0') + ')';

            element.FadeState = element.FadeState == 1 ? 2 : -2;
            return;
        }

        element.FadeTimeLeft -= elapsedTicks;
        var newOpVal = element.FadeTimeLeft / TimeToFade;

        if (element.FadeState == 1)
            newOpVal = 1 - newOpVal;

        setOpacity(element, newOpVal);

        //setTimeout(" + "\"animateFade(\" + curTick + \",'\" + eid + \"')\", 33);"
        setTimeout(" + animateFade(" + new Date().getTime() + ",'" + eid + "')", 33);
    }

    function hideAllDivs() {
   //     alert("hideAllDivs");
        var id;

        for (var i = 1; i < (numberOfImgs + 1); i++) {
            id = 'Image' + i.toString();
            element = document.getElementById(id);


            if (id == 'Image1')
                setOpacity(element, 1);
            else {
                setOpacity(element, 0);
                element.style.display = 'none';
            }
        }
    }

    function setOpacity(myElement, opacityValue) {
  //      alert("setOpacity");
        if (window.ActiveXObject) {
            myElement.style.filter = 'alpha(opacity=' + opacityValue * 100 + ')'; //IE
        }
        else {
            myElement.style.opacity = opacityValue; // Gecko/Opera
        }
    }

    function showDiv(switchId, currentId) {
 //       alert("showDiv");
        var idStr = 'Image' + switchId;
        var currentIdStr = 'Image' + currentId;

        fade(currentIdStr);

        document.getElementById(idStr).style.display = 'block';
        fade(idStr);

        hide(currentIdStr);
    }

    function stopCarousel(intervalID) {
    //    alert("stopCarousel");
        clearInterval(intervalID);
    }

    hideAllDivs();
	       	

</script>

