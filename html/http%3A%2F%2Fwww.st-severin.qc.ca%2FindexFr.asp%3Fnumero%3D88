		alert("Vous devez inscrire votre telephone.")
		return false;
	}



return true;
}

function  validation4() {






	if (document.courriel.AlocalisationFrancais.value =="")
	{
		alert("Vous devez inscrire la location.")
		return false;
	}

if (document.courriel.AdateDemandeFrancais && !EW_hasValue(document.courriel.AdateDemandeFrancais, "TEXT" )) {
	if (!EW_onError(document.courriel, document.courriel.AdateDemandeFrancais, "TEXT", "S.V.P. complétez le champs requis - Date "))
		return false;
}


	if (document.courriel.AnomFrancais.value =="")
	{
		alert("Vous devez inscrire votre nom.")
		return false;
	}

	if (document.courriel.AadresseFrancais.value =="")
	{
		alert("Vous devez inscrire votre adresse.")
		return false;
	}

	if (document.courriel.AcodePostalFrancais.value =="")
	{
		alert("Vous devez inscrire votre code postal.")
		return false;
	}
	if (document.courriel.AtelResFrancais.value =="")
	{
		alert("Vous devez inscrire votre numero de téléphone.")
		return false;
	}

	if (document.courriel.Anom2Francais.value =="")
	{
		alert("Vous devez inscrire le nom de votre entrepreneur. ")
		return false;
	}

	if (document.courriel.Aadresse2Francais.value =="")
	{
		alert("Vous devez inscrire l'adresse de votre entrepreneur.")
		return false;
	}

	if (document.courriel.AcodePosta2lFrancais.value =="")
	{
		alert("Vous devez inscrire le code postal de votre entrepreneur.")
		return false;
	}
	if (document.courriel.AtelRes2Francais.value =="")
	{
		alert("Vous devez inscrire le numero de téléphone de votre entrepreneur.")
		return false;
	}





if (document.courriel.ADatePrevueDebut && !EW_hasValue(document.courriel.ADatePrevueDebut, "TEXT" )) {
	if (!EW_onError(document.courriel, document.courriel.ADatePrevueDebut, "TEXT", "S.V.P. complétez le champs requis - Date "))
		return false;
}

if (document.courriel.ADatePrevueFin && !EW_hasValue(document.courriel.ADatePrevueFin, "TEXT" )) {
	if (!EW_onError(document.courriel, document.courriel.ADatePrevueFin, "TEXT", "S.V.P. complétez le champs requis - Date "))
		return false;
}

	if (document.courriel.ACout.value =="")
	{
		alert("Vous devez inscrire le coût approximatif des travaux.")
		return false;
	}


return true;
}

