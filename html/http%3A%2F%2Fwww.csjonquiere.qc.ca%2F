		<section id="zone_calendrier">
			<script type="text/javascript">
				var initializeCalendar = function(url) {
					$.get(url, function(data, textStatus, jqXHR) {
						$('#zone_calendrier').html(data);
						$('.calendar_controls').click(function(event) {
							initializeCalendar($(this).attr('href'));
							return false;
						});

						var tooltipTimeout;
						$('td.calendar_day.calendar_category').hover(function(event) {
							clearTimeout(tooltipTimeout);
							$('.calendar_day_tooltip').hide();
							$('.calendar_day_tooltip', this).show();
						}, function(event) {
							tooltipTimeout = setTimeout($.proxy(function() {
								$('.calendar_day_tooltip', this).hide();
							}, this), 200);
						});

						$('td.calendar_day.calendar_category .calendar_day_tooltip').append(
							$('<img />').attr('src','images/cal_popup_arrow.png').addClass('calendar_day_tooltip_popup')
						);
					});
				};
				initializeCalendar('http://www.csjonquiere.qc.ca/mini_calendrier.php');
			</script>
		</section>

	</section>
	
	
