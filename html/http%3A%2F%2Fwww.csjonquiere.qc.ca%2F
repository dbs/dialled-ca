										</script>
			
			<script>
				var nextNewsAutoEvent = null;
				
				function setNews(index) {
					$('.article_nav').removeClass('selected');
					$('#article_nav_' + index).addClass('selected');
					$('.article').hide();
					$('#article_' + index).show();
					
					if (nextNewsAutoEvent) {
						clearTimeout(nextNewsAutoEvent);
					}
				}
				
				function getNextNews(delay){
					nextNewsAutoEvent = setTimeout(function(){					 
						var actualElement = $('.article_nav.selected');
						var index = actualElement.index();
						var nextElement = $(actualElement).next('.article_nav');
						if(nextElement.length > 0)
							index = $(nextElement).index();
						else
							index = $('.article_nav').first().index();

						setNews(index);
						getNextNews(delay);
					},delay);
				}
				
				$(function(){
					getNextNews(12000);
				});
			</script>
			<div id="nouvelles">
									<div id="homeboxNews">					
						<div id="articleNavigation">
							<ul>
																	<li class="article_nav selected " onclick="setNews('0');" id="article_nav_0">
										<div class="article_nav_content">
											<div class="date"></div>
											<div class="titre">L'école de la Mosaïque s'est vêtue de rose!</div>				
										</div>
									</li>
																	<li class="article_nav  " onclick="setNews('1');" id="article_nav_1">
										<div class="article_nav_content">
											<div class="date"></div>
											<div class="titre">Les élèves de la CSDLJ performent au concours national de Netmaths</div>				
										</div>
									</li>
