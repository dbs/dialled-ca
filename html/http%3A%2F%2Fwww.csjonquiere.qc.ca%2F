				<script>		
					$('#bannieresContainer')
						.after('<div id="bannieresContainerNav">')
						.cycle({
							fx: 'scrollVert',
							timeout: 6000,
							speed: 900,
							easing:'easeInOutQuart',
							slideExpr: '.banniere',
							containerResize: false,
							slideResize: false,
							rev: true,
							pager:  '#bannieresContainerNav',
