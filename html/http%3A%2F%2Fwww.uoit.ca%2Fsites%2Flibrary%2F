                          <section class="spotlight small-8 medium-8 columns">
    <div class="module_home_contentWrapper">
        <h3 class="module_home_heading">Find resources</h3>
        <div class="spotlight_content_wrapper">
                      
<div id="librarySearch">
    <ul class="tabs" data-tab role="tablist">
      <li class="tab-title active" role="presentation"><a href="#QS" role="tab" tabindex="0" aria-selected="true" aria-controls="panel2-1"><h3>Quick Search</h3></a></li>
      <li class="tab-title" role="presentation"><a href="#journal" role="tab" tabindex="0" aria-selected="false" aria-controls="panel2-2"><h3>Journals A-Z</h3></a></li>
