<script type="text/javascript">
    jQuery('a[href="http://www.cfa.ca"], a[href="http://www.cfa.ca/"], a[href="/"]').click(function(e) {
        e.preventDefault(); 
        e.stopPropagation(); 
        jQuery( "#run_inv" ).submit();
    });
    jQuery('.span_zone .glyphicon.glyphicon-eye-open').click(function(){
        if ($(this).is('[target]')) {
            pass = jQuery('.span_zone '+$(this).attr('target'));
        } else {
            pass = jQuery('.span_zone [name="login[password]"]');
        };
        if (pass.is('[type="password"]')) {
            pass.attr('type','text');
        } else {
            pass.attr('type','password');
        };
    });
    jQuery('.dropdown-menu').parent( ). each(function(){
        var myTime;
        jQuery(this).on({
            mouseenter: function() {
                clearTimeout(myTime);
                jQuery(jQuery(this).children('.dropdown-menu')[0]).show();
            },
            mouseleave: function() {
                var obj = jQuery(jQuery(this).children('.dropdown-menu')[0]);
                myTime = setTimeout(function(){
                    obj.hide();
                }, 125);  
            }
        });
    });
    // setTimeout(function(){
    //     jQuery('.menu-item-has-children').removeClass('menu-item-has-children');
    // }, 3000);    
</script>
