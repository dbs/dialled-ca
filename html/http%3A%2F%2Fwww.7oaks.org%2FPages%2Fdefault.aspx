            '46': 'U',
            '47': 'S'
        };

        var element = $('#sl-Weather');

        if (options.lightFg) element.addClass('sl-LightFg');

        $.simpleWeather({
            location: options.locationOverride || options.location,
            unit: 'c',
            success: function (weather) {

                var formatTemp = function (temp) {
                    return $('<div/>').html(temp + '&deg;' + weather.units.temp).text();
                };

                var weatherCode = codeIcons[(weather.code || '').toString()];
                if (!weatherCode) {
                    element.find('.sl-WeatherLogo').css('font-family', 'WebSymbols').text('\uf128');
                } else {
                    element.find('.sl-WeatherLogo').text(weatherCode);
                }
                element.find('.sl-WeatherLogo').attr('title', weather.currently);

                element.find('.sl-Temp').text(formatTemp(weather.temp));
                element.find('.sl-Status').text(weather.currently);
                
                element.find('.sl-HighTemp').text('High: ' + formatTemp(weather.high));
                element.find('.sl-LowTemp').text('Low: ' + formatTemp(weather.low));

                element.find('.sl-WeatherLocation').text(options.locationOverride || [(options.cityName || weather.city), weather.region].join(', '));

                var forecast = element.find('.sl-Forecast');

                if (!options.includeForecast) {
                    forecast.hide();
                } else {
                    forecast.each(function (index) {

                        // index + 1 because weather.forecast[0] is today
                        var forecast = weather.forecast[index + 1];

                        $(this).find('.sl-Date .sl-Day').text(forecast.day);
                        $(this).find('.sl-Date .sl-Number').text(new Date(forecast.date).getDate());

                        $(this).find('.sl-Temps').text(formatTemp(forecast.high) + ' | ' + formatTemp(forecast.low));
                    });
                }
            },
            error: function (error) {
                element.addClass('sl-WeatherError');
                element.find('.sl-WeatherLogo').css('font-family', 'WebSymbols').text('\uf128');
                element.find('.sl-Status').text(error.error || 'Could not connect to weather API.');
                element.find('.sl-Forecast, .sl-Chiclets').hide();
            }
        });
    });
</script>
