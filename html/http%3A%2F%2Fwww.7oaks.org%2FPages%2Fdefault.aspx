<script type="text/javascript">
    $(document).ready(function () {

        var options = {
            includeForecast: false,
            lightFg: true,
            location: 'R2V 4E7',
            locationOverride: "",
            cityName: 'Winnipeg'
        };

        var codeIcons = {
            '0': ':',
