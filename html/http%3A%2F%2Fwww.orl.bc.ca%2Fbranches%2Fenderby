                    <script language="javascript">
                        String.prototype.trim = function () { return this.replace(/^\s*|\s*$/g, ''); }
                        function ProcessInput(e) {
                            if (window.event)       // IE
                                keyNum = e.keyCode;
                            else if (e.which)       // Netscape/Firefox/Opera
                                keyNum = e.which;
                            if (keyNum == 13) {
                                e.returnValue = false;
                                e.cancelBubble = true;
                                DoKeywordSearch();
                                return false;
                            }
                            return true;
                        }
                        function DoKeywordSearch() {
                            if (document.getElementById('textboxTerm').value.trim() != '')
                                window.location = 'http://ipac.orl.bc.ca/polaris/view.aspx?keyword=' + escape(document.getElementById('textboxTerm').value);
                            else
                                document.getElementById('textboxTerm').focus();
                        }
                    </script>
                    <a class="redbutton" href="javascript:;" onclick="DoKeywordSearch();"><span>Go</span></a>
                    <input type=text id=textboxTerm size=45 class="cleartxt inactive search" value="Search Catalogue..." 
                     onblur="if (this.value == '') {this.value = 'Search Catalogue...';}"
                     onfocus="if(this.value == 'Search Catalogue...') { this.value = ''; }" 
                     onkeypress="return ProcessInput(event)" />
                    <script language="javascript">
                        document.getElementById('textboxTerm').blur();
                    </script>
