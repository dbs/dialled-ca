<script type="text/javascript">            
	$(document).ready(function () {
		
	 		$("#home_window").jqxWindow({position:{ x: 30, y: 10}, isModal: true , height:400, width: 700, maxWidth: 700, theme: '' }); 			
			$("#home_window").jqxWindow('close'); 		
		
	 		$("#open_house_window").jqxWindow({position:{ x: 30, y: 5}, isModal: true , height:570, width: 800, maxWidth: 800, theme: '' }); 			
			$("#open_house_window").jqxWindow('close'); 		
				
	});        

	function openHomeWindow(){
  	window.scrollTo(0,0);
		//center_window('#home_window');
  	$('#home_window').jqxWindow({ position:'center' });
  	$('#home_window').jqxWindow({ position:{y:50} });
    $('#home_window').jqxWindow('open');		
	}

	function OpenHouseWindow(){
  	window.scrollTo(0,0);
  	$('#open_house_window').jqxWindow({ position:'center' });
  	$('#open_house_window').jqxWindow({ position:{y:50} });
    $('#open_house_window').jqxWindow('open');		
	}

</script>

