<script type="text/javascript">   
    var bandeau = document.getElementById("Bandeau");
    var liste_elements  = bandeau.getElementsByTagName("span");
    var bandeau = document.getElementById("BandeauResume");
    var liste_elements_resume  = bandeau.getElementsByTagName("span");
    //PrÃ©condition: Nombre d'elements du Bandeau = Nombre d'elements du Bandeau RÃ©sumÃ©'
    var taille_liste = liste_elements.length;
    var timer;
    var frecuence = 8000;//En millisecondes
    //On habilite la posiblitÃ© de cliquer sur les rÃ©sumÃ©s
    habiliterSelectionElement();
    //On dÃ©marre le carrousel  avec l'image d'ordre "0"
    mettreEnMarcheBandeau(0);

    /*Cette fonction est utilisÃ©e de facon rÃ©cursive pour l'affichage des
     *Ã©lÃ©ments concernant le bandeau.
     *@param _index ordre de l'Ã©lÃ©ment Ã  afficher (image et rÃ©sumÃ©)
     **/
    function mettreEnMarcheBandeau(_index){
        var index = _index;
        cacherTous();
        afficherUnElement(index);
        index++;
        if (index == taille_liste)
            index = 0;
        var fonction = 'mettreEnMarcheBandeau('+index+')';
        timer = window.setTimeout(fonction,frecuence);
    }

    function cacherTous(){
        for(var i=0;i < taille_liste;i++){
            liste_elements[i].style.display = 'none';
            liste_elements_resume[i].className = '';
        }
    }

    function afficherUnElement(_index){
        liste_elements[_index].style.display = 'block';
        liste_elements_resume[_index].className = 'resumeSelected';
    }

    function habiliterSelectionElement(){
        for(var i=0;i < liste_elements_resume.length;i++){
            liste_elements_resume[i].id =i;
            liste_elements_resume[i].onclick = function (){
                var indexResume = this.id;
                window.clearTimeout(timer);
                mettreEnMarcheBandeau(indexResume);
            };
        }
    }

</script>


