<!-- random search image background--> 
<script type="text/javascript">
window.onload = function () {
    var header = document.getElementById('searchbox');
    var pictures = new Array('_site_graphics/seasonal/bg1.png','_site_graphics/seasonal/bg2.png','_site_graphics/seasonal/bg3.png','_site_graphics/seasonal/bg4.png','_site_graphics/seasonal/bg5.png','_site_graphics/seasonal/bg6.png','_site_graphics/seasonal/bg7.png'	);
    var numPics = pictures.length;
    if (document.images) {
        var chosenPic = Math.floor((Math.random() * numPics));
        header.style.background = 'url(' + pictures[chosenPic] + ')';
    }
	<!-- autofocuses page to summon searchbox-->
	var summonbox = document.getElementsByName('q');
	summonbox [0].focus();
}
</script>

