<script type="text/javascript">
$(document).ready(function(){
	
	$('ul.sf-menu').supersubs({ 
            minWidth:    12,   // minimum width of sub-menus in em units 
            maxWidth:    27,   // maximum width of sub-menus in em units 
            extraWidth:  1     // extra width can ensure lines don't sometimes turn over 
                               // due to slight rounding differences and font-family 
        }).superfish({ 
    	animation: {opacity:'show',height:'show'},   // slide-down effect without fade-in 
    	speed:       'fast', 
    	delay:     1000               // 1.2 second delay on mouseout 
	});	
	

	// diapo carre clic
	$(".diapocarre").click(function(){
		if ( !$(this).hasClass("visible") ) {
			$(document).stopTime("tmpDiapo");
			diapo( $(this).index() );
			$(document).oneTime(5000, "tmpAttente", function(){
				timerDiapo();
			});
		}
	});

	 timerDiapo();
	 
	 
	 //$('#nouvelles a[title]=""').removeAttr('title');
	 
	 $('#calendar a[title]').qtip({
	 		style: { name: 'light', tip: true, border: {radius: 5} },
			position: {
      			corner: {
         			target: 'topMiddle',
         			tooltip: 'bottomMiddle'
				}
			}
		});
	 $('#nouvelles a[title]').qtip({
	 		style: { name: 'light', tip: true, border: {radius: 5} },
			position: {
      			corner: {
         			target: 'topMiddle',
         			tooltip: 'bottomLeft'
				}
			}
		});

	 
});

function timerDiapo(){
	$(document).everyTime(5000, "tmpDiapo", function(){	
		var imax = $(".diapocarre").length;
		var i = $(".diapocarre.visible").index();
		i++;
		if(i >= imax) i = 0;
		diapo(i);
	});
}


function diapo(i){	
	
	var t = 500;

	var photo1 = $(".diapo.visible");
	var diapocarre1 = $(".diapocarre.visible");
	var photo2 = $('.diapo:eq('+i+')');
	var diapocarre2 = $('.diapocarre:eq('+i+')');	
	
	$(diapocarre1).removeClass("visible");
	$(diapocarre2).addClass('visible');
						
	$(photo1).fadeOut(t, function(){
		$(photo1).removeClass('visible');
	
	});
	$(photo2).fadeIn(t, function(){
		$(photo2).addClass('visible');	
				
	});
			
}

