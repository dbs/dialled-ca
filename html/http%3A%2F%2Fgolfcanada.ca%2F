  <script type="text/javascript">
    jQuery(document).ready( function(){    
      $('#club_typeahead').typeahead({
        name: 'club',
        template: function(d) {
          return '<p>'+d.value+' <small class="text-muted">('+d.location+')</small></p>'
        },
        prefetch: {
          url: 'http://sc2.staging.golfcanada.ca/ajax/refreshClubs',
          ttl: 1
        }
      }).on('typeahead:selected', function(obj, d) {
        $('#club_typeahead_id').val(d.key);
      });
    });
  </script>

  
