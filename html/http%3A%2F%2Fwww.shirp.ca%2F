<!-- EMP 2012 Feb 20 Try to switch style sheets based on screen size (ripped off from somewhere): -->
  <link id="size-stylesheet" rel='stylesheet' type='text/css' href='/sites/www.shirp.ca/themes/pre_shirp_ca/pre_shirp_ca.css?6' />
	
<!-- EMP 2012 Feb 29 Change method to https to avoid https page conflict:  -->
  <script type='text/javascript' src='https://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js'></script>
  <script type='text/javascript'>
    // This would be better with a page-global var to track the last size.
    var prevWidth = 9999;
    function adjustStyle(width) {
      width = parseInt(width);
      if ((width < 701) && (prevWidth >= 701)) {
        $("#size-stylesheet").attr("href", "/sites/www.shirp.ca/themes/pre_shirp_ca/pre_shirp_ca_narrow.css?8");
        // Hack here: adjust the main page blocks for the smallest screen size:
        // re-order the div id="left-top" name="hcNews" to go at the end of the 1-col style
        $(".front #left-main [name='hcNews']").appendTo($(".front #right-main"))
      } 
      else if ((width >= 701) && (width < 900) && ((prevWidth < 701) || (900 <= prevWidth))) {
        $("#size-stylesheet").attr("href", "/sites/www.shirp.ca/themes/pre_shirp_ca/pre_shirp_ca_medium.css?7");
        // Hack here: adjust the main page blocks for the smallest screen size:
        // re-order the div id="left-top" name="hcNews" to go at the end of the 1-col style
        $(".front #left-main [name='hcNews']").appendTo($(".front #right-main"))
      } 
      else if ((width >= 900) && ((prevWidth < 900) || (9999 <= prevWidth))) {
        $("#size-stylesheet").attr("href", "/sites/www.shirp.ca/themes/pre_shirp_ca/pre_shirp_ca.css?6"); 
        // Hack here: adjust the main page blocks for the smallest screen size:
        // re-order the div id="left-top" name="hcNews" to go at the end of the 1st col in 2-col style
        $(".front #right-main [name='hcNews']").appendTo($(".front #left-main"))
      }
      prevWidth = width;
    }

    $(function() {
      adjustStyle($(this).width());
      $(window).resize(function() {
        adjustStyle($(this).width());
      });
    });

    $(document).ready(function() {
      adjustStyle($(window).width()); // maybe document instead of window?
    });
  </script>
