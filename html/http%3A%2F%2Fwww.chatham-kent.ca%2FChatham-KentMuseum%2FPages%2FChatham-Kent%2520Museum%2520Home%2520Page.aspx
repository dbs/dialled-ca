        itvTopMenu = topMenuHidden();
        }
      }


      //Change the state of each items in footprint
      function changeBg(command, hasChild, nextItemID, listElementID, level)
      {        
        var currentLi = document.getElementById("li_" + listElementID);
        var currentA = document.getElementById("a_" + listElementID); 
        var menuLiElemID = "";
        
        //Keep parent menu highlight if current menu is sub menu
        if (level == '2')
        {
          menuLiElemID = listElementID.substr(0, listElementID.length-2);
          showParentMenu(menuLiElemID, level);
        }

        //Show item highlight
        if (command == 'show')
        {             
                        
          if (hasChild == 'yes')
          {
              clearTimeout(itvSubMenu);

              currentLi.className = "liOver_" + level;
              currentA.className = "overWithImg_" + level;
              
              //Show the fly-out menu  
              footprintToggleChildren(lastSubMenuElementID, false);                  
              footprintToggleChildren(listElementID, true);
              lastSubMenuElementID = listElementID;
          }
          else
          {
              closeAllSubMenu();
              currentA.className = "over_" + level;
          }

        }
        //Show item normally
        else if (command == 'hidden')
        {
          if (hasChild == 'yes')
          {
              clearTimeout(itvSubMenu);

              currentLi.className = "";
              currentA.className = "withImg_" + level;

              //Hide the fly-out menu
              footprintToggleChildren(lastSubMenuElementID, false);
              footprintToggleChildren(listElementID, false);
              lastSubMenuElementID = listElementID;
          }
          else
          {
              footprintToggleChildren(lastSubMenuElementID, false);

              if (listElementID.charAt(listElementID.length-1) == "0"){
                currentA.className = "firstLi";
              }
              else{
                currentA.className = "";
              }
          }
          
        }
        
      }

      function footprintToggleChildren(listElementID, showFlag)
      {                     
        var subMenuDiv = document.getElementById("div_" + listElementID);            
        if (subMenuDiv != null)
        {
            if(showFlag){  
              if (lastSubMenuElem != listElementID)
              {
                var lastSubMenuElem = document.getElementById("div_" + lastSubMenuElementID);
                if (lastSubMenuElem != null)
                  lastSubMenuElem.style.display = "none";        
              }
              subMenuDiv.style.display='block';
            }
            else {    
              if (lastSubMenuElem != listElementID)      
                clearTimeout(itvSubMenu);

              itvSubMenu =  setTimeout('document.getElementById(\'div_' + listElementID + '\').style.display=\'none\';', 500);              
            }
        }
      }

      function footprintMenuOnMouseOver(flag)
      {
        
        footprintToggleChildren(lastSubMenuElementID, false); 
        if (flag)
        {        
          showFootprint(true);
        }
        else
        {             
          clearTimeout(itvImgLogo);
          itvImgLogo = imgLogoChangeBack();

          clearTimeout(itvTopMenu);
          itvTopMenu = topMenuHidden();

          //closeAllSubMenu();
        }
      }


      //Change back the logo image to normal
      function imgLogoChangeBack()
      {
        return setTimeout('document.getElementById("imgLogo").src = \'' + imgLogo + '\'',500);
      }

      //Hide footprint
      function topMenuHidden()
      {            
        return setTimeout('document.getElementById("divFootprint").style.display = "none"',500);
      }

      function showParentMenu(parentMenuLiID, level)
      {
        if (parentMenuLiID != null && parentMenuLiID != "" )
        var parentMenu = document.getElementById("li_" + parentMenuLiID);
        var parentMenuA = document.getElementById("a_" + parentMenuLiID); 

        if (parentMenu != null)
        {
          parentMenu.className = "liOver_" + level;
          parentMenuA.className = "overWithImg_" + level;
        }
      }
      
      
      function closeAllSubMenu()
      {
        $j("div[id~=div_CKFootprint]").hide();
      }
      

      
