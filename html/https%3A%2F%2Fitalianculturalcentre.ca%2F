				var winWid = jQuery(window).width();
				 if (winWid < 480){
						jQuery('.featCalendar .fancy').attr("href", "http://italianculturalcentre.ca/event-registration/");
						jQuery('.featCalendar .fancy').attr("class", "notFancy resize");
				 } else {
					 jQuery('.featCalendar .notFancy').attr("href", "http://italianculturalcentre.ca/blog/hidden_type/icc-manual-fancybox//");
					 jQuery('.featCalendar .notFancy').attr("class", "fancy fancybox.iframe resize");
				 }
			}
			jQuery(".fancy").fancybox({
				  minHeight : 338,
			  });

			 jQuery( window ).resize(windowWidthStuff);

			jQuery('#espresso_calendar, .ui-tooltip-content').click(function(){
				
				jQuery('#espresso_calendar a').attr("target","_parent");								   
		   });
			jQuery('#school-calendar, .featSchool').click(function(){
				
				jQuery('#school-calendar a').attr("target","_parent");								   
		   });

			jQuery("#school-calendar a").attr("target","_parent");



			jQuery(window).load(function() {
				jQuery( "#main .upcomingIlCentro .event_data" ).each(function() {
					jQuery(this).prepend('<h4 style="background:#acacac; margin-bottom:8px;">Featured Il Centro Event</h4>');
				});

				jQuery('.toggleButton').click(function() {
					jQuery(this).siblings('.navToggleWrapper').toggleClass('donotshowme');
					jQuery(this).siblings('.navToggleWrapper').toggleClass('showme');
				});

				jQuery("#event_espresso_registration_form .a_register_link").hide();
					   
				jQuery('.spaces-available').hide();
	
				jQuery( ".register-link-footer" ).each(function() {
						var linkText = jQuery(this).html();
						var newLinkText=linkText.replace(' or ','');
						//alert (newLinkText);
						 jQuery(this).html(newLinkText);
				});
	
				jQuery( ".price" ).each(function() {
						var priceText = $(this).text();
						var newPriceText=priceText.replace("Surcharge","GST");
						//alert (newPriceText);
						$(this).text(newPriceText);
				});
				jQuery('.formTrigger').click(function(){
					$('.formToggle').slideToggle();
				});
				var minHeightCalendarDate = jQuery('.fc-sun div').height();
				var newsContainerHeight = jQuery('.featNews').height;
				var eventContainerHeight = jQuery('.featEvents').height();
				if (newsContainerHeight >= eventContainerHeight){
					jQuery ('.featEvents').height(newsContainerHeight);
				} else if (eventContainerHeight > newsContainerHeight) {
					jQuery ('.featNews').height(eventContainerHeight);
				}
				var CalendarHeight = jQuery ('.featCalendar').height();
				var NewsletterHeight = jQuery ('.newsletterWrap').height();
				if (CalendarHeight >= NewsletterHeight){
					jQuery ('.eventWrap').height(CalendarHeight);
					jQuery ('.newsWrap').height(CalendarHeight);
					jQuery ('.newsletterWrap').height(CalendarHeight);
				} else if (NewsletterHeight > CalendarHeight) {
					jQuery ('.eventWrap').height(NewsletterHeight);
					jQuery ('.newsWrap').height(NewsletterHeight);
					jQuery ('.newsletterWrap').height(NewsletterHeight);
					jQuery ('.featCalendar').height(NewsletterHeight);
				}
				
				jQuery('.event-data-display').each(function () {
					jQuery(this).find('.showEventDesc h4.clickHere').click(function() {
						jQuery(this).siblings('.event-desc').slideDown('slow', function() {
						// Animation complete.
						
						});
					});
				});	
				 jQuery( "#content .ee_view_cart" ).fadeIn('fast');											 
			});
        </script>
		
