	<tr>
		<td class="collapse">
			<table class="collapse" align="center">
				<tr>
					<td class="collapse container">
						<table class="collapse maxWidth">
							<tr>
								<td class="collapse maxWidth">
                             <!--googleoff: snippet -->
                                <a href="homepage.cfm"><img alt="Hamilton Health Sciences" src="images/logo.jpg" style="border:0;"></a>
