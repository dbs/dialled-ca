  <!--<script src="/sites/all/themes/cegepderimouski/js/jquery.animateNumber.min.js"></script>-->
 
  

  
<script type="text/javascript">   
	
	


	////////////////////////////////////////
	//Orizon Media - Benoit ////////////////
	////////////////////////////////////////
	

		
	jQuery(document).ready(function($) {
		
	




		// Animation de l'icone des statistiques de placement dans les pages programmes
		$('#stats').waypoint(function() {
		   start_count();
		   this.destroy();
		}, {
		    offset: '100%',

		});	
		
		function start_count() {
			$('#stats_pourcentage').each(function () {
			    $(this).prop('Counter',0).animate({
			        Counter: $(this).text()
			    }, {
			        duration: 3000,
			        easing: 'swing',
			        step: function (now) {
			            $(this).text(Math.ceil(now));
			        }
			    });
			});
			$("#stats").addClass("action");
			 //$(this).toggleClass("icon-arrow-up3");
		}




		
		//****************************************************
		// Je fais appatraitre le Input de recherche
		//****************************************************
	
		$( "#recherche_icone" ).click(function() {
			$('#block-search-form').slideToggle( "fast");
			$('#edit-search-block-form--2').focus().val();
		});

		// Overture des accordéons dans la page /formation-continue
	    $("#open_programmes").click(function(){
	        $box = $("#block-block-11 .content");
	        minimumHeight = 170;	        
	        // get current height
	        currentHeight = $box.innerHeight();
	        // get height with auto applied
	        autoHeight = $box.css('height', 'auto').innerHeight();
	        // reset height and revert to original if current and auto are equal
	        $box.css('height', currentHeight).animate({
	            height: (currentHeight == autoHeight ? minimumHeight : autoHeight)
	        })
	        $(this).toggleClass("icon-arrow-up3");
	        
	    });
    
	    $("#open_reconnaissance").click(function(){
	        $box = $("#block-block-22 .content");
	        minimumHeight = 170;
	        // get current height
	        currentHeight = $box.innerHeight();
	        // get height with auto applied
	        autoHeight = $box.css('height', 'auto').innerHeight();
	        // reset height and revert to original if current and auto are equal
	        $box.css('height', currentHeight).animate({
	            height: (currentHeight == autoHeight ? minimumHeight : autoHeight)
	        })
	        $(this).toggleClass("icon-arrow-up3");
	    });
    
    
	     $("#open_entreprises").click(function(){
	        $box = $("#block-block-23 .content");
	        minimumHeight = 170;
	        
	        // get current height
	        currentHeight = $box.innerHeight();
	
	        // get height with auto applied
	        autoHeight = $box.css('height', 'auto').innerHeight();
	        
	        // reset height and revert to original if current and auto are equal
	        $box.css('height', currentHeight).animate({
	            height: (currentHeight == autoHeight ? minimumHeight : autoHeight)
	        })
	        $(this).toggleClass("icon-arrow-up3");
	    });
    
	    $("#open_cours").click(function(){
	        $box = $("#block-block-24 .content");
	        minimumHeight = 170;
	        // get current height
	        currentHeight = $box.innerHeight();
	        // get height with auto applied
	        autoHeight = $box.css('height', 'auto').innerHeight();
	        // reset height and revert to original if current and auto are equal
	        $box.css('height', currentHeight).animate({
	            height: (currentHeight == autoHeight ? minimumHeight : autoHeight)
	        })
	        $(this).toggleClass("icon-arrow-up3");
	    });
	});
	
	
	
</script>
  
  
