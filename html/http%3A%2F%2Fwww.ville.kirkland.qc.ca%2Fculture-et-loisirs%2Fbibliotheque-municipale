      $('#Month').bind('change', function () {
          var url = $(this).val() + $('#Category').val();
          if (url) {
                $("#CalendarDetails").innerHTML = "";
                $("#CalendarDetails").load('_cal' + url);
          }
          return false;
      });
    });
    
