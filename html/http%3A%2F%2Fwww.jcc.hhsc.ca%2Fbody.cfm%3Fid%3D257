				// }else 	 if(!eval(SM1_BannerItems[SM1_ThisBanner][2]+'document')){
	//				window.open(SM1_BannerItems[SM1_ThisBanner][1],); 
				}else{
					eval(SM1_BannerItems[SM1_ThisBanner][2] + "location.href=SM1_BannerItems[SM1_ThisBanner][1]");
				}
			}
			function SM1_BannerWindowStatus(){SM1_MO=true;window.status=SM1_BannerItems[SM1_ThisBanner][1]; return true}
			function SM1_SwapBanner(){
				iNum=SM1_ThisBanner+1;if(iNum>=SM1_BannerItems.length)iNum=0;
				document.BodyLeftTop.src=SM1_BannerItems[iNum][0].src; 
				SM1_ThisBanner=iNum;
				SM1_Timer=setTimeout('SM1_SwapBanner()',20000);
				if(SM1_MO){SM1_BannerWindowStatus();}else{window.status='';}
			}
				
