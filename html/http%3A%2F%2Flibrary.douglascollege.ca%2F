        jQuery(document).ready(function ($) {
            /* toggle nav */
            $("#divMobileSearch").on("click", function () {
                $("#search_nav_container").slideToggle();
                //$(this).toggleClass("selectedMobileSearch");
                $(this).toggleClass("active");
            });



            /* search mobile  */
            $(".navbar-toggle").on("click", function () {
                $("#mobNav").slideToggle();
                $(this).toggleClass("active");
            });

            // working on a way to remove the mobile nav when screen resize from small to big
            $(window).resize(function (e) {
                if ($(".navbar-toggle").height() == 0) {
                    $("#mobNav").hide();
                    $("#containervMobileSearch").hide();
                }
            });

            /* Mobile nav*/
            $("#mobNav .hasDropdown .toggleMob").on("click", function () {
                //$("#mobNav").slideToggle();
                $(this).toggleClass('up');
                $(this).closest(".hasDropdown").find(".dropdown").first().slideToggle();
            });

        });

    </script>

