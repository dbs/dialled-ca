<script type="text/javascript">
    var $window = $(window);

    function isMobileWidth() {
        return $window.width() <= 767;
    }

    function searchAllForm() {
        var queryParam = "";
        $("#bquery").val($("#uquery").val());

        $('#search-all input').each(function() {
            queryParam += "&" + $(this)[0].name + '=' + encodeURIComponent($(this).val());
        });

        window.open("http://search.ebscohost.com/login.aspx?" + queryParam.trim('&'));
    }
    
    function catalogueSearchForm() {
        var queryParam = "";

        
        if (isMobileWidth()) {
            queryParam += "searchtype=Y";
            queryParam += "&" + $('#catalogue-search input[name="searcharg"]')[0].name + "=" + encodeURIComponent($('#catalogue-search input[name="searcharg"]').val());
            queryParam += "&" + "searchscope=9";
            window.open("http://m-orca.douglascollege.ca/search/?" + queryParam);
        } else {
            queryParam += $('#catalogue-search select[name="searchtype"]')[0].name + "=" + $('#catalogue-search select[name="searchtype"]').find('option:selected').val();
            queryParam += "&" + $('#catalogue-search input[name="searcharg"]')[0].name + "=" + encodeURIComponent($('#catalogue-search input[name="searcharg"]').val());
            queryParam += "&" + $('#catalogue-search select[name="SORT"]')[0].name + "=" + $('#catalogue-search select[name="SORT"]').find('option:selected').val();
            queryParam += "&" + $('#catalogue-search input[name="submit"]')[0].name + "=" + $('#catalogue-search input[name="submit"]').val();
queryParam += "&" + "searchscope=9";
            window.open("http://orca.douglascollege.ca/search/~?" + queryParam);
        }
    }
    
    function courseReservesForm() {
        var queryParam = "";

        queryParam += $('#course-reserves-search input[name="SORT"]')[0].name + "=" + $('#course-reserves-search input[name="SORT"]').val();
        queryParam += "&" + $('#course-reserves-search select[name="searchtype"]')[0].name + "=" + $('#course-reserves-search select[name="searchtype"]').find('option:selected').val();
        queryParam += "&" + $('#course-reserves-search input[name="searcharg"]')[0].name + "=" + encodeURIComponent($('#course-reserves-search input[name="searcharg"]').val());
        queryParam += "&" + $('#course-reserves-search input[name="submit"]')[0].name + "=" + $('#course-reserves-search input[name="submit"]').val();

        if (isMobileWidth()) {
            window.open("http://m-orca.douglascollege.ca/search/~?" + queryParam);
        } else {
            window.open("http://orca.douglascollege.ca/search/~?" + queryParam);
        }
    }

function journalTitleForm() {
   var url = "http://search.ebscohost.com/login.aspx?direct=true" +
          "&site=eds-live&scope=site&type=44&db=edspub&custid=s5672421&groupid=main&profid=eds&mode=bool&authtype=ip,guest&bquery=";
   url += $("#C").val();
  console.log("url" , url);
   window.open(url);
}





    $(document).ready(function() {
        $('.search-input-container').hide();
        $($('.search-input-container')[$('.search-options ul li.selected').index()]).show();
        $('.search-options ul > li').click(function () {
            $('.search-input-container').hide();
            $('.search-options ul li').each(function() {
                $(this).removeClass();
            });
            $(this).addClass('selected');
            $($('.search-input-container')[$($(this)).index()]).show();
        });
    });

</script>



