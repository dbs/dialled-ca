<!-- ImageReady Preload Script (navbar.psd) -->

<script type="text/javascript">
//<![CDATA[

function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		business_over = newImage("images/images/business-over.gif");
		literacy_over = newImage("images/images/literacy-over.gif");
		prairie_over = newImage("images/images/prairie-over.gif");
		film_over = newImage("images/images/film-over.gif");
		dag_over = newImage("images/images/dag-over.gif");
		classroom_over = newImage("images/images/classroom-over.gif");
		db_over = newImage("images/topbanner_db_over.gif");
		ask_over = newImage("images/topbanner_ask_over.gif");	
		preloadFlag = true;
	}
}


// ]]>

</script>
<!-- End Preload Script -->


