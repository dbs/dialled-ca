			options.ClosePost			= 'Close this post';
		  	options.parentObj = {};
		  	CoolFeed.objectCF[options.prefix + 'options-1'] = options;
			
		  	window.addEvent('domready', function(){
		  		 CoolFeed.loadFeeds(CoolFeed.objectCF['cf-module-1855656817options-1']);
			  	 CoolFeed.enableSearchFeed(0, CoolFeed.objectCF['cf-module-1855656817options-1']);
			});
		</script>
		
		
