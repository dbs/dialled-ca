<form id="amazingSearch" method="get">
	<div class="search">
		<input id="txtSearchKeyword" maxlength="50" size="35" name="SearchTerm1" value="Search Catalogue" onFocus="this.value=''" />
		
		<script type="text/javascript"> 
		    
		    var form = document.getElementById("amazingSearch");
		    
		    try{
    		    form.addEventListener("submit", submitSearch, false);
		    }
		    catch(err) {
    		    form.attachEvent('onsubmit', submitSearch);
		    }
    		
    		function submitSearch(e) { 
    		
    		    // stop the form from submitting
    		    e.returnValue = false;
                if (e.preventDefault) e.preventDefault();
    		    
    		    var keyword = document.getElementById('txtSearchKeyword').value;
    		    var url = "http://catalogue.lclibrary.ca/iguana/www.main.cls?sUrl=search#index1=1*Keywordsbib&searchTerm1="  + keyword;
        		if (keyword){
            		try{
                		location.href = url;
            		} 
            		catch(err){
            	       	window.location = url;
            		}
        		} 
                
            } 
        </script>
	</div>
<div class="searchsubmit">
<input type="submit" value="" />
</div>
</form>

