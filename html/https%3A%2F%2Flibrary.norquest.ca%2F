  <script>
    jQuery(document).ready(function () {    
      
      var hashtag = /#\S+/;
      var pageURL;
      pageURL = jQuery(location).attr('pathname')
        // alert(pageURL);
        jQuery('.nav-tabs li a').click(function(){
          
          var match = jQuery(this).attr('href').match(hashtag);
          ga('send', 'pageview', pageURL + match[0]);
          
        })
          
          });
  </script> 
  
    
    
