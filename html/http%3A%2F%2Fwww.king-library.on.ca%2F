<script type="text/javascript">
zoomIndex = 100;
function zoom(vTemp) {
	if  ((zoomIndex * vTemp > 200) || (zoomIndex * vTemp < 70))  {vTemp = 1 };
	zoomIndex = zoomIndex * vTemp;
	document.body.style.zoom = zoomIndex + "%" 
	document.body.style['MozTransform'] = 'scale(' + zoomIndex/100 + ')';
	document.body.style.MozTransformOrigin = "0 0";

        }
function startIt()
{
//	document.getElementById('loadingImg').style.visibility = 'hidden';
	marqueeInit({
		uniqueid: 'mycrawler2',
		style: {
			'padding': '5px',
			'width': '450px',
			'height': '210px'
		},
		inc: .5, //speed - pixel increment for each iteration of this marquee's movement
		mouse: 'pause' //mouseover behavior ('pause' 'cursor driven' or false)
	});
}


var vPanelHeight;
var arMenus = new Array ("topBarMenu1","topBarMenu2" ,"topBarMenu3", "topBarMenu4","topBarMenu5","topBarMenu6","menuBox1", "menuBox2", "menuBox3", "menuBox4", "menuBox5", "menuBox6", "menuBox7")

function setPanelContents(item)
{	document.getElementById('panelContents').innerHTML = "<center>" + document.getElementById(item.id + "hide").innerHTML + "</center>";}
function getPanelHeight()
{
	var xShort = document.getElementById('panel').style;
	xShort.visibility='hidden'; xShort.position='absolute'; xShort.display='block'; xShort.height='auto';
	vPanelHeight = document.getElementById('panel').offsetHeight + 20;
	xShort.position='relative'; xShort.display='none';
}

function panel_down(vIndex, vAmt)
{	// set up variables
	var xShort = document.getElementById('panel').style; var xShortC = document.getElementById('panelContents').style;
	// visibility set, time set, height set, and Content top set
	xShort.visibility='visible'; xShort.display='block'; xShort.height = vIndex + 'px';
	vTime =  Math.min( (vPanelHeight/vIndex),1); vIndex = vIndex + vAmt ;
	xShortC.top = 10 - (10*(vPanelHeight/vIndex)) + "px"
	if (vIndex <= 0) { xShort.display = 'none'; }

 	if ( (vAmt > 0 && vIndex < vPanelHeight ) || (vAmt < 0 && vIndex > 0 )) { t = setTimeout( function(){panel_down(vIndex, vAmt);} , vTime); }
 	if (vAmt < 0  && vIndex < 10 ) { boxMenuClear("xxx"); }	
}	

function boxMenuChange(item) { 
	document.getElementById('panelContents').innerHTML = "<center>" + document.getElementById(item.id + "Hide").innerHTML  + "</center>"; 
//	document.getElementById('panelContents').innerHTML = document.getElementById(item.id + "Hide").innerHTML  ; 
	item.style.background='#700015'; item.style.color='white'; boxMenuClear(item.id); 
	}
function boxMenuClear(itemID) { 
	for(var i in arMenus){ if (arMenus[i] != itemID) { document.getElementById(arMenus[i]).style.color= ""; document.getElementById(arMenus[i]).style.background="";} }
}


</script>


