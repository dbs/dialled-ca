					<div id="carte">
		
	
			<!-- GEOCODING - MAPS -->
				
			<!-- API -->
			<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
			
			
			
			<script type="text/javascript">
									$(document).ready(
						function () 
						{
							initialize();
						}
					);
								
				var geocoder; // Le dÃ©codeur d'adresse
				var map; // La carte
				//var mapmobile; //la carte mobile
			
				// Initialisation de la carte
				function initialize() 
				{
					// GÃ©coder
					geocoder = new google.maps.Geocoder();
					
					// Options de base
					var latlng = new google.maps.LatLng(-34.397, 150.644);
					var myOptions = { zoom: 17, center: latlng, mapTypeId: google.maps.MapTypeId.ROADMAP };
					
					// La carte
					map = new google.maps.Map(document.getElementById("map"), myOptions);
					//mapmobile = new google.maps.Map(document.getElementById("mapmobile"), myOptions);
					
					// Afficher le marqueur
					setupMarker();
				}

				// CrÃ©er le marqueur
				function setupMarker() 
				{
					// Obtenir l'adresse
					var address = document.getElementById("adresse-entreprise").innerHTML;
					
					address = address.replace(/<br>/g, '');
					address = address.replace(/&nbsp;/g, ' ');
					address = address.replace(/\n/g, ' ');
					
					// Transformer l'adresse
					geocoder.geocode( { 'address': address}, 
						function(results, status) 
						{
							// Geocoder fonctionnel
							if (status == google.maps.GeocoderStatus.OK) 
							{
								// DÃ©finir le centre de la carte
								map.setCenter(results[0].geometry.location);
								//mapmobile.setCenter(results[0].geometry.location);

								// CrÃ©er le marqueur de position
								var marker = new google.maps.Marker({ map: map, position: results[0].geometry.location });
								//var markermobile = new google.maps.Marker({ map: mapmobile, position: results[0].geometry.location });
							}
						}
					);
				}
			</script>
				
			<!-- Le div qui se fait manipuler par jQuery. -->
			<div id="map" style="width: 100%; height: 275px;"></div>
				
			<script type="text/javascript">
				map = new GMap2(document.getElementById("map")); 
				//map.addControl(new GLargeMapControl()); 
				//map.addControl(new GMapTypeControl()); 
				map.setCenter(new GLatLng(0, 0), 6); 
				geocoder = new GClientGeocoder(); 
				var customUI = map.getDefaultUI();
				// Remove MapType.G_HYBRID_MAP
				customUI.maptypes.hybrid = false;
				map.setUI(customUI);
				  
				function geocode(address) 
				{ 
					geocoder.getLatLng( address, function(point)
					{ 
						if (!point)
						{ 
							//alert(address); 
						}
						else
						{ 
							map.setCenter(point, 15); 
							var marker = new GMarker(point);
							map.addOverlay(marker);
							GEvent.addListener(marker, "click", function()
								{
									marker.openInfoWindowHtml(	"<strong>BibliothÃ¨que de Terrebonne - secteur La Plaine</strong><br />6900, GuÃ©rin<br />Terrebonne
(QuÃ©bec)<br />CanadaJ7M 1L9"															);
								}
							);
						}
					} 
					); 
					return false; 
				}
				  
				//var adresse = "";
				var adresse = "6900, GuÃ©rinTerrebonne
 J7M 1L9 QuÃ©bec";
				geocode(adresse);
			</script>
		
