<div id="EvenementCalendrier"></div>
<script type="text/javascript">
//<![CDATA[
	/* - Calendrier des evenements
	------------------------------------*/
    function LoadCalendar(dateToSelect) {
        jQuery.getJSON('EventsCalendar.ashx?unique=35815bf6-dfe8-44f8-9cf0-adf23d0f9269&lang=fr-CA&cs=0',
	        function(json) {
	            if (jQuery("body").attr("id") == "lang-FR") { init_FR(); }
	            jQuery("#EvenementCalendrier")
	            .datePicker({
	                inline: true,
	                showYearNavigation: false,
	                hoverClass: false,
	                month: 2,
	                year: 2016,
	                renderCallback: function($td, thisDate, month, year) {
	                    var foundit = false;

                        $td.attr("title", "");
	                    for (i = 0; i < json.length; i++) {
	                        if (thisDate.toString() == new Date(json[i].result).toString()) {
	                            var title = new String($td.attr("title"));
	                            if (title != "undefined")
	                                $td.attr("title", json[i].value + " (...)");
	                            else
	                                $td.attr("title", json[i].value);
	                            $td.addClass("event");
	                            foundit = true;
	                        }
	                    }
	                    if (!foundit)
	                        $td.removeClass("event");
	                    if (jQuery(this).hasClass("other-month")) {
	                         $td.removeClass("event");
	                        jQuery(this).unbind("click").html("&nbsp;");
	                    }
	                    else if (!jQuery(this).hasClass("event")) {
	                        jQuery(this).unbind("click");
	                    }
	                }
	            })
	            .bind("dpMonthChanged",
	                function(e, displayedMonth, displayedYear) {
	                    adjustCalendarStyle();
                        jQuery("#EvenementCalendrier").dpClearSelected();
	                    if ('' != '')
	                        __doPostBack('', displayedMonth + 1 + ',' + displayedYear);
	                })
	            .bind("dateSelected",
	                function(e, selectedDate, $td, status) {
	                    $("#EvenementCalendrier").unbind('dateSelected');
	                    if ('' != '')
	                        __doPostBack('', selectedDate.asString() + ',' + selectedDate.asString());
	                    else
	                        window.location = "http://www.cssslaval.qc.ca/evenements.html?d=" + selectedDate.asString();
	                });

	            if (dateToSelect != '') {
	                jQuery("#EvenementCalendrier").unbind("dateSelected");
	                jQuery("#EvenementCalendrier").unbind("dpMonthChanged");
	                jQuery("#EvenementCalendrier").dpSetSelected(dateToSelect);
	                jQuery("#EvenementCalendrier").bind("dpMonthChanged",
	                    function(e, displayedMonth, displayedYear) {
	                        adjustCalendarStyle();
                            jQuery("#EvenementCalendrier").dpClearSelected();
	                        if ('' != '')
	                            __doPostBack('', displayedMonth + 1 + ',' + displayedYear);
	                    })
                    jQuery("#EvenementCalendrier").bind("dateSelected",
                        function(e, selectedDate, $td) {
                            $("#EvenementCalendrier").unbind('dateSelected');
                            if ('' != '')
                                __doPostBack('', selectedDate.asString() + ',' + selectedDate.asString());
                            else
                                window.location = "http://www.cssslaval.qc.ca/evenements.html?d=" + selectedDate.asString();
                        });
	            }
	            adjustCalendarStyle();
	        });
	    }

	    LoadCalendar('');

        var prm = Sys.WebForms.PageRequestManager.getInstance();
        prm.add_endRequest(function() {
            $("#EvenementCalendrier").bind('dateSelected',
                function(e, selectedDate, $td) {
                    $("#EvenementCalendrier").unbind('dateSelected');
                    __doPostBack('', selectedDate.asString() + ',' + selectedDate.asString());
                });
        });
//]]>
</script>

    <p><a href="http://www.cssslaval.qc.ca/evenements.html">Événements du mois courant</a></p>

