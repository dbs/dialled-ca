	jQuery(document).ready(function($){
	 $('#tracPacLogin').tabSlideOut({
		 tabHandle: '.handle',
		 tabLocation: 'right', 
		 speed: 300,
		 action: 'click',
		 topPos: '57px',
		 fixedPosition: true
	 });
