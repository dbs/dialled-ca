		<script>

		$(document).ready(function(e) {
			$('div.leftNav-item-wrapper a, div.homepage-tile a, div.homepage-tile div.tile-content div.tile-text h3.title a').on('click touchend', function(e) {
      		var el = $(this);
      		var link = el.attr('href');
      		var param = window.location.search;
      		if(el.attr('target') == '_blank'){
      			e.preventDefault();
      			window.open(link + param,'_blank');
      		}else{
      			e.preventDefault();
      			window.location = link + param;
      		}
   			});
		});
	</script>
				
