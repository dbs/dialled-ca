    <script type="text/javascript">

        function changeGoogleTranslate() {
            if ($('.goog-te-combo option:first-child').text() == "Select Language") {
                $('.goog-te-combo').attr('title', 'Google Translate');
                $('.goog-logo-link img').attr('alt', 'Google');
                $('#goog-gt-tt div.logo img').attr('alt', 'Google Translate Logo');
            }
            else {
                setTimeout(changeGoogleTranslate, 50);
            }
        }

        function BindHomeControlEvents() {

            changeGoogleTranslate();

        }

        //Initial bind
        $(document).ready(function () {
            var Height1 = $('.resourcesbox:first-child').height();
            var Height2 = $('.resourcesbox:nth-child(2)').height();
            if (Height1 > Height2) {
                $('.resourcesbox:nth-child(2)').css({ 'height': Height1 + 'px' });
            }
            else {
                $('.resourcesbox:first-child').css({ 'height': Height2 + 'px' });
            }
            BindHomeControlEvents();
        });

        //Re-bind for callbacks 
        var prm = Sys.WebForms.PageRequestManager.getInstance();

        prm.add_endRequest(function () {
            BindHomeControlEvents();
        });
    </script>
