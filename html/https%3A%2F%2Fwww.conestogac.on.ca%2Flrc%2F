		<script type="text/javascript">
		//<![CDATA[
		function fixURL(myForm){
			var url = "http://search.ebscohost.com/login.aspx?";
			$(myForm).find("input,select").each(function(){
				url += "&"+$(this).attr("name")+"="+$(this).val();
			});
			url = url.replace(/\&/,'');
			window.open("http://ra.ocls.ca/ra/login.aspx?inst=conestoga&amp;url="+url,"_blank");
			return false;
		}
		//]]>
		</script>
		<script type="text/javascript">
	   //<![CDATA[
	   function fixURLJournal(myForm){
		   var url = "http://atoz.ebsco.com/titles/searchresults/11973?";
		   $(myForm).find("input,select").each(function(){
				   url += "&"+$(this).attr("name")+"="+$(this).val();
		   });
		   url = url.replace(/\&/,'');
		   console.log("http://ra.ocls.ca/ra/login.aspx?inst=conestoga&amp;url="+url);
		   window.open("http://ra.ocls.ca/ra/login.aspx?inst=conestoga&amp;url="+url,"_blank");
		   return false;
	   }
	   //]]>
	   </script>

