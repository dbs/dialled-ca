$(function(){
    $('.form_styled').jqTransform({imgPath:'/static/embedded/commons-themes/themes/V2_h/images/'});
    $('.form_styled').jqTransform({imgPath:'/static/layouts/classic/images/'});
    $('img').each(function () {
    	if($(this).parents('a').size() == 0) {
    		$(this).wrap("<a href='" + $(this).attr("src").replace(new RegExp("(.*/)\.(.*)\_[omst]\.(.*)"), "$1$2.$3") + "' />");
    	}
    });
    
    if (-1 != -1) {
	    $('img').click(function () {
	    	setTimeout(function () {
	    		correction();
	    	}, 2000);
	    });
	    function correction() {
	     	$('img').each(function () {
	       		if($(this).attr("src").indexOf('lightbox-btn') != -1) {
	       			$(this).attr("src","../../"+$(this).attr("src"));
	       		}
	        });
	    };
    }
    
	$("a[href$=.png]").lightBox();
	$("a[href$=.jpg]").lightBox();
	$("a[href$=.gif]").lightBox();
 	$("a[href$=.swf]").lightBox();
 	/* Specific JS for theme V6h made by CRB */
 	});</script>


