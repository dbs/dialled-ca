		return false;
}


	if (document.courriel.AnomFrancais.value =="")
	{
		alert("Vous devez inscrire votre nom.")
		return false;
	}

	if (document.courriel.AadresseFrancais.value =="")
	{
		alert("Vous devez inscrire votre adresse.")
		return false;
	}

	if (document.courriel.AcodePostalFrancais.value =="")
	{
		alert("Vous devez inscrire votre code postal.")
		return false;
	}
	if (document.courriel.AtelResFrancais.value =="")
	{
		alert("Vous devez inscrire votre numero de téléphone.")
		return false;
	}

	if (document.courriel.Anom2Francais.value =="")
	{
		alert("Vous devez inscrire le nom de votre entrepreneur. ")
		return false;
	}

