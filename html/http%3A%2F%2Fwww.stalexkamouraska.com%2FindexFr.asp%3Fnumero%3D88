	{
		alert("Vous devez inscrire l'adresse de votre entrepreneur.")
		return false;
	}

	if (document.courriel.AcodePosta2lFrancais.value =="")
	{
		alert("Vous devez inscrire le code postal de votre entrepreneur.")
		return false;
	}
	if (document.courriel.AtelRes2Francais.value =="")
	{
		alert("Vous devez inscrire le numero de téléphone de votre entrepreneur.")
		return false;
	}





if (document.courriel.ADatePrevueDebut && !EW_hasValue(document.courriel.ADatePrevueDebut, "TEXT" )) {
