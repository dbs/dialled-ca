jQuery('#menumobile ul.nav-child > li').click(function(e) {
    e.stopPropagation();
    if (jQuery(this).children("ul").size() == 0)
      return true;
    maxheight = jQuery(this).children("ul").css('max-height');

    previousul = jQuery(this).parent();
    previousul.children("li").children("ul").css('max-height','0');
    if (maxheight != '1000px') {
      jQuery(this).children("ul").css('max-height','1000px');
    }
    return false;
  });

jQuery('#menumobile ul.nav > li').click(function(e) {
    e.stopPropagation();
    if (jQuery(this).children("ul").size() == 0)
      return true;
    maxheight = jQuery(this).children("ul").css('max-height');

    previousul = jQuery(this).parent();
    previousul.children("li").children("ul").css('max-height','0');
    if (maxheight != '1000px') {
      jQuery(this).children("ul").css('max-height','1000px');
    }
    return false;
  });

jQuery('.leftcolmodule ul.nav-child > li').click(function(e) {
    e.stopPropagation();
    if (jQuery(this).children("ul").size() == 0)
      return true;
    maxheight = jQuery(this).children("ul").css('max-height');

    previousul = jQuery(this).parent();
    previousul.children("li").children("ul").css('max-height','0');
    if (maxheight != '1000px') {
      jQuery(this).children("ul").css('max-height','1000px');
    }
    return false;
  });

jQuery('.leftcolmodule ul.nav > li').click(function(e) {
    e.stopPropagation();
    if (jQuery(this).children("ul").size() == 0)
      return true;
    maxheight = jQuery(this).children("ul").css('max-height');

    previousul = jQuery(this).parent();
    previousul.children("li").children("ul").css('max-height','0');
    if (maxheight != '1000px') {
      jQuery(this).children("ul").css('max-height','1000px');
    }
    return false;
  });

jQuery('.topmodule').click(function(e) {
    e.stopPropagation();
    maxheight = jQuery(this).next().css('max-height');;
    if (maxheight != '2000px') {
      jQuery(this).next().css('max-height','2000px');
    } else {
      jQuery(this).next().css('max-height','0');
    }
    return false;
  });
