        $(function($) {
            var bgImages = new Array();
            bgImages[0] = "/images/bg-1.jpg";
            bgImages[1] = "/images/bg-2.jpg";
            bgImages[2] = "/images/bg-3.jpg";
            bgImages[3] = "/images/bg-4.jpg";
            var rndNum = Math.floor(Math.random() * 3);
            $.supersized({
                //Background image
                slides: [{ image: bgImages[rndNum]}]
            });
            $("#accordion").accordion({ header: "h2", autoHeight: false, collapsible: false, active: true });
        });

        $(document).ready(function() {
            $(".section-content").fitVids();        
            $("#loginLink").click(function () {
                $("#loginEmailAddress").parent().find('span').hide();
                $("#loginPassword").parent().find('span').hide();
                $("#loginPopupMessageText").hide();
                $('.loginPopup').toggle("slide");
            });

            $(".login-close a").click(function () {
                $("#loginEmailAddress").parent().find('span').hide();
                $("#loginPassword").parent().find('span').hide();
                $("#loginPopupMessageText").hide();
                $('.loginPopup').hide();
            });
            $(".utilityBoxToggle a").click(function() {
                $(".utilityBox").fadeIn();
            });
        });

        
        function validateLogin() {
            var username = $("#ctl00_loginEmailAddress").val();
            var password = $("#ctl00_loginPassword").val();
            $("#ctl00_msg_Email").html("");
            $("#msg_Password").html("");
            var isValid = true;
            if (username == "") {
                $("#ctl00_msg_Email").html("Required");
                isValid = false;
            }
            if (password == "") {
                $("#msg_Password").html("Required");
                isValid = false;
            }

            return isValid;
        }

        function validateEmail(emailAddr) {
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            if (reg.test(emailAddr) == false) {
                return false;
            }
            return true;
        }
