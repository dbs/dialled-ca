      // very simple to use! (need bootstrap-hover-dropdown.js)
      $(document).ready(function () {
          $('.js-activated').dropdownHover().dropdown();
      });


      //this script to get the viewport size of the device and display on the <span id="monitor"></span>
      /*$('#monitor').html($(window).width());
      $(window).resize(function () {
          var viewportWidth = $(window).width();
          $('#monitor').html(viewportWidth);
      });*/

      //this clickMe will make the drop down top level navigation make a clickable link
      function clickMe(obj) {
          if ($(obj).data('clicked') == 'true') {
              location.href = $(obj).attr('href');
          }
          else {
              $(obj).data('clicked', 'true');
          }
      }
</script>

