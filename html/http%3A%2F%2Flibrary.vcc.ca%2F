	a.next:hover {		background-position: -50px -50px; }
	a.next.disabled {	background-position: -50px -100px !important;  }
	a.prev.disabled, a.next.disabled {
		cursor: default;
	}
	
	a.prev span, a.next span {
		display: none;
	}
	
	.carousel_wrapper_wide a.prev, .carousel_wrapper_wide a.next {
	display: none;
	}
	
	.pagination {
		text-align: center;
	}
	.pagination a {
		background: url(jquery/carouFredSel/miscellaneous_sprite.png) 0 -300px no-repeat transparent;
		width: 15px;
		height: 15px;
		margin: 0 5px 0 0;
		display: inline-block;
	}
	.pagination a.selected {
		background-position: -25px -300px;
		cursor: default;
	}
	.pagination a span {
		display: none;
	}
	.clearfix {
		float: none;
		clear: both;
	}
	
	#carousel ul li img, #carousel ul li p { display: inline; }
	#carousel ul li img { width: 30%; }
	#carousel ul li p { width: 60%; }

</style>

