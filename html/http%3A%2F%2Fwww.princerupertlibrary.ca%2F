    <h2 class="nodeTitle"><a href="/search_catalogue">Search Our Catalogue</a>
		</h2>    
	    
    <div class="content">
	
	
	<form target="_blank" method="get" action="http://bpr.bc.catalogue.libraries.coop/eg/opac/results" name="catsearch">
<select name="qtype" onchange="return changeTp();">
    <option value="keyword">Keyword</option><br />
    <option value="title">Title</option><br />
    <option value="author">Author</option><br />
    <option value="subject">Subject</option><br />
    </select>
<p>     <input type="hidden" name="qtype" value="keyword" /> <input type="hidden" name="locg" value="2" /> <input type="text" name="query" size="25" style="color: rgb(153, 153, 153);" maxlength="128" id="t" onblur="this.value = this.value || this.defaultValue; this.style.color = '#999';" onfocus="this.value=''; this.style.color = '#000'; " value="Search Library Catalogue..." /><input type="submit" value="Search" onclick="return ActionDeterminator();" /><br />
</p></form>
</div>
    
    
        
                
    
    
    
