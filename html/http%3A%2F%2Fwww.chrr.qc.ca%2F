<!-- Initialize each slider on the page. Each slider must have a unique id -->
<script type="text/javascript">
$(document).ready(function() {
	$("img.rollover").hover(
		function() {
			this.src = this.src.replace("_off", "_on");
	},
	function() {
		this.src = this.src.replace("_on", "_off");
	});
});

</script>



