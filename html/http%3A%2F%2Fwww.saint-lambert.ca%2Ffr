<div id="block-nodeblock-235" class="block block-nodeblock feedback-form">

  <h2 class="block-title">
  	<a href="javascript:void(0)">  	Donnez-nous votre avis  	</a>  </h2>
  
  <div class="content" class="content clearfix">
    <div id="node-235" class="node node-webform clearfix" about="/fr/donnez-nous-votre-avis" typeof="sioc:Item foaf:Document" class="node node-webform node-published node-not-promoted node-not-sticky author-sa odd clearfix" id="node-webform-235">

        <span property="dc:title" content="Donnez-nous votre avis" class="rdf-meta element-hidden"></span>
  
  <div class="content" class="content clearfix">
    <form class="webform-client-form" enctype="multipart/form-data" action="/fr" method="post" id="webform-client-form-235" accept-charset="UTF-8"><div><div class="form-item webform-component webform-component-select" id="webform-component-type-de-requete">
