<script type="text/javascript">
//<![CDATA[
$(document).ready(function() {
        
    $('#search-box-terme').keypress(function(key){
        if (key.which == 13){
            $('#searchLink').click();
        }
    });

    $('#search-box-terme').click(function(event){
        if($('#search-box-terme').val() == 'Rechercher')
        {
            $(this).val('');
        }
    });

    

    $('#searchLink').click(function(event){
        event.preventDefault();
        if($('#search-box-terme').val() != 'Rechercher'
            && $('#search-box-terme').val() != '')
        {
            var url = $('#search-url').val()+"/" + $('#search-box-terme').val();
            window.location = url;
        }
    });

});
//]]>
</script>  
    
    
 <script type="text/javascript">   
    
    $(document).ready(function() { 
        
        $('div.button_round').each(function(i){             
            $(this).removeClass("button_round");
            $(this).wrap('<div class="button_round_outer" />');
            $(this).before('<div class="button_round_right" />');
            $(this).after('<div class="button_round_left" />');
            $(this).addClass("button_round_center");
        });

        $('div.button_round').click(function(){
            window.location.href = $(this).find('a').attr('href');
        });
        
        
        $('div.button_round_right2').each(function(i){             
            $(this).removeClass("button_round_right2");
            $(this).wrap('<div class="button_right" />');
            $(this).before('<div class="button_right_corner" />');
            $(this).after('<div class="button_left_corner" />');
            $(this).addClass("button_center");
        });

        $('div.button_right').click(function(){
            window.location.href = $(this).find('a').attr('href');
        });
        
        $('div.button_round_left2').each(function(i){             
            $(this).removeClass("button_round_left2");
            $(this).wrap('<div class="button_left" />');
            $(this).before('<div class="button_right_corner" />');
            $(this).after('<div class="button_left_corner" />');
            $(this).addClass("button_center");
        });

        $('div.button_left').click(function(){
            window.location.href = $(this).find('a').attr('href');
        });
        
        
        
        
        
        function getOffset( el ) {
            var _x = 0;
            var _y = 0;
            while( el && !isNaN( el.offsetLeft ) && !isNaN( el.offsetTop ) ) {
                _x += el.offsetLeft - el.scrollLeft;
                _y += el.offsetTop - el.scrollTop;
                el = el.parentNode;
            }
            return { top: _y, left: _x };
        }        
             
          
       
       
       $('ul.top-menu li.level-1 ul.level-1 li').mouseenter(function(){
           
            var heightV = $(this).height();         
            var par = $(this).attr('id');
            var child1 = $('#' + par + ' ul');
            var msieV = parseInt($.browser.version);
            if(($.browser.msie)&&(msieV<8)){                   
                child1.css('margin-top',-(heightV+6));              
                
            }            
            else{
               
                child1.css('margin-top',-(heightV+12)); 
            }
         }   
       );    
        
   });
</script>            </div>
