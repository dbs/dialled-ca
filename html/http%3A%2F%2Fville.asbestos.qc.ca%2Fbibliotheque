       $('ul.sitemap li.appendSiteMap').each(function(i){             
            $('ul.sitemap li.couleur0_siteMap div.li_between_site_map1').append($(this));
          
        }); 
        
        if($('h1').length>1){
             $('h1.siteMapTitle').replaceWith(function() {
                return '<h2 class="siteMapTitle"><div class="divSiteMap">' + $(this).text() + '</div></h2>';
            });
        }
        
        
        
        
        
    });
</script>

