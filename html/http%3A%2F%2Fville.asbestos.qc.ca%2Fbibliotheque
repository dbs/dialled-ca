<script type="text/javascript">
    $(document).ready(function() {
        $('#ul_top_menu_rapide li').click(function(){
            window.location.href = $(this).find('a').attr('href');
        });
        
        $('#ul_main-menu ul.level-1 li').click(function(){
            window.location.href = $(this).find('a').attr('href');
        });
        
        $('#ul_main-menu ul.level-2').parent().addClass('has_sub-menu');
        
        $('#ul_submenu ul.level-1').parent().addClass('has_sub-menu');
        
        $('#ul_main-menu li.level-1').each(function(i){
            $(this).addClass("li_couleur" + i);
        });
        
        $('#ul_main-menu li.level-1').each(function(i){
            // $(this).find('a.level-1').addClass('hahaha');
            var textA = $(this).find('a.level-1').text();
            var myArray = textA.split(' ');
            var one = "";
            var two = "";
            for(var i=0;i<myArray.length;i++){
                if(i==0){
                    one = "<div class='li_one'>" + myArray[i] + "</div>";
                }
                else{
                    two = "<div class='li_two'>" + myArray[i] + "</div>";
                }                
            }
            $(this).find('a.level-1').html(one + two);
        });
        
        
        
    });
</script>            </div>
