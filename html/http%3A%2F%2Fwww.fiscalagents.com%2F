            <script>

	function BOBHighlight(selTable, selRow) {
		btnName = "sbrButtonOver"; if (selTable == 'ACC') { btnName = "savButtonOver"; }
		var numCols = 7; if (selTable == 'RIF') { numCols = 8; }
		if (selRow != undefined) { selRow = "_" + selRow; } else { selRow = ""; }
		for (var iSeg = 1; iSeg < numCols+1; iSeg++) {
			eval("oElement = document.getElementById('" + selTable + selRow + "_Col" + String(iSeg) + "');"); oElement.className = btnName; }
	}

	function BOBDeselect(selTable, selRow) {
		btnName = "sbrButton"; if (selTable == 'ACC') { btnName = "savButton"; }
		var numCols = 7; if (selTable == 'RIF') { numCols = 8; }
		if (selRow != undefined) { selRow = "_" + selRow; } else { selRow = "" };
		for (var iSeg = 1; iSeg < numCols+1; iSeg++) {
			eval("oElement = document.getElementById('" + selTable + selRow + "_Col" + String(iSeg) + "');"); oElement.className = btnName; }
	}

</script>
