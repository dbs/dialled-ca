<div id="content" class="panel-container">
    
    <div class="region region-content">
    <div id="block-system-main" class="block block-system">

        
  <div class="content">
    
<div class="panel-display moscone clearfix radix-moscone" >

  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 radix-layouts-header panel-panel">
        <div class="panel-panel-inner">
          <div class="panel-pane pane-entity-field pane-node-field-texte-page-accueil"  >
  
      
  
  <div class="pane-content">
    <div class="field field-name-field-texte-page-accueil field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even"><!--Téléphones-->

<div class="text-center couleur1 visible-xs" style="padding-top: 10%;">
    <h4 style="font-size: 44px">
        Être plus<br>près Aller<br>plus loin
    </h4>
    <div class="text-center margin-top-30" style="font-size: 13px; border: solid #000 1px; margin: 30px; padding: 5px;">
        <a href="/premiercycle" style="color: #000">PREMIER CYCLE</a> | <a href="/cyclessuperieurs" style="color:#000;">CYCLES SUPÉRIEURS</a>
    </div>
</div>

<!--Tablettes-->
<div class="text-center couleur1 visible-sm" style="padding-top: 4em;">
    <h1 class="couleur-1 xlarge" style="line-height:85%;"><span class="letter-spacing-minus">Être plus près</span><br> <span class="letter-spacing-plus">Aller plus loin</span></h1>
    <p class="couleur-1 lead padding-top-15">L'université de l'Outaouais et des Laurentides</p>

    <a href="/premiercycle" class="btn btn-sm btn-transparent width-150">ÉTUDES DE<br>PREMIER CYCLE</a>
