<script language="javascript" type="text/javascript">
	function setSearchField(whatfield) {
		switch(whatfield.value) {
			case '':
				whatfield.value = 'Search Perth County';
			break;
			case 'Search Perth County':
				whatfield.value = '';
			break;
		}
		
	}
/* ]]> */
</script>

