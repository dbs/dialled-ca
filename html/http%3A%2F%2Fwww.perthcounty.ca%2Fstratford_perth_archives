		$(this).text('');
		
		
		//$(this).after("<a name=\"fb_share\" type=\"button_count\" href=\"http://www.facebook.com/sharer.php\"><img src='/images/fbShare.gif' alt='Share' style='padding:0px; margin:0px;' /></a>");
	
		
  		flowplayer(this, "/flx/flowplayer-3.1.4.swf", {
		
