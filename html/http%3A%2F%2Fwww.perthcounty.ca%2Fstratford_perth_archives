<script type="text/javascript"> 
/* <![CDATA[ */

$(document).ready(function(){

//video
 $("a[href*='.flv']").each(function() {
 		
		$(this).attr('style', 'display: block; width: 320px; height: 240px;');
		$(this).text('');
		
		
		//$(this).after("<a name=\"fb_share\" type=\"button_count\" href=\"http://www.facebook.com/sharer.php\"><img src='/images/fbShare.gif' alt='Share' style='padding:0px; margin:0px;' /></a>");
	
		
  		flowplayer(this, "/flx/flowplayer-3.1.4.swf", {
		
		clip: {
			autoPlay:false,
			autoBuffering: true
			
		}
		
		});
		
 });
 //end of video

/********************** vv Magic Style Sheet Changing Code vv **********************/
/********************** vv Magic Style Sheet Changing Code vv **********************/
function switchStylestyle(styleName, clickedLink) {
	$('link[rel*=style][title]').each(function(i) {

		this.disabled = true;
		if (this.getAttribute('title') == styleName) { 
			this.disabled = false;
		}
	});
	createCookie('style', styleName, 365);
}
function createCookie(name,value,days) {
	if (days) {
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	} else { 
		var expires = "";
	}
	document.cookie = name+"="+value+expires+"; path=/";
}
function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
//	alert(ca);
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') { 
			c = c.substring(1,c.length);
		}
		if (c.indexOf(nameEQ) == 0) {
			return c.substring(nameEQ.length,c.length);
		}
	}
	return null;
}

/********************** ^^ Magic Style Sheet Changing Code ^^ **********************/
/********************** ^^ Magic Style Sheet Changing Code ^^ **********************/

/********************** vv Magic Style Sheet Changing Code vv **********************/
/********************** vv Magic Style Sheet Changing Code vv **********************/
	$('.styleswitch').click(function() {
		switchStylestyle(this.getAttribute("rel"), this);
		return false;
	});
	var c = readCookie('style');
	if (c) switchStylestyle(c);
/********************** ^^ Magic Style Sheet Changing Code ^^ **********************/
/********************** ^^ Magic Style Sheet Changing Code ^^ **********************/
	
/********************** vv Magic Text Resizing Code vv **********************/
/********************** vv Magic Text Resizing Code vv **********************/
	var whatToScale = '#content p, #content a:not(.searchResultItemLink)';
	var MINSIZE = 7;
	var BASSIZE = Array();
	var MAXSIZE = 18;
	var runResize = true;
 
 
	/*********** vv init all font-size vv ****************/
	var initCounter = 0;
	
	
	// home-font is the homepage font size 
	// always one pixel larger than the other pages
	var storedSize = readCookie('stored-font');
	var currentSizeInc = 0;
	
	var bodyBaseSize = $('body').css('font-size');
	bodyBaseSize = parseFloat(bodyBaseSize, 10);
	
	$(whatToScale).each(function(i) {
		
	 	var currentSize = $(this).css('font-size');
			
		// parse the number value out of the font size value, set as a var called 'num'
		var num = parseFloat(currentSize, 10);
		// make sure current size is 2 digit number, save as var called 'unit'
		var unit = currentSize.slice(-2);
	
		
		// BASESIZE is the value of difference between the body font size
		BASSIZE[initCounter++] = num;
		
			
		if (storedSize != 'normal') {
			if (storedSize != undefined) {
				currentSizeInc = storedSize - bodyBaseSize;
			}
			
			/*
if (initCounter == 1) { 
				alert(storedSize + ' - ' +  bodyBaseSize);
				alert(currentSizeInc);
				alert(num + ' => ' + unit);
				alert((num + currentSizeInc));
			}
*/
		
		
			//alert((num + currentSizeInc) + unit);
			
			$(this).css('font-size', (num + currentSizeInc) + unit);
			
		}
		
	});  
	/*********** ^^ init all font-size ^^ ****************/


	$("a.textScaler").click(function(){
		$clickedLink = this;
		var loopCounter = 0;
		var runResize = true;
	
		/***** vv Test for limits vv *****/
		if($clickedLink.id != 'linkNormal') {
			$(whatToScale).each(function(i) {
				
				var currentSize = $(this).css('font-size'); 
				var num = parseFloat(currentSize, 10);			
			
				if ($clickedLink.id == 'linkLarge' && (storedSize) >= MAXSIZE) {				
					runResize = false;
					return false;
				} else if ($clickedLink.id == 'linkSmall' && num <= MINSIZE) {			
					runResize = false;
					return false;
				} else {
					//$('#linkLarge').css('text-decoration','underline');					
				}
			});
		}
		/***** ^^ Test for limits ^^ *****/
	
		/***** vv  Change Text size vv *****/
		if(runResize) {
			
			var num;
			var unit;
			var clicked;
			$(whatToScale).each(function(i) {
			
				var currentSize = $(this).css('font-size');
				// parse the number value out of the font size value, set as a var called 'num'
				num = parseFloat(currentSize, 10);
				// make sure current size is 2 digit number, save as var called 'unit'
				unit = currentSize.slice(-2);
			
				// javascript lets us choose which link was clicked, by ID
				if ($clickedLink.id == 'linkLarge') { 
					num = num + 2;
					clicked = 'large';
				} else if ($clickedLink.id == 'linkSmall' && num > MINSIZE) { 
					num = num - 2; 
					clicked = 'small';
				} else if ($clickedLink.id == 'linkNormal') { 					
					clicked = 'normal';
					num = BASSIZE[loopCounter++];
				}
				// jQuery lets us set the font Size value of the mainText div
				$(this).css('font-size', num + unit);
				
			});
			
			if (clicked == 'normal') { 
				createCookie('stored-font', 'normal', 365);
			} else if (clicked == 'large') {
				if (storedSize == 'normal' || storedSize == 'NaN' || storedSize == null) storedSize = bodyBaseSize;
				storedSize = (+storedSize + 2);
				createCookie('stored-font', +storedSize, 365);
				
				
			
			} else if (clicked == 'small') {
				if (storedSize == 'normal' || storedSize == 'NaN' || storedSize == null) storedSize = bodyBaseSize;
				
				storedSize = (+storedSize - 2);
				createCookie('stored-font', +storedSize, 365);
				
			}
			
		}		
		/***** ^^ Change Text size ^^ *****/
	
	});
	
/********************** ^^ Magic Text Resizing Code ^^ **********************/
/********************** ^^ Magic Text Resizing Code ^^ **********************/


	
	
	//BUILD OUT THE NAV	
	
	var arrowimages={down:['downarrowclass', '/images/template/standard/downSmall.gif', 10], right:['rightarrowclass', '/images/template/standard/rightSmall.gif']}
	jqueryslidemenu.buildmenu("primaryNavItems", arrowimages);
	
	var arrowimages={down:['downarrowclass', '/images/layout/transparent.gif', 10], right:['rightarrowclass', '/images/layout/transparent.gif']}
	jqueryslidemenu.buildmenu("externalLinks", arrowimages);
	
	var arrowimages={down:['downarrowclass', '/images/layout/transparent.gif', 10], right:['rightarrowclass', '/images/layout/transparent.gif']}
	jqueryslidemenu.buildmenu("helpYou", arrowimages);
	
	if ($.browser.msie) {

		$("#quickLinkCapture").css("left", "auto");
		$("#quickLinkCapture").css("top", "2px");
		$("#quickLinkCapture").css("position", "absolute");
		$("#quickLinkCapture").css("z-index", "900");
		$("#quickLinkCapture").css("width", "100px");
		$("#quickLinkCapture").css("height", "25px");
		
	}
	
	
	var arrowimages={down:['downarrowclass', '/images/template/standard/down.gif', 23], right:['rightarrowclass', '/images/template/standard/right.gif']}
	jqueryslidemenu.buildmenu("quickLinkDropDown", arrowimages);
	//PNG fix for the Home Banner
	$.ifixpng('/images/pixel.gif');
	$("img.homeBannerFeaturedGraphic").ifixpng();
	
	$("#primaryNavItems ul li ul").ifixpng();

	
	


});//$(document).ready(function(){


