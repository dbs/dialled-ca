<div id="innerSubNav"><script type="text/javascript">
$(document).ready(function(){

	// accord function for primary nav  
	//Brendan Farr-Gaynor
	//brendan@resolutionim.com
	
	//for each navClickerTog inside a li that has a child ul add this click event
	$('#innerNav ul.navTree li:has(ul)').children("span.navClickerTog").click(function() {
		
		//each time the button is clicked toggle it's class to swap the display graphic
		$(this).toggleClass("navClickerTogUp");
		//slide up/down the child ul inside this li to open it if it's closed, close it if it's open
		$($(this).parent().get(0)).children("ul").slideToggle('slow', function() { 
			//there is a problem with flow as both the li label and navClickerTog are floated so the li was collapsing
			//a 'flowKill div was inserted in normal flow between these two elements and set to a height of 27px to force the li to display properly
			//when the child ul was collapsed
			if($(this).is(':hidden')) { $($(this).parent().get(0)).children("div.flowKill").show(); }
			
			
			
		
		});
		
		//indent each level of ul children by 10 pixels padding to the left
		$($(this).parent().get(0)).children("ul").children("li").addClass('innerLi');
		
		//if the ul is no longer hidden the li no longer needs to be held open by the flowKill, so hide it for now so that we don't have a big gap at the bottom
		//Note: toggle() is not used because the animation requires a different sequence to the hide/show, the show must happen after the slide has completed but the hide
		//must happen immediately
		if(!$($(this).parent().get(0)).children("ul").is(':hidden')) {
			$($(this).parent().get(0)).children("div.flowKill").hide();
		}
		
		
		
	});
	
	
	//toggle open the current item and any child uls
	$("#innerNav .currentPage").children("span.navClickerTog").click();
	
	//go through the parents
	$("#innerNav .currentPage").parents("li").children("span.navClickerTog").click();



	//color the currently selected item white
	$("#innerNav .currentPage").children("a.navLinkLabel").css('color','#FFFFFF');
	$("#innerNav .currentPage").css('background','url(/images/template/standard/currentPageLiBack.gif) no-repeat left top #9BB5A0');
	//$(".currentPage").css('border-bottom','1px solid #C86C2F');
	//$(".currentPage:has(span.navClickerTog)").css('background','url(/images/template/standard/currentPageLiBackTrans.gif) no-repeat left top');

	
	
	
	$('#noscriptNav').hide();
	$('#scriptNav').show();

});


</script>

