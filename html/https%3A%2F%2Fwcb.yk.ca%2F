                <script type="text/javascript">
                    head.ready(function () {
                        $("#ctl00_cphContent_txtQuestion").focus();
                        $("#ctl00_cphContent_txtQuestion").on("keydown", function (e) {
                            if (e.keyCode == 13) {
                                e.preventDefault();
                                $("#ctl00_cphContent_btnQuestionSubmit").trigger("click");
                            }
                        });
                        $("#ctl00_cphContent_btnQuestionSubmit").on("click", function () {
                            if ($("#ctl00_cphContent_txtQuestion").val() == "") {
                                alert("Please enter a statement or question that you'd like us to respond to.");
                                return false;
                            } else {
                                try {
                                    _gaq.push(['_trackEvent', 'Questions', 'Home-Search', $("#ctl00_cphContent_txtQuestion").val()]);
                                } catch (e) {
                                    //
                                }
                            }
                        });
                    });
                </script>
