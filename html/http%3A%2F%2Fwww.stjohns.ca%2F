  <div class="content">
    
<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery('#block-views-banner-block-1 .view-content').cycle({
    fx: 'fade',
    timeout: 5000,
    
    activePagerClass: 'active'
    
  });
});
</script>

