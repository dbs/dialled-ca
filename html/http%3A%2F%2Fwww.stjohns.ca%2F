<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery('.slideshow-homenews').cycle({
    fx: 'fade',
    timeout: 8000,
    pager: '.pagination-news',
    activePagerClass: 'active',
    prev: '.prev-btn-news',
    next: '.next-btn-news'
  });
  //CSJMOD - Sept 30, 2013 - R.Power - Set variable for default pause button on show or not, in case we have
  //to allow the pause to be defaulted again like before.  W.O. # 53904
  var defaultpause = false;
  if(defaultpause){
  //CSJMOD - August 29, 2013 - Temporarily pause the slideshow and add green background to pause button
  	jQuery('.slideshow-homenews').cycle('pause');
  	jQuery('#pause_button').css('background', '#69b245');
  }
  //CSJMOD - End
});
function pauseSlideShowNews()
{
	jQuery('.slideshow-homenews').cycle('pause');
	//CSJMOD - August 29, 2013 - Temporarily pause the slideshow and add green background to pause button
	jQuery('#pause_button_news').css('background-position', '-181px -133px');
	//CSJMOD - End
}
function resumeSlideShowNews()
{
	jQuery('.slideshow-homenews').cycle('resume');
	//CSJMOD - August 29, 2013 - Temporarily pause the slideshow and add green background to pause button
	jQuery('#pause_button_news').css('background-position', '-151px -133px');
	//CSJMOD - End
}
</script>


