<!-- Scroll 2 top -->
<script src='/Gsc/respons-v2/scroll2top/scroll2top.js'></script>
<script type="text/javascript">
var scroll_pos = 0;
var scroll_time;

$(window).scroll(function() {
    clearTimeout(scroll_time);
    var current_scroll = $(window).scrollTop();
    
    if (current_scroll >= $('#h-header').outerHeight()) {
        if (current_scroll <= scroll_pos) {
            $('#h-header').removeClass('hidden');    
        }
        else {
            $('#h-header').addClass('hidden');  
        }
    }
        
    scroll_time = setTimeout(function() {
        scroll_pos = $(window).scrollTop();
    }, 10);
});
</script>






