        $("table[id$='-subMenu']").hover(function() {
            var Menuid = $(this).attr('id').replace("-subMenu", "");
            $('#' + Menuid).removeClass('menu').addClass('menu_over');


        }, function() {
            var Menuid = $(this).attr('id').replace("-subMenu", "");
            $('#' + Menuid).removeClass('menu_over').addClass('menu');
        });

        //$("table[id$='-subMenu']").wrap("<div style='position:relative;overflow:hidden;'></div>");

        $(".menu").mouseenter(function() {
            var submenu = $(this).attr('id') + '-subMenu';
            var h3 = document.getElementById(submenu).scrollHeight + 'px';
            $('#' + submenu).css('height', '0px');

