<script type="text/javascript">
    $(document).ready(function () {
        var maxHeight = 0;
        var height = 0;
        $('.featured').each(function () {
            height = parseInt($(this).css('height').replace("px", ""));
            if (height > maxHeight)
                maxHeight = height;
        });
        $('.featured').each(function () {
            $(this).css('height', maxHeight + 'px');
        });
    });
</script>

