      <script type="text/javascript">
			    function gov_switch_search_url() {
				$("#gov_search_form").attr("action", "http://discovery.gov.mb.ca/search");
				$("#gov_search_form_ie").attr("value","iso-8859-1");
				$("#gov_search_form_oe").attr("value","iso-8859-1");
				switch ($("#gov_search_form_url").attr("value")) {
					case "manitoba":
						$("#gov_search_form_css").attr("value", "manitoba");
						$("#gov_search_form_client").attr("value", "manitoba");
						$("#gov_search_form_site").attr("value", "default_collection");
						break;
					case "education":
						$("#gov_search_form_css").attr("value", "edu");
						$("#gov_search_form_client").attr("value", "edu");
						$("#gov_search_form_site").attr("value", "edu");

						break;
					default:
						break;
				}
				return true;
			}
		</script>
