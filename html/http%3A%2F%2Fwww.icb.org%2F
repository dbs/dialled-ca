         	//TIMER Function Start- When the page loads wait 5 seconds and the survey button will popout
         	
         		//This setTimeout is for showing the feedback button.
         	//setTimeout(
         			//function show() {
         				
         				//$('.SurveyButton').animate({left: "0px"}, 1000);
         									
                		//},2000);
         	
         	//TIMER Function END- When the page loads wait 5 seconds and the survey button will popout
         	
         	//when the survey button has been clicked.
         	$(".SurveyButton").click(function(){
         		$('#panel1').animate({left: "0px"}, 500);
          		});
         	$("#close").click(function(){
         	
           			$('#panel1').animate({left: "-300px"}, 500);			
         		
          		});
         	
         });
         	
            
      </script>
