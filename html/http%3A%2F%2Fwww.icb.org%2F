<!--Script below is for Back to top Button Start-->
<script>
$(document).ready(function(){
	
	//Check to see if the window is top if not then display button
	$(window).scroll(function(){
		if ($(this).scrollTop() > 800) {
			$('.scrollToTop').fadeIn();
		} else {
			$('.scrollToTop').fadeOut(100);
		}
	});
	
	//Click event to scroll to top
	$('.scrollToTop').click(function(){
		$('html, body').animate({scrollTop : 0},800);
		return false;
	});
	
});
</script>
<!--Script below is for Back to top Button END-->
<script>

	$(document).ready(function(){
		
		
		
		//Product page Slider
		 $('.product_page_slider').bxSlider({
		 	randomStart: true, //Randomize the slides
		 	preloadImages:true, //Load all the images before it plays
			pager: false
		 });
		
		//login page Slider
		 $('.login_page_slider').bxSlider({
		 	randomStart: true, //Randomize the slides
		 	preloadImages:true, //Load all the images before it plays
			pager: false
		 });
		 
		 //designation portal Sliders
		 $('.designationportal_slider').bxSlider({
		 	randomStart: true, //Randomize the slides
		 	preloadImages:true, //Load all the images before it plays
			pager: false,
			infiniteLoop: true, //continue the slide show when you reach the end
			preloadImages:true, //Load all the images before it plays
			speed:1000, //Speed of the transition in (ms)
			controls:false, //If true, "Next" / "Prev" controls will be added
			autoControls: false, //If true, "Start" / "Stop" controls will be added
			auto:true, //Start the slideshow on load
			autoHover:true, //AutoHover will pause when mouse hovers over slide
			pause:10000, //The amount of time (in ms) between each auto transition
			mode:'fade'

		 });
	});
		
	
</script>

