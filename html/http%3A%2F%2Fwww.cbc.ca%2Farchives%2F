){case 27:for(var a=this.nav,i=a.length;i--;)a[i].escape()}},isdefined(function(){return document.body},function(){new GlobalNavController}),function(){"undefined"==typeof CBC&&(CBC={}),CBC.APP||(CBC.APP={});var app=CBC.APP;app.globalnav={personalize:function(o){o.preventDefault&&o.preventDefault();var init=function(Y){app.personalize=new app.Personalize,app.personalize.init(Y,o.tab?{tab:o.tab}:{}),app.Instances.add("personalize",app.personalize),app.globalnav.personalize=function(o){o.preventDefault&&o.preventDefault(),app.personalize.showMenu({},o.tab?{tab:o.tab}:{})}};try{YUI(_CBC_LOADER).use("cbc_personalize",init)}catch(e){}}}}(),isdefined(function(){return document.getElementById("content")||void 0},function(err){if(!err){var e,li,a;e=document.querySelector("#accessibilitylinks ul"),e&&!document.getElementById("skipnav")&&(document.getElementById("content").tabIndex=0,a=document.createElement("a"),a.href="#content",a.id="skipnav",a.onclick=function(){document.getElementById("content").focus()},a.textContent="Skip to main content",li=document.createElement("li"),li.appendChild(a),e.insertBefore(li,e.firstChild))}})}({},function(){return this}());</script>
