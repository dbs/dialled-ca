    <script type="text/javascript">
//<![CDATA[

    $(document).ready (
    function(){
    // Hide sub-nav if no children
    if ($("ul#subNavItems li").size() == 0) {
        $("div#subNav").css('display', 'none');
    }

    // Remove end border from Utility nav
    $("ul#utilityNav li:last-child").addClass("last");

    var firstAnc = $(".subContentItems li a.plus:first");
    showInfoItem(firstAnc, null);

    $(".subContentItems li a.plus, .subContentItems li a.minus").click (
    function() {
       
       var selectedAnc = $(this);
       var openAnc = $(".subContentItems li a.minus");
       if(selectedAnc.attr('class')=='plus') {
               if(openAnc) {
            hideInfoItem(openAnc , null);
           }
            showInfoItem(selectedAnc, 400);
        }
       
       else {
               hideInfoItem(selectedAnc, 400);
       }
       
       }
    );

    }
    );
    function showInfoItem(anc, speed) {
    anc.removeClass("plus").addClass("minus");
    anc.next().slideToggle(speed);
    }
    function hideInfoItem(anc, speed) {
    anc.removeClass("minus").addClass("plus");
    anc.next().slideToggle(speed);
    }

    //]]>
    </script>
