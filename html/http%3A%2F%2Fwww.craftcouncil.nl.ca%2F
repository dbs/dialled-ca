							<div id="cont" class="section"></div><script type="text/javascript">

var eventstartdate=[];
var eventenddate=[];
var eventName=[];
var eventSlug=[];
eventSlug.push('/calendar/date/');

var classstartdate=[];
var classenddate=[];
var className=[];
var classSlug=[];

jQuery(document).ready(function(){

	var LEFT_CAL = Calendar.setup({
	   cont: "cont",
       weekNumbers: false,	                     
       showTime: false,
       dateInfo : getDateInfo,
       fdow: 0
	});	   
			
	function getDateInfo(date, wantsClassName) {

	  var as_number = Calendar.dateToInt(date);
	 
		for (i=0; i<eventstartdate.length; i++){
					
			if ((as_number >= eventstartdate[i]) && (as_number <= eventenddate[i])){
				
				d = eventstartdate[i];
				jQuery('div[dyc-date='+as_number+']').bind("mousedown", function(){
					window.location.href=eventSlug[0]+ as_number +'/';
				});
				return {
			      klass   : "event_day",
			      tooltip :  eventName[i]
			    };
			    
	    	}
	    	//if (as_number == eventenddate[i]){
			//	
			//	d = eventenddate[i];
			//	jQuery('div[dyc-date='+d+']').bind("mousedown", function(){
			//		window.location.href=eventSlug[i]+'/';
			//	});
			//	return {
			//      klass   : "event_day",
			//      tooltip :  eventName[i]
			//    };
			//    
	    	//}
	 	}
	 	
	 	for (i=0; i<classstartdate.length; i++){
					
			if ((as_number >= classstartdate[i]) && (as_number <= classenddate[i])){
				
				d = classstartdate[i];
				jQuery('div[dyc-date='+as_number+']').bind("mousedown", function(){
					window.location.href=classSlug[0]+ as_number +'/';
				});
				return {
			      klass   : "event_day",
			      tooltip :  className[i]
			    };
			    
	    	}
	    	
	 	}
	 	
	};
	
});
		
		
</script>	


