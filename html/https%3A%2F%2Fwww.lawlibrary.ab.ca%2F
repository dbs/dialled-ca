<script type="text/javascript">
    var $jq = jQuery.noConflict();
</script>
 
<script type="text/javascript">
$jq(document).ready(function($) {


	$jq('.hoverMenu').mouseover(function(e) {
		$jq('.hiddenNav').hide();
		$(".hoverMenu").css("background-image", "url(images/arrow2.gif)");
		$(".hoverMenu").css("color", "#006F51");
		$(this).css("background-image", "url(images/arrow.gif)");  
		$(this).css("color", "white");
		
		id=$jq(this).attr('id');
		if (id=="hoverMenu1"){
			$jq('#findLegalHelp').show();
		}else if (id=="hoverMenu2"){
			$jq('#findLegislation').show();
		}else if (id=="hoverMenu3"){
			$jq('#findCases').show();
		}else if (id=="hoverMenu4"){
			$jq('#findArticles').show();
		}else if (id=="hoverMenu5"){
			$jq('#findForms').show();
		}
	});
	
	
	$jq('.hoverMenu').mouseout(function(e) {
		/*$jq('.hiddenNav').hide();*/
	});
	
	//Print button
	$jq("#printLink").click(function(event){
		window.print();
	});
});
</script>




