        function Generate_376cc56ed25c4d0ebe12f79874e3dc01contentPargenerichtmlretriever() { 
            try {
                $j.getJSON("https://util.library.dal.ca/librariesSearch/librariesSearch4AEM.php?ins=on&callback=?" ,function(data) {
                    $j("#content_376cc56ed25c4d0ebe12f79874e3dc01contentPargenerichtmlretriever").html(data.message);
                }); 
            } catch (err) {
                setTimeout('Generate_376cc56ed25c4d0ebe12f79874e3dc01contentPargenerichtmlretriever()',1000);
            }
        }
