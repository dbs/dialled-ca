                <option value="BasicSearch"  data-provider="http://novanet-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do?institution=DAL&vid=DAL&query=any,contains,${q}" >Basic Search</option>
                <option value="Books" data-emptyprovider="http://novanet-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do?institution=DAL&vid=DAL&tab=dal_books&search_scope=NOVANET_BOOKS" data-provider="http://novanet-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do?institution=DAL&vid=DAL&tab=dal_books&search_scope=NOVANET_BOOKS&query=any,contains,${q}" >Books +</option>
                <option value="Articles" data-emptyprovider="http://novanet-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do?institution=DAL&vid=DAL&tab=dal_articles&search_scope=primo_central" data-provider="http://novanet-primo.hosted.exlibrisgroup.com/primo_library/libweb/action/dlSearch.do?institution=DAL&vid=DAL&tab=dal_articles&search_scope=primo_central&query=any,contains,${q}" >Articles +</option>
                <option value="Databases"  data-provider="http://util.library.dal.ca/dmsearch/dmsearch.php?q=1&z=1&t=0&r=2&type=databases&field=bool&keyword=${q}" >Databases</option>
                <option value="eJournals"  data-provider="http://sfxhosted.exlibrisgroup.com/dal/az?param_pattern_value=${q}" >e-Journals</option>
