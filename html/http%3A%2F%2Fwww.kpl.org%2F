	<script>
		(function ($) {
			$(document).ready(function(){
				$('.browseAloud').click(function(e){
					browsealoud.toggleBar();
					e.stopPropagation()
				})
				
				if ($.browser.msie) {
				 $("html").addClass("ie");
				}
				makeAccordionMenu($(".l-content .responsive-menus"))
			})
			
			function makeAccordionMenu(menu){
				menu.find(".responsive-menus-simple > li:not(.leaf, .collapsed )").prepend("<span class='expander'></span>")
				
				menu.find(".responsive-menus-simple > li").addClass("closed");
				
				
				menu.find(".closed > .expander").text("+");
				menu.find(".opened > .expander").text("-");
				
				menu.on("click", ".closed > .expander", function(){
					
					var parent = $(this).closest("li");
					parent.removeClass("closed").find("> ul").slideDown(function(){
						parent.addClass("opened");
					})
					
					$(this).text("-");
				})
				
				menu.on("click", ".opened > .expander", function(){
					var parent = $(this).closest("li");
					parent.find("> ul").slideUp(function(){
						parent.removeClass("opened").addClass("closed");
					})
					$(this).text("+");

				})
			}
			
			
		})(jQuery);
	</script>
	
