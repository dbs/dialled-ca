var newsfeed=new gfeedfetcher("example3", "example3class", "_new")
newsfeed.addFeed("NGPL News Feed", "http://www.rideaulakeslibrary.ca/rss/rss.xml") //Specify "label" plus URL to RSS feed
newsfeed.displayoptions("date, title") //show the specified additional fields
newsfeed.setentrycontainer("p") //Display each entry as a paragraph
newsfeed.filterfeed(5, "date") //Show 5 entries, sort by date
newsfeed.init() //Always call this last


</script>

