    $(document).ready(function(){
        $(".li_active").parent().parent().parent().addClass("true_opened");
        $(".true_opened").parent().find('li:first').removeClass("opened");
        $(".true_opened").addClass("opened");
        if($(".li_active").parent().parent().hasClass("level2")){
            $(".li_active").parent().parent().parent().find('a:first').addClass("active");
        }
        else{
            $(".true_opened").parent().parent().parent().find('a:first').addClass("active");
        }
        var level3_div = $(".level3");
        for(i=0;i<level3_div.length;i++){
            nb_opt_page = $(level3_div[i]).find('li:first').attr("data-nb-pages");
            if(nb_opt_page > 7){
                $(level3_div[i]).addClass("level3-extended");
            }
        }
    });
</script>
