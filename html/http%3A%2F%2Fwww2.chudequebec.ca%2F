<script type="text/javascript">
//<![CDATA[
 /* CREATE THE MAP */
        var map
        var hospitals
        var setMarkers;
        var myCenter = new google.maps.LatLng(46.80350, -71.24000);
        
        function initialize() {
            var mapOptions = {
                disableDefaultUI: true,
                center: myCenter,
                zoom: 13,
                mapTypeId: google.maps.MapTypeId.ROADMAP,

            }

            var map = new google.maps.Map(document.getElementById("map"), mapOptions);
            setMarkers(map, hospitals);
        }
        var hospitals = [{
    "title": "Centre hospitalier de l’Université Laval",
    "lat": 46.769317,
    "lng": -71.282514,
    "description": "<div class='infowWindow'><img src='/CMSPages/GetFile.aspx?guid=445cbcf0-2907-48ce-85db-6cbf66755d70' width='100px' alt='Centre hospitalier de l’Université Laval.' /><div class='rightTXT'><h2>Centre hospitalier de l’Université Laval</h2><p>2705, boulevard Laurier</p><p>Québec, QC</p><p>G1V 4G2</p><i>418 525-4444</i><a href='/centre-hospitaliers/centre-hospitalier-de-l’universite-laval-(chul).aspx'>En savoir plus</a></div></div>"
},{
    "title": "Hôpital de l’Enfant-Jésus",
    "lat": 46.8366127,
    "lng": -71.2256926,
    "description": "<div class='infowWindow'><img src='/CMSPages/GetFile.aspx?guid=f2fb00d5-775a-44b0-b8da-c7037218381c' width='100px' alt='Hôpital de l’Enfant-Jésus.' /><div class='rightTXT'><h2>Hôpital de l’Enfant-Jésus</h2><p>1401, 18e Rue</p><p>Québec, QC</p><p>G1J 1Z4</p><i>418 525-4444</i><a href='/centre-hospitaliers/hopital-de-l-enfant-jesus.aspx'>En savoir plus</a></div></div>"
},{
    "title": "Hôpital du Saint-Sacrement",
    "lat": 46.7993801,
    "lng": -71.2464927,
    "description": "<div class='infowWindow'><img src='/CMSPages/GetFile.aspx?guid=df084663-9f71-4f66-9834-b0d737152c5e' width='100px' alt='Hôpital du Saint-Sacrement.' /><div class='rightTXT'><h2>Hôpital du Saint-Sacrement</h2><p>1050, Ch Ste-Foy</p><p>Québec, QC</p><p>G1S 4L8</p><i>418 525-4444</i><a href='/centre-hospitaliers/hopital-du-saint-sacrement.aspx'>En savoir plus</a></div></div>"
},{
    "title": "Hôpital Saint-François d’Assise",
    "lat": 46.827533,
    "lng": -71.236389,
    "description": "<div class='infowWindow'><img src='/CMSPages/GetFile.aspx?guid=1fb2818a-17bd-4e9a-94bf-866ae61696e0' width='100px' alt='Hôpital Saint-François d’Assise.' /><div class='rightTXT'><h2>Hôpital Saint-François d’Assise</h2><p>10, Rue de l'Espinay</p><p>Québec, QC</p><p>G1L 3L5</p><i>418 525-4444</i><a href='/centre-hospitaliers/hopital-saint-francois-d’assise.aspx'>En savoir plus</a></div></div>"
},{
    "title": "L’Hôtel-Dieu de Québec",
    "lat": 46.815034,
    "lng": -71.21214,
    "description": "<div class='infowWindow'><img src='/CMSPages/GetFile.aspx?guid=b5c2f1a3-2028-4462-a6e0-5e0940e9f54d' width='100px' alt='L’Hôtel-Dieu de Québec.' /><div class='rightTXT'><h2>L’Hôtel-Dieu de Québec</h2><p>11, côte du Palais</p><p>Québec, QC</p><p>G1R 2J6</p><i>418 525-4444</i><a href='/centre-hospitaliers/l’hotel-dieu-de-quebec.aspx'>En savoir plus</a></div></div>"
}  ]
function setMarkers(map, locations) {
            /* Create the icons image */
            var image = '/App_Themes/CHUQ/images/anchorMap.png';

            var infowindow = new google.maps.InfoWindow({
                content: ""
            });

            // Creating all markers 
            for (var i = 0, length = hospitals.length; i < length; i++) {

                var data = hospitals[i];
                var latLng = new google.maps.LatLng(data.lat, data.lng);

                // Creating a marker and putting it on the map
                var marker = new MarkerWithLabel({
                    position: latLng,
                    map: map,
                    title: data.title,
                    icon: image,
                    /*anchorPoint: new google.maps.Point(-260, 20),*/
                    labelContent: "<span class='labelArrows'></span>" + "<div class='labelTXT'>" + data.title + "</div>",
                    labelAnchor: new google.maps.Point(125, -5),
                    labelClass: "labels" // the CSS class for the label
                });

                bindInfoWindow(marker, map, infowindow, data.description);
            }
            /* Open infowWindow */
            google.maps.event.addListener(marker, 'click', function () {
                infowindow.open(map, marker);
            });

            function bindInfoWindow(marker, map, infowindow, description) {
                google.maps.event.addListener(marker, 'click', function () {
                    infowindow.setContent(description);
                    infowindow.open(map, marker);
                });
            }
        }

    //]]>
</script> 
       
