<script type="text/javascript">
    $(document).ready(function () {
        // hide all tabs, unset active tab           
        $('#tabs li').removeClass('active');
        // hide menu unless on hospital tab       
        $('.menued340').hide();
        // set active class on parent
        if (window.location.href.indexOf("/Foundations") > -1)
            $('#tabFoundations').addClass('active');
        else if (window.location.href.indexOf("/Research") > -1)
            $('#tabResearch').addClass('active');
        else if (window.location.href.indexOf("/Education") > -1)
            $('#tabEducation').addClass('active');
        else {
            $('#tabHospital').addClass('active');
            // show menu
            $('.menued340').show();
        }
    });
</script>


