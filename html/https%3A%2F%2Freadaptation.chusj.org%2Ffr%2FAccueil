$(document).ready(function() {

//Menu ON mobile
  $("nav li a").each(function(index,text) {
    $(this).html(function(index,text) {
      if ($(this).attr('href')=='/fr/Accueil') {
        var IDUL = '#'+$(this).closest("ul").attr("id");
