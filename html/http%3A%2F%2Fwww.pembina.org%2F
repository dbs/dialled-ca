function subscribeFormPreSubmit() {
	var oForm = document.subscribeFormPre;

	if (oForm.FNAME.value == "" || oForm.FNAME.value == oForm.FNAME.defaultValue) {
		alert('Please provide your first name.');
		oForm.FNAME.focus();
		return false;
	}
	if (oForm.LNAME.value == "" || oForm.LNAME.value == oForm.LNAME.defaultValue) {
		alert('Please provide your last name.');
		oForm.LNAME.focus();
		return false;
