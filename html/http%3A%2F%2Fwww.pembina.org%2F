function subscribeFormPreSubmit() {
	var oForm = document.subscribeFormPre;

	if (oForm.FNAME.value == "" || oForm.FNAME.value == oForm.FNAME.defaultValue) {
		alert('Please provide your first name.');
		oForm.FNAME.focus();
		return false;
	}
	if (oForm.LNAME.value == "" || oForm.LNAME.value == oForm.LNAME.defaultValue) {
		alert('Please provide your last name.');
		oForm.LNAME.focus();
		return false;
	}	
	if (oForm.EMAIL.value == "" || oForm.EMAIL.value == oForm.EMAIL.defaultValue) {
		alert('Please enter your email address.');
		oForm.EMAIL.focus();
		return false;
	}
	if (oForm.EMAIL.value.indexOf('@') == -1) {
		alert('Please enter a valid email address.');
		oForm.EMAIL.focus();
		return false;
	}
	if (oForm.EMAIL.value.indexOf('.') == -1) {
		alert('Please enter a valid email address.');
		oForm.EMAIL.focus();
		return false;
	}

	oForm.action.value = "subscribe";
	
	var destinationUrl = '/subscription?fname='+oForm.FNAME.value+'&lname='+oForm.LNAME.value+'&email='+oForm.EMAIL.value;
	var ajaxUrl = destinationUrl + '&ajax=true';
	
	var throbber = '<div class="modal-backdrop"><div class="throbber-outer"><div class="throbber">Loading...</div></div></div>';
	
	
	if (!isMobile.any()) { //only do ajax on non-mobile
		$j("body").append(throbber);
		function closeModal() {
			$j(".modal-backdrop").remove();
			$j(document).unbind('keyup.escAjax');				
		}
	
		$j.get(ajaxUrl,function(data) {						
			$j("body").append(data);
			$j(".ajax-close").click(function(){
				 closeModal();
			});
			$j(document).on('keyup.escAjax',function(e) {
				if (e.keyCode == 27) { // escape key maps to keycode `27`
					closeModal();
				}   
			});
			$j(".ajax-content .cancel-button").click(function(){
					closeModal();
			});
		});
	}
			
	else {
		window.location.href = destinationUrl;
	}
	
	return false;
	
}



