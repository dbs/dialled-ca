function loadApp() {

	// Create the flipbook

	$('.flipbook').turn({
			// Width

			width:922,
			
			// Height

			height:600,

			// Elevation

			elevation: 50,
			
			// Enable gradients

			gradients: true,
			
			// Auto center this flipbook

			autoCenter: true

	});
	$(".flipbook").click(function(){
        $(".flipbook-viewport").zoom({
        flipbook: $(".flipbook"),
        max:3
        });
    });
}

// Load the HTML4 version if there's not CSS transform

yepnope({
	test : Modernizr.csstransforms,
	yep: ['lib/turn.js'],
	nope: ['lib/turn.html4.min.js'],
	both: ['lib/zoom.min.js','lib/zoom.js','lib/basic.js','css/basic.css'],
	complete: loadApp
});

</script>
  
