   location.replace("http://www1.gnb.ca/0003/mlibrary.asp?code=FG");
}
else if ((navigator.userAgent.match(/BlackBerry/i)) || (navigator.userAgent.match(/BlackBerry/i))) {
   location.replace("http://www1.gnb.ca/0003/mlibrary.asp?code=FG");
}
else if ((navigator.userAgent.match(/iPhone|iPad|iPod/i)) || (navigator.userAgent.match(/iPhone|iPad|iPod/i))) {
   location.replace("http://www1.gnb.ca/0003/mlibrary.asp?code=FG");
}
else if ((navigator.userAgent.match(/Opera Min/i)) || (navigator.userAgent.match(/Opera Min/i))) {
   location.replace("http://www1.gnb.ca/0003/mlibrary.asp?code=FG");
}
else if ((navigator.userAgent.match(/IEMobil/i)) || (navigator.userAgent.match(/IEMobil/i))) {
   location.replace("http://www1.gnb.ca/0003/mlibrary.asp?code=FG");
}
else {
