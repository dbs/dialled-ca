                <h1 id="content_title"><span id="ctl00_cph_lblTitle" style="display:inline-block;width:100px;">Capilano University Library</span></h1>    
                <div class="clear"></div> 
                <div id="column-zone-1">                     
                
        <div id="ctl00_cph_dzMainContent_uxUpdatePanel" class="">
	
                <div id="ctl00_cph_dzMainContent_uxDropZone" data-ux-pagebuilder="DropZone">
                    
                            <div data-ux-pagebuilder="Column" style='width:730px'>
                                
                                <ul>
                                    
                                            <li>
                                                
        <div id="ctl00_cph_dzMainContent_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxUpdatePanel">
		
                <div data-ux-pagebuilder="Widget">
                    
                    <div class="widgetBody">
                        
<div style="padding: 12px;">
             
           <span id="ctl00_cph_dzMainContent_uxColumnDisplay_ctl00_uxControlColumn_ctl00_uxWidgetHost_uxWidgetHost_widget_TextLabel"><!DOCTYPE html>
<head>
	<title>Capilano University Library services homepage hours display</title>

	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<!-- for HTML 4 -->
	<meta charset="utf-8">
	<!-- for HTML 5 -->
	<meta name="msapplication-tap-highlight" content="no" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content="Display library hours on library homepage for Capilano University Library cloud-based library hours.">
	<meta name="author" content="Glen Greenly">
	<meta name="copyright" content="Copyright(c) 2016 Glen Greenly for Capilano University. All rights reserved. Last rev. 2016-02-10" />

	<link rel="stylesheet" type="text/css" href="https://dl.dropbox.com/s/nixpfiku85fww03/Homepage%20library%20hours%2020150720.css?dl=0">

	<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!--[if lt IE 9]>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js"></script>
	<script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
	<![endif]-->

<!-- Can't use newer JQuery as it conflicts with expectations of other page elements -->
<!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>-->
