<body>

<div>
<span class="borderless-table">
<table class="hours">
	<tr class="hours_headerrow">
    	<td class="hours_leftheader"><a href="http://www.capilanou.ca/library/hours/" style="color: #FFFFFF"><font color="FFFFFF">TODAY'S HOURS</font></a></td>
    	<td class="hours_rightheader" id="xdate"></td>
    </tr>
    <tr class="hours">
		<td class="tdhours" id="libhours"></td>
		<td class="status" id="status_library"></td>
	</tr>
</table>
</span>

<script type="text/javascript">

// JavaScript execution starts here when document is ready ---------------------------------------------------------------------

$.support.cors = true;			// fixes JQuery bug when using CORS on Internet Explorer
now = new Date();				// Global variable used by several functions
	
$(document).ready(function() {
get_JSON_hours();			// GET library hours from cloud, display it, loop every 2 seconds
});

// support functions -----------------------------------------------------------------------------------------------------------

function get_JSON_hours() { // other page element (slideshow) relies on depricated JQ functions in older JQ, so use JS for JSON CORS, not JQ Ajax method in newer JQ
var createCORSRequest = function(e, t) {
        var a = new XMLHttpRequest;
        return "withCredentials" in a ? a.open(e, t, !0) : "undefined" != typeof XDomainRequest ? (a = new XDomainRequest, a.open(e, t)) : a = null, a
    },
    url = "https://api.myjson.com/bins/4ybjg",
    method = "GET",
    xhr = createCORSRequest(method, url);

xhr.onload = function() {
    JSONdata = JSON.parse(xhr.response);
	libdata = JSONdata.hours.lib;
	datetime();									// Validate, format,and display library hours
	setInterval(function() {datetime()}, 2000);	// LOOP to update hours display every 2 seconds
},
xhr.onerror = function() {
	document.getElementById("xdate").innerHTML = gettodaysdate();
	document.getElementById("libhours").innerHTML = gethoursbyhand(now);
},
xhr.send();
};

// -----------------------------------------------------------------------------------------------------------------

function gethoursbyhand(today) {
	var todaysdate=today.getDate();
	var todaysmonth=today.getMonth();
	if (todaysdate==10 && todaysmonth==1) {
		hoursnoJSON="8:00am-5:00pm";
		return (hoursnoJSON);
	};
	if (todaysdate==11 && todaysmonth==1) {
		hoursnoJSON="8:00am-5:00pm";
		return (hoursnoJSON);
	};
	if (todaysdate==12 && todaysmonth==1) {
		hoursnoJSON="8:00am-5:00pm";
		return (hoursnoJSON);
	};
	if (todaysdate==13 && todaysmonth==1) {
		hoursnoJSON="11:00am-6:00pm";
		return (hoursnoJSON);
	};
	if (todaysdate==14 && todaysmonth==1) {
		hoursnoJSON="Closed";
		return (hoursnoJSON);
	};
	if (todaysdate==15 && todaysmonth==1) {
		hoursnoJSON="8am-10pm";
		return (hoursnoJSON);
	};
	if (todaysdate==16 && todaysmonth==1) {
		hoursnoJSON="8am-10pm";
		return (hoursnoJSON);
	}
	else
	var hoursnoJSON="Mon-Thu 8am-10pm; Fri 8am-6pm; Sat 11am-6pm; Sun Closed";
	return(hoursnoJSON);
}

function datetime() {															// display todays library hours and open/closed status
	opencolor = "#0bce50";
	closedcolor = "#f00";

	term = libdata[now.getMonth()][now.getDate()-1][0];							// calculate and display library hours and open status
	text = libdata[now.getMonth()][now.getDate()-1][1];
	h1   = libdata[now.getMonth()][now.getDate()-1][2];
	m1   = libdata[now.getMonth()][now.getDate()-1][3];
	h2   = libdata[now.getMonth()][now.getDate()-1][4];
	m2   = libdata[now.getMonth()][now.getDate()-1][5];
	h3   = libdata[now.getMonth()][now.getDate()-1][6];
	m3   = libdata[now.getMonth()][now.getDate()-1][7];
	h4   = libdata[now.getMonth()][now.getDate()-1][8];
	m4   = libdata[now.getMonth()][now.getDate()-1][9];
	status = "CLOSED";															// default
	libtimestring="CLOSED";														// default

	document.getElementById("xdate").innerHTML = gettodaysdate(now);

	if (validtime(h1,m1) && validtime(h2,m2)) {
 		var time1=ConstructTime(h1,m1);
		var time2=ConstructTime(h2,m2);
		if (time1 <= time2) {
			libtimestring=Time_Display(time1,time2);
			status=getopenstatus_single(now, time1, time2);							// calculate open status using single valid time range
			if (validtime(h3,m3) && validtime(h4,m4)) {
				var time3=ConstructTime(h3,m3);
				var time4=ConstructTime(h4,m4);	
				if ((time2 <= time3) && (time3 <= time4)) {
					libtimestring+="; " + Time_Display(time3,time4);
					status=getopenstatus_double(now, time1, time2, time3, time4);	// re-calculate open status using two valid time ranges 
				}
			}
		}
	}
	if ((text!="x") && (text!="")){
		libtimestring+="  (" + text + ")";
	}

	document.getElementById("xdate").innerHTML = gettodaysdate();
	document.getElementById("libhours").innerHTML = libtimestring;
	document.getElementById("status_library").innerHTML = status;
	document.getElementById("status_library").style.color = (status=="OPEN")? opencolor : closedcolor ;	
}

// -----------------------------------------------------------------------------------------------------------------

function gettodaysdate(){
	var months = [
	"JAN","FEB","MAR","APR","MAY","JUN","JUL","AUG","SEPT","OCT","NOV","DEC"];
	var datetext = months[now.getMonth()] + " " + now.getDate() + ", " + now.getFullYear() + " ";
	return datetext;
};

// -----------------------------------------------------------------------------------------------------------------

function GetTime(date) {
	var currentTime = (new Date(date))
	var hours = currentTime.getHours()
	var suffix = '';
	if (hours > 11) {
			suffix += "pm";
		} else {
			suffix += "am";
		}
		var minutes = currentTime.getMinutes()
		if (minutes < 10) {
			minutes = "0" + minutes
		}
		if (hours > 12) {
			hours -= 12;
		} else if (hours === 0) {
			hours = 12;
		}
		var time = hours + ":" + minutes + suffix;
		return time;
};

// -----------------------------------------------------------------------------------------------------------------

function isInteger(value) {
  return !isNaN(value) && (function(x) { return (x | 0) === x; })(parseFloat(value))
}

// -----------------------------------------------------------------------------------------------------------------------------

function validtime(hour,minute) {
	if (isInteger(hour) && (hour >=0) && (hour <24) && isInteger(minute) && (minute >=0) && (minute <60)) {
		return true
	}
	else {
		return false
	}
}

// -----------------------------------------------------------------------------------------------------------------------------

function ConstructTime(time_h,time_m) {
	var time = new Date();
	time.setHours(time_h);
	time.setMinutes(time_m);
	time.setSeconds(0);
return time;
};

// -----------------------------------------------------------------------------------------------------------------------------

function Time_Display(time_1,time_2) {
	var timerange=GetTime(time_1) + "-" + GetTime(time_2);
return timerange;
};

// -----------------------------------------------------------------------------------------------------------------------------

function getterm(termcode){
	switch(termcode)
		{
		case "S":
				   term="SPRING TERM";
				   break;
		case "I":
				   term="INTERTERM";
				   break;
		case "E":
				   term="EXAM PERIOD";
				   break;
		case "U":
				   term="SUMMER TERM";
				   break;
		case "F":
				   term="FALL TERM";
				   break;
		default:   term="";
				   break;
		}
	return term;
};

// -----------------------------------------------------------------------------------------------------------------------------

function getopenstatus_single(tnow, topen1, tclose1){
	status = ((tnow >= topen1) && (tnow < tclose1)) ? "OPEN" : "CLOSED";
	return status;
};

// -----------------------------------------------------------------------------------------------------------------------------

function getopenstatus_double(tnow, topen1, tclose1, topen2, tclose2){
	status = ((tnow >= topen1) && (tnow < tclose1)) || ((tnow >= topen2) && (tnow < tclose2)) ? "OPEN" : "CLOSED";
	return status;
};

</script>
</div>
</body>
