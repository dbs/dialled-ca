        <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js"></script>
        <script type="text/javascript">
            $(document).ready(function() {
                $('#campus-dir-radio').click(function() {
                    $('#site-radio').removeClass("radio-selected");
                    $('#site-radio').addClass("radio-not-selected");
                    $('#campus-dir-radio').removeClass("radio-not-selected");
                    $('#campus-dir-radio').addClass("radio-selected");
                });
                $('#site-radio').click(function() {
                    $('#campus-dir-radio').removeClass("radio-selected");
                    $('#campus-dir-radio').addClass("radio-not-selected");
                    $('#site-radio').removeClass("radio-not-selected");
                    $('#site-radio').addClass("radio-selected");
                });
                //handles search button
                $("#search-button").click(function() {
                    searchStr = jQuery.trim($("#search-box").val());
                    if (searchStr != "") {
                        if ($("#site-radio").hasClass("radio-selected")) {                            
                            window.location = "/search.aspx?query=" + searchStr;
                        } else if ($("#campus-dir-radio").hasClass("radio-selected")) {
                            window.location = "http://phonebook.capilanou.ca/phonebook/phonebook.serv?search=" + searchStr;
                        }
                    }
                });
                //handles enter key
                $('#aspnetForm').keypress(function(e) {
                    if (e.keyCode == 13) {
                        var searchStr = "";
                        //if on search page...
                        if (location.href.search((/search.aspx/i)) != -1) {
                            if ($("#site-radio").hasClass("radio-selected")) {
                                if (jQuery.trim($("#search-box").val()) != "") {
                                    $('.gsc-input').val(jQuery.trim($("#search-box").val()));
                                    $("#search-box").val("");
                                    $('.gsc-search-button').trigger('click');
                                } else {
                                    $('.gsc-search-button').trigger('click');
                                }
                            } else if ($("#campus-dir-radio").hasClass("radio-selected")) {
                                window.location = "http://phonebook.capilanou.ca/phonebook/phonebook.serv?search=" + jQuery.trim($("#search-box").val());
                            }
                        } else {
                            searchStr = jQuery.trim($("#search-box").val());
                            if (searchStr != "") {
                                if ($("#site-radio").hasClass("radio-selected")) {
                                    window.location = "/search.aspx?query=" + searchStr;
                                } else if ($("#campus-dir-radio").hasClass("radio-selected")) {
                                    window.location = "http://phonebook.capilanou.ca/phonebook/phonebook.serv?search=" + searchStr;
                                }
                            }
                        }
                        return false;
                    }
                });
            });         
        </script>
        <script type="text/javascript">
        function selectReplacement(obj) {
        var ul = document.createElement('ul');
        ul.className = 'selectReplacement';
        // collect our object’s options
        var opts = obj.options;
        // iterate through them, creating <li>s
        for (var i=0; i<opts.length; i++) {
            var li = document.createElement('li');
            var txt = document.createTextNode(opts[i].text);
            li.appendChild(txt);
            ul.appendChild(li);
        }
        // add the ul to the form
        obj.parentNode.appendChild(ul);
        }
        </script>
